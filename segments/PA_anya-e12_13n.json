[
    {
        "segmentnr": "PA_anya-e12_13n:0",
        "original": "Saddabindu pakaraṇaṃ",
        "analyzed": "pakaraṇam ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_13n:1",
        "original": "Namo tassa bhagavato arahato sammāsambuddhassa",
        "analyzed": "namaḥ tassa bhagavataḥ arahataḥ sammāsambuddhassa ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_13n:3",
        "original": "Yassañeyyesu dhammesu, nāṇumattampaveditaṃ,",
        "analyzed": "yassa ñeyyesu dhammesu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_13n:4",
        "original": "Natvāsaddhammasaṅghaṃtaṃ, saddabinduṃsamārabhe;",
        "analyzed": "natvā asat dhamma saṅghaṃtam saddabinduṃsamārabhe ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_13n:6",
        "original": "Kādiritā navasaṅkhyā, kamenaṭā di yādica,",
        "analyzed": "ka ādiḥ itā nava saṅkhyā kamena ṭāḥ di ya ādi ca ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_13n:7",
        "original": "Pādayopañca saṅkhyātā, suññanāmā saraññanā;",
        "analyzed": "saṅkhyātā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_13n:9",
        "original": "Sarehevasarāpubbe luttāvāvīpareramā,",
        "analyzed": "sare heva sarā pubbe lutta āvā vī pare ramā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_13n:10",
        "original": "Byañjanācāgamāvāvī dīgharassādisambhavā;",
        "analyzed": "vyañjana āc āgama āvā āvī dīgha rassa ādi sambhavā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_13n:12",
        "original": "Kākāsenāgatosisa keniddhimaccadassayi,",
        "analyzed": "kākāsena āgata usisa ken iddhi macca dassayi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_13n:13",
        "original": "Arājakhvaggimesīnaṃ sotukammeghayitthiyo;",
        "analyzed": "a rājakhvaggim esīnam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_13n:14",
        "original": "Iti sandhikappo samatto.",
        "analyzed": "iti sandhi kappaḥ samattaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_13n:16",
        "original": "Buddhapumayuvasanta rājabrahmasakhācasā,",
        "analyzed": "rāja brahma sakha acasā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_13n:17",
        "original": "Yatādidehījantuca satthupitābhibhūvidū;",
        "analyzed": "yata ādidehī jantu ca satthu pita abhibhū vidū ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_13n:19",
        "original": "Kaññāmmārattithipo, kkharaṇīnadirumātubhū,",
        "analyzed": "kaññā mmārattithipaḥ kkharaṇī nadiru mātubhū ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_13n:20",
        "original": "Napuṃsaketiyantāca, padakammadadhāyuno;",
        "analyzed": "napuṃsaka iti yantāḥ ca pada kammada dhāyunaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_anya-e12_13n:22",
        "original": "Gahitāgahaṇenettha suddhosyādyantakāpume,",
        "analyzed": "gahitā gahaṇena ittha ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_anya-e12_13n:23",
        "original": "Vimalāhontijāntehi thyaṃpañcantehidādhikā;",
        "analyzed": "thyam pañca ante hi da adhikā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_anya-e12_13n:25",
        "original": "Napuṃsakepayogātu janakāhontityantato,",
        "analyzed": "napuṃsaka ipa yoga tu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_anya-e12_13n:26",
        "original": "Padhānānugatāsabba nāmasamāsataddhitā;",
        "analyzed": "nāma samāsa taddhitā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_anya-e12_13n:28",
        "original": "Attiliṅgānipātādi tatoluttāvasyādayo,",
        "analyzed": "atti liṅga a nipāta ādi tatas lutta avasyā ādayaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_anya-e12_13n:29",
        "original": "Suttānurūpatosiddhā hontivattāmanādayo;",
        "analyzed": "sutta anurūpato siddhāḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_anya-e12_13n:30",
        "original": "Iti nāmakappo samatto.",
        "analyzed": "iti nāmakappo samatto ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_13n:32",
        "original": "Chakārakesasāmismiṃ samāsohotisambhavā,",
        "analyzed": "chakāra kesa sāmismin ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_13n:33",
        "original": "Taddhītākattukammasa, mpadānokāsasāmisu;",
        "analyzed": "tad dhītā kattu kamma sa mpadāna ukāsa sāmisu ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_13n:35",
        "original": "Sādhattayamhiākhyāto kitakosattasādhane,",
        "analyzed": "kita kosatta sādhane ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_13n:36",
        "original": "Sabbatthapaṭhamāvutte avuttedutiyādayo;",
        "analyzed": "sabbattha paṭhama a vutte a vutte dutiya ādayaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_13n:38",
        "original": "Manasāmuninovutyā vanebuddhenavaṇṇite,",
        "analyzed": "vane buddhena vaṇṇite ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_13n:39",
        "original": "Vaṭṭāhitovivaṭṭatthaṃ bhikkhubhāvetibhāvanaṃ;",
        "analyzed": "bhikkhu bhāva iti bhāvanam ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_13n:40",
        "original": "Iti kārakakappo samatto.",
        "analyzed": "iti kāraka kappaḥ samatto ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_13n:42",
        "original": "Rāsīdvipadikādvandā liṅgenavacanenaca,",
        "analyzed": "rāsī dvipadikā dvandā liṅgena vacanena ca ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_13n:43",
        "original": "Luttātulyādhikaraṇā bahubbīhītukhemarū;",
        "analyzed": "lutta a tulya adhikaraṇāḥ bahup bīhī tu khema rū ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_13n:45",
        "original": "Tappurisācakhemorā dayācakammadhārayā,",
        "analyzed": "tappurisāca khema ūrāḥ dayā ca kammadhārayā ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_13n:46",
        "original": "Digavocāvyayāhārā etesabbepihāritā;",
        "analyzed": "diś avoca avyaya āhārāḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_13n:47",
        "original": "Iti samāsakappo samatto.",
        "analyzed": "iti samāsa kappaḥ samatto ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_13n:49",
        "original": "Kaccāditopiekamhā saddatoniyamaṃvinā,",
        "analyzed": "kacca āditaḥ pi ekamhā ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_13n:50",
        "original": "Nekatthesatibhonteva sabbetaddhitapaccayā;",
        "analyzed": "na ika attha isa ti bho ntā iva sabba itaddhita paccayā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_13n:51",
        "original": "Iti taddhitakappo samatto.",
        "analyzed": "iti taddhita kappaḥ samatto ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_13n:53",
        "original": "Kattarināññathākamme tathābhāvetumerayā,",
        "analyzed": "kattari na aññathā kamme tathā bhāvetumerayā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_13n:54",
        "original": "Sabbetepacadhātumhi saṅkhepenamarūmayā;",
        "analyzed": "sabba ete paca dhātumhi saṅkhepena marūmayā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_13n:56",
        "original": "Gamīmhātiguṇāphatto sambhavāaññadhātusu,",
        "analyzed": "sambhava aañña dhātusu ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_13n:57",
        "original": "Anantāvapayogāte ādesapaccayādihi;",
        "analyzed": "ādesa paccaya ādihi ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_13n:58",
        "original": "Iti ākhyātakappo samatto.",
        "analyzed": "iti ākhyāta kappo samatto ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_13n:60",
        "original": "Kitādipaccayāsabbe, ekamhāapidhātuto,",
        "analyzed": "k it ādi paccaya a sabbe eka mhā api dhātutaḥ ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_13n:61",
        "original": "Siyuṃnurūpatosatta, sādhanesatipāyato;",
        "analyzed": "sādhane sati pāyato ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_13n:62",
        "original": "Iti kitakappo samatto.",
        "analyzed": "iti kita kappaḥ samatto ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_13n:64",
        "original": "Iminākiñcilesena, sakkāñātuṃjināgame,",
        "analyzed": "iminā akiñcilesena ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_13n:65",
        "original": "Payogāñāṇināsindhu, rasovekenabindunā;",
        "analyzed": "rasa uvekena bindunā ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_13n:67",
        "original": "Rammaṃsīghappavesāya, puraṃpiṭakasaññitaṃ,",
        "analyzed": "rammaṃsīgha ppavesāya puraṃpiṭaka saññitam ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_13n:68",
        "original": "Maggojumaggataṃmaggaṃ, saddāraññevisodhito;",
        "analyzed": "maggojumaggataṃmaggam ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_13n:70",
        "original": "Teneva kiñci jalito jalito padīpo",
        "analyzed": "tena iva kiñci jalitaḥ jalitaḥ ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:71",
        "original": "Kaccāyanuttiratano citagabbhakoṇe,",
        "analyzed": "cita gabbha koṇe ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:72",
        "original": "Dhammādirājagurunā garumāmakena",
        "analyzed": "dhamma ādi rāja gurunā garu māmakena ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:73",
        "original": "Dhammena yobbipatinā sagaruttanīto;",
        "analyzed": "dhammena yobbi patinā ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:74",
        "original": "Iti saddabindu pakaraṇaṃ parisamattaṃ.",
        "analyzed": "iti sadda bindu pakaraṇam parisamattam ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:75",
        "original": "Yosaññamo guṇadhano nayanaṃ nijaṃva",
        "analyzed": "yosaññamaḥ guṇa dhanaḥ nayanam nijam va ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:76",
        "original": "Sikkhāpayī mama mavaṃ sugatāgamādo,",
        "analyzed": "mama mavam sugata āgamādo ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:77",
        "original": "Salloka puñja suhado padumādi rāma",
        "analyzed": "rāma ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:78",
        "original": "Nāmo mahā yativarā cariyo samayhaṃ;",
        "analyzed": "nāma u mahā yati varā ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:79",
        "original": "Saddhādhanena vasatā viditamhi pupphā",
        "analyzed": "saddhā dhanena vasatā viditamhi pupphā ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_13n:80",
        "original": "Rāmedhunā ariyavaṃsa dhajavhayena,",
        "analyzed": "rāmedhunā ariyavaṃsa dhajavhayena ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_13n:81",
        "original": "Santena ñāṇatilako tyaparākhyakena",
        "analyzed": "santena ña aṇ tilakau tya para ākhyakena ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_13n:82",
        "original": "Bālānametamavidhīyi mayāhitāya;",
        "analyzed": "bālānam etam avidhīyi mayā ahitāya ",
        "folio": "80"
    }
]