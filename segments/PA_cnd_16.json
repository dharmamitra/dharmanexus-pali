    },
    {
        "segmentnr": "PA_cnd_16:8.1",
        "original": "Nānājanā janapadehi saṅgatā,",
        "analyzed": "nānājanā janapadehi saṅga tā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_cnd_16:8.2",
        "original": "Tava vīra vākyaṁ abhikaṅkhamānā;",
        "analyzed": "tava vīra vākyam abhikaṅkhamānā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_cnd_16:8.3",
        "original": "Tesaṁ tuvaṁ sādhu viyākarohi,",
        "analyzed": "tesam tuvam sādhu viyākarohi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_cnd_16:8.4",
        "original": "Tathā hi te vidito esa dhammo.",
