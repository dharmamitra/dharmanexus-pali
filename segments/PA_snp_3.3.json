    },
    {
        "segmentnr": "PA_snp_3.3:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.3:1.2",
        "original": "eka samayaṁ bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "samayam bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.3:1.3",
        "original": "Tatra kho bhagavā bhikkhū āmantesi:",
        "analyzed": "tatra kho bhagavā bhikkhū āmantesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.3:1.4",
        "original": "“bhikkhavo”ti.",
        "analyzed": "bhikkhavaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.3:1.5",
        "original": "“Bhadante”ti te bhikkhū bhagavato paccassosuṁ.",
        "analyzed": "bhadanta iti te bhikkhū bhagavataḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.3:1.6",
        "original": "Bhagavā etadavoca:",
        "analyzed": "bhagavān etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.3:2.1",
        "original": "“Catūhi, bhikkhave, aṅgehi samannāgatā vācā subhāsitā hoti, na dubbhāsitā, anavajjā ca ananuvajjā ca viññūnaṁ.",
        "analyzed": "catūhi bhikkhave aṅgehi samannāgatā vācā su bhāsitā hoti na dubbhāsitā anavajjā ca ananuvajjā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.3:2.2",
        "original": "Katamehi catūhi?",
        "analyzed": "katamehi catūhi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.3:2.3",
        "original": "Idha, bhikkhave, bhikkhu subhāsitaṁyeva bhāsati no dubbhāsitaṁ, dhammaṁyeva bhāsati no adhammaṁ, piyaṁyeva bhāsati no appiyaṁ, saccaṁyeva bhāsati no alikaṁ.",
        "analyzed": "idha bhikkhave bhikkhu su bhāsitayā iva bhāsati no dubbhāsita dhammayā iva bhāsati no adhamma piyayā iva bhāsati no appiya saccayā iva bhāsati no alika ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.3:2.4",
        "original": "Imehi kho, bhikkhave, catūhi aṅgehi samannāgatā vācā subhāsitā hoti, no dubbhāsitā, anavajjā ca ananuvajjā ca viññūnan”ti.",
        "analyzed": "imehi kho bhikkhave catūhi aṅgehi samannāgatā vācā su bhāsitā hoti no dubbhāsitā anavajjā ca ananuvajjā ca viññūnanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.3:2.5",
        "original": "Idamavoca bhagavā.",
        "analyzed": "idam avoca bhagavān ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.3:2.6",
        "original": "Idaṁ vatvāna sugato, athāparaṁ etadavoca satthā:",
        "analyzed": "idam vatvāna sugataḥ atha aparam etat avoca satthā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.3:3.1",
        "original": "“Subhāsitaṁ uttamamāhu santo,",
        "analyzed": "su bhāsitam uttamam āhu santaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.3:3.2",
        "original": "Dhammaṁ bhaṇe nādhammaṁ taṁ dutiyaṁ;",
        "analyzed": "dhammam bhaṇe na adhammam tam dutiya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.3:3.3",
        "original": "Piyaṁ bhaṇe nāppiyaṁ taṁ tatiyaṁ,",
        "analyzed": "piyam bhaṇe na appiyam tam tatiya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.3:3.4",
        "original": "Saccaṁ bhaṇe nālikaṁ taṁ catutthan”ti.",
        "analyzed": "saccam bhaṇe nālikam tam catuttham ti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.3:4.1",
        "original": "Atha kho āyasmā vaṅgīso uṭṭhāyāsanā ekaṁsaṁ cīvaraṁ katvā yena bhagavā tenañjaliṁ paṇāmetvā bhagavantaṁ etadavoca:",
        "analyzed": "atha kho āyasmai vaṅgīsaḥ uṭṭhāya āsanā ekasam cīvaram katvā yena bhagavā tena añjalim paṇāmetvā bhagavantam etat avoca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.3:4.2",
        "original": "“paṭibhāti maṁ, bhagavā, paṭibhāti maṁ, sugatā”ti.",
        "analyzed": "paṭibhāti ma bhagavā paṭibhāti ma sugata ati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.3:4.3",
        "original": "“Paṭibhātu taṁ, vaṅgīsā”ti bhagavā avoca.",
        "analyzed": "paṭibhātu te vaṅgīsāti bhagavā avoca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.3:4.4",
        "original": "Atha kho āyasmā vaṅgīso bhagavantaṁ sammukhā sāruppāhi gāthāhi abhitthavi:",
        "analyzed": "atha kho āyasmā vaṅgīsaḥ bhagavantam sammukhā sāruppāhi gāthāhi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.3:5.1",
        "original": "“Tameva vācaṁ bhāseyya,",
        "analyzed": "tam eva vācam bhāseyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.3:5.2",
        "original": "yāyattānaṁ na tāpaye;",
        "analyzed": "yāyattānam na tāpaye ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.3:5.3",
        "original": "Pare ca na vihiṁseyya,",
        "analyzed": "pare ca na vihiseyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.3:5.4",
        "original": "sā ve vācā subhāsitā.",
        "analyzed": "sā ve vācā su bhāsitā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.3:6.1",
        "original": "Piyavācameva bhāseyya,",
        "analyzed": "piya vācam eva bhāseyya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.3:6.2",
        "original": "yā vācā paṭinanditā;",
        "analyzed": "yā vācā paṭinanditā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.3:6.3",
        "original": "Yaṁ anādāya pāpāni,",
        "analyzed": "yam an ādāya pāpāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.3:6.4",
        "original": "paresaṁ bhāsate piyaṁ.",
        "analyzed": "paresam bhāsate piya ",
        "folio": "6"
    },
    {
