    {
        "segmentnr": "PA_ja_415:0.4",
        "original": "10. Kummāsapiṇḍijātaka",
        "analyzed": "kummāsapiṇḍijātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_415:1.1",
        "original": "“Na kiratthi anomadassisu,",
        "analyzed": "na kiratthi anomadassisu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_415:1.2",
        "original": "Pāricariyā buddhesu appikā;",
        "analyzed": "pāricariyā buddhesu appikā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_415:1.3",
        "original": "Sukkhāya aloṇikāya ca,",
        "analyzed": "sukkhāya a loṇikāya ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_415:1.4",
        "original": "Passaphalaṁ kummāsapiṇḍiyā.",
        "analyzed": "passa phalam kummāsapiṇḍiyā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_415:2.1",
        "original": "Hatthigavassā cime bahū,",
        "analyzed": "hatthi gavassā cime bahū ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_415:2.2",
        "original": "Dhanadhaññaṁ pathavī ca kevalā;",
        "analyzed": "dhana dhaññam pathavī ca kevalā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_415:2.3",
        "original": "Nāriyo cimā accharūpamā,",
        "analyzed": "nāriyaḥ cimāḥ accharūpamā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_415:2.4",
        "original": "Passa phalaṁ kummāsapiṇḍiyā”.",
        "analyzed": "passa phalam kummāsapiṇḍiyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_415:3.1",
        "original": "“Abhikkhaṇaṁ rājakuñjara,",
        "analyzed": "abhikkhaṇam rāja kuñjara ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_415:3.2",
        "original": "Gāthā bhāsasi kosalādhipa;",
        "analyzed": "gāthāḥ bhāsasi kosala adhipa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_415:3.3",
        "original": "Pucchāmi taṁ raṭṭhavaḍḍhana,",
        "analyzed": "pucchāmi tam raṭṭhavaḍḍhana ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_415:3.4",
        "original": "Bāḷhaṁ pītimano pabhāsasi”.",
        "analyzed": "bāḷham pīti mano pabhāsasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_415:4.1",
        "original": "“Imasmiññeva nagare,",
        "analyzed": "imasmin ñeva nagare ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_415:4.2",
        "original": "kule aññatare ahuṁ;",
        "analyzed": "kule aññatare ahu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_415:4.3",
        "original": "Parakammakaro āsiṁ,",
        "analyzed": "para kamma karaḥ āsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_415:4.4",
        "original": "bhatako sīlasaṁvuto.",
        "analyzed": "bhatakaḥ sīla savutaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_415:5.1",
        "original": "Kammāya nikkhamantohaṁ,",
        "analyzed": "kammāya nikkhamantā uha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_415:5.2",
        "original": "caturo samaṇeddasaṁ;",
        "analyzed": "caturaḥ samaṇa iddasa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_415:5.3",
        "original": "Ācārasīlasampanne,",
        "analyzed": "ācāra sīla sampanne ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_415:5.4",
        "original": "sītibhūte anāsave.",
        "analyzed": "sīti bhūte anāsave ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_415:6.1",
        "original": "Tesu cittaṁ pasādetvā,",
        "analyzed": "tesu cittam pasādetvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_415:6.2",
        "original": "nisīdetvā paṇṇasanthate;",
        "analyzed": "nisīdetvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_415:6.3",
        "original": "Adaṁ buddhāna kummāsaṁ,",
        "analyzed": "adam buddhāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_415:6.4",
        "original": "pasanno sehi pāṇibhi.",
        "analyzed": "pasannaḥ sehi pāṇibhi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_415:7.1",
        "original": "Tassa kammassa kusalassa,",
        "analyzed": "tassa kammassa kusalassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_415:7.2",
        "original": "idaṁ me edisaṁ phalaṁ;",
        "analyzed": "idam me edisam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_415:7.3",
        "original": "Anubhomi idaṁ rajjaṁ,",
        "analyzed": "anubhomi idam rajja ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_415:7.4",
        "original": "phītaṁ dharaṇimuttamaṁ”.",
        "analyzed": "phītam dharaṇim uttama ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_415:8.1",
        "original": "“Dadaṁ bhuñja mā ca pamādo,",
        "analyzed": "dadam bhuñja mā ca pamādaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_415:8.2",
        "original": "Cakkaṁ vattaya kosalādhipa;",
        "analyzed": "cakkam vattaya kosala adhipa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_415:8.3",
        "original": "Mā rāja adhammiko ahu,",
        "analyzed": "mā rāja adhammikaḥ ahu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_415:8.4",
        "original": "Dhammaṁ pālaya kosalādhipa”.",
        "analyzed": "dhammam pālaya kosala adhipa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_415:9.1",
        "original": "“Sohaṁ tadeva punappunaṁ,",
        "analyzed": "sā uham tat eva punar punar ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_415:9.2",
        "original": "Vaṭumaṁ ācarissāmi sobhane;",
        "analyzed": "vaṭumam ācarissāmi sobhane ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_415:9.3",
        "original": "Ariyācaritaṁ sukosale,",
        "analyzed": "ariyā caritam sukosale ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_415:9.4",
        "original": "Arahanto me manāpāva passituṁ.",
        "analyzed": "arahantaḥ me manāpā ava passitu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_415:10.1",
        "original": "Devī viya accharūpamā,",
        "analyzed": "devī viya accharūpamā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_415:10.2",
        "original": "Majjhe nārigaṇassa sobhasi;",
        "analyzed": "majjhe nāri gaṇassa sobhasi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_415:10.3",
        "original": "Kiṁ kammamakāsi bhaddakaṁ,",
        "analyzed": "kim bhaddaka ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_415:10.4",
        "original": "Kenāsi vaṇṇavatī sukosale”.",
        "analyzed": "kena asi vaṇṇavatī su kosale ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_415:11.1",
        "original": "“Ambaṭṭhakulassa khattiya,",
        "analyzed": "ambaṭṭha kulassa khattiya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_415:11.2",
        "original": "Dāsyāhaṁ parapesiyā ahuṁ;",
        "analyzed": "dāsyā aham parapesiyā ahu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_415:11.3",
        "original": "Saññatā ca dhammajīvinī,",
        "analyzed": "saññatā ca dhamma jīvinī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_415:11.4",
        "original": "Sīlavatī ca apāpadassanā.",
        "analyzed": "sīlavatī ca a pāpa dassanā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_415:12.1",
        "original": "Uddhaṭabhattaṁ ahaṁ tadā,",
        "analyzed": "aham tadā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_415:12.2",
        "original": "Caramānassa adāsi bhikkhuno;",
        "analyzed": "caramānassa adāsi bhikkhunaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_415:12.3",
        "original": "Vittā sumanā sayaṁ ahaṁ,",
        "analyzed": "vittā sumanā sayam aha ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_415:12.4",
        "original": "Tassa kammassa phalaṁ mamedisan”ti.",
        "analyzed": "tassa kammassa phalam mama īdisanti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_415:13.1",
        "original": "Kummāsapiṇḍijātakaṁ dasamaṁ.",
        "analyzed": "kummāsapiṇḍijātakam dasama ",
        "folio": "13"
    }
]