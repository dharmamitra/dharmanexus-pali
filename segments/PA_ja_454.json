        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_454:19.2",
        "original": "Raja sattaha kassa ca takkalinā;",
        "analyzed": "ca ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_454:19.3",
        "original": "Dhammaṁ kukkuṭa kuṇḍali bhojanadā,",
        "analyzed": "dhammam bhojana dā ",
        "folio": "19"
    },
    {
