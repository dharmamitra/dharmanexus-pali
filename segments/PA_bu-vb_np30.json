        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_np30:2.27",
        "original": "Cetiyassa pariṇataṁ aññacetiyassa vā saṅghassa vā puggalassa vā pariṇāmeti, āpatti dukkaṭassa.",
        "analyzed": "cetiyassa pariṇatam añña cetiyassa vā saṅghassa vā puggalassa vā pariṇāmeti āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_np30:2.28",
        "original": "Puggalassa pariṇataṁ aññapuggalassa vā saṅghassa vā cetiyassa vā pariṇāmeti, āpatti dukkaṭassa.",
        "analyzed": "puggalassa pariṇatam añña puggalassa vā saṅghassa vā cetiyassa vā pariṇāmeti āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_np30:2.29",
        "original": "Apariṇate pariṇatasaññī, āpatti dukkaṭassa.",
        "analyzed": "a pariṇate pariṇata saññī āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_np30:2.30",
        "original": "Apariṇate vematiko, āpatti dukkaṭassa.",
        "analyzed": "a pariṇate vematikaḥ āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_np30:2.31",
        "original": "Apariṇate apariṇatasaññī, anāpatti.",
