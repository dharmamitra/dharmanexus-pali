        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_4.10:2.1",
        "original": "Na hi dhammo adhammo ca,",
        "analyzed": "na hi dhammaḥ adhammaḥ ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_4.10:2.2",
