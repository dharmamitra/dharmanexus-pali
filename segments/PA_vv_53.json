        "analyzed": "mahā ratha vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_53:0.4",
        "original": "3. Chattamāṇavakavimānavatthu",
        "analyzed": "chatta māṇavaka vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_53:1.1",
        "original": "“Yo vadataṁ pavaro manujesu,",
        "analyzed": "yaḥ vadatam pavaraḥ manujesu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_53:1.2",
        "original": "Sakyamunī bhagavā katakicco;",
        "analyzed": "sakyamunī bhagavā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_53:1.3",
        "original": "Pāragato balaviriyasamaṅgī,",
        "analyzed": "pāragataḥ bala viriya samaṅgī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_53:1.4",
        "original": "Taṁ sugataṁ saraṇatthamupehi.",
        "analyzed": "tam sugatam saraṇa attham upehi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_53:2.1",
        "original": "Rāgavirāgamanejamasokaṁ,",
        "analyzed": "rāga virāgam anejam asoka ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_53:2.2",
        "original": "Dhammamasaṅkhatamappaṭikūlaṁ;",
        "analyzed": "dhammam asaṅkhatam appaṭikūla ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_53:2.3",
        "original": "Madhuramimaṁ paguṇaṁ suvibhattaṁ,",
        "analyzed": "madhuram imam paguṇam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_53:2.4",
        "original": "Dhammamimaṁ saraṇatthamupehi.",
        "analyzed": "dhammam imam saraṇa attham upehi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_53:3.1",
        "original": "Yattha ca dinna mahapphalamāhu,",
        "analyzed": "ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_53:3.2",
        "original": "Catūsu sucīsu purisayugesu;",
        "analyzed": "catūsu purisa yugesu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_53:3.3",
        "original": "Aṭṭha ca puggaladhammadasā te,",
        "analyzed": "aṭṭha ca puggala dhamma dasā te ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_53:3.4",
        "original": "Saṅghamimaṁ saraṇatthamupehi.",
        "analyzed": "saṅgham imam saraṇa attham upehi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_53:4.1",
        "original": "Na tathā tapati nabhe sūriyo,",
        "analyzed": "na tathā tapati nabhe sūriyaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_53:4.2",
        "original": "Cando ca na bhāsati na phusso;",
        "analyzed": "candaḥ ca na bhāsati na phussaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_53:4.3",
        "original": "Yathā atulamidaṁ mahappabhāsaṁ,",
        "analyzed": "yathā atulam idam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_53:4.4",
        "original": "Ko nu tvaṁ tidivā mahiṁ upāgā.",
        "analyzed": "kaḥ nu tvam tidivā mahim upāgāḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_53:5.1",
        "original": "Chindati raṁsī pabhaṅkarassa,",
        "analyzed": "chindati rasī pabhaṅkarassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_53:5.2",
        "original": "Sādhikavīsatiyojanāni ābhā;",
        "analyzed": "sādhika vīsati yojanāni ābhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_53:5.3",
        "original": "Rattimapi yathā divaṁ karoti,",
        "analyzed": "rattim api yathā divam karoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_53:5.4",
        "original": "Parisuddhaṁ vimalaṁ subhaṁ vimānaṁ.",
        "analyzed": "parisuddham vimalam subham ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_53:6.1",
        "original": "Bahupadumavicitrapuṇḍarīkaṁ,",
        "analyzed": "bahupaduma vicitrapuṇḍarīka ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_53:6.2",
        "original": "Vokiṇṇaṁ kusumehi nekacittaṁ;",
        "analyzed": "vokiṇṇam kusumehi nekacitta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_53:6.3",
        "original": "Arajavirajahemajālachannaṁ,",
        "analyzed": "araja viraja hema jāla channa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_53:6.4",
        "original": "Ākāse tapati yathāpi sūriyo.",
        "analyzed": "ākāse tapati yathā api sūriyaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_53:7.1",
        "original": "Rattambarapītavāsasāhi,",
        "analyzed": "ratta ambara pīta vāsasā ahi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_53:7.2",
        "original": "Agarupiyaṅgucandanussadāhi;",
        "analyzed": "agaru piyaṅgu candanuḥ sadā ahi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_53:7.3",
        "original": "Kañcanatanusannibhattacāhi,",
        "analyzed": "kañcana tanu sannibha ttaca ahi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_53:7.4",
        "original": "Paripūraṁ gaganaṁva tārakāhi.",
        "analyzed": "paripūram tārakā ahi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_53:8.1",
        "original": "Naranāriyo bahuketthanekavaṇṇā,",
        "analyzed": "nara nāriyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_53:8.2",
        "original": "Kusumavibhūsitābharaṇettha sumanā;",
        "analyzed": "su manāḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_53:8.3",
        "original": "Anilapamuñcitā pavanti surabhiṁ,",
        "analyzed": "anila apamuñcitā pavanti surabhi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_53:8.4",
        "original": "Tapaniyavitatā suvaṇṇachannā.",
        "analyzed": "tapaniya vitatā suvaṇṇa channā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_53:9.1",
        "original": "Kissa saṁyamassa ayaṁ vipāko,",
        "analyzed": "kissa sayamassa ayam vipākaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_53:9.2",
        "original": "Kenāsi kammaphalenidhūpapanno;",
        "analyzed": "kena asi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_53:9.3",
        "original": "Yathā ca te adhigatamidaṁ vimānaṁ,",
        "analyzed": "yathā ca te adhigatam idam vimāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_53:9.4",
        "original": "Tadanupadaṁ avacāsi iṅgha puṭṭho”ti.",
        "analyzed": "tad anupadam avacāsi iṅgha puṭṭha iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_53:10.1",
        "original": "“Sayamidha pathe samecca māṇavena,",
        "analyzed": "sayamidha pathe samet ca māṇavena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_53:10.2",
        "original": "Satthānusāsi anukampamāno;",
        "analyzed": "satthā anusāsi anukampamānaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_53:10.3",
        "original": "Tava ratanavarassa dhammaṁ sutvā,",
        "analyzed": "tava ratana varassa dhammam sutvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_53:10.4",
        "original": "Karissāmīti ca bravittha chatto.",
        "analyzed": "karissāmi iti ca bravittha chatto ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_53:11.1",
        "original": "Jinavarapavaraṁ upehi saraṇaṁ,",
        "analyzed": "jinavara pavaram upehi saraṇa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_53:11.2",
        "original": "Dhammañcāpi tatheva bhikkhusaṅghaṁ;",
        "analyzed": "dhammam ca api tathā iva bhikkhu saṅgha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_53:11.3",
        "original": "Noti paṭhamaṁ avocahaṁ bhante,",
        "analyzed": "no ti paṭhamam avocaham bhante ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_53:11.4",
        "original": "Pacchā te vacanaṁ tathevakāsiṁ.",
        "analyzed": "pacchā te vacanam tathevakā asi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_53:12.1",
        "original": "Mā ca pāṇavadhaṁ vividhaṁ carassu asuciṁ,",
        "analyzed": "mā ca pāṇa vadham vividham carassu asuci ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_53:12.2",
        "original": "Na hi pāṇesu asaññataṁ avaṇṇayiṁsu sappaññā;",
        "analyzed": "na hi pāṇesu asaññatam a vaṇṇayisu sappaññā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_53:12.3",
        "original": "Noti paṭhamaṁ avocahaṁ bhante,",
        "analyzed": "no ti paṭhamam avocaham bhante ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_53:12.4",
        "original": "Pacchā te vacanaṁ tathevakāsiṁ.",
        "analyzed": "pacchā te vacanam tathevakā asi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_53:13.1",
        "original": "Mā ca parajanassa rakkhitampi,",
        "analyzed": "mā ca para janassa rakkhitam pi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_53:13.2",
        "original": "Ādātabbamamaññitho adinnaṁ;",
        "analyzed": "ādātabba mamaññithaḥ adinna ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_53:13.3",
        "original": "Noti paṭhamaṁ avocahaṁ bhante,",
        "analyzed": "no ti paṭhamam avocaham bhante ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_53:13.4",
        "original": "Pacchā te vacanaṁ tathevakāsiṁ.",
        "analyzed": "pacchā te vacanam tathevakā asi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_53:14.1",
        "original": "Mā ca parajanassa rakkhitāyo,",
        "analyzed": "mā ca para janassa rakkhitāyo ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_53:14.2",
        "original": "Parabhariyā agamā anariyametaṁ;",
        "analyzed": "parabhariyāḥ agamāḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_53:14.3",
        "original": "Noti paṭhamaṁ avocahaṁ bhante,",
        "analyzed": "no ti paṭhamam avocaham bhante ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_53:14.4",
        "original": "Pacchā te vacanaṁ tathevakāsiṁ.",
        "analyzed": "pacchā te vacanam tathevakā asi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_53:15.1",
        "original": "Mā ca vitathaṁ aññathā abhāṇi,",
        "analyzed": "mā ca vitatham aññathā abhāṇi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_53:15.2",
        "original": "Na hi musāvādaṁ avaṇṇayiṁsu sappaññā;",
        "analyzed": "na hi musāvādam a vaṇṇayisu sappaññā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_53:15.3",
        "original": "Noti paṭhamaṁ avocahaṁ bhante,",
        "analyzed": "no ti paṭhamam avocaham bhante ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_53:15.4",
        "original": "Pacchā te vacanaṁ tathevakāsiṁ.",
        "analyzed": "pacchā te vacanam tathevakā asi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_53:16.1",
        "original": "Yena ca purisassa apeti saññā,",
        "analyzed": "yena ca purisassa apeti saññā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_53:16.2",
        "original": "Taṁ majjaṁ parivajjayassu sabbaṁ;",
        "analyzed": "tam majjam parivajjayassu sabba ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_53:16.3",
        "original": "Noti paṭhamaṁ avocahaṁ bhante,",
        "analyzed": "no ti paṭhamam avocaham bhante ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_53:16.4",
        "original": "Pacchā te vacanaṁ tathevakāsiṁ.",
        "analyzed": "pacchā te vacanam tathevakā asi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_53:17.1",
        "original": "Svāhaṁ idha pañca sikkhā karitvā,",
        "analyzed": "svāham idha pañca sikkhā karitvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_53:17.2",
        "original": "Paṭipajjitvā tathāgatassa dhamme;",
        "analyzed": "paṭipajjitvā tathāgatassa dhamme ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_53:17.3",
        "original": "Dvepathamagamāsiṁ coramajjhe,",
        "analyzed": "cora majjhe ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_53:17.4",
        "original": "Te maṁ tattha vadhiṁsu bhogahetu.",
        "analyzed": "te mam tattha vadhisu bhoga hetu ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_53:18.1",
        "original": "Ettakamidaṁ anussarāmi kusalaṁ,",
        "analyzed": "ettakam idam anussarāmi kusala ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_53:18.2",
        "original": "Tato paraṁ na me vijjati aññaṁ;",
        "analyzed": "tatas param na me vijjati ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_53:18.3",
        "original": "Tena sucaritena kammunāhaṁ,",
        "analyzed": "tena su caritena kammunā āha ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_53:18.4",
        "original": "Uppanno tidivesu kāmakāmī.",
        "analyzed": "uppannaḥ tidivesu kāma kāmī ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_53:19.1",
        "original": "Passa khaṇamuhuttasaññamassa,",
        "analyzed": "passa khaṇa muhutta saññamassa ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_53:19.2",
        "original": "Anudhammappaṭipattiyā vipākaṁ;",
        "analyzed": "anudhamma paṭipattiyāḥ vipāka ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_53:19.3",
        "original": "Jalamiva yasasā samekkhamānā,",
        "analyzed": "jalam iva yasasā samekkhamānā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_53:19.4",
        "original": "Bahukā maṁ pihayanti hīnakammā.",
        "analyzed": "bahukāḥ mam pihayanti hīna kammā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_53:20.1",
        "original": "Passa katipayāya desanāya,",
        "analyzed": "passa katipayāya desanāya ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_53:20.2",
        "original": "Sugatiñcamhi gato sukhañca patto;",
        "analyzed": "sugatim camhi gataḥ sukham ca pattaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_53:20.3",
        "original": "Ye ca te satataṁ suṇanti dhammaṁ,",
        "analyzed": "ye ca te satatam suṇanti dhamma ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_53:20.4",
        "original": "Maññe te amataṁ phusanti khemaṁ.",
        "analyzed": "maññe te amatam phusanti khema ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_53:21.1",
        "original": "Appampi kataṁ mahāvipākaṁ,",
        "analyzed": "katam mahā vipāka ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_53:21.2",
        "original": "Vipulaṁ hoti tathāgatassa dhamme;",
        "analyzed": "vipulam hoti tathāgatassa dhamme ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_53:21.3",
        "original": "Passa katapuññatāya chatto,",
        "analyzed": "passa kata puñña tāya chatto ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_53:21.4",
        "original": "Obhāseti pathaviṁ yathāpi sūriyo.",
        "analyzed": "pathavim yathā api sūriyaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_53:22.1",
        "original": "Kimidaṁ kusalaṁ kimācarema,",
        "analyzed": "kim idam kusalam kim ācarema ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_53:22.2",
        "original": "Icceke hi samecca mantayanti;",
        "analyzed": "id ca eke hi sam ed ca mantayanti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_53:22.3",
        "original": "Te mayaṁ punareva laddha mānusattaṁ,",
        "analyzed": "te mayam punar eva mānusatta ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_53:22.4",
        "original": "Paṭipannā viharemu sīlavanto.",
        "analyzed": "paṭipannāḥ viharemu sīlavantaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_53:23.1",
        "original": "Bahukāro anukampako ca satthā,",
        "analyzed": "bahu kāraḥ anukampakaḥ ca satthā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_53:23.2",
        "original": "Iti me sati agamā divā divassa;",
        "analyzed": "iti me sati agamā divā divassa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_53:23.3",
        "original": "Svāhaṁ upagatomhi saccanāmaṁ,",
        "analyzed": "svāham upagataḥ asmi sacca nāma ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_53:23.4",
        "original": "Anukampassu punapi suṇemu dhammaṁ.",
        "analyzed": "anukampassu puna api suṇemu dhamma ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_53:24.1",
        "original": "Ye cidha pajahanti kāmarāgaṁ,",
        "analyzed": "ye cidha pajahanti kāma rāga ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_vv_53:24.2",
        "original": "Bhavarāgānusayañca pahāya mohaṁ;",
        "analyzed": "bhava rāga anusayam ca pahāya ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_vv_53:24.3",
        "original": "Na ca te punamupenti gabbhaseyyaṁ,",
        "analyzed": "na ca te punar upenti gabbhaseyya ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_vv_53:24.4",
        "original": "Parinibbānagatā hi sītibhūtā”ti.",
        "analyzed": "parinibbāna gatā hi sīti bhūtā ati ",
        "folio": "24"
    },
