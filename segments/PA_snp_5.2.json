        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.2:3.4",
        "original": "Sotānaṁ saṁvaraṁ brūhi,",
        "analyzed": "sotānam savaram brūhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.2:3.5",
