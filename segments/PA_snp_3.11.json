[
    {
        "segmentnr": "PA_snp_3.11:0.1",
        "original": "Sutta Nipāta 3.11",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.11:0.2",
        "original": "Nālakasutta",
        "analyzed": "nālaka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.11:1.1",
        "original": "Ānandajāte tidasagaṇe patīte,",
        "analyzed": "ānanda jāte ti dasa gaṇe patīte ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.11:1.2",
        "original": "Sakkañca indaṁ sucivasane ca deve;",
        "analyzed": "sakkañcaḥ indam suci vasane ca deve ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.11:1.3",
        "original": "Dussaṁ gahetvā atiriva thomayante,",
        "analyzed": "dussam gahetvā atiriva thomayante ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.11:1.4",
        "original": "Asito isi addasa divāvihāre.",
        "analyzed": "divāvihāre ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.11:2.1",
        "original": "Disvāna deve muditamane udagge,",
        "analyzed": "disvāna deve mudita mane udagge ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.11:2.2",
        "original": "Cittiṁ karitvāna idamavoca tattha;",
        "analyzed": "cittim karitvānaḥ idam avoca tattha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.11:2.3",
        "original": "“Kiṁ devasaṅgho atiriva kalyarūpo,",
        "analyzed": "kim deva saṅghaḥ atiḥ iva kalya rūpaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.11:2.4",
        "original": "Dussaṁ gahetvā ramayatha kiṁ paṭicca.",
        "analyzed": "dussam gahetvā ramayatha kim paṭicca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.11:3.1",
        "original": "Yadāpi āsī asurehi saṅgamo,",
        "analyzed": "yadā api āsī asurehi saṅgamaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.11:3.2",
        "original": "Jayo surānaṁ asurā parājitā;",
        "analyzed": "jaya asurā parājitā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.11:3.3",
        "original": "Tadāpi netādiso lomahaṁsano,",
        "analyzed": "tadā api lomahasanaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.11:3.4",
        "original": "Kimabbhutaṁ daṭṭhu marū pamoditā.",
        "analyzed": "kim abbhutam daṭṭhu marū ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.11:4.1",
        "original": "Seḷenti gāyanti ca vādayanti ca,",
        "analyzed": "seḷenti gāyanti ca vādayanti ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.11:4.2",
        "original": "Bhujāni phoṭenti ca naccayanti ca;",
        "analyzed": "bhujāni phoṭenti ca naccayanti ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.11:4.3",
        "original": "Pucchāmi vohaṁ merumuddhavāsine,",
        "analyzed": "pucchāmi merum uddha vāsine ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.11:4.4",
        "original": "Dhunātha me saṁsayaṁ khippa mārisā”.",
        "analyzed": "dhunātha me sa sayam khippa mārisā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.11:5.1",
        "original": "“So bodhisatto ratanavaro atulyo,",
        "analyzed": "saḥ bodhisattaḥ rata navaraḥ atulyaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.11:5.2",
        "original": "Manussaloke hitasukhatthāya jāto;",
        "analyzed": "manussa loke hita sukha tthāya jātaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.11:5.3",
        "original": "Sakyāna gāme janapade lumbineyye,",
        "analyzed": "sakyāna gāme janapade lumbineyye ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.11:5.4",
        "original": "Tenamha tuṭṭhā atiriva kalyarūpā.",
        "analyzed": "tenamha tuṭṭhāḥ atiḥ iva kalya rūpā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.11:6.1",
        "original": "So sabbasattuttamo aggapuggalo,",
        "analyzed": "so sabba sattuttamaḥ agga puggalo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.11:6.2",
        "original": "Narāsabho sabbapajānamuttamo;",
        "analyzed": "narāsabho sabba pajānam uttamo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.11:6.3",
        "original": "Vattessati cakkamisivhaye vane,",
        "analyzed": "vattessati cakkamisi vhaye vane ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.11:6.4",
        "original": "Nadaṁva sīho balavā migābhibhū”.",
        "analyzed": "sīhaḥ balavā miga abhibhū ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.11:7.1",
        "original": "Taṁ saddaṁ sutvā turitamavasarī so,",
        "analyzed": "tam saddam sutvā saḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.11:7.2",
        "original": "Suddhodanassa tada bhavanaṁ upāvisi;",
        "analyzed": "suddhodanassa bhavanam upāvisi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.11:7.3",
        "original": "Nisajja tattha idamavocāsi sakye,",
        "analyzed": "nisajja tattha idam avocāsi sakye ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.11:7.4",
        "original": "“Kuhiṁ kumāro ahamapi daṭṭhukāmo”.",
        "analyzed": "kuhim kumāraḥ aham api daṭṭhu kāmaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.11:8.1",
        "original": "Tato kumāraṁ jalitamiva suvaṇṇaṁ,",
        "analyzed": "tatas kumāram jalitam iva suvaṇṇa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.11:8.2",
        "original": "Ukkāmukheva sukusalasampahaṭṭhaṁ;",
        "analyzed": "ukkā mukha iva su kusala sampahaṭṭha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.11:8.3",
        "original": "Daddallamānaṁ siriyā anomavaṇṇaṁ,",
        "analyzed": "siriyā anomavaṇṇa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.11:8.4",
        "original": "Dassesu puttaṁ asitavhayassa sakyā.",
        "analyzed": "dassesu puttam asitavhayassa sakyā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.11:9.1",
        "original": "Disvā kumāraṁ sikhimiva pajjalantaṁ,",
        "analyzed": "disvā kumāram sikhim iva ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.11:9.2",
        "original": "Tārāsabhaṁva nabhasigamaṁ visuddhaṁ;",
        "analyzed": "tārāsabhava nabhasigamam visuddha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.11:9.3",
        "original": "Suriyaṁ tapantaṁ saradarivabbhamuttaṁ,",
        "analyzed": "suriyam tapantam ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.11:9.4",
        "original": "Ānandajāto vipulamalattha pītiṁ.",
        "analyzed": "ānanda jātaḥ vipulam alattha pīti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.11:10.1",
        "original": "Anekasākhañca sahassamaṇḍalaṁ,",
        "analyzed": "aneka sākham ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.11:10.2",
        "original": "Chattaṁ marū dhārayumantalikkhe;",
        "analyzed": "chattam marū dhārayumantalikkhe ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.11:10.3",
        "original": "Suvaṇṇadaṇḍā vītipatanti cāmarā,",
        "analyzed": "suvaṇṇa daṇḍāḥ vītipatanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.11:10.4",
        "original": "Na dissare cāmarachattagāhakā.",
        "analyzed": "na dissare cāmara chatta gāhakā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.11:11.1",
        "original": "Disvā jaṭī kaṇhasirivhayo isi,",
        "analyzed": "disvā jaṭī kaṇha sirivhayaḥ isi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.11:11.2",
        "original": "Suvaṇṇanikkhaṁ viya paṇḍukambale;",
        "analyzed": "suvaṇṇa nikkham viya paṇḍu kambale ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.11:11.3",
        "original": "Setañca chattaṁ dhariyanta muddhani,",
        "analyzed": "setam ca chattam muddhani ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.11:11.4",
        "original": "Udaggacitto sumano paṭiggahe.",
        "analyzed": "udagga cittaḥ sumanaḥ paṭiggahe ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.11:12.1",
        "original": "Paṭiggahetvā pana sakyapuṅgavaṁ,",
        "analyzed": "paṭiggahetvā pana sakya puṅgava ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.11:12.2",
        "original": "Jigīsako lakkhaṇamantapāragū;",
        "analyzed": "jigīsakaḥ lakkhaṇa manta pāragū ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.11:12.3",
        "original": "Pasannacitto giramabbhudīrayi,",
        "analyzed": "pasanna cittaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.11:12.4",
        "original": "“Anuttarāyaṁ dvipadānamuttamo”.",
        "analyzed": "an uttarāyam ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.11:13.1",
        "original": "Athattano gamanamanussaranto,",
        "analyzed": "athattanaḥ gamanam anussaranto ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.11:13.2",
        "original": "Akalyarūpo gaḷayati assukāni;",
        "analyzed": "akalya rūpaḥ gaḷayati assukāni ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.11:13.3",
        "original": "Disvāna sakyā isimavocuṁ rudantaṁ,",
        "analyzed": "disvāna sakyāḥ isim avocum rudanta ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.11:13.4",
        "original": "“No ce kumāre bhavissati antarāyo”.",
        "analyzed": "no ce kumāre bhavissati antarāyaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.11:14.1",
        "original": "Disvāna sakye isimavoca akalye,",
        "analyzed": "disvāna sakye isim avoca akalye ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.11:14.2",
        "original": "“Nāhaṁ kumāre ahitamanussarāmi;",
        "analyzed": "na aham kumāre ahitam anussarāmi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.11:14.3",
        "original": "Na cāpimassa bhavissati antarāyo,",
        "analyzed": "na ca api imassa bhavissati antarāyaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.11:14.4",
        "original": "Na orakāyaṁ adhimānasā bhavātha.",
        "analyzed": "na orakāyam adhimānasāḥ bhavātha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.11:15.1",
        "original": "Sambodhiyaggaṁ phusissatāyaṁ kumāro,",
        "analyzed": "sambodhi yaggam phusissatā ayam kumāraḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.11:15.2",
        "original": "So dhammacakkaṁ paramavisuddhadassī;",
        "analyzed": "so dhammacakkam parama visuddha dassī ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.11:15.3",
        "original": "Vattessatāyaṁ bahujanahitānukampī,",
        "analyzed": "vatteḥ satā ayam bahu jana hita anukampī ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.11:15.4",
        "original": "Vitthārikassa bhavissati brahmacariyaṁ.",
        "analyzed": "vitthārikassa bhavissati brahmacariya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.11:16.1",
        "original": "Mamañca āyu na ciramidhāvaseso,",
        "analyzed": "mamañcaḥ āyu na ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.11:16.2",
        "original": "Athantarā me bhavissati kālakiriyā;",
        "analyzed": "athantarā me bhavissati kālakiriyā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.11:16.3",
        "original": "Sohaṁ na sossaṁ asamadhurassa dhammaṁ,",
        "analyzed": "na sossam a sama dhurassa dhamma ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.11:16.4",
        "original": "Tenamhi aṭṭo byasanaṅgato aghāvī”.",
        "analyzed": "aṭṭaḥ byasanaṅgataḥ aghāvī ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.11:17.1",
        "original": "So sākiyānaṁ vipulaṁ janetvā pītiṁ,",
        "analyzed": "saḥ sākiyānam vipulam janetvā pīti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.11:17.2",
        "original": "Antepuramhā niggamā brahmacārī;",
        "analyzed": "antepuramhā niggamā brahmacārī ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.11:17.3",
        "original": "So bhāgineyyaṁ sayaṁ anukampamāno,",
        "analyzed": "saḥ bhāgineyyam sayam anukampamānaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.11:17.4",
        "original": "Samādapesi asamadhurassa dhamme.",
        "analyzed": "samādapesi a sama dhurassa dhamme ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.11:18.1",
        "original": "“Buddhoti ghosaṁ yada parato suṇāsi,",
        "analyzed": "buddha uti ghosam paratas suṇāsi ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.11:18.2",
        "original": "Sambodhipatto vivarati dhammamaggaṁ;",
        "analyzed": "sambodhi pattaḥ vivarati dhamma magga ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.11:18.3",
        "original": "Gantvāna tattha samayaṁ paripucchamāno,",
        "analyzed": "gantvāna tattha samayam paripucchamānaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.11:18.4",
        "original": "Carassu tasmiṁ bhagavati brahmacariyaṁ”.",
        "analyzed": "carassu tasmin bhagavati brahmacariya ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.11:19.1",
        "original": "Tenānusiṭṭho hitamanena tādinā,",
        "analyzed": "tena anusiṭṭhaḥ hitam anena tādinā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.11:19.2",
        "original": "Anāgate paramavisuddhadassinā;",
        "analyzed": "anāgate parama visuddha dassinā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.11:19.3",
        "original": "So nālako upacitapuññasañcayo,",
        "analyzed": "so nālako upacita puñña sañcayo ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.11:19.4",
        "original": "Jinaṁ patikkhaṁ parivasi rakkhitindriyo.",
        "analyzed": "jinam patikkham parivasi rakkhitindriyaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.11:20.1",
        "original": "Sutvāna ghosaṁ jinavaracakkavattane,",
        "analyzed": "sutvāna ghosam jina vara cakkavattane ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.11:20.2",
        "original": "Gantvāna disvā isinisabhaṁ pasanno;",
        "analyzed": "gantvāna disvā isinisabham pasannaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.11:20.3",
        "original": "Moneyyaseṭṭhaṁ munipavaraṁ apucchi,",
        "analyzed": "moneyya seṭṭham muni pavaram apucchi ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.11:20.4",
        "original": "Samāgate asitāvhayassa sāsaneti.",
        "analyzed": "samāgate asitāvhayassa sāsane iti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.11:20.5",
        "original": "Vatthugāthā niṭṭhitā.",
        "analyzed": "vatthu gāthā niṭṭhitā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.11:21.1",
        "original": "“Aññātametaṁ vacanaṁ,",
        "analyzed": "aññātam etam vacana ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.11:21.2",
        "original": "Asitassa yathātathaṁ;",
        "analyzed": "asitassa yathātatha ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.11:21.3",
        "original": "Taṁ taṁ gotama pucchāmi,",
        "analyzed": "tam tam gotama pucchāmi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.11:21.4",
        "original": "Sabbadhammāna pāraguṁ.",
        "analyzed": "sabba dhammāna pāragu ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.11:22.1",
        "original": "Anagāriyupetassa,",
        "analyzed": "anagāriya upetassa ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.11:22.2",
        "original": "Bhikkhācariyaṁ jigīsato;",
        "analyzed": "bhikkhā cariyam jigīsato ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.11:22.3",
        "original": "Muni pabrūhi me puṭṭho,",
        "analyzed": "muni pabrūhi me puṭṭhaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.11:22.4",
        "original": "Moneyyaṁ uttamaṁ padaṁ”.",
        "analyzed": "moneyyam uttamam ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.11:23.1",
        "original": "“Moneyyaṁ te upaññissaṁ,",
        "analyzed": "moneyyam te upaññissa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.11:23.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.11:23.3",
        "original": "Dukkaraṁ durabhisambhavaṁ;",
        "analyzed": "dukkaram durabhisambhava ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.11:23.4",
        "original": "Handa te naṁ pavakkhāmi,",
        "analyzed": "handa te nam pavakkhāmi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.11:23.5",
        "original": "Santhambhassu daḷho bhava.",
        "analyzed": "santhambhaḥsu daḷhaḥ bhava ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.11:24.1",
        "original": "Samānabhāgaṁ kubbetha,",
        "analyzed": "samāna bhāgam kubbetha ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.11:24.2",
        "original": "Gāme akkuṭṭhavanditaṁ;",
        "analyzed": "gāme akkuṭṭha vandita ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.11:24.3",
        "original": "Manopadosaṁ rakkheyya,",
        "analyzed": "manaḥ upadosam rakkheyya ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.11:24.4",
        "original": "Santo anuṇṇato care.",
        "analyzed": "santaḥ an uṇṇataḥ care ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.11:25.1",
        "original": "Uccāvacā niccharanti,",
        "analyzed": "uccāvacāḥ niccharanti ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.11:25.2",
        "original": "Dāye aggisikhūpamā;",
        "analyzed": "dāye aggi sikhu upamā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.11:25.3",
        "original": "Nāriyo muniṁ palobhenti,",
        "analyzed": "nāriyaḥ munim palobhenti ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.11:25.4",
        "original": "Tā su taṁ mā palobhayuṁ.",
        "analyzed": "tā su tam mā palobhayu ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.11:26.1",
        "original": "Virato methunā dhammā,",
        "analyzed": "virataḥ methunā dhammā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.11:26.2",
        "original": "Hitvā kāme paropare;",
        "analyzed": "hitvā kāme para upare ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.11:26.3",
        "original": "Aviruddho asāratto,",
        "analyzed": "a viruddhaḥ a sārattaḥ ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.11:26.4",
        "original": "Pāṇesu tasathāvare.",
        "analyzed": "pāṇesu tasa tha avare ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.11:27.1",
        "original": "Yathā ahaṁ tathā ete,",
        "analyzed": "yathā aham tathā ete ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.11:27.2",
        "original": "Yathā ete tathā ahaṁ;",
        "analyzed": "yathā ete tathā aha ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.11:27.3",
        "original": "Attānaṁ upamaṁ katvā,",
        "analyzed": "attānam upamam katvā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.11:27.4",
        "original": "Na haneyya na ghātaye.",
        "analyzed": "na haneyya na ghātaye ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.11:28.1",
        "original": "Hitvā icchañca lobhañca,",
        "analyzed": "hitvā icchan ca lobhan ca ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.11:28.2",
        "original": "Yattha satto puthujjano;",
        "analyzed": "yattha satto puthujjanaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.11:28.3",
        "original": "Cakkhumā paṭipajjeyya,",
        "analyzed": "cakkhumā paṭipajjeyya ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.11:28.4",
        "original": "Tareyya narakaṁ imaṁ.",
        "analyzed": "tareyya narakam ima ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.11:29.1",
        "original": "Ūnūdaro mitāhāro,",
        "analyzed": "ūnūdaraḥ mita āhāraḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.11:29.2",
        "original": "Appicchassa alolupo;",
        "analyzed": "appicchassa a lolupaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.11:29.3",
        "original": "Sadā icchāya nicchāto,",
        "analyzed": "sadā icchāya nicchāto ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.11:29.4",
        "original": "Aniccho hoti nibbuto.",
        "analyzed": "anicchaḥ hoti nibbutaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.11:30.1",
        "original": "Sa piṇḍacāraṁ caritvā,",
        "analyzed": "sa piṇḍacāram caritvā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.11:30.2",
        "original": "Vanantamabhihāraye;",
        "analyzed": "vanantam abhihāraye ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.11:30.3",
        "original": "Upaṭṭhito rukkhamūlasmiṁ,",
        "analyzed": "upaṭṭhitaḥ rukkha mūlasmi ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.11:30.4",
        "original": "Āsanūpagato muni.",
        "analyzed": "āsanūpagataḥ muni ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.11:31.1",
        "original": "Sa jhānapasuto dhīro,",
        "analyzed": "sa jhānapa sutaḥ dhīraḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.11:31.2",
        "original": "Vanante ramito siyā;",
        "analyzed": "vanante ramitaḥ siyā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.11:31.3",
        "original": "Jhāyetha rukkhamūlasmiṁ,",
        "analyzed": "jhāyetha rukkha mūlasmi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.11:31.4",
        "original": "Attānamabhitosayaṁ.",
        "analyzed": "attānam abhitosaya ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.11:32.1",
        "original": "Tato ratyā vivasāne,",
        "analyzed": "tatas ratyā vivasāne ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.11:32.2",
        "original": "Gāmantamabhihāraye;",
        "analyzed": "gām antam abhihāraye ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.11:32.3",
        "original": "Avhānaṁ nābhinandeyya,",
        "analyzed": "a vhānam na abhinandeyya ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.11:32.4",
        "original": "Abhihārañca gāmato.",
        "analyzed": "abhihārañca gāmataḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.11:33.1",
        "original": "Na munī gāmamāgamma,",
        "analyzed": "na munī ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.11:33.2",
        "original": "Kulesu sahasā care;",
        "analyzed": "kulesu sahasā care ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.11:33.3",
        "original": "Ghāsesanaṁ chinnakatho,",
        "analyzed": "ghāsesanam chinnakathaḥ ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.11:33.4",
        "original": "Na vācaṁ payutaṁ bhaṇe.",
        "analyzed": "na vācam payutam bhaṇe ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.11:34.1",
        "original": "Alatthaṁ yadidaṁ sādhu,",
        "analyzed": "alattham yat idam sādhu ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.11:34.2",
        "original": "Nālatthaṁ kusalaṁ iti;",
        "analyzed": "na alattham kusalam iti ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.11:34.3",
        "original": "Ubhayeneva so tādī,",
        "analyzed": "ubhayena iva saḥ tādī ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.11:34.4",
        "original": "Rukkhaṁvupanivattati.",
        "analyzed": "rukkhau upanivattati ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.11:35.1",
        "original": "Sa pattapāṇi vicaranto,",
        "analyzed": "sa patta pāṇi vicarantaḥ ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.11:35.2",
        "original": "Amūgo mūgasammato;",
        "analyzed": "a mūgaḥ mūga sammataḥ ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.11:35.3",
        "original": "Appaṁ dānaṁ na hīḷeyya,",
        "analyzed": "appam dānam na hi īḷeyya ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.11:35.4",
        "original": "Dātāraṁ nāvajāniyā.",
        "analyzed": "dātāram na avajāniyā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.11:36.1",
        "original": "Uccāvacā hi paṭipadā,",
        "analyzed": "uccāvacā hi paṭipadā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.11:36.2",
        "original": "Samaṇena pakāsitā;",
        "analyzed": "samaṇena pakāsitā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.11:36.3",
        "original": "Na pāraṁ diguṇaṁ yanti,",
        "analyzed": "na pāram diguṇam yanti ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.11:36.4",
        "original": "Nayidaṁ ekaguṇaṁ mutaṁ.",
        "analyzed": "na yidam eka guṇam muta ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.11:37.1",
        "original": "Yassa ca visatā natthi,",
        "analyzed": "yassa ca visatā natthi ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.11:37.2",
        "original": "Chinnasotassa bhikkhuno;",
        "analyzed": "chinna sotassa bhikkhuno ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.11:37.3",
        "original": "Kiccākiccappahīnassa,",
        "analyzed": "kicca akicca ppahīnassa ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.11:37.4",
        "original": "Pariḷāho na vijjati.",
        "analyzed": "pariḷāhaḥ na vijjati ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.11:38.1",
        "original": "Moneyyaṁ te upaññissaṁ,",
        "analyzed": "moneyyam te upaññissa ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.11:38.2",
        "original": "Khuradhārūpamo bhave;",
        "analyzed": "khura dhārū upamaḥ bhave ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.11:38.3",
        "original": "Jivhāya tālumāhacca,",
        "analyzed": "jivhāya tālum āhat ca ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.11:38.4",
        "original": "Udare saññato siyā.",
        "analyzed": "udare saññataḥ siyā ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.11:39.1",
        "original": "Alīnacitto ca siyā,",
        "analyzed": "a līna cittaḥ ca siyā ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.11:39.2",
        "original": "Na cāpi bahu cintaye;",
        "analyzed": "na ca api bahu cintaye ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.11:39.3",
        "original": "Nirāmagandho asito,",
        "analyzed": "nirāma gandhaḥ asitaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.11:39.4",
        "original": "Brahmacariyaparāyaṇo.",
        "analyzed": "brahmacariya parāyaṇaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.11:40.1",
        "original": "Ekāsanassa sikkhetha,",
        "analyzed": "eka āsanassa sikkhetha ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.11:40.2",
        "original": "Samaṇūpāsanassa ca;",
        "analyzed": "samaṇu upāsanassa ca ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.11:40.3",
        "original": "Ekattaṁ monamakkhātaṁ,",
        "analyzed": "ekattam mā ūnam akkhāta ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.11:40.4",
        "original": "Eko ce abhiramissasi;",
        "analyzed": "ekaḥ ce abhiramissasi ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.11:40.5",
        "original": "Atha bhāhisi dasadisā.",
        "analyzed": "atha bhāhisi dasa disā ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.11:41.1",
        "original": "Sutvā dhīrānaṁ nighosaṁ,",
        "analyzed": "sutvā dhīrānam nighosa ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_3.11:41.2",
        "original": "Jhāyīnaṁ kāmacāginaṁ;",
        "analyzed": "jhāyīnam kāmacāgina ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_3.11:41.3",
        "original": "Tato hiriñca saddhañca,",
        "analyzed": "tatas hiriñca saddhan ca ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_3.11:41.4",
        "original": "Bhiyyo kubbetha māmako.",
        "analyzed": "bhiyyas kubbetha māmakaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_3.11:42.1",
        "original": "Taṁ nadīhi vijānātha,",
        "analyzed": "tam nadīhi vijānātha ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.11:42.2",
        "original": "Sobbhesu padaresu ca;",
        "analyzed": "sa ubbhesu padaresu ca ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.11:42.3",
        "original": "Saṇantā yanti kusobbhā,",
        "analyzed": "saṇantāḥ yanti kusobbhā ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.11:42.4",
        "original": "Tuṇhī yanti mahodadhī.",
        "analyzed": "tuṇhī yanti mahā udadhī ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.11:43.1",
        "original": "Yadūnakaṁ taṁ saṇati,",
        "analyzed": "yat ūnakam tam ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.11:43.2",
        "original": "Yaṁ pūraṁ santameva taṁ;",
        "analyzed": "yam pūram santam eva te ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.11:43.3",
        "original": "Aḍḍhakumbhūpamo bālo,",
        "analyzed": "aḍḍha kumbha upamaḥ bālaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.11:43.4",
        "original": "Rahado pūrova paṇḍito.",
        "analyzed": "paṇḍito ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.11:44.1",
        "original": "Yaṁ samaṇo bahuṁ bhāsati,",
        "analyzed": "yam samaṇaḥ bahum bhāsati ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.11:44.2",
        "original": "Upetaṁ atthasañhitaṁ;",
        "analyzed": "upetam attha sañhita ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.11:44.3",
        "original": "Jānaṁ so dhammaṁ deseti,",
        "analyzed": "jānam saḥ dhammaṃ deseti ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.11:44.4",
        "original": "Jānaṁ so bahu bhāsati.",
        "analyzed": "jānam saḥ bahu bhāsati ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.11:45.1",
        "original": "Yo ca jānaṁ saṁyatatto,",
        "analyzed": "yaḥ ca jānam sayatattaḥ ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.11:45.2",
        "original": "Jānaṁ na bahu bhāsati;",
        "analyzed": "jānam na bahu bhāsati ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.11:45.3",
        "original": "Sa munī monamarahati,",
        "analyzed": "sa munī ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.11:45.4",
        "original": "Sa munī monamajjhagā”ti.",
        "analyzed": "sa munī monam ajjhagāti ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.11:45.5",
        "original": "Nālakasuttaṁ ekādasamaṁ.",
        "analyzed": "nālaka suttam ekādasama ",
        "folio": "45"
    }
]