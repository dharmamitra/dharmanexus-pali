        "folio": "56"
    },
    {
        "segmentnr": "PA_ja_526:56.4",
        "original": "Āsajja naṁ nassati brahmacārī”ti.",
        "analyzed": "āsajja nam brahmacārī iti ",
        "folio": "56"
    },
    {
