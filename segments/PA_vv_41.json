    },
    {
        "segmentnr": "PA_vv_41:1.1",
        "original": "“Alaṅkatā maṇikañcanācitaṁ,",
        "analyzed": "alaṅkatā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_41:1.2",
        "original": "Sovaṇṇajālacitaṁ mahantaṁ;",
        "analyzed": "sovaṇṇa jāla citam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_41:1.3",
        "original": "Abhiruyha gajavaraṁ sukappitaṁ,",
        "analyzed": "abhiruyha gaja varam su kappita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_41:1.4",
        "original": "Idhāgamā vehāyasaṁ antalikkhe.",
        "analyzed": "idha āgamā vehāyasam antalikkhe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_41:2.1",
        "original": "Nāgassa dantesu duvesu nimmitā,",
        "analyzed": "nāgassa dantesu duvesu nimmitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_41:2.2",
        "original": "Acchodakā paduminiyo suphullā;",
        "analyzed": "accha udakā paduminiyaḥ su phullā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_41:2.3",
        "original": "Padumesu ca tūriyagaṇā pabhijjare,",
        "analyzed": "padumesu ca tūriya gaṇā pabhijjare ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_41:2.4",
        "original": "Imā ca naccanti manoharāyo.",
        "analyzed": "imā ca naccanti manoharāyo ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_41:3.1",
        "original": "Deviddhipattāsi mahānubhāve,",
        "analyzed": "devi iddhipattā asi mahā anubhāve ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_41:3.2",
        "original": "Manussabhūtā kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtā kim akāsi puñña ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_41:3.3",
        "original": "Kenāsi evaṁ jalitānubhāvā,",
        "analyzed": "kena asi evam jalita anubhāvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_41:3.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_41:4.1",
        "original": "“Bārāṇasiyaṁ upasaṅkamitvā,",
        "analyzed": "bārāṇasiyam upasaṅkamitvā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_41:4.2",
        "original": "Buddhassahaṁ vatthayugaṁ adāsiṁ;",
        "analyzed": "buddhassaham vattha yugam adāsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_41:4.3",
        "original": "Pādāni vanditvā chamā nisīdiṁ,",
        "analyzed": "pādāni vanditvā chamā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_41:4.4",
        "original": "Vittā cahaṁ añjalikaṁ akāsiṁ.",
        "analyzed": "vittā ca ham añjalikam akāsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_41:5.1",
        "original": "Buddho ca me kañcanasannibhattaco,",
        "analyzed": "buddhaḥ ca me kañcana sannibha tacaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_41:5.2",
        "original": "Adesayi samudayadukkhaniccataṁ;",
        "analyzed": "samudaya dukkha nicca ta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_41:5.3",
        "original": "Asaṅkhataṁ dukkhanirodhasassataṁ,",
        "analyzed": "asaṅkhatam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_41:5.4",
        "original": "Maggaṁ adesayi yato vijānisaṁ.",
        "analyzed": "maggam adesayi yatas vijānisa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_41:6.1",
        "original": "Appāyukī kālakatā tato cutā,",
        "analyzed": "appāyukī kālakatā tato cutā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_41:6.2",
        "original": "Upapannā tidasagaṇaṁ yasassinī;",
        "analyzed": "upapannā tidasa gaṇam yasassinī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_41:6.3",
        "original": "Sakkassahaṁ aññatarā pajāpati,",
        "analyzed": "sakkassaham aññatarā pajāpati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_41:6.4",
        "original": "Yasuttarā nāma disāsu vissutā”ti.",
        "analyzed": "yasuttarā nāma disāsu vissutā ati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_41:7.1",
        "original": "Nāgavimānaṁ tatiyaṁ.",
        "analyzed": "nāga vimānam tatiya ",
        "folio": "7"
    }
]