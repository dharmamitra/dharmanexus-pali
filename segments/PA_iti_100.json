        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_100:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_100:2.1",
        "original": "“Ahamasmi, bhikkhave, brāhmaṇo yācayogo sadā payatapāṇi antimadehadharo anuttaro bhisakko sallakatto.",
        "analyzed": "aham asmi bhikkhave brāhmaṇaḥ yāca yogaḥ sadā payata pāṇi antima deha dharaḥ anuttaraḥ bhisakkaḥ sallakatto ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_100:2.2",
        "original": "Tassa me tumhe puttā orasā mukhato jātā dhammajā dhammanimmitā dhammadāyādā, no āmisadāyādā.",
        "analyzed": "tassa me tumhe puttāḥ orasāḥ mukhataḥ jātāḥ dhamma jāḥ dhamma nimmitāḥ dhamma dāyādāḥ no āmisa dāyādā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_100:3.1",
        "original": "Dvemāni, bhikkhave, dānāni—",
        "analyzed": "dve imāni bhikkhave dānāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_100:3.2",
        "original": "āmisadānañca dhammadānañca.",
        "analyzed": "āmisa dānañca dhamma dānañca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_100:3.3",
        "original": "Etadaggaṁ, bhikkhave, imesaṁ dvinnaṁ dānānaṁ yadidaṁ dhammadānaṁ.",
        "analyzed": "etat agga bhikkhave imesam dvinnam dānānam yat idam dhamma dāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_100:4.1",
        "original": "Dveme, bhikkhave, saṁvibhāgā—",
        "analyzed": "dve me bhikkhave savibhāgā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_100:4.2",
        "original": "āmisasaṁvibhāgo ca dhammasaṁvibhāgo ca.",
        "analyzed": "āmisa savibhāgo ca dhamma savibhāgo ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_100:4.3",
        "original": "Etadaggaṁ, bhikkhave, imesaṁ dvinnaṁ saṁvibhāgānaṁ yadidaṁ dhammasaṁvibhāgo.",
        "analyzed": "etat agga bhikkhave imesam dvinnam savibhāgānam yat idam dhamma savibhāgaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_100:5.1",
        "original": "Dveme, bhikkhave, anuggahā—",
        "analyzed": "dve me bhikkhave anuggahā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_100:5.2",
        "original": "āmisānuggaho ca dhammānuggaho ca.",
        "analyzed": "āmisa anuggahaḥ ca dhamma anuggahaḥ ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_100:5.3",
        "original": "Etadaggaṁ, bhikkhave, imesaṁ dvinnaṁ anuggahānaṁ yadidaṁ dhammānuggaho.",
        "analyzed": "etat agga bhikkhave imesam dvinnam anuggahānam yat idam dhamma anuggahaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_100:6.1",
        "original": "Dveme, bhikkhave, yāgā—",
        "analyzed": "dve me bhikkhave yāgā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_100:6.2",
        "original": "āmisayāgo ca dhammayāgo ca.",
        "analyzed": "āmisa yāgo ca dhamma yāgo ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_100:6.3",
        "original": "Etadaggaṁ, bhikkhave, imesaṁ dvinnaṁ yāgānaṁ yadidaṁ dhammayāgo”ti.",
        "analyzed": "etat agga bhikkhave imesam dvinnam yāgānam yat idam dhamma yāgaḥ ti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_100:6.4",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_100:6.5",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_100:7.1",
        "original": "“Yo dhammayāgaṁ ayajī amaccharī,",
        "analyzed": "yaḥ dhamma yāgam ayajī amaccharī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_100:7.2",
        "original": "Tathāgato sabbabhūtānukampī;",
        "analyzed": "tathāgataḥ sabba bhūta anukampī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_100:7.3",
        "original": "Taṁ tādisaṁ devamanussaseṭṭhaṁ,",
        "analyzed": "tam tādisam deva manussa seṭṭha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_100:7.4",
        "original": "Sattā namassanti bhavassa pāragun”ti.",
        "analyzed": "sattāḥ namassanti bhavassa pāragunti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_100:8.1",
        "original": "Ayampi attho vutto bhagavatā, iti me sutanti.",
        "analyzed": "ayam pi atthaḥ vuttaḥ bhagavatā iti me sutam ti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_100:8.2",
        "original": "Paṭhamaṁ.",
        "analyzed": "paṭhama ",
        "folio": "8"
    }
]