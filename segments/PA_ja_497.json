[
    {
        "segmentnr": "PA_ja_497:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_497:0.2",
        "original": "Vīsatinipāta",
        "analyzed": "vīsati nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_497:0.3",
        "original": "Mātaṅgavagga",
        "analyzed": "mātaṅga vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_497:0.4",
        "original": "1. Mātaṅgajātaka",
        "analyzed": "mātaṅga jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_497:1.1",
        "original": "“Kuto nu āgacchasi dummavāsī,",
        "analyzed": "kutas nu āgacchasi dummavāsī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_497:1.2",
        "original": "Otallako paṁsupisācakova;",
        "analyzed": "o tallakaḥ pa supi sācakaḥ uva ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_497:1.3",
        "original": "Saṅkāracoḷaṁ paṭimuñca kaṇṭhe,",
        "analyzed": "saṅkāra coḷam kaṇṭhe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_497:1.4",
        "original": "Ko re tuvaṁ hosi adakkhiṇeyyo”.",
        "analyzed": "kaḥ re tuvam hosi a dakkhiṇeyyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_497:2.1",
        "original": "“Annaṁ tavedaṁ pakataṁ yasassi,",
        "analyzed": "annam tava idam pakatam yasassi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_497:2.2",
        "original": "Taṁ khajjare bhuñjare piyyare ca;",
        "analyzed": "tam khajjare bhuñjare piyyare ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_497:2.3",
        "original": "Jānāsi maṁ tvaṁ paradattūpajīviṁ,",
        "analyzed": "jānāsi mam tvam para dattu upajīvi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_497:2.4",
        "original": "Uttiṭṭhapiṇḍaṁ labhataṁ sapāko”.",
        "analyzed": "uttiṭṭha piṇḍam labhatam sa pākaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_497:3.1",
        "original": "“Annaṁ mamedaṁ pakataṁ brāhmaṇānaṁ,",
        "analyzed": "annam mama idam pakatam brāhmaṇāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_497:3.2",
        "original": "Attatthāya saddahato mamedaṁ;",
        "analyzed": "attatthāya saddahataḥ mama idam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_497:3.3",
        "original": "Apehi etto kimidhaṭṭhitosi,",
        "analyzed": "apehi ettas kim idha ṭṭhitaḥ asi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_497:3.4",
        "original": "Na mādisā tuyhaṁ dadanti jamma”.",
        "analyzed": "na mā adisā tuyham dadanti jamma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_497:4.1",
        "original": "“Thale ca ninne ca vapanti bījaṁ,",
        "analyzed": "thale ca ninne ca vapanti bīja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_497:4.2",
        "original": "Anūpakhette phalamāsamānā;",
        "analyzed": "anūpa khette phalam āsamānā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_497:4.3",
        "original": "Etāya saddhāya dadāhi dānaṁ,",
        "analyzed": "etāya saddhāya dadāhi dāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_497:4.4",
        "original": "Appeva ārādhaye dakkhiṇeyye”.",
        "analyzed": "appā iva ārādhaye dakkhiṇeyye ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_497:5.1",
        "original": "“Khettāni mayhaṁ viditāni loke,",
        "analyzed": "khettāni mayham viditāni loke ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_497:5.2",
        "original": "Yesāhaṁ bījāni patiṭṭhapemi;",
        "analyzed": "yesāham bījāni patiṭṭhapemi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_497:5.3",
        "original": "Ye brāhmaṇā jātimantūpapannā,",
        "analyzed": "ye brāhmaṇāḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_497:5.4",
        "original": "Tānīdha khettāni supesalāni”.",
        "analyzed": "tāni idha khettāni su pesalāni ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_497:6.1",
        "original": "“Jātimado ca atimānitā ca,",
        "analyzed": "ca atimāni tā ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_497:6.2",
        "original": "Lobho ca doso ca mado ca moho;",
        "analyzed": "lobho ca doso ca madaḥ ca moho ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_497:6.3",
        "original": "Ete aguṇā yesu ca santi sabbe,",
        "analyzed": "ete aguṇāḥ yesu ca santi sabbe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_497:6.4",
        "original": "Tānīdha khettāni apesalāni.",
        "analyzed": "tāni idha khettāni apesalāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_497:7.1",
        "original": "Jātimado ca atimānitā ca,",
        "analyzed": "ca atimāni tā ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_497:7.2",
        "original": "Lobho ca doso ca mado ca moho;",
        "analyzed": "lobho ca doso ca madaḥ ca moho ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_497:7.3",
        "original": "Ete aguṇā yesu na santi sabbe,",
        "analyzed": "ete aguṇāḥ yesu na santi sabbe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_497:7.4",
        "original": "Tānīdha khettāni supesalāni”.",
        "analyzed": "tāni idha khettāni su pesalāni ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_497:8.1",
        "original": "“Kvettha gatā upajotiyo ca,",
        "analyzed": "kva ittha gatāḥ ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_497:8.2",
        "original": "Upajjhāyo ca atha vā gaṇḍakucchi;",
        "analyzed": "upajjhāyaḥ ca atha vā gaṇḍakucchi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_497:8.3",
        "original": "Imassa daṇḍañca vadhañca datvā,",
        "analyzed": "imassa daṇḍañca vadhañca da tvā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_497:8.4",
        "original": "Gale gahetvā khalayātha jammaṁ”.",
        "analyzed": "gale gahetvā khalayā atha jamma ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_497:9.1",
        "original": "“Giriṁ nakhena khaṇasi,",
        "analyzed": "girim nakhena khaṇasi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_497:9.2",
        "original": "ayo dantehi khādasi;",
        "analyzed": "ayaḥ khādasi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_497:9.3",
        "original": "Jātavedaṁ padahasi,",
        "analyzed": "jātavedam padahasi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_497:9.4",
        "original": "yo isiṁ paribhāsasi”.",
        "analyzed": "yaḥ isim paribhāsasi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_497:10.1",
        "original": "“Idaṁ vatvāna mātaṅgo,",
        "analyzed": "idam vatvāna mātaṅgo ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_497:10.2",
        "original": "isi saccaparakkamo;",
        "analyzed": "isi sacca parakkamaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_497:10.3",
        "original": "Antalikkhasmiṁ pakkāmi,",
        "analyzed": "antalikkhasmim pakkāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_497:10.4",
        "original": "brāhmaṇānaṁ udikkhataṁ”.",
        "analyzed": "brāhmaṇānaṃ udikkhata ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_497:11.1",
        "original": "“Āvellitaṁ piṭṭhito uttamaṅgaṁ,",
        "analyzed": "āvellitam uttamaṅga ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_497:11.2",
        "original": "Bāhuṁ pasāreti akammaneyyaṁ;",
        "analyzed": "bāhum pasāreti a kammaneyya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_497:11.3",
        "original": "Setāni akkhīni yathā matassa,",
        "analyzed": "setāni akkhīni yathā matassa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_497:11.4",
        "original": "Ko me imaṁ puttamakāsi evaṁ”.",
        "analyzed": "kaḥ me imam eva ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_497:12.1",
        "original": "“Idhāgamā samaṇo dummavāsī,",
        "analyzed": "idha agamā samaṇaḥ dummavāsī ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_497:12.2",
        "original": "Otallako paṁsupisācakova;",
        "analyzed": "o tallakaḥ pa supi sācakaḥ uva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_497:12.3",
        "original": "Saṅkāracoḷaṁ paṭimuñca kaṇṭhe,",
        "analyzed": "saṅkāra coḷam kaṇṭhe ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_497:12.4",
        "original": "So te imaṁ puttamakāsi evaṁ”.",
        "analyzed": "saḥ te imam eva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_497:13.1",
        "original": "“Katamaṁ disaṁ agamā bhūripañño,",
        "analyzed": "katamam disam agamaḥ bhūri paññaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_497:13.2",
        "original": "Akkhātha me māṇavā etamatthaṁ;",
        "analyzed": "akkhātha me māṇavāḥ etam attha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_497:13.3",
        "original": "Gantvāna taṁ paṭikaremu accayaṁ,",
        "analyzed": "gantvāna tam paṭikaremu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_497:13.4",
        "original": "Appeva naṁ putta labhemu jīvitaṁ”.",
        "analyzed": "appā iva nam putta labhemu jīvita ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_497:14.1",
        "original": "“Vehāyasaṁ agamā bhūripañño,",
        "analyzed": "vehāyasam agamaḥ bhūri paññaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_497:14.2",
        "original": "Pathaddhuno pannaraseva cando;",
        "analyzed": "pathaddhunaḥ pannarasā iva cando ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_497:14.3",
        "original": "Api cāpi so purimadisaṁ agacchi,",
        "analyzed": "api ca api saḥ purima disam agacchi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_497:14.4",
        "original": "Saccappaṭiñño isi sādhurūpo”.",
        "analyzed": "saccappaṭiññaḥ isi sādhu rūpaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_497:15.1",
        "original": "“Āvellitaṁ piṭṭhito uttamaṅgaṁ,",
        "analyzed": "āvellitam uttamaṅga ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_497:15.2",
        "original": "Bāhuṁ pasāreti akammaneyyaṁ;",
        "analyzed": "bāhum pasāreti a kammaneyya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_497:15.3",
        "original": "Setāni akkhīni yathā matassa,",
        "analyzed": "setāni akkhīni yathā matassa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_497:15.4",
        "original": "Ko me imaṁ puttamakāsi evaṁ”.",
        "analyzed": "kaḥ me imam eva ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_497:16.1",
        "original": "“Yakkhā have santi mahānubhāvā,",
        "analyzed": "yakkhā have santi mahā anubhāvā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_497:16.2",
        "original": "Anvāgatā isayo sādhurūpā;",
        "analyzed": "anvāgatā sādhu rūpā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_497:16.3",
        "original": "Te duṭṭhacittaṁ kupitaṁ viditvā,",
        "analyzed": "te duṭṭha cittam kupitam viditvā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_497:16.4",
        "original": "Yakkhā hi te puttamakaṁsu evaṁ”.",
        "analyzed": "yakkhā hi te puttamakasu eva ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_497:17.1",
        "original": "“Yakkhā ca me puttamakaṁsu evaṁ,",
        "analyzed": "yakkhā ca me puttamakasu eva ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_497:17.2",
        "original": "Tvaññeva me mā kuddho brahmacāri;",
        "analyzed": "tvañc eva me mā kuddhaḥ brahmacāri ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_497:17.3",
        "original": "Tumheva pāde saraṇaṁ gatāsmi,",
        "analyzed": "tumhā iva pāde saraṇam gatā asmi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_497:17.4",
        "original": "Anvāgatā puttasokena bhikkhu”.",
        "analyzed": "anvāgatā putta sokena bhikkhu ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_497:18.1",
        "original": "“Tadeva hi etarahi ca mayhaṁ,",
        "analyzed": "tat eva hi etarahi ca mayha ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_497:18.2",
        "original": "Manopadoso na mamatthi koci;",
        "analyzed": "manaḥ upadosaḥ na mamatthi koci ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_497:18.3",
        "original": "Putto ca te vedamadena matto,",
        "analyzed": "putto ca te veda madena matto ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_497:18.4",
        "original": "Atthaṁ na jānāti adhicca vede”.",
        "analyzed": "attham na jānāti adhicca vede ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_497:19.1",
        "original": "“Addhā have bhikkhu muhuttakena,",
        "analyzed": "addhā have bhikkhu muhuttakena ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_497:19.2",
        "original": "Sammuyhateva purisassa saññā;",
        "analyzed": "sammuyhatā iva purisassa saññā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_497:19.3",
        "original": "Ekāparādhaṁ khama bhūripañña,",
        "analyzed": "eka aparādham khama bhūri pañña ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_497:19.4",
        "original": "Na paṇḍitā kodhabalā bhavanti”.",
        "analyzed": "na paṇḍitā kodha balāḥ bhavanti ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_497:20.1",
        "original": "“Idañca mayhaṁ uttiṭṭhapiṇḍaṁ,",
        "analyzed": "idam ca mayham uttiṭṭha piṇḍa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_497:20.2",
        "original": "Tava maṇḍabyo bhuñjatu appapañño;",
        "analyzed": "tava maṇḍabyaḥ bhuñjatu appapañño ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_497:20.3",
        "original": "Yakkhā ca te naṁ na viheṭhayeyyuṁ,",
        "analyzed": "yakkhā ca te nam na viheṭhayeyyu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_497:20.4",
        "original": "Putto ca te hessati so arogo”.",
        "analyzed": "putto ca te hessati saḥ arogaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_497:21.1",
        "original": "“Maṇḍabya bālosi parittapañño,",
        "analyzed": "maṇḍabya bālaḥ asi paritta pañño ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_497:21.2",
        "original": "Yo puññakhettānamakovidosi;",
        "analyzed": "yaḥ puñña khettānam a kovidaḥ asi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_497:21.3",
        "original": "Mahakkasāvesu dadāsi dānaṁ,",
        "analyzed": "mahakkasāvesu dadāsi dāna ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_497:21.4",
        "original": "Kiliṭṭhakammesu asaññatesu.",
        "analyzed": "kiliṭṭha kammesu a saññatesu ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_497:22.1",
        "original": "Jaṭā ca kesā ajinā nivatthā,",
        "analyzed": "jaṭā ca kesā ajinā nivatthā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_497:22.2",
        "original": "Jarūdapānaṁva mukhaṁ parūḷhaṁ;",
        "analyzed": "mukham parūḍha ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_497:22.3",
        "original": "Pajaṁ imaṁ passatha dummarūpaṁ,",
        "analyzed": "pajam imam passatha dummarūpa ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_497:22.4",
        "original": "Na jaṭājinaṁ tāyati appapaññaṁ.",
        "analyzed": "na jaṭā ajinam tāyati appapañña ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_497:23.1",
        "original": "Yesaṁ rāgo ca doso ca,",
        "analyzed": "rāgaḥ ca dosaḥ ca ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_497:23.2",
        "original": "avijjā ca virājitā;",
        "analyzed": "avijjā ca virājitā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_497:23.3",
        "original": "Khīṇāsavā arahanto,",
        "analyzed": "khīṇa āsavāḥ arahantaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_497:23.4",
        "original": "tesu dinnaṁ mahapphalan”ti.",
        "analyzed": "tesu dinnam mahapphalanti ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_497:24.1",
        "original": "Mātaṅgajātakaṁ paṭhamaṁ.",
        "analyzed": "mātaṅga jātakam ",
        "folio": "24"
    }
]