        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.7:16.3",
        "original": "Āgataṁ nappaṭipūjeti,",
        "analyzed": "āgatam na paṭipūjeti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.7:16.4",
        "original": "Taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.7:17.1",
        "original": "Yo brāhmaṇaṁ samaṇaṁ vā,",
        "analyzed": "yaḥ brāhmaṇam samaṇam vā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.7:17.2",
        "original": "Aññaṁ vāpi vanibbakaṁ;",
        "analyzed": "aññam vā api vanibbaka ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.7:17.3",
        "original": "Musāvādena vañceti,",
        "analyzed": "musā vādena vañca iti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.7:17.4",
        "original": "Taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.7:18.1",
        "original": "Yo brāhmaṇaṁ samaṇaṁ vā,",
        "analyzed": "yaḥ brāhmaṇam samaṇam vā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_1.7:18.2",
        "original": "Bhattakāle upaṭṭhite;",
