[
    {
        "segmentnr": "PA_snp_4.6:0.1",
        "original": "Sutta Nipāta 4.6",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.6:0.2",
        "original": "Jarāsutta",
        "analyzed": "jarāsutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.6:1.1",
        "original": "Appaṁ vata jīvitaṁ idaṁ,",
        "analyzed": "appam vata jīvitam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.6:1.2",
        "original": "Oraṁ vassasatāpi miyyati;",
        "analyzed": "oram ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.6:1.3",
        "original": "Yo cepi aticca jīvati,",
        "analyzed": "yaḥ cepi aticca jīvati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.6:1.4",
        "original": "Atha kho so jarasāpi miyyati.",
        "analyzed": "atha khalu saḥ jarasā api miyyati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.6:2.1",
        "original": "Socanti janā mamāyite,",
        "analyzed": "socanti janāḥ mamāyite ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.6:2.2",
        "original": "Na hi santi niccā pariggahā;",
        "analyzed": "na hi santi niccā pariggahā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.6:2.3",
        "original": "Vinābhāvasantamevidaṁ,",
        "analyzed": "vinābhāva santam evida ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.6:2.4",
        "original": "Iti disvā nāgāramāvase.",
        "analyzed": "iti disvā na āgāram āvase ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.6:3.1",
        "original": "Maraṇenapi taṁ pahīyati,",
        "analyzed": "maraṇena api tam pahīyati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.6:3.2",
        "original": "Yaṁ puriso mamidanti maññati;",
        "analyzed": "yam purisaḥ mamidanti maññati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.6:3.3",
        "original": "Etampi viditvā paṇḍito,",
        "analyzed": "etam pi viditvā paṇḍitaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.6:3.4",
        "original": "Na mamattāya nametha māmako.",
        "analyzed": "na mamattāya nametha māmakaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.6:4.1",
        "original": "Supinena yathāpi saṅgataṁ,",
        "analyzed": "supinena yathā api ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.6:4.2",
        "original": "Paṭibuddho puriso na passati;",
        "analyzed": "paṭibuddhaḥ puriso na passati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.6:4.3",
        "original": "Evampi piyāyitaṁ janaṁ,",
        "analyzed": "evam pi piyāyitam jana ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.6:4.4",
        "original": "Petaṁ kālaṅkataṁ na passati.",
        "analyzed": "petam kālaṅkatam na passati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.6:5.1",
        "original": "Diṭṭhāpi sutāpi te janā,",
        "analyzed": "diṭṭhā api sutā api te janā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.6:5.2",
        "original": "Yesaṁ nāmamidaṁ pavuccati;",
        "analyzed": "nāmam idam pavuccati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.6:5.3",
        "original": "Nāmaṁyevāvasissati,",
        "analyzed": "nāmayā iva avasissati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.6:5.4",
        "original": "Akkheyyaṁ petassa jantuno.",
        "analyzed": "akkheyyam petassa jantunaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.6:6.1",
        "original": "Sokapparidevamaccharaṁ,",
        "analyzed": "sokapparidevamat śara ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.6:6.2",
        "original": "Na jahanti giddhā mamāyite;",
        "analyzed": "na jahanti giddhāḥ mamāyite ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.6:6.3",
        "original": "Tasmā munayo pariggahaṁ,",
        "analyzed": "tasmai munayaḥ pariggaha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.6:6.4",
        "original": "Hitvā acariṁsu khemadassino.",
        "analyzed": "hitvā acarisu khemadassinaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.6:7.1",
        "original": "Patilīnacarassa bhikkhuno,",
        "analyzed": "patilīna carassa bhikkhuno ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.6:7.2",
        "original": "Bhajamānassa vivittamāsanaṁ;",
        "analyzed": "bhajamānassa vivittam āsana ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.6:7.3",
        "original": "Sāmaggiyamāhu tassa taṁ,",
        "analyzed": "sāmaggiyam āhu tassa ta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.6:7.4",
        "original": "Yo attānaṁ bhavane na dassaye.",
        "analyzed": "yaḥ attānam bhavane na dassaye ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.6:8.1",
        "original": "Sabbattha munī anissito,",
        "analyzed": "sabbattha muniḥ anissito ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.6:8.2",
        "original": "Na piyaṁ kubbati nopi appiyaṁ;",
        "analyzed": "na piyam kubbati na upi appiya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.6:8.3",
        "original": "Tasmiṁ paridevamaccharaṁ,",
        "analyzed": "tasmin paridevamat śara ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.6:8.4",
        "original": "Paṇṇe vāri yathā na limpati.",
        "analyzed": "paṇṇe vāri yathā na limpati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.6:9.1",
        "original": "Udabindu yathāpi pokkhare,",
        "analyzed": "uda bindu yathā api pokkhare ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.6:9.2",
        "original": "Padume vāri yathā na limpati;",
        "analyzed": "padume vāri yathā na limpati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.6:9.3",
        "original": "Evaṁ muni nopalimpati,",
        "analyzed": "evam muni na upalimpati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.6:9.4",
        "original": "Yadidaṁ diṭṭhasutaṁ mutesu vā.",
        "analyzed": "yat idam diṭṭha sutam mutesu vā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.6:10.1",
        "original": "Dhono na hi tena maññati,",
        "analyzed": "dhonaḥ na hi tena maññati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.6:10.2",
        "original": "Yadidaṁ diṭṭhasutaṁ mutesu vā;",
        "analyzed": "yat idam diṭṭha sutam mutesu vā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.6:10.3",
        "original": "Nāññena visuddhimicchati,",
        "analyzed": "na aññena visuddhim icchati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.6:10.4",
        "original": "Na hi so rajjati no virajjatīti.",
        "analyzed": "na hi saḥ rajjati na u virajjati iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.6:10.5",
        "original": "Jarāsuttaṁ chaṭṭhaṁ.",
        "analyzed": "jarāsuttam chaṭṭha ",
        "folio": "10"
    }
]