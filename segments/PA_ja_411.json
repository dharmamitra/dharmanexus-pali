    {
        "segmentnr": "PA_ja_411:0.4",
        "original": "6. Susīmajātaka",
        "analyzed": "susīmajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_411:1.1",
        "original": "“Kāḷāni kesāni pure ahesuṁ,",
        "analyzed": "kāḷāni kesāni pure ahesu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_411:1.2",
        "original": "Jātāni sīsamhi yathāpadese;",
        "analyzed": "jātāni sīsamhi yathā apadese ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_411:1.3",
        "original": "Tānajja setāni susīma disvā,",
        "analyzed": "tānajja setāni su sīma disvā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_411:1.4",
        "original": "Dhammaṁ cara brahmacariyassa kālo”.",
        "analyzed": "dhammam cara brahmacariyassa kālo ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_411:2.1",
        "original": "“Mameva deva palitaṁ na tuyhaṁ,",
        "analyzed": "mama iva deva palitam na tuyha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_411:2.2",
        "original": "Mameva sīsaṁ mama uttamaṅgaṁ;",
        "analyzed": "mama iva sīsam mama uttamaṅga ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_411:2.3",
        "original": "‘Atthaṁ karissan’ti musā abhāṇiṁ,",
        "analyzed": "attham karissanti musā abhāṇi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_411:2.4",
        "original": "Ekāparādhaṁ khama rājaseṭṭha.",
        "analyzed": "eka aparādham khama rāja seṭṭha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_411:3.1",
        "original": "Daharo tuvaṁ dassaniyosi rāja,",
        "analyzed": "daharaḥ tuvam dassaniyaḥ asi rāja ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_411:3.2",
        "original": "Paṭhamuggato hosi yathā kaḷīro;",
        "analyzed": "paṭhama uggataḥ hosi yathā kaḷīraḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_411:3.3",
        "original": "Rajjañca kārehi mamañca passa,",
        "analyzed": "rajjañca kārehi mamañca passa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_411:3.4",
        "original": "Mā kālikaṁ anudhāvī janinda”.",
        "analyzed": "mā kālikam anudhāvī janinda ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_411:4.1",
        "original": "“Passāmi vohaṁ dahariṁ kumāriṁ,",
        "analyzed": "passāmi voham daharim kumāri ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_411:4.2",
        "original": "Sāmaṭṭhapassaṁ sutanuṁ sumajjhaṁ;",
        "analyzed": "sāmaṭṭhapassam su tanum su majjha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_411:4.3",
        "original": "Kāḷappavāḷāva pavellamānā,",
        "analyzed": "kāḷappa vāḷā ava pavellamānā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_411:4.4",
        "original": "Palobhayantīva naresu gacchati.",
        "analyzed": "palobhayantī iva naresu gacchati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_411:5.1",
        "original": "Tamena passāmiparena nāriṁ,",
        "analyzed": "passāmi parena nāri ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_411:5.2",
        "original": "Āsītikaṁ nāvutikaṁ va jaccā;",
        "analyzed": "āsītikam nāvutikam va jaccā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_411:5.3",
        "original": "Daṇḍaṁ gahetvāna pavedhamānaṁ,",
        "analyzed": "daṇḍam gahetvāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_411:5.4",
        "original": "Gopānasībhoggasamaṁ carantiṁ.",
        "analyzed": "caranti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_411:6.1",
        "original": "Sohaṁ tamevānuvicintayanto,",
        "analyzed": "saḥ ham tam eva anuvicintayantaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_411:6.2",
        "original": "Eko sayāmi sayanassa majjhe;",
        "analyzed": "ekaḥ sayāmi sayanassa majjhe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_411:6.3",
        "original": "‘Ahampi evaṁ’ iti pekkhamāno,",
        "analyzed": "aham pi eva iti pekkhamāno ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_411:6.4",
        "original": "Na gahe rame brahmacariyassa kālo.",
        "analyzed": "na gahe rame brahmacariyassa kālo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_411:7.1",
        "original": "Rajjuvālambanī cesā,",
        "analyzed": "rajju vālambanī ca isā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_411:7.2",
        "original": "Yā gehe vasato rati;",
        "analyzed": "yā gehe vasataḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_411:7.3",
        "original": "Evampi chetvāna vajanti dhīrā,",
        "analyzed": "evampi chetvāna vajanti dhīrā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_411:7.4",
        "original": "Anapekkhino kāmasukhaṁ pahāyā”ti.",
        "analyzed": "anapekkhinaḥ kāma sukham pahāya ati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_411:8.1",
        "original": "Susīmajātakaṁ chaṭṭhaṁ.",
        "analyzed": "susīma jātakam chaṭṭha ",
        "folio": "8"
    }
]