    {
        "segmentnr": "PA_mil_7.4.5:0.4",
        "original": "5. Pabbataṅgapañha",
        "analyzed": "pabbataṅgapañha ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.1",
        "original": "“Bhante nāgasena, ‘pabbatassa pañca aṅgāni gahetabbānī’ti yaṁ vadesi, katamāni tāni pañca aṅgāni gahetabbānī”ti?",
        "analyzed": "bhante nāgasena pabbatassa pañca aṅgāni gahetabbāni iti yam vadesi katamāni tāni pañca aṅgāni gahetabbāni iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.2",
        "original": "“Yathā, mahārāja, pabbato acalo akampito asampavedhī;",
        "analyzed": "yathā mahā rāja pabbataḥ acalaḥ a kampitaḥ a sampavedhī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.3_0",
        "original": "evameva kho, mahārāja, yoginā yogāvacarena sammānane vimānane sakkāre asakkāre garukāre",
        "analyzed": "evam eva khalu mahā rāja yoginā yogāvacarena sammānane vimānane sakkāre asakkāre garukāre ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.3_1",
        "original": "agarukāre yase ayase nindāya pasaṁsāya sukhe dukkhe iṭṭhāniṭṭhesu sabbattha rūpasaddagandharasaphoṭṭhabbadhammesu",
        "analyzed": "agarukāre yase ayase nindāya pasasāya sukhe dukkhe iṭṭha aniṭṭhesu sabbattha rūpa sadda gandha rasa phoṭṭhabba dhammesu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.3_2",
        "original": "rajanīyesu na rajjitabbaṁ, dussanīyesu na dussitabbaṁ, muyhanīyesu na muyhitabbaṁ,",
        "analyzed": "rajanīyesu na rajjitabba dussanīyesu na dussitabba muyhanīyesu na muyhitabba ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.3_3",
        "original": "na kampitabbaṁ na calitabbaṁ, pabbatena viya acalena bhavitabbaṁ.",
        "analyzed": "na kampitabba na calitabba pabbatena viya acalena bhavitabba ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.4",
        "original": "Idaṁ, mahārāja, pabbatassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
        "analyzed": "idam mahā rāja pabbatassa paṭhamam aṅgam gahetabba ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:1.5",
        "original": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
        "analyzed": "mahā rāja bhagavatā devātidevena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.4.5:2.1",
        "original": "‘Selo yathā ekaghano,",
        "analyzed": "selaḥ yathā eka ghanaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.4.5:2.2",
        "original": "vātena na samīrati;",
        "analyzed": "vātena na samīrati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.4.5:2.3",
        "original": "Evaṁ nindāpasaṁsāsu,",
        "analyzed": "evam nindā apasasāsu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.4.5:2.4",
        "original": "na samiñjanti paṇḍitā’ti.",
        "analyzed": "na samiñjanti paṇḍitā ati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.4.5:3.1",
        "original": "Puna caparaṁ, mahārāja, pabbato thaddho na kenaci saṁsaṭṭho;",
        "analyzed": "puna capara mahā rāja pabbataḥ thaddhaḥ na kenacid sa saṭṭhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.4.5:3.2",
        "original": "evameva kho, mahārāja, yoginā yogāvacarena thaddhena asaṁsaṭṭhena bhavitabbaṁ, na kenaci saṁsaggo karaṇīyo.",
        "analyzed": "evam eva kho mahā rāja yoginā yogāvacarena thaddhena a sasaṭṭhena bhavitabba na kenacid sa saggaḥ karaṇīyaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.4.5:3.3",
        "original": "Idaṁ, mahārāja, pabbatassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
        "analyzed": "ida mahā rāja pabbatassa dutiyam aṅgam gahetabba ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.4.5:3.4",
        "original": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
        "analyzed": "mahā rāja bhagavatā devātidevena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.4.5:4.1",
        "original": "‘Asaṁsaṭṭhaṁ gahaṭṭhehi,",
        "analyzed": "a sasaṭṭham gahaṭṭhehi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.4.5:4.2",
        "original": "anāgārehi cūbhayaṁ;",
        "analyzed": "anāgārehi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.4.5:4.3",
        "original": "Anokasārimappicchaṁ,",
        "analyzed": "an oka sārim appiccha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.4.5:4.4",
        "original": "tamahaṁ brūmi brāhmaṇan’ti.",
        "analyzed": "tam aham brūmi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.4.5:5.1",
        "original": "Puna caparaṁ, mahārāja, pabbate bījaṁ na virūhati;",
        "analyzed": "puna capara mahā rāja pabbate bījam na virūhati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mil_7.4.5:5.2",
        "original": "evameva kho, mahārāja, yoginā yogāvacarena sakamānase kilesā na virūhāpetabbā.",
        "analyzed": "evam eva khalu mahā rāja yoginā yogāvacarena saka mānase kilesāḥ na virūhāpetabbā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mil_7.4.5:5.3",
        "original": "Idaṁ, mahārāja, pabbatassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
        "analyzed": "ida mahā rāja pabbatassa tatiyam aṅgam gahetabba ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mil_7.4.5:5.4",
        "original": "Bhāsitampetaṁ, mahārāja, therena subhūtinā—",
        "analyzed": "mahā rāja therena subhūtinā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mil_7.4.5:6.1",
        "original": "‘Rāgūpasaṁhitaṁ cittaṁ,",
        "analyzed": "rāj ūpa sahitam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mil_7.4.5:6.2",
        "original": "yadā uppajjate mama;",
        "analyzed": "yadā uppajjate mama ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mil_7.4.5:6.3",
        "original": "Sayaṁva paccavekkhāmi,",
        "analyzed": "sayava paccavekkhāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mil_7.4.5:6.4",
        "original": "ekaggo taṁ damemahaṁ.",
        "analyzed": "ekaggaḥ tam damemaha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mil_7.4.5:7.1",
        "original": "Rajjase rajanīye ca,",
        "analyzed": "rajjase rajanīye ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_mil_7.4.5:7.2",
        "original": "dussanīye ca dussase;",
        "analyzed": "dussanīye ca dussase ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_mil_7.4.5:7.3",
        "original": "Muyhase mohanīye ca,",
        "analyzed": "muyhase mohanīye ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_mil_7.4.5:7.4",
        "original": "nikkhamassu vanā tuvaṁ.",
        "analyzed": "nikkhamassu vanā tuva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_mil_7.4.5:8.1",
        "original": "Visuddhānaṁ ayaṁ vāso,",
        "analyzed": "visuddhānam ayam vāsaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_mil_7.4.5:8.2",
        "original": "nimmalānaṁ tapassinaṁ;",
        "analyzed": "nimmalānam tapassina ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_mil_7.4.5:8.3",
        "original": "Mā kho visuddhaṁ dūsesi,",
        "analyzed": "mā kho visuddham dūsesi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_mil_7.4.5:8.4",
        "original": "nikkhamassu vanā tuvan’ti.",
        "analyzed": "nikkhamassu vanā tuvanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_mil_7.4.5:9.1",
        "original": "Puna caparaṁ, mahārāja, pabbato accuggato;",
        "analyzed": "puna capara mahā rāja pabbataḥ accuggataḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_mil_7.4.5:9.2",
        "original": "evameva kho, mahārāja, yoginā yogāvacarena ñāṇaccuggatena bhavitabbaṁ.",
        "analyzed": "evam eva kho mahā rāja yoginā yogāvacarena ñāṇa cuggatena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_mil_7.4.5:9.3",
        "original": "Idaṁ, mahārāja, pabbatassa catutthaṁ aṅgaṁ gahetabbaṁ.",
        "analyzed": "idam mahā rāja pabbatassa catuttham aṅgam gahetabba ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_mil_7.4.5:9.4",
        "original": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
        "analyzed": "mahā rāja bhagavatā devātidevena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_mil_7.4.5:10.1",
        "original": "‘Pamādaṁ appamādena,",
        "analyzed": "pamādam ap pamādena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_mil_7.4.5:10.2",
        "original": "yadā nudati paṇḍito;",
        "analyzed": "yadā nudati paṇḍitaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_mil_7.4.5:10.3",
        "original": "Paññāpāsādamāruyha,",
        "analyzed": "paññāpāsādam āruyha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_mil_7.4.5:10.4",
        "original": "asoko sokiniṁ pajaṁ;",
        "analyzed": "asoko sokinim paja ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_mil_7.4.5:10.5",
        "original": "Pabbataṭṭhova bhūmaṭṭhe,",
        "analyzed": "pabbata aṭṭhaḥ uva bhūma aṭṭhe ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_mil_7.4.5:10.6",
        "original": "dhīro bāle avekkhatī’ti.",
        "analyzed": "dhīraḥ bāle avekkhati iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_mil_7.4.5:11.1",
        "original": "Puna caparaṁ, mahārāja, pabbato anunnato anonato;",
        "analyzed": "puna capara mahā rāja pabbataḥ an unnataḥ an onataḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_mil_7.4.5:11.2",
        "original": "evameva kho, mahārāja, yoginā yogāvacarena unnatāvanati na karaṇīyā.",
        "analyzed": "evam eva khalu mahā rāja yoginā yogāvacarena unnati avanati na karaṇīyā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_mil_7.4.5:11.3",
        "original": "Idaṁ, mahārāja, pabbatassa pañcamaṁ aṅgaṁ gahetabbaṁ.",
        "analyzed": "idam mahā rāja pabbatassa pañcamam aṅgam gahetabba ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_mil_7.4.5:11.4",
        "original": "Bhāsitampetaṁ, mahārāja, upāsikāya cūḷasubhaddāya sakasamaṇe parikittayamānāya—",
        "analyzed": "mahā rāja upāsikāya cūḷasubhaddāya saka samaṇe parikittayamānāya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_mil_7.4.5:12.1",
        "original": "‘Lābhena unnato loko,",
        "analyzed": "lābhena unnataḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_mil_7.4.5:12.2",
        "original": "alābhena ca onato;",
        "analyzed": "alābhena ca onataḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_mil_7.4.5:12.3",
        "original": "Lābhālābhena ekaṭṭhā,",
        "analyzed": "lābha alābhena ekaṭṭhā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_mil_7.4.5:12.4",
        "original": "tādisā samaṇā mamā’”ti.",
        "analyzed": "tādisā samaṇā mama ati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_mil_7.4.5:13.1",
        "original": "Pabbataṅgapañho pañcamo.",
        "analyzed": "pabbataṅga pañhaḥ pañcamaḥ ",
        "folio": "13"
    }
]