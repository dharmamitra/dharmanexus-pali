        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.9",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.10",
        "original": "“kathañhi nāma bhikkhuniyo dadhiṁ viññāpetvā bhuñjissanti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ dadhim viññāpetvā bhuñjissanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.11",
        "original": "Kassa sampannaṁ na manāpaṁ, kassa sāduṁ na ruccatī”ti.",
        "analyzed": "kassa sampannam na manāpa kassa sādum na ruccati iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.12",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.13",
        "original": "Yā tā bhikkhuniyo appicchā … pe … tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.14",
        "original": "“kathañhi nāma chabbaggiyā bhikkhuniyo dadhiṁ viññāpetvā bhuñjissantī”ti … pe …",
        "analyzed": "katham hi nāma chabbaggiyāḥ bhikkhuniyaḥ dadhim viññāpetvā bhuñjissanti iti pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.15",
        "original": "“saccaṁ kira, bhikkhave, chabbaggiyā bhikkhuniyo dadhiṁ viññāpetvā bhuñjantī”ti?",
        "analyzed": "saccam kira bhikkhave chabbaggiyāḥ bhikkhuniyaḥ dadhim viññāpetvā bhuñjanti iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.16",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.17",
        "original": "Vigarahi buddho bhagavā … pe …",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.18",
        "original": "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo dadhiṁ viññāpetvā bhuñjissanti.",
        "analyzed": "katham hi nāma bhikkhave chabbaggiyāḥ bhikkhuniyaḥ dadhim viññāpetvā bhuñjissanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.19",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya … pe …",
        "analyzed": "na ita bhikkhave appasannānam vā pasādāya pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.20",
        "original": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
        "analyzed": "evam ca pana bhikkhave bhikkhuniyaḥ imam sikkhāpadam uddisantu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.21.1",
        "original": "“Yā pana bhikkhunī dadhiṁ viññāpetvā bhuñjeyya, paṭidesetabbaṁ tāya bhikkhuniyā—",
        "analyzed": "yā pana bhikkhunī dadhim viññāpetvā bhuñjeyya paṭidesetabbam tāya bhikkhuniyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.22",
        "original": "‘gārayhaṁ, ayye, dhammaṁ āpajjiṁ asappāyaṁ pāṭidesanīyaṁ, taṁ paṭidesemī’”ti.",
        "analyzed": "gārayha ayye dhammaṃ āpajjim asappāyam pāṭidesanīya tam paṭidesemi iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.23",
        "original": "Evañcidaṁ bhagavatā bhikkhunīnaṁ sikkhāpadaṁ paññattaṁ hoti.",
        "analyzed": "evañca idam bhagavatā bhikkhunīnam sikkhāpadam paññattam hoti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.2.1",
        "original": "Tena kho pana samayena bhikkhuniyo gilānā honti.",
        "analyzed": "tena kho pana samayena bhikkhuniyaḥ gilānāḥ honti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.2.2",
        "original": "Gilānapucchikā bhikkhuniyo gilānā bhikkhuniyo etadavocuṁ—",
        "analyzed": "gilānapucchikāḥ bhikkhuniyaḥ gilānāḥ bhikkhuniyaḥ etat avocuḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.2.3",
        "original": "“kacci, ayye, khamanīyaṁ, kacci yāpanīyan”ti?",
        "analyzed": "kacci ayye khamanīya kacci ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.2.4",
