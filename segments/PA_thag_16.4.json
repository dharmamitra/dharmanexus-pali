    },
    {
        "segmentnr": "PA_thag_16.4:0.2",
        "original": "Vīsatinipāta",
        "analyzed": "vīsati nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_16.4:0.3",
        "original": "Paṭhamavagga",
        "analyzed": "paṭhama vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_16.4:0.4",
        "original": "Raṭṭhapālattheragāthā",
        "analyzed": "raṭṭhapāla tthera gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_16.4:1.1",
        "original": "“Passa cittakataṁ bimbaṁ,",
        "analyzed": "passa citta katam bimba ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.4:1.2",
        "original": "arukāyaṁ samussitaṁ;",
        "analyzed": "aru kāyam samussita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.4:1.3",
        "original": "Āturaṁ bahusaṅkappaṁ,",
        "analyzed": "āturam bahu saṅkappa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.4:1.4",
        "original": "yassa natthi dhuvaṁ ṭhiti.",
        "analyzed": "yassa na atthi dhuvam ṭhiti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.4:2.1",
        "original": "Passa cittakataṁ rūpaṁ,",
        "analyzed": "passa citta katam rūpa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.4:2.2",
        "original": "maṇinā kuṇḍalena ca;",
        "analyzed": "maṇinā kuṇḍalena ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.4:2.3",
        "original": "Aṭṭhiṁ tacena onaddhaṁ,",
        "analyzed": "aṭṭhim tacena onaddha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.4:2.4",
        "original": "saha vatthehi sobhati.",
        "analyzed": "saha vatthehi sobhati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.4:3.1",
        "original": "Alattakakatā pādā,",
        "analyzed": "alattakakatā pādā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.4:3.2",
        "original": "mukhaṁ cuṇṇakamakkhitaṁ;",
        "analyzed": "mukham cuṇṇakam akkhita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.4:3.3",
        "original": "Alaṁ bālassa mohāya,",
        "analyzed": "alam bālassa mohāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.4:3.4",
        "original": "no ca pāragavesino.",
        "analyzed": "no ca pāragavesinaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.4:4.1",
        "original": "Aṭṭhapadakatā kesā,",
        "analyzed": "aṭṭha pada katā kesā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.4:4.2",
        "original": "nettā añjanamakkhitā;",
        "analyzed": "nettā añjana makkhitā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.4:4.3",
        "original": "Alaṁ bālassa mohāya,",
        "analyzed": "alam bālassa mohāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.4:4.4",
        "original": "no ca pāragavesino.",
        "analyzed": "no ca pāragavesinaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.4:5.1",
        "original": "Añjanīva navā cittā,",
        "analyzed": "añjanī iva navā cittā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.4:5.2",
        "original": "pūtikāyo alaṅkato;",
        "analyzed": "pūti kāyaḥ alaṅkato ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.4:5.3",
        "original": "Alaṁ bālassa mohāya,",
        "analyzed": "alam bālassa mohāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.4:5.4",
        "original": "no ca pāragavesino.",
        "analyzed": "no ca pāragavesinaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.4:6.1",
        "original": "Odahi migavo pāsaṁ,",
        "analyzed": "odahi migavaḥ pāsa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.4:6.2",
        "original": "nāsadā vāguraṁ migo;",
        "analyzed": "nāsadā vāguram migo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.4:6.3",
        "original": "Bhutvā nivāpaṁ gacchāma,",
        "analyzed": "bhutvā nivāpam gacchāma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.4:6.4",
        "original": "kandante migabandhake.",
        "analyzed": "kandante migabandhake ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.4:7.1",
        "original": "Chinno pāso migavassa,",
        "analyzed": "chinnaḥ pāsaḥ migavassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.4:7.2",
        "original": "nāsadā vāguraṁ migo;",
        "analyzed": "nāsadā vāguram migo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.4:7.3",
        "original": "Bhutvā nivāpaṁ gacchāma,",
        "analyzed": "bhutvā nivāpam gacchāma ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.4:7.4",
        "original": "socante migaluddake.",
        "analyzed": "socante migaluddake ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.4:8.1",
        "original": "Passāmi loke sadhane manusse,",
        "analyzed": "passāmi loke sa dhane manusse ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.4:8.2",
        "original": "Laddhāna vittaṁ na dadanti mohā;",
        "analyzed": "vittam na dadanti mohā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.4:8.3",
        "original": "Luddhā dhanaṁ sannicayaṁ karonti,",
        "analyzed": "luddhāḥ dhanam sannicayam karonti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.4:8.4",
        "original": "Bhiyyova kāme abhipatthayanti.",
        "analyzed": "bhiyyas uva kāme abhipatthayanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.4:9.1",
        "original": "Rājā pasayhappathaviṁ vijetvā,",
        "analyzed": "rājā pasayhappathavim vijetvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.4:9.2",
        "original": "Sasāgarantaṁ mahimāvasanto;",
        "analyzed": "sa sāgarantam ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.4:9.3",
        "original": "Oraṁ samuddassa atittarūpo,",
        "analyzed": "oram samuddassa a titta rūpaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.4:9.4",
        "original": "Pāraṁ samuddassapi patthayetha.",
        "analyzed": "pāram samuddassa api patthayetha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.4:10.1",
        "original": "Rājā ca aññe ca bahū manussā,",
        "analyzed": "rājā ca aññe ca bahū manussā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.4:10.2",
        "original": "Avītataṇhā maraṇaṁ upenti;",
        "analyzed": "a vīta taṇhāḥ maraṇam upenti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.4:10.3",
        "original": "Ūnāva hutvāna jahanti dehaṁ,",
        "analyzed": "ūnāva hutvāna jahanti deha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.4:10.4",
        "original": "Kāmehi lokamhi na hatthi titti.",
        "analyzed": "kāmehi lokamhi na hatthi titti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.4:11.1",
        "original": "Kandanti naṁ ñātī pakiriya kese,",
        "analyzed": "kandanti nam ñātī pakiriya kese ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.4:11.2",
        "original": "Aho vatā no amarāti cāhu;",
        "analyzed": "aho vata naḥ ca āhu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.4:11.3",
        "original": "Vatthena naṁ pārutaṁ nīharitvā,",
        "analyzed": "vatthena nam pārutam nīharitvā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.4:11.4",
        "original": "Citaṁ samodhāya tato ḍahanti.",
        "analyzed": "citam samodhāya tatas ḍahanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.4:12.1",
        "original": "So ḍayhati sūlehi tujjamāno,",
        "analyzed": "saḥ ḍayhati sūlehi tujjamānaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.4:12.2",
        "original": "Ekena vatthena pahāya bhoge;",
        "analyzed": "ekena vatthena pahāya bhoge ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.4:12.3",
        "original": "Na mīyamānassa bhavanti tāṇā,",
        "analyzed": "na mīyamānassa bhavanti tāṇā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.4:12.4",
        "original": "Ñātī ca mittā atha vā sahāyā.",
        "analyzed": "ñātī ca mittā atha vā sahāyā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.4:13.1",
        "original": "Dāyādakā tassa dhanaṁ haranti,",
        "analyzed": "dāyādakā tassa dhanam haranti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.4:13.2",
        "original": "Satto pana gacchati yena kammaṁ;",
        "analyzed": "satto pana gacchati yena kamma ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.4:13.3",
        "original": "Na mīyamānaṁ dhanamanveti kiñci,",
        "analyzed": "na mīyamānam dhanam anveti kiñci ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.4:13.4",
        "original": "Puttā ca dārā ca dhanañca raṭṭhaṁ.",
        "analyzed": "puttā ca dārā ca dhanañca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.4:14.1",
        "original": "Na dīghamāyuṁ labhate dhanena,",
        "analyzed": "na dīgham āyum labhate dhanena ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.4:14.2",
        "original": "Na cāpi vittena jaraṁ vihanti;",
        "analyzed": "na ca api vittena jaram vihanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.4:14.3",
        "original": "Appaṁ hidaṁ jīvitamāhu dhīrā,",
        "analyzed": "appam hi dam jīvitam āhu dhīrā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.4:14.4",
        "original": "Asassataṁ vippariṇāmadhammaṁ.",
        "analyzed": "asassatam vippariṇāma dhamma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.4:15.1",
        "original": "Aḍḍhā daliddā ca phusanti phassaṁ,",
        "analyzed": "aḍḍhā daliddā ca phusanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.4:15.2",
        "original": "Bālo ca dhīro ca tatheva phuṭṭho;",
        "analyzed": "bālo ca dhīro ca tathā iva phuṭṭhaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.4:15.3",
        "original": "Bālo hi bālyā vadhitova seti,",
        "analyzed": "bālaḥ hi bālyā vadhitaḥ uva sā iti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.4:15.4",
        "original": "Dhīro ca no vedhati phassaphuṭṭho.",
        "analyzed": "dhīro ca naḥ vedhati phassa phuṭṭhaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.4:16.1",
        "original": "Tasmā hi paññāva dhanena seyyā,",
        "analyzed": "tasmā hi paññā ava dhanena seyyā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.4:16.2",
        "original": "Yāya vosānamidhādhigacchati;",
        "analyzed": "yāya vosānam idhā adhigacchati ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.4:16.3",
        "original": "Abyositattā hi bhavābhavesu,",
        "analyzed": "hi bhava abhavesu ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.4:16.4",
        "original": "Pāpāni kammāni karoti mohā.",
        "analyzed": "pāpāni kammāni karoti mohā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.4:17.1",
        "original": "Upeti gabbhañca parañca lokaṁ,",
        "analyzed": "upeti gabbhañca parañca loka ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.4:17.2",
        "original": "Saṁsāramāpajja paramparāya;",
        "analyzed": "sa sāram āpajja paramparāya ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.4:17.3",
        "original": "Tassappapañño abhisaddahanto,",
        "analyzed": "tassa appapaññaḥ abhisaddahantaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.4:17.4",
        "original": "Upeti gabbhañca parañca lokaṁ.",
        "analyzed": "upeti gabbhañca parañca loka ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.4:18.1",
        "original": "Coro yathā sandhimukhe gahīto,",
        "analyzed": "coraḥ yathā sandhi mukhe ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_16.4:18.2",
        "original": "Sakammunā haññati pāpadhammo;",
        "analyzed": "sa kammunā haññati pāpa dhammaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_16.4:18.3",
        "original": "Evaṁ pajā pecca paramhi loke,",
        "analyzed": "evam pajā paramhi loke ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_16.4:18.4",
        "original": "Sakammunā haññati pāpadhammo.",
        "analyzed": "sa kammunā haññati pāpa dhammaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_16.4:19.1",
        "original": "Kāmā hi citrā madhurā manoramā,",
        "analyzed": "kāmā hi citrā madhurā manoramā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_16.4:19.2",
        "original": "Virūparūpena mathenti cittaṁ;",
        "analyzed": "virūpa rūpena mathenti citta ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_16.4:19.3",
        "original": "Ādīnavaṁ kāmaguṇesu disvā,",
        "analyzed": "ādīnavam kāma guṇesu disvā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_16.4:19.4",
        "original": "Tasmā ahaṁ pabbajitomhi rāja.",
        "analyzed": "tasmai aham pabbajitaḥ asmi rāja ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_16.4:20.1",
        "original": "Dumapphalānīva patanti māṇavā,",
        "analyzed": "duma pphalāni iva patanti māṇavā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.4:20.2",
        "original": "Daharā ca vuḍḍhā ca sarīrabhedā;",
        "analyzed": "daharā ca vuḍḍhā ca sarīra bhedā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.4:20.3",
        "original": "Etampi disvā pabbajitomhi rāja,",
        "analyzed": "etam pi disvā pabbajitaḥ amhi rāja ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.4:20.4",
        "original": "Apaṇṇakaṁ sāmaññameva seyyo.",
        "analyzed": "a paṇṇakam sāmaññam eva seyyaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.4:21.1",
        "original": "Saddhāyāhaṁ pabbajito,",
        "analyzed": "saddhāya aham pabbajitaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_16.4:21.2",
        "original": "upeto jinasāsane;",
        "analyzed": "upetaḥ jina sāsane ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_16.4:21.3",
        "original": "Avañjhā mayhaṁ pabbajjā,",
        "analyzed": "avañjhā mayham pabbajjā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_16.4:21.4",
        "original": "anaṇo bhuñjāmi bhojanaṁ.",
        "analyzed": "anaṇaḥ bhuñjāmi bhojana ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_16.4:22.1",
        "original": "Kāme ādittato disvā,",
        "analyzed": "kāme ādit tatas disvā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_16.4:22.2",
        "original": "Jātarūpāni satthato;",
        "analyzed": "jātarūpāni satthataḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_16.4:22.3",
        "original": "Gabbhavokkantito dukkhaṁ,",
        "analyzed": "gabbha vokkantitaḥ dukkha ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_16.4:22.4",
        "original": "Nirayesu mahabbhayaṁ.",
        "analyzed": "nirayesu ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_16.4:23.1",
        "original": "Etamādīnavaṁ ñatvā,",
        "analyzed": "etam ādīnavam ñatvā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_16.4:23.2",
        "original": "saṁvegaṁ alabhiṁ tadā;",
        "analyzed": "sa vegam alabhim tadā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_16.4:23.3",
        "original": "Sohaṁ viddho tadā santo,",
        "analyzed": "saḥ ham viddhaḥ tadā santaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_16.4:23.4",
        "original": "sampatto āsavakkhayaṁ.",
        "analyzed": "sampattaḥ āsava kkhaya ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_16.4:24.1",
        "original": "Pariciṇṇo mayā satthā,",
        "analyzed": "pariciṇṇaḥ mayā satthā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thag_16.4:24.2",
        "original": "kataṁ buddhassa sāsanaṁ;",
        "analyzed": "katam buddhassa sāsana ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thag_16.4:24.3",
        "original": "Ohito garuko bhāro,",
        "analyzed": "ohitaḥ garukaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thag_16.4:24.4",
        "original": "bhavanetti samūhatā.",
        "analyzed": "samūha tā ",
        "folio": "24"
    },
    {
