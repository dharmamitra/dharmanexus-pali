        "analyzed": "sāsanasmim apekkhavā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.4:3.1",
        "original": "Bhagavā tattha āgacchi,",
        "analyzed": "bhagavā tattha āgacchi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.4:3.2",
        "original": "sīsaṁ mayhaṁ parāmasi;",
        "analyzed": "sīsam mayham parāmasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.4:3.3",
        "original": "Bāhāya maṁ gahetvāna,",
        "analyzed": "bāhāya mam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.4:3.4",
        "original": "saṅghārāmaṁ pavesayi.",
        "analyzed": "saṅgha ārāmam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.4:4.1",
        "original": "Anukampāya me satthā,",
        "analyzed": "anukampāya me satthā ",
        "folio": "4"
    },
