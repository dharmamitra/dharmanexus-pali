        "original": "Sutta Nipāta 1.10",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.10:0.2",
        "original": "Āḷavakasutta",
        "analyzed": "āḷavaka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.10:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.10:1.2",
        "original": "ekaṁ samayaṁ bhagavā āḷaviyaṁ viharati āḷavakassa yakkhassa bhavane.",
        "analyzed": "ekam samayam bhagavā āḷaviyam viharati āḷavakassa yakkhassa bhavane ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.10:1.3",
        "original": "Atha kho āḷavako yakkho yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṁ etadavoca:",
        "analyzed": "atha kho āḷavakaḥ yakkhaḥ yena bhagavā tena upasaṅkami upasaṅkamitvā bhagavantam etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.10:1.4",
        "original": "“nikkhama, samaṇā”ti.",
        "analyzed": "nikkhama samaṇa ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.10:1.5",
        "original": "“Sādhāvuso”ti bhagavā nikkhami.",
        "analyzed": "sādha avuso iti bhagavā nikkhami ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.10:1.6",
        "original": "“Pavisa, samaṇā”ti.",
        "analyzed": "pavisa samaṇa ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.10:1.7",
        "original": "“Sādhāvuso”ti bhagavā pāvisi.",
        "analyzed": "sādhāvuso iti bhagavā pāvisi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.10:2.1",
        "original": "Dutiyampi kho …pe…",
        "analyzed": "dutiyam pi kho pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.10:2.2",
        "original": "tatiyampi kho āḷavako yakkho bhagavantaṁ etadavoca:",
        "analyzed": "tatiyam pi kho āḷavakaḥ yakkhaḥ bhagavantam etat avoca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.10:2.3",
        "original": "“nikkhama, samaṇā”ti.",
        "analyzed": "nikkhama samaṇa ati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.10:2.4",
        "original": "“Sādhāvuso”ti bhagavā nikkhami.",
        "analyzed": "sādha avuso iti bhagavā nikkhami ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.10:2.5",
        "original": "“Pavisa, samaṇā”ti.",
        "analyzed": "pavisa samaṇa ati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.10:2.6",
        "original": "“Sādhāvuso”ti bhagavā pāvisi.",
        "analyzed": "sādhāvuso iti bhagavā pāvisi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.10:3.1",
        "original": "Catutthampi kho āḷavako yakkho bhagavantaṁ etadavoca:",
        "analyzed": "catuttham pi kho āḷavakaḥ yakkhaḥ bhagavantam etat avoca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.10:3.2",
        "original": "“nikkhama, samaṇā”ti.",
        "analyzed": "nikkhama samaṇa ati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.10:3.3",
        "original": "“Na khvāhaṁ taṁ, āvuso, nikkhamissāmi.",
        "analyzed": "na khvā aham te āvuso nikkhamissāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.10:3.4",
        "original": "Yaṁ te karaṇīyaṁ, taṁ karohī”ti.",
        "analyzed": "yam te karaṇīya tam karohi iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.10:4.1",
        "original": "“Pañhaṁ taṁ, samaṇa, pucchissāmi.",
        "analyzed": "pañham ta samaṇa pucchissāmi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.10:4.2",
        "original": "Sace me na byākarissasi, cittaṁ vā te khipissāmi, hadayaṁ vā te phālessāmi, pādesu vā gahetvā pāragaṅgāya khipissāmī”ti.",
        "analyzed": "sace me na byākarissasi cittam vā te khipissāmi hadayam vā te phālessāmi pādesu vā gahetvā pāragaṅgāya khipissāmi iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.10:5.1_0",
        "original": "“Na khvāhaṁ taṁ, āvuso, passāmi sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā",
        "analyzed": "na khvā aham te āvuso passāmi sa devake loke sa mārake sa brahmake sa ssamaṇa brāhmaṇiyā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.10:5.1_1",
        "original": "pajāya sadevamanussāya yo me cittaṁ vā khipeyya hadayaṁ vā phāleyya pādesu vā gahetvā",
        "analyzed": "pajāya sa deva manussāya yaḥ me cittam vā khipeyya hadayam vā phāleyya pādesu vā gahetvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.10:5.1_2",
        "original": "pāragaṅgāya khipeyya.",
        "analyzed": "pāragaṅgāya khipeyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.10:5.2",
        "original": "Api ca tvaṁ, āvuso, puccha yadākaṅkhasī”ti.",
        "analyzed": "api ca tvai āvuso puccha yat ākaṅkhasi iti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.10:5.3",
        "original": "Atha kho āḷavako yakkho bhagavantaṁ gāthāya ajjhabhāsi:",
        "analyzed": "atha kho āḷavakaḥ yakkhaḥ bhagavantam gāthāya ajjhabhāsi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.10:6.1",
        "original": "“Kiṁ sūdha vittaṁ purisassa seṭṭhaṁ,",
        "analyzed": "kim sūdha vittam purisassa seṭṭha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.10:6.2",
        "original": "Kiṁ su suciṇṇaṁ sukhamāvahāti;",
        "analyzed": "kim su su ciṇṇam sukham āvahāti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.10:6.3",
        "original": "Kiṁ su have sādutaraṁ rasānaṁ,",
        "analyzed": "kim su have sādutaram rasāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.10:6.4",
        "original": "Kathaṁjīviṁ jīvitamāhu seṭṭhaṁ”.",
        "analyzed": "kathajīvim jīvitam āhuḥ seṭṭha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.10:7.1",
        "original": "“Saddhīdha vittaṁ purisassa seṭṭhaṁ,",
        "analyzed": "saddhi idha vittam purisassa seṭṭha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.10:7.2",
        "original": "Dhammo suciṇṇo sukhamāvahāti;",
        "analyzed": "dhammaḥ su ciṇṇaḥ sukham āvahāti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.10:7.3",
        "original": "Saccaṁ have sādutaraṁ rasānaṁ,",
        "analyzed": "saccam have sādutaram rasāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.10:7.4",
        "original": "Paññājīviṁ jīvitamāhu seṭṭhaṁ”.",
        "analyzed": "paññājīvim jīvitam āhu seṭṭha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.10:8.1",
        "original": "“Kathaṁ su tarati oghaṁ,",
        "analyzed": "katham su tarati ogha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.10:8.2",
        "original": "kathaṁ su tarati aṇṇavaṁ;",
        "analyzed": "katham su tarati anṇava ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.10:8.3",
        "original": "Kathaṁ su dukkhamacceti,",
        "analyzed": "katham su ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.10:8.4",
        "original": "kathaṁ su parisujjhati”.",
        "analyzed": "katham su parisujjhati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.10:9.1",
        "original": "“Saddhāya tarati oghaṁ,",
        "analyzed": "saddhāya tarati ogha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.10:9.2",
        "original": "appamādena aṇṇavaṁ;",
        "analyzed": "appamādena aṇṇava ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.10:9.3",
        "original": "Vīriyena dukkhamacceti,",
        "analyzed": "vīriyena dukkham acca iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.10:9.4",
        "original": "paññāya parisujjhati”.",
        "analyzed": "paññāya parisujjhati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.10:10.1",
        "original": "“Kathaṁ su labhate paññaṁ,",
        "analyzed": "katham su labhate pañña ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.10:10.2",
        "original": "kathaṁ su vindate dhanaṁ;",
        "analyzed": "katham su vindate dhana ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.10:10.3",
        "original": "Kathaṁ su kittiṁ pappoti,",
        "analyzed": "katham su kittim pappoti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.10:10.4",
        "original": "kathaṁ mittāni ganthati;",
        "analyzed": "katham mittāni ganthati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.10:10.5",
        "original": "Asmā lokā paraṁ lokaṁ,",
        "analyzed": "asmai loka param loka ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.10:10.6",
        "original": "kathaṁ pecca na socati”.",
        "analyzed": "katham na socati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.10:11.1",
        "original": "“Saddahāno arahataṁ,",
        "analyzed": "sat dahānaḥ arahata ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.10:11.2",
        "original": "dhammaṁ nibbānapattiyā;",
        "analyzed": "dhammam nibbāna pattiyā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.10:11.3",
        "original": "Sussūsaṁ labhate paññaṁ,",
        "analyzed": "sussūsam labhate ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.10:11.4",
        "original": "appamatto vicakkhaṇo.",
        "analyzed": "appamattaḥ vicakkhaṇo ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.10:12.1",
        "original": "Patirūpakārī dhuravā,",
        "analyzed": "pati rūpa kārī dhuravā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.10:12.2",
        "original": "uṭṭhātā vindate dhanaṁ;",
        "analyzed": "uṭṭhātā vindate dhana ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.10:12.3",
        "original": "Saccena kittiṁ pappoti,",
        "analyzed": "saccena kittim pappoti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.10:12.4",
        "original": "dadaṁ mittāni ganthati.",
        "analyzed": "dadam mittāni ganthati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.10:13.1",
        "original": "Yassete caturo dhammā,",
        "analyzed": "yassa ete caturaḥ dhammā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.10:13.2",
        "original": "saddhassa gharamesino;",
        "analyzed": "saddhassa gharamesinaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.10:13.3",
        "original": "Saccaṁ dhammo dhiti cāgo,",
        "analyzed": "saccam dhammaḥ cāgaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.10:13.4",
        "original": "sa ve pecca na socati.",
        "analyzed": "sa ve pet ca na socati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.10:14.1",
        "original": "Iṅgha aññepi pucchassu,",
        "analyzed": "iṅgha aññe pi pucchassu ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.10:14.2",
        "original": "puthū samaṇabrāhmaṇe;",
        "analyzed": "puthū samaṇa brāhmaṇe ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.10:14.3",
        "original": "Yadi saccā damā cāgā,",
        "analyzed": "yadi sat cā damā ca āgā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.10:14.4",
        "original": "khantyā bhiyyodha vijjati”.",
        "analyzed": "khantyā bhiyyaḥ udha vijjati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.10:15.1",
        "original": "“Kathaṁ nu dāni puccheyyaṁ,",
        "analyzed": "katham nu dāni puccheyya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.10:15.2",
        "original": "Puthū samaṇabrāhmaṇe;",
        "analyzed": "puthū samaṇa brāhmaṇe ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.10:15.3",
        "original": "Yohaṁ ajja pajānāmi,",
        "analyzed": "yaḥ ham ajja pajānāmi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.10:15.4",
        "original": "Yo attho samparāyiko.",
        "analyzed": "yaḥ attho samparāyiko ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.10:16.1",
        "original": "Atthāya vata me buddho,",
        "analyzed": "atthāya vata me buddhaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.10:16.2",
        "original": "Vāsāyāḷavimāgamā;",
        "analyzed": "vāsā āyāḷa vima āgamā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.10:16.3",
        "original": "Yohaṁ ajja pajānāmi,",
        "analyzed": "yaḥ ham ajja pajānāmi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.10:16.4",
        "original": "Yattha dinnaṁ mahapphalaṁ.",
        "analyzed": "yattha dinnam mahapphala ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.10:17.1",
        "original": "So ahaṁ vicarissāmi,",
        "analyzed": "saḥ aham vicarissāmi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.10:17.2",
        "original": "Gāmā gāmaṁ purā puraṁ;",
        "analyzed": "gāmā gāmam purā pura ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.10:17.3",
        "original": "Namassamāno sambuddhaṁ,",
        "analyzed": "namassamānaḥ sambuddha ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.10:17.4",
        "original": "Dhammassa ca sudhammatan”ti.",
        "analyzed": "dhammassa ca su dhamma tanti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.10:17.5",
        "original": "Āḷavakasuttaṁ dasamaṁ.",
        "analyzed": "āḷavaka suttam dasama ",
        "folio": "17"
    }
]