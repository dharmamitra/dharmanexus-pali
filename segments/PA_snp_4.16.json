[
    {
        "segmentnr": "PA_snp_4.16:0.1",
        "original": "Sutta Nipāta 4.16",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.16:0.2",
        "original": "Sāriputtasutta",
        "analyzed": "sāriputta sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.16:1.1",
        "original": "“Na me diṭṭho ito pubbe,",
        "analyzed": "na me diṭṭhaḥ itas pubbe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.16:1.2",
        "original": "(iccāyasmā sāriputto)",
        "analyzed": "iccā ayasmā sāriputto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.16:1.3",
        "original": "Na suto uda kassaci;",
        "analyzed": "na sutaḥ uda kassaci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.16:1.4",
        "original": "Evaṁ vagguvado satthā,",
        "analyzed": "evam vaggu vado satthā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.16:1.5",
        "original": "Tusitā gaṇimāgato.",
        "analyzed": "tusitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.16:2.1",
        "original": "Sadevakassa lokassa,",
        "analyzed": "sa devakassa lokassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.16:2.2",
        "original": "yathā dissati cakkhumā;",
        "analyzed": "yathā dissati cakkhumā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.16:2.3",
        "original": "Sabbaṁ tamaṁ vinodetvā,",
        "analyzed": "sabbam tamam vinodetvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.16:2.4",
        "original": "ekova ratimajjhagā.",
        "analyzed": "ekā iva rati majjhagā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.16:3.1",
        "original": "Taṁ buddhaṁ asitaṁ tādiṁ,",
        "analyzed": "tam buddham asitam tādi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.16:3.2",
        "original": "akuhaṁ gaṇimāgataṁ;",
        "analyzed": "akuham gaṇim āgata ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.16:3.3",
        "original": "Bahūnamidha baddhānaṁ,",
        "analyzed": "bahūnamidha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.16:3.4",
        "original": "atthi pañhena āgamaṁ.",
        "analyzed": "atthi pañhena āgama ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.16:4.1",
        "original": "Bhikkhuno vijigucchato,",
        "analyzed": "bhikkhunaḥ vijigucchataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.16:4.2",
        "original": "bhajato rittamāsanaṁ;",
        "analyzed": "bhajataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.16:4.3",
        "original": "Rukkhamūlaṁ susānaṁ vā,",
        "analyzed": "rukkha mūlam susānam vā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.16:4.4",
        "original": "pabbatānaṁ guhāsu vā.",
        "analyzed": "pabbatānam guhāsu vā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.16:5.1",
        "original": "Uccāvacesu sayanesu,",
        "analyzed": "uccāvacesu sayanesu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.16:5.2",
        "original": "kīvanto tattha bheravā;",
        "analyzed": "kīvantaḥ tattha bheravā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.16:5.3",
        "original": "Yehi bhikkhu na vedheyya,",
        "analyzed": "yehi bhikkhu na vedheyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.16:5.4",
        "original": "nigghose sayanāsane.",
        "analyzed": "nigghose sayanāsane ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.16:6.1",
        "original": "Katī parissayā loke,",
        "analyzed": "katī parissayāḥ loke ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.16:6.2",
        "original": "gacchato agataṁ disaṁ;",
        "analyzed": "gacchataḥ agatam disa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.16:6.3",
        "original": "Ye bhikkhu abhisambhave,",
        "analyzed": "ye bhikkhu abhisambhave ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.16:6.4",
        "original": "pantamhi sayanāsane.",
        "analyzed": "sayanāsane ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.16:7.1",
        "original": "Kyāssa byappathayo assu,",
        "analyzed": "assu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.16:7.2",
        "original": "kyāssassu idha gocarā;",
        "analyzed": "kyāssassu idha gocarā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.16:7.3",
        "original": "Kāni sīlabbatānāssu,",
        "analyzed": "kāni ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.16:7.4",
        "original": "pahitattassa bhikkhuno.",
        "analyzed": "pahitattassa bhikkhuno ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.16:8.1",
        "original": "Kaṁ so sikkhaṁ samādāya,",
        "analyzed": "kam saḥ sikkham samādāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.16:8.2",
        "original": "ekodi nipako sato;",
        "analyzed": "eka udi nipakaḥ sato ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.16:8.3",
        "original": "Kammāro rajatasseva,",
        "analyzed": "kammāraḥ rajatassa iva ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.16:8.4",
        "original": "niddhame malamattano”.",
        "analyzed": "niddhame malamattano ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.16:9.1",
        "original": "“Vijigucchamānassa yadidaṁ phāsu,",
        "analyzed": "vijigucchamānassa yat idam phāsu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.16:9.2",
        "original": "(sāriputtāti bhagavā)",
        "analyzed": "sāriputta ati bhagavā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.16:9.3",
        "original": "Rittāsanaṁ sayanaṁ sevato ce;",
        "analyzed": "ritta āsanam sayanam sevataḥ ce ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.16:9.4",
        "original": "Sambodhikāmassa yathānudhammaṁ,",
        "analyzed": "sambodhi kāmassa yathā anudhamma ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.16:9.5",
        "original": "Taṁ te pavakkhāmi yathā pajānaṁ.",
        "analyzed": "tam te pavakkhāmi yathā pajāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.16:10.1",
        "original": "Pañcannaṁ dhīro bhayānaṁ na bhāye,",
        "analyzed": "pañca annam dhīraḥ bhayānam na bhāye ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.16:10.2",
        "original": "Bhikkhu sato sapariyantacārī;",
        "analyzed": "bhikkhu sataḥ sapariyanta cārī ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.16:10.3",
        "original": "Ḍaṁsādhipātānaṁ sarīsapānaṁ,",
        "analyzed": "sarīsapāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.16:10.4",
        "original": "Manussaphassānaṁ catuppadānaṁ.",
        "analyzed": "manussa phassānam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.16:11.1",
        "original": "Paradhammikānampi na santaseyya,",
        "analyzed": "para dhammikānam pi na santaseyya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.16:11.2",
        "original": "Disvāpi tesaṁ bahubheravāni;",
        "analyzed": "bahu bheravāni ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.16:11.3",
        "original": "Athāparāni abhisambhaveyya,",
        "analyzed": "atha aparāni abhisambhaveyya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.16:11.4",
        "original": "Parissayāni kusalānuesī.",
        "analyzed": "kusala anuesī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.16:12.1",
        "original": "Ātaṅkaphassena khudāya phuṭṭho,",
        "analyzed": "ātaṅka phassena khudāya phuṭṭhaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.16:12.2",
        "original": "Sītaṁ athuṇhaṁ adhivāsayeyya;",
        "analyzed": "sītam athuṇham adhivāsayeyya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.16:12.3",
        "original": "So tehi phuṭṭho bahudhā anoko,",
        "analyzed": "saḥ tehi phuṭṭhaḥ bahudhā anokaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.16:12.4",
        "original": "Viriyaṁ parakkammadaḷhaṁ kareyya.",
        "analyzed": "viriyam parakkamma daḷham kareyya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.16:13.1",
        "original": "Theyyaṁ na kāre na musā bhaṇeyya,",
        "analyzed": "theyyam na kāre na musā bhaṇeyya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.16:13.2",
        "original": "Mettāya phasse tasathāvarāni;",
        "analyzed": "mettāya phasse tasathāvarāni ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.16:13.3",
        "original": "Yadāvilattaṁ manaso vijaññā,",
        "analyzed": "manasaḥ vijaññā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.16:13.4",
        "original": "Kaṇhassa pakkhoti vinodayeyya.",
        "analyzed": "kaṇhassa pakkhaḥ iti vinodayeyya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.16:14.1",
        "original": "Kodhātimānassa vasaṁ na gacche,",
        "analyzed": "kodha ati mānassa vasam na gacche ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.16:14.2",
        "original": "Mūlampi tesaṁ palikhañña tiṭṭhe;",
        "analyzed": "mūlam pi tesam palikhañña tiṭṭhe ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.16:14.3",
        "original": "Athappiyaṁ vā pana appiyaṁ vā,",
        "analyzed": "athappiyam vā pana appiyam vā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.16:14.4",
        "original": "Addhā bhavanto abhisambhaveyya.",
        "analyzed": "addhā bhavantaḥ abhisambhaveyya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.16:15.1",
        "original": "Paññaṁ purakkhatvā kalyāṇapīti,",
        "analyzed": "paññam purakkhatvā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.16:15.2",
        "original": "Vikkhambhaye tāni parissayāni;",
        "analyzed": "vikkhambhaye tāni parissayāni ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.16:15.3",
        "original": "Aratiṁ sahetha sayanamhi pante,",
        "analyzed": "aratim sahetha sayanamhi pante ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.16:15.4",
        "original": "Caturo sahetha paridevadhamme.",
        "analyzed": "caturaḥ sahetha parideva dhamme ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.16:16.1",
        "original": "Kiṁsū asissāmi kuva vā asissaṁ,",
        "analyzed": "kisū asissāmi kuva vā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.16:16.2",
        "original": "Dukkhaṁ vata settha kvajja sessaṁ;",
        "analyzed": "dukkham vata settha kvajja sessa ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.16:16.3",
        "original": "Ete vitakke paridevaneyye,",
        "analyzed": "ete vitakke paridevaneyye ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.16:16.4",
        "original": "Vinayetha sekho aniketacārī.",
        "analyzed": "vinayetha sekhaḥ a niketa cārī ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.16:17.1",
        "original": "Annañca laddhā vasanañca kāle,",
        "analyzed": "annañca laddhā vasanañca kāle ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.16:17.2",
        "original": "Mattaṁ so jaññā idha tosanatthaṁ;",
        "analyzed": "mattam saḥ jaññāḥ idha tosanattha ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.16:17.3",
        "original": "So tesu gutto yatacāri gāme,",
        "analyzed": "saḥ tesu guttaḥ yata cāri gāme ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.16:17.4",
        "original": "Rusitopi vācaṁ pharusaṁ na vajjā.",
        "analyzed": "rusitaḥ upi vācam pharusam na vajjā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.16:18.1",
        "original": "Okkhittacakkhu na ca pādalolo,",
        "analyzed": "na ca pāda lolo ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.16:18.2",
        "original": "Jhānānuyutto bahujāgarassa;",
        "analyzed": "jhāna anuyuttaḥ bahu jāgarassa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.16:18.3",
        "original": "Upekkhamārabbha samāhitatto,",
        "analyzed": "upekkham ārabbha samāhitattaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.16:18.4",
        "original": "Takkāsayaṁ kukkucciyūpachinde.",
        "analyzed": "takkā sayam kukkucciyu upachinde ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.16:19.1",
        "original": "Cudito vacībhi satimābhinande,",
        "analyzed": "cuditaḥ vacībhiḥ satim ā abhinande ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.16:19.2",
        "original": "Sabrahmacārīsu khilaṁ pabhinde;",
        "analyzed": "sabrahmacārīsu khilam pabhinde ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.16:19.3",
        "original": "Vācaṁ pamuñce kusalaṁ nātivelaṁ,",
        "analyzed": "vācam pamuñce kusalam ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.16:19.4",
        "original": "Janavādadhammāya na cetayeyya.",
        "analyzed": "janavāda dhammāya na cetayeyya ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.16:20.1",
        "original": "Athāparaṁ pañca rajāni loke,",
        "analyzed": "atha aparam pañca rajāni loke ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.16:20.2",
        "original": "Yesaṁ satīmā vinayāya sikkhe;",
        "analyzed": "vinayāya sikkhe ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.16:20.3",
        "original": "Rūpesu saddesu atho rasesu,",
        "analyzed": "rūpesu saddesu atho rasesu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.16:20.4",
        "original": "Gandhesu phassesu sahetha rāgaṁ.",
        "analyzed": "gandhesu phassesu sahetha rāga ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.16:21.1",
        "original": "Etesu dhammesu vineyya chandaṁ,",
        "analyzed": "etesu dhammesu vineyya chanda ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_4.16:21.2",
        "original": "Bhikkhu satimā suvimuttacitto;",
        "analyzed": "bhikkhu satimā su vimutta cittaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_4.16:21.3",
        "original": "Kālena so sammā dhammaṁ parivīmaṁsamāno,",
        "analyzed": "kālena so sammā dhammam parivīmasamānaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_4.16:21.4",
        "original": "Ekodibhūto vihane tamaṁ so”ti.",
        "analyzed": "eka udi bhūtaḥ vihane tamam ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_4.16:21.5",
        "original": "Sāriputtasuttaṁ soḷasamaṁ.",
        "analyzed": "sāriputta suttam soḷasama ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_4.16:21.6",
        "original": "Aṭṭhakavaggo catuttho.",
        "analyzed": "aṭṭhaka vaggo catutthaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_4.16:22.0",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_4.16:22.1",
        "original": "Kāmaṁ guhañca duṭṭhā ca,",
        "analyzed": "kāmam guham ca duṭṭhā ca ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_4.16:22.2",
        "original": "suddhañca paramā jarā;",
        "analyzed": "paramā jarā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_4.16:22.3",
        "original": "Metteyyo ca pasūro ca,",
        "analyzed": "metteyyaḥ ca pasūraḥ ca ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_4.16:22.4",
        "original": "māgaṇḍi purābhedanaṁ.",
        "analyzed": "māgaṇḍi purābhedana ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_4.16:23.1",
        "original": "Kalahaṁ dve ca byūhāni,",
        "analyzed": "kalaham dve ca byūhāni ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_4.16:23.2",
        "original": "punadeva tuvaṭṭakaṁ;",
        "analyzed": "punadeva tuvaṭṭaka ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_4.16:23.3",
        "original": "Attadaṇḍavaraṁ suttaṁ,",
        "analyzed": "atta daṇḍa varam sutta ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_4.16:23.4",
        "original": "therapuṭṭhena soḷasa;",
        "analyzed": "thera puṭṭhena soḷasa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_4.16:23.5",
        "original": "Iti etāni suttāni,",
        "analyzed": "iti etāni suttāni ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_4.16:23.6",
        "original": "sabbānaṭṭhakavaggikāti.",
        "analyzed": "sabba an aṭṭhaka vaggikā ti ",
        "folio": "23"
    }
]