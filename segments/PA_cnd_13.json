        "folio": "17"
    },
    {
        "segmentnr": "PA_cnd_13:17.2",
        "original": "Sakkāti sakko bhagavā.",
        "analyzed": "sakkā ati sakkaḥ bhagavā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_cnd_13:17.3",
        "original": "Sakyakulā pabbajitotipi sakko.",
        "analyzed": "sakya kulā pabbajito tipi sakko ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_cnd_13:17.4",
        "original": "Atha vā aḍḍho mahaddhano dhanavātipi sakko.",
