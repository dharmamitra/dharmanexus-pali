    },
    {
        "segmentnr": "PA_ja_484:0.3",
        "original": "Sālikedāravagga",
        "analyzed": "sāli kedāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_484:0.4",
        "original": "1. Sālikedārajātaka",
        "analyzed": "sā ali kedārajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_484:1.1",
        "original": "“Sampannaṁ sālikedāraṁ,",
        "analyzed": "sampannam sālikedāra ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_484:1.2",
        "original": "suvā bhuñjanti kosiya;",
        "analyzed": "suva bhuñjanti kosiya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_484:1.3",
        "original": "Paṭivedemi te brahme,",
        "analyzed": "paṭivedemi te brahme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_484:1.4",
        "original": "na ne vāretumussahe.",
        "analyzed": "na ne vāretum ussahe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_484:2.1",
        "original": "Eko ca tattha sakuṇo,",
        "analyzed": "ekaḥ ca tattha sakuṇaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_484:2.2",
        "original": "yo nesaṁ sabbasundaro;",
        "analyzed": "yaḥ nesam sabba sundaraḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_484:2.3",
        "original": "Bhutvā sāliṁ yathākāmaṁ,",
        "analyzed": "bhutvā sālim yathākāma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_484:2.4",
        "original": "tuṇḍenādāya gacchati”.",
        "analyzed": "tuṇḍena ādāya gacchati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_484:3.1",
        "original": "“Oḍḍentu vāḷapāsāni,",
        "analyzed": "oḍḍentu vāḷapāsāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_484:3.2",
        "original": "yathā bajjhetha so dijo;",
        "analyzed": "yathā bajjhetha saḥ dijaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_484:3.3",
        "original": "Jīvañca naṁ gahetvāna,",
        "analyzed": "jīvan ca nam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_484:3.4",
        "original": "ānayehi mamantike”.",
        "analyzed": "ānayehi mama antike ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_484:4.1",
        "original": "“Ete bhutvā pivitvā ca,",
        "analyzed": "ete bhutvā pivitvā ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_484:4.2",
        "original": "pakkamanti vihaṅgamā;",
        "analyzed": "pakkamanti vihaṅgamā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_484:4.3",
        "original": "Eko baddhosmi pāsena,",
        "analyzed": "ekaḥ baddhaḥ asmi pāsena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_484:4.4",
        "original": "kiṁ pāpaṁ pakataṁ mayā”.",
        "analyzed": "kim pāpam pakatam mayā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_484:5.1",
        "original": "“Udaraṁ nūna aññesaṁ,",
        "analyzed": "udaram nūna aññā īsa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_484:5.2",
        "original": "suva accodaraṁ tava;",
        "analyzed": "suva acca udaram tava ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_484:5.3",
        "original": "Bhutvā sāliṁ yathākāmaṁ,",
        "analyzed": "bhutvā sālim yathākāma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_484:5.4",
        "original": "tuṇḍenādāya gacchasi.",
        "analyzed": "tuṇḍena ādāya gacchasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_484:6.1",
        "original": "Koṭṭhaṁ nu tattha pūresi,",
        "analyzed": "koṭṭham nu tattha pūresi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_484:6.2",
        "original": "suva veraṁ nu te mayā;",
        "analyzed": "suva veram nu te mayā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_484:6.3",
        "original": "Puṭṭho me samma akkhāhi,",
        "analyzed": "puṭṭhaḥ me samma akkhāhi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_484:6.4",
        "original": "kuhiṁ sāliṁ nidāhasi”.",
        "analyzed": "kuhim sālim nidāhasi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_484:7.1",
        "original": "“Na me veraṁ tayā saddhiṁ,",
        "analyzed": "na me veram tayā saddhi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_484:7.2",
        "original": "koṭṭho mayhaṁ na vijjati;",
        "analyzed": "koṭṭhaḥ mayham na vijjati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_484:7.3",
        "original": "Iṇaṁ muñcāmiṇaṁ dammi,",
        "analyzed": "iṇam muñcāmi iṇam dammi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_484:7.4",
        "original": "sampatto koṭasimbaliṁ;",
        "analyzed": "sampattaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_484:7.5",
        "original": "Nidhimpi tattha nidahāmi,",
        "analyzed": "nidhim pi tattha nidahāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_484:7.6",
        "original": "evaṁ jānāhi kosiya”.",
        "analyzed": "evam jānāhi kosiya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_484:8.1",
        "original": "“Kīdisaṁ te iṇadānaṁ,",
        "analyzed": "kīdisam te iṇa dāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_484:8.2",
        "original": "iṇamokkho ca kīdiso;",
        "analyzed": "iṇamokkhaḥ ca kīdiso ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_484:8.3",
        "original": "Nidhinidhānamakkhāhi,",
        "analyzed": "nidhi nidhānam akkhāhi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_484:8.4",
        "original": "atha pāsā pamokkhasi”.",
        "analyzed": "atha pāsā pamokkhasi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_484:9.1",
        "original": "“Ajātapakkhā taruṇā,",
        "analyzed": "a jāta pakkhā taruṇā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_484:9.2",
        "original": "puttakā mayha kosiya;",
        "analyzed": "puttakāḥ mayha kosiya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_484:9.3",
        "original": "Te maṁ bhatā bharissanti,",
        "analyzed": "te mam bhatāḥ bharissanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_484:9.4",
        "original": "tasmā tesaṁ iṇaṁ dade.",
        "analyzed": "tasmai iṇam dade ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_484:10.1",
        "original": "Mātā pitā ca me vuddhā,",
        "analyzed": "mātā pitā ca me vuddhā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_484:10.2",
        "original": "jiṇṇakā gatayobbanā;",
        "analyzed": "jiṇṇakā gata yobbanā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_484:10.3",
        "original": "Tesaṁ tuṇḍena hātūna,",
        "analyzed": "tesam tuṇḍena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_484:10.4",
        "original": "muñce pubbakataṁ iṇaṁ.",
        "analyzed": "muñce pubba katam iṇa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_484:11.1",
        "original": "Aññepi tattha sakuṇā,",
        "analyzed": "aññe pi tattha sakuṇā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_484:11.2",
        "original": "khīṇapakkhā sudubbalā;",
        "analyzed": "khīṇa pakkhā su dubbalā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_484:11.3",
        "original": "Tesaṁ puññatthiko dammi,",
        "analyzed": "tesam puññatthikaḥ dammi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_484:11.4",
        "original": "taṁ nidhiṁ āhu paṇḍitā.",
        "analyzed": "tam nidhim āhu paṇḍitā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_484:12.1",
        "original": "Īdisaṁ me iṇadānaṁ,",
        "analyzed": "īdisam me iṇa dāna ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_484:12.2",
        "original": "iṇamokkho ca īdiso;",
        "analyzed": "iṇamokkhaḥ ca īdisaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_484:12.3",
        "original": "Nidhinidhānamakkhāmi,",
        "analyzed": "nidhi nidhānam akkhāmi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_484:12.4",
        "original": "evaṁ jānāhi kosiya”.",
        "analyzed": "evam jānāhi kosiya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_484:13.1",
        "original": "“Bhaddako vatayaṁ pakkhī,",
        "analyzed": "bhaddakaḥ vatayam pakkhī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_484:13.2",
        "original": "dijo paramadhammiko;",
        "analyzed": "dijo parama dhammiko ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_484:13.3",
        "original": "Ekaccesu manussesu,",
        "analyzed": "ekaccesu manussesu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_484:13.4",
        "original": "ayaṁ dhammo na vijjati.",
        "analyzed": "ayam dhammaḥ na vijjati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_484:14.1",
        "original": "Bhuñja sāliṁ yathākāmaṁ,",
        "analyzed": "bhuñja sālim yathākāma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_484:14.2",
        "original": "saha sabbehi ñātibhi;",
        "analyzed": "saha sabbehi ñātibhi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_484:14.3",
        "original": "Punāpi suva passemu,",
        "analyzed": "puna api suva passemu ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_484:14.4",
        "original": "piyaṁ me tava dassanaṁ”.",
        "analyzed": "piyam me tava dassana ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_484:15.1",
        "original": "“Bhuttañca pītañca tavassamamhi,",
        "analyzed": "bhuttañca pītañca tavassamamhi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_484:15.2",
        "original": "Ratī ca no kosiya te sakāse;",
        "analyzed": "ratī ca naḥ kosiya te sakāse ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_484:15.3",
        "original": "Nikkhittadaṇḍesu dadāhi dānaṁ,",
        "analyzed": "nikkhitta daṇḍesu dadāhi dāna ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_484:15.4",
        "original": "Jiṇṇe ca mātāpitaro bharassu”.",
        "analyzed": "jiṇṇe ca mātā pitaraḥ bharassu ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_484:16.1",
        "original": "“Lakkhī vata me udapādi ajja,",
        "analyzed": "lakkhī vata me udapādi ajja ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_484:16.2",
        "original": "Yo addasāsiṁ pavaraṁ dijānaṁ;",
        "analyzed": "yaḥ addasā asim pavaram dijāna ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_484:16.3",
        "original": "Suvassa sutvāna subhāsitāni,",
        "analyzed": "suvassa sutvāna su bhāsitāni ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_484:16.4",
        "original": "Kāhāmi puññāni anappakāni”.",
        "analyzed": "kāhāmi puññāni anappakāni ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_484:17.1",
        "original": "So kosiyo attamano udaggo,",
        "analyzed": "saḥ kosiyaḥ atta manaḥ udaggaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_484:17.2",
        "original": "Annañca pānañcabhisaṅkharitvā;",
        "analyzed": "annañca pānañca abhisaṅkharitvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_484:17.3",
        "original": "Annena pānena pasannacitto,",
        "analyzed": "annena pānena pasanna cittaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_484:17.4",
        "original": "Santappayi samaṇabrāhmaṇe cāti.",
        "analyzed": "santappayi samaṇa brāhmaṇe ca ati ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_484:18.1",
        "original": "Sālikedārajātakaṁ paṭhamaṁ.",
        "analyzed": "sāli kedāra jātakam ",
        "folio": "18"
    }
]