[
    {
        "segmentnr": "PA_ja_417:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_417:0.2",
        "original": "Aṭṭhakanipāta",
        "analyzed": "aṭṭhaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_417:0.3",
        "original": "Kaccānivagga",
        "analyzed": "kaccā nivagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_417:0.4",
        "original": "1. Kaccānijātaka",
        "analyzed": "kaccānijātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_417:1.1",
        "original": "“Odātavatthā suci allakesā,",
        "analyzed": "odātavatthā suci alla kesā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_417:1.2",
        "original": "Kaccāni kiṁ kumbhimadhissayitvā;",
        "analyzed": "kaccāni kim ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_417:1.3",
        "original": "Piṭṭhā tilā dhovasi taṇḍulāni,",
        "analyzed": "piṭṭhā tilā dhovasi taṇḍulāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_417:1.4",
        "original": "Tilodano hehiti kissahetu”.",
        "analyzed": "tilodanaḥ hehiti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_417:2.1",
        "original": "“Na kho ayaṁ brāhmaṇa bhojanatthā,",
        "analyzed": "na kho ayam brāhmaṇa bhojanatthā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_417:2.2",
        "original": "Tilodano hehiti sādhupakko;",
        "analyzed": "tilodanaḥ sādhu pakkaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_417:2.3",
        "original": "Dhammo mato tassa pahuttamajja,",
        "analyzed": "dhammo mataḥ tassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_417:2.4",
        "original": "Ahaṁ karissāmi susānamajjhe”.",
        "analyzed": "aham karissāmi susāna majjhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_417:3.1",
        "original": "“Anuvicca kaccāni karohi kiccaṁ,",
        "analyzed": "anuvicca kaccāni karohi kicca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_417:3.2",
        "original": "Dhammo mato ko nu taveva saṁsi;",
        "analyzed": "dhammaḥ mataḥ kaḥ nu tava iva sasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_417:3.3",
        "original": "Sahassanetto atulānubhāvo,",
        "analyzed": "sahassanettaḥ atula anubhāvaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_417:3.4",
        "original": "Na miyyatī dhammavaro kadāci”.",
        "analyzed": "na miyyatī dhamma varo kadāci ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_417:4.1",
        "original": "“Daḷhappamāṇaṁ mama ettha brahme,",
        "analyzed": "daḷha ppamāṇam mama ettha brahme ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_417:4.2",
        "original": "Dhammo mato natthi mamettha kaṅkhā;",
        "analyzed": "dhammaḥ mataḥ na atthi mama ittha kaṅkhā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_417:4.3",
        "original": "Yeyeva dāni pāpā bhavanti,",
        "analyzed": "ye iye iva dāni pāpāḥ bhavanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_417:4.4",
        "original": "Te teva dāni sukhitā bhavanti.",
        "analyzed": "te teva dāni sukhitāḥ bhavanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_417:5.1",
        "original": "Suṇisā hi mayhaṁ vañjhā ahosi,",
        "analyzed": "su nisā hi mayham vañjhā ahosi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_417:5.2",
        "original": "Sā maṁ vadhitvāna vijāyi puttaṁ;",
        "analyzed": "sā mam vadhitvāna vijāyi putta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_417:5.3",
        "original": "Sā dāni sabbassa kulassa issarā,",
        "analyzed": "sā dāni sabbassa kulassa issarā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_417:5.4",
        "original": "Ahaṁ panamhi apaviddhā ekikā”.",
        "analyzed": "aham pana amhi apaviddhā ekikā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_417:6.1",
        "original": "“Jīvāmi vohaṁ na matohamasmi,",
        "analyzed": "jīvāmi vā ūham na mata ūham asmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_417:6.2",
        "original": "Taveva atthāya idhāgatosmi;",
        "analyzed": "tava iva atthāya idha āgataḥ asmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_417:6.3",
        "original": "Yā taṁ vadhitvāna vijāyi puttaṁ,",
        "analyzed": "yā tam vadhitvāna vijāyi putta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_417:6.4",
        "original": "Sahāva puttena karomi bhasmaṁ”.",
        "analyzed": "saha ava puttena karomi bhasma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_417:7.1",
        "original": "“Evañca te ruccati devarāja,",
        "analyzed": "evañca te ruccati devarāja ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_417:7.2",
        "original": "Mameva atthāya idhāgatosi;",
        "analyzed": "mama eva atthāya idha āgataḥ asi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_417:7.3",
        "original": "Ahañca putto suṇisā ca nattā,",
        "analyzed": "aham ca putto suṇisā ca nattā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_417:7.4",
        "original": "Sammodamānā gharamāvasema”.",
        "analyzed": "sammodamānāḥ gharam āvasema ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_417:8.1",
        "original": "“Evañca te ruccati kātiyāni,",
        "analyzed": "evañca te ruccati kātiyāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_417:8.2",
        "original": "Hatāpi santā na jahāsi dhammaṁ;",
        "analyzed": "hatā api santā na jahāsi dhamma ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_417:8.3",
        "original": "Tuvañca putto suṇisā ca nattā,",
        "analyzed": "tuvam ca puttaḥ suṇisā ca nattā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_417:8.4",
        "original": "Sammodamānā gharamāvasetha.",
        "analyzed": "sammodamānā gharam āvasetha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_417:9.1",
        "original": "Sā kātiyānī suṇisāya saddhiṁ,",
        "analyzed": "sā kātiyānī suṇisāya saddhi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_417:9.2",
        "original": "Sammodamānā gharamāvasittha;",
        "analyzed": "sammodamānāḥ gharam āvasittha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_417:9.3",
        "original": "Putto ca nattā ca upaṭṭhahiṁsu,",
        "analyzed": "putto ca nattā ca upaṭṭhahisu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_417:9.4",
        "original": "Devānamindena adhiggahītā”ti.",
        "analyzed": "devānamindena adhiggahītā ati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_417:10.1",
        "original": "Kaccānijātakaṁ paṭhamaṁ.",
        "analyzed": "kaccānijātakam ",
        "folio": "10"
    }
]