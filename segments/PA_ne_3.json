[
    {
        "segmentnr": "PA_ne_3:0.1",
        "original": "Netti",
        "analyzed": "na itti ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ne_3:0.2",
        "original": "3. Niddesavāra",
        "analyzed": "niddesavāra ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ne_3:1.1",
        "original": "Tattha saṅkhepato netti kittitā.",
        "analyzed": "tattha saṅkhepataḥ na itti kitti tā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ne_3:2.0",
        "original": "3.1. Hārasaṅkhepa",
        "analyzed": "hāra saṅkhepa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_3:2.1",
        "original": "Assādādīnavatā,",
        "analyzed": "as sāda ādīnavatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_3:2.2",
        "original": "Nissaraṇampi ca phalaṁ upāyo ca;",
        "analyzed": "nissaraṇam pi ca phalam upāyaḥ ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_3:2.3",
        "original": "Āṇattī ca bhagavato,",
        "analyzed": "āṇattī ca bhagavataḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_3:2.4",
        "original": "Yogīnaṁ desanāhāro.",
        "analyzed": "desanā āhāro ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_3:3.1",
        "original": "Yaṁ pucchitañca vissajjitañca,",
        "analyzed": "yam pucchitañca vissajjitañca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_3:3.2",
        "original": "Suttassa yā ca anugīti;",
        "analyzed": "suttassa yā ca anugīti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_3:3.3",
        "original": "Suttassa yo pavicayo,",
        "analyzed": "suttassa yo pavicayo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_3:3.4",
        "original": "Hāro vicayoti niddiṭṭho.",
        "analyzed": "hāraḥ vicayaḥ ti niddiṭṭho ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_3:4.1",
        "original": "Sabbesaṁ hārānaṁ,",
        "analyzed": "sabbesam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_3:4.2",
        "original": "Yā bhūmī yo ca gocaro tesaṁ;",
        "analyzed": "yā bhūmī yā ca gocaraḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_3:4.3",
        "original": "Yuttāyuttaparikkhā,",
        "analyzed": "yutta ayutta parikkhā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_3:4.4",
        "original": "Hāro yuttīti niddiṭṭho.",
        "analyzed": "hāraḥ yuttī iti niddiṭṭhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_3:5.1",
        "original": "Dhammaṁ deseti jino,",
        "analyzed": "dhammam deseti jinaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_3:5.2",
        "original": "Tassa ca dhammassa yaṁ padaṭṭhānaṁ;",
        "analyzed": "tassa ca dhammassa yam padaṭṭhāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_3:5.3",
        "original": "Iti yāva sabbadhammā,",
        "analyzed": "iti yāva sabba dhammā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_3:5.4",
        "original": "Eso hāro padaṭṭhāno.",
        "analyzed": "eso hāro padaṭṭhāno ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_3:6.1",
        "original": "Vuttamhi ekadhamme,",
        "analyzed": "vuttamhi ekadhamme ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_3:6.2",
        "original": "Ye dhammā ekalakkhaṇā keci;",
        "analyzed": "ye dhammā eka lakkhaṇā keci ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_3:6.3",
        "original": "Vuttā bhavanti sabbe,",
        "analyzed": "vuttāḥ bhavanti sabbe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_3:6.4",
        "original": "So hāro lakkhaṇo nāma.",
        "analyzed": "so hāraḥ lakkhaṇaḥ nāma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_3:7.1",
        "original": "Neruttamadhippāyo,",
        "analyzed": "neḥ uttam adhippāyaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_3:7.2",
        "original": "Byañjanamatha desanānidānañca;",
        "analyzed": "byañjanam atha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_3:7.3",
        "original": "Pubbāparānusandhī,",
        "analyzed": "pubba apara anusandhī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_3:7.4",
        "original": "Eso hāro catubyūho.",
        "analyzed": "esaḥ hāraḥ catubyūhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_3:8.1",
        "original": "Ekamhi padaṭṭhāne,",
        "analyzed": "ekamhi padaṭṭhāne ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ne_3:8.2",
        "original": "Pariyesati sesakaṁ padaṭṭhānaṁ;",
        "analyzed": "pariyesati sesakam padaṭṭhāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ne_3:8.3",
        "original": "Āvaṭṭati paṭipakkhe,",
        "analyzed": "āvaṭṭati paṭipakkhe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ne_3:8.4",
        "original": "Āvaṭṭo nāma so hāro.",
        "analyzed": "āvaṭṭaḥ nāma saḥ hāraḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ne_3:9.1",
        "original": "Dhammañca padaṭṭhānaṁ,",
        "analyzed": "dhammañca padaṭṭhāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ne_3:9.2",
        "original": "Bhūmiñca vibhajjate ayaṁ hāro;",
        "analyzed": "bhūmiñca vibhajjate ayam hāraḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ne_3:9.3",
        "original": "Sādhāraṇe asādhā-",
        "analyzed": "sādhāraṇe asādhā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ne_3:9.4",
        "original": "Raṇe ca neyyo vibhattīti.",
        "analyzed": "raṇe ca na iyyaḥ vibhatti iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ne_3:10.1",
        "original": "Kusalākusale dhamme,",
        "analyzed": "kusala akusale dhamme ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ne_3:10.2",
        "original": "Niddiṭṭhe bhāvite pahīne ca;",
        "analyzed": "niddiṭṭhe bhāvite pahīne ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ne_3:10.3",
        "original": "Parivattati paṭipakkhe,",
        "analyzed": "parivattati paṭipakkhe ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ne_3:10.4",
        "original": "Hāro parivattano nāma.",
        "analyzed": "hāraḥ parivattanaḥ nāma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ne_3:11.1",
        "original": "Vevacanāni bahūni tu,",
        "analyzed": "vevacanāni bahūni tu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ne_3:11.2",
        "original": "Sutte vuttāni ekadhammassa;",
        "analyzed": "sutte vuttāni eka dhammassa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ne_3:11.3",
        "original": "Yo jānāti suttavidū,",
        "analyzed": "yaḥ jānāti sutta vidū ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ne_3:11.4",
        "original": "Vevacano nāma so hāro.",
        "analyzed": "vevacanaḥ nāma saḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ne_3:12.1",
        "original": "Ekaṁ bhagavā dhammaṁ,",
        "analyzed": "ekam bhagavā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ne_3:12.2",
        "original": "Paññattīhi vividhāhi deseti;",
        "analyzed": "paññattīhi vividhāhi deseti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ne_3:12.3",
        "original": "So ākāro ñeyyo,",
        "analyzed": "saḥ ākāraḥ ñeyyaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ne_3:12.4",
        "original": "Paññattī nāma hāroti.",
        "analyzed": "paññattī nāma hāroti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ne_3:13.1",
        "original": "Yo ca paṭiccuppādo,",
        "analyzed": "yaḥ ca paṭiccuppādaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ne_3:13.2",
        "original": "Indriyakhandhā ca dhātu āyatanā;",
        "analyzed": "indriya khandhā ca dhātu āyatanā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ne_3:13.3",
        "original": "Etehi otarati yo,",
        "analyzed": "etehi otarati yaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ne_3:13.4",
        "original": "Otaraṇo nāma so hāro.",
        "analyzed": "otaraṇaḥ nāma saḥ hāraḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ne_3:14.1",
        "original": "Vissajjitamhi pañhe,",
        "analyzed": "vissajjitamhi pañhe ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ne_3:14.2",
        "original": "Gāthāyaṁ pucchitā yamārabbha;",
        "analyzed": "gāthā ayam pucchitā yam ārabbha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ne_3:14.3",
        "original": "Suddhāsuddhaparikkhā,",
        "analyzed": "suddha asuddha parikkhā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ne_3:14.4",
        "original": "Hāro so sodhano nāma.",
        "analyzed": "saḥ sodhanaḥ nāma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ne_3:15.1",
        "original": "Ekattatāya dhammā,",
        "analyzed": "ekatta tāya dhammā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ne_3:15.2",
        "original": "Yepi ca vemattatāya niddiṭṭhā;",
        "analyzed": "ye api ca vemattatāya niddiṭṭhā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ne_3:15.3",
        "original": "Te na vikappayitabbā,",
        "analyzed": "te na vikappayitabbā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ne_3:15.4",
        "original": "Eso hāro adhiṭṭhāno.",
        "analyzed": "eso hāraḥ adhiṭṭhāno ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ne_3:16.1",
        "original": "Ye dhammā yaṁ dhammaṁ,",
        "analyzed": "ye dhammāḥ yam dhamma ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ne_3:16.2",
        "original": "Janayantippaccayā paramparato;",
        "analyzed": "janayantip paccayā paramparataḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ne_3:16.3",
        "original": "Hetumavakaḍḍhayitvā,",
        "analyzed": "hetum avakaḍḍhayitvā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ne_3:16.4",
        "original": "Eso hāro parikkhāro.",
        "analyzed": "eso hāro parikkhāro ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ne_3:17.1",
        "original": "Ye dhammā yaṁmūlā,",
        "analyzed": "ye dhammā ya mūlā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ne_3:17.2",
        "original": "Ye cekatthā pakāsitā muninā;",
        "analyzed": "ye cekatthā pakāsitāḥ muninā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ne_3:17.3",
        "original": "Te samaropayitabbā,",
        "analyzed": "te samaropayitabbā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ne_3:17.4",
        "original": "Esa samāropano hāro.",
        "analyzed": "samāropanaḥ hāraḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ne_3:18.0",
        "original": "3.2. Nayasaṅkhepa",
        "analyzed": "naya saṅkhepa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ne_3:18.1",
        "original": "Taṇhañca avijjampi ca,",
        "analyzed": "taṇham ca avijjam pi ca ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ne_3:18.2",
        "original": "Samathena vipassanāya yo neti;",
        "analyzed": "samathena vipassanāya yaḥ na iti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ne_3:18.3",
        "original": "Saccehi yojayitvā,",
        "analyzed": "yojayitvā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ne_3:18.4",
        "original": "Ayaṁ nayo nandiyāvaṭṭo.",
        "analyzed": "ayam nayaḥ nandiyāvaṭṭaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ne_3:19.1",
        "original": "Yo akusale samūlehi,",
        "analyzed": "yaḥ akusale samūlehi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ne_3:19.2",
        "original": "Neti kusale ca kusalamūlehi;",
        "analyzed": "na iti kusale ca kusala mūlehi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ne_3:19.3",
        "original": "Bhūtaṁ tathaṁ avitathaṁ,",
        "analyzed": "bhūtam tatham avitatha ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ne_3:19.4",
        "original": "Tipukkhalaṁ taṁ nayaṁ āhu.",
        "analyzed": "tipukkhalam tam nayam āhu ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ne_3:20.1",
        "original": "Yo neti vipallāsehi,",
        "analyzed": "yaḥ na iti vipallāsehi ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ne_3:20.2",
        "original": "Kilese indriyehi saddhamme;",
        "analyzed": "kilese indriyehi saddhamme ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ne_3:20.3",
        "original": "Etaṁ nayaṁ nayavidū,",
        "analyzed": "etam nayam naya vidū ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ne_3:20.4",
        "original": "Sīhavikkīḷitaṁ āhu.",
        "analyzed": "āhu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ne_3:21.1",
        "original": "Veyyākaraṇesu hi ye,",
        "analyzed": "veyyākaraṇesu hi ye ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ne_3:21.2",
        "original": "Kusalākusalā tahiṁ tahiṁ vuttā;",
        "analyzed": "kusala akusalā tahim tahim vuttā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ne_3:21.3",
        "original": "Manasā volokayate,",
        "analyzed": "manasā vā ālokayate ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ne_3:21.4",
        "original": "Taṁ khu disālocanaṁ āhu.",
        "analyzed": "tam khu disālocanam āhu ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ne_3:22.1",
        "original": "Oloketvā disalocanena,",
        "analyzed": "oloketvā disa locanena ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ne_3:22.2",
        "original": "Ukkhipiya yaṁ samāneti;",
        "analyzed": "ukkhipiya yam samāneti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ne_3:22.3",
        "original": "Sabbe kusalākusale,",
        "analyzed": "sabbe kusala akusale ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ne_3:22.4",
        "original": "Ayaṁ nayo aṅkuso nāma.",
        "analyzed": "ayam nayaḥ aṅkusaḥ nāma ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ne_3:23.1",
        "original": "Soḷasa hārā paṭhamaṁ,",
        "analyzed": "soḷasa hārā paṭhama ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ne_3:23.2",
        "original": "Disalocanato disā viloketvā;",
        "analyzed": "disa locanataḥ disā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ne_3:23.3",
        "original": "Saṅkhipiya aṅkusena hi,",
        "analyzed": "saṅkhipiya aṅkusena hi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ne_3:23.4",
        "original": "Nayehi tīhi niddise suttaṁ.",
        "analyzed": "nayehi tīhi niddise sutta ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ne_3:24.0",
        "original": "3.3. Dvādasapada",
        "analyzed": "dvādasapada ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ne_3:24.1",
        "original": "Akkharaṁ padaṁ byañjanaṁ,",
        "analyzed": "akkharam padam byañjana ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ne_3:24.2",
        "original": "nirutti tatheva niddeso;",
        "analyzed": "nirutti tathā iva niddeso ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ne_3:24.3",
        "original": "Ākārachaṭṭhavacanaṁ,",
        "analyzed": "ākāra chaṭṭhavacana ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ne_3:24.4",
        "original": "ettāva byañjanaṁ sabbaṁ.",
        "analyzed": "byañjanam ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ne_3:25.1",
        "original": "Saṅkāsanā pakāsanā,",
        "analyzed": "saṅkāsanā pakāsanā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ne_3:25.2",
        "original": "Vivaraṇā vibhajanuttānīkammapaññatti;",
        "analyzed": "vivaraṇā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ne_3:25.3",
        "original": "Etehi chahi padehi,",
        "analyzed": "etehi chahi padehi ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ne_3:25.4",
        "original": "Attho kammañca niddiṭṭhaṁ.",
        "analyzed": "atthaḥ kammañca niddiṭṭha ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ne_3:26.1",
        "original": "Tīṇi ca nayā anūnā,",
        "analyzed": "tīṇi ca nayā anūnā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ne_3:26.2",
        "original": "Atthassa ca chappadāni gaṇitāni;",
        "analyzed": "atthassa ca chappadāni gaṇitāni ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ne_3:26.3",
        "original": "Navahi padehi bhagavato,",
        "analyzed": "navahi padehi bhagavataḥ ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ne_3:26.4",
        "original": "Vacanassattho samāyutto.",
        "analyzed": "vacanassa atthaḥ samāyuttaḥ ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ne_3:27.1",
        "original": "Atthassa navappadāni,",
        "analyzed": "atthassa nava padāni ",
        "folio": "27"
