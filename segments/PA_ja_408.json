    {
        "segmentnr": "PA_ja_408:1.1",
        "original": "“Ambāhamaddaṁ vanamantarasmiṁ,",
        "analyzed": "vanam antar asmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_408:1.2",
        "original": "Nīlobhāsaṁ phalitaṁ saṁvirūḷhaṁ;",
        "analyzed": "nīla ubhāsam phalitam sa virūḍha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_408:1.3",
        "original": "Tamaddasaṁ phalahetu vibhaggaṁ,",
        "analyzed": "tam addasam phala hetu vibhagga ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_408:1.4",
        "original": "Taṁ disvā bhikkhācariyaṁ carāmi.",
        "analyzed": "tam disvā bhikkhā cariyam carāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_408:2.1",
        "original": "Selaṁ sumaṭṭhaṁ naravīraniṭṭhitaṁ,",
        "analyzed": "selam su maṭṭham nara vīra niṭṭhita ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_408:2.2",
        "original": "Nārī yugaṁ dhārayi appasaddaṁ;",
        "analyzed": "nārī yugam dhārayi appasadda ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_408:2.3",
        "original": "Dutiyañca āgamma ahosi saddo,",
        "analyzed": "dutiyañca āgamma ahosi saddo ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_408:2.4",
        "original": "Taṁ disvā bhikkhācariyaṁ carāmi.",
        "analyzed": "tam disvā bhikkhā cariyam carāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_408:3.1",
        "original": "Dijā dijaṁ kuṇapamāharantaṁ,",
        "analyzed": "dijāḥ dijam kuṇapam āharanta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_408:3.2",
        "original": "Ekaṁ samānaṁ bahukā samecca;",
        "analyzed": "ekam samānam bahukā samet ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_408:3.3",
        "original": "Āhārahetū paripātayiṁsu,",
        "analyzed": "āhāra hetū ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_408:3.4",
        "original": "Taṁ disvā bhikkhācariyaṁ carāmi.",
        "analyzed": "tam disvā bhikkhā cariyam carāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_408:4.1",
        "original": "Usabhāhamaddaṁ yūthassa majjhe,",
        "analyzed": "yūthassa majjhe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_408:4.2",
        "original": "Calakkakuṁ vaṇṇabalūpapannaṁ;",
        "analyzed": "calakkakum vaṇṇa balūpapanna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_408:4.3",
        "original": "Tamaddasaṁ kāmahetu vitunnaṁ,",
        "analyzed": "tamaddasam kāma hetu vitunna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_408:4.4",
        "original": "Taṁ disvā bhikkhācariyaṁ carāmi”.",
