        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_4.10:2.3",
        "original": "Adhammo nirayaṁ neti,",
        "analyzed": "adhammaḥ nirayam na iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_4.10:2.4",
        "original": "dhammo pāpeti suggatiṁ.",
        "analyzed": "dhammaḥ pāpeti suggati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_4.10:3.1",
        "original": "Tasmā hi dhammesu kareyya chandaṁ,",
        "analyzed": "tasmai hi dhammesu kareyya chanda ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_4.10:3.2",
        "original": "Iti modamāno sugatena tādinā;",
