        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_506:15.4",
        "original": "Dakkhemu te nāga nivesanāni”.",
        "analyzed": "dakkhemu te nāga nivesanāni ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_506:16.1",
        "original": "“Sacepi vāto girimāvaheyya,",
        "analyzed": "saced ipi vātaḥ girim āvaheyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_506:16.2",
        "original": "Cando ca suriyo ca chamā pateyyuṁ;",
        "analyzed": "cando ca suriyo ca chamā ",
        "folio": "16"
    },
