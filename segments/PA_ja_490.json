[
    {
        "segmentnr": "PA_ja_490:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_490:0.2",
        "original": "Pakiṇṇakanipāta",
        "analyzed": "pakiṇṇaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_490:0.3",
        "original": "Sālikedāravagga",
        "analyzed": "sāli kedāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_490:0.4",
        "original": "7. Pañcuposathikajātaka",
        "analyzed": "pañca uposathika jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_490:1.1",
        "original": "“Appossukko dāni tuvaṁ kapota,",
        "analyzed": "appa ussukkaḥ dāni tuvam kapota ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_490:1.2",
        "original": "Vihaṅgama na tava bhojanattho;",
        "analyzed": "vihaṅgama na tava bhojanattho ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_490:1.3",
        "original": "Khudaṁ pipāsaṁ adhivāsayanto,",
        "analyzed": "khudam pipāsam adhivāsayantaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_490:1.4",
        "original": "Kasmā bhavaṁposathiko kapota”.",
        "analyzed": "kasmai kapota ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_490:2.1",
        "original": "“Ahaṁ pure giddhigato kapotiyā,",
        "analyzed": "aham pure giddhigataḥ kapotiyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_490:2.2",
        "original": "Asmiṁ padesasmimubho ramāma;",
        "analyzed": "asmin pade sasmin ubhaḥ ramāma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_490:2.3",
        "original": "Athaggahī sākuṇiko kapotiṁ,",
        "analyzed": "athaggahī sākuṇikaḥ kapoti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_490:2.4",
        "original": "Akāmako tāya vinā ahosiṁ.",
        "analyzed": "akāmakaḥ tāya vinā ahosi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_490:3.1",
        "original": "Nānābhavā vippayogena tassā,",
        "analyzed": "nānā bhavāḥ vippayogena tassā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_490:3.2",
        "original": "Manomayaṁ vedana vedayāmi;",
        "analyzed": "manaḥ mayam vedana vedayāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_490:3.3",
        "original": "Tasmā ahaṁposathaṁ pālayāmi,",
        "analyzed": "tasmai ahaposatham pālayāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_490:3.4",
        "original": "Rāgo mamaṁ mā punarāgamāsi”.",
        "analyzed": "rāgaḥ mamam mā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_490:4.1",
        "original": "“Anujjugāmī uragā dujivha,",
        "analyzed": "an ujjugāmī uragāḥ dujivha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_490:4.2",
        "original": "Dāṭhāvudho ghoravisosi sappa;",
        "analyzed": "dāṭhā vudhaḥ ghora visa usi sappa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_490:4.3",
        "original": "Khudaṁ pipāsaṁ adhivāsayanto,",
        "analyzed": "khudam pipāsam adhivāsayantaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_490:4.4",
        "original": "Kasmā bhavaṁposathiko nu dīgha.",
        "analyzed": "kasmai bhava posathikaḥ nu dīgha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_490:5.1",
        "original": "Usabho ahū balavā gāmikassa,",
        "analyzed": "usabhaḥ ahū balavā gāmikassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_490:5.2",
        "original": "Calakkakū vaṇṇabalūpapanno;",
        "analyzed": "cala kkakū vaṇṇa bala upapannaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_490:5.3",
        "original": "So maṁ akkami taṁ kupito aḍaṁsiṁ,",
        "analyzed": "saḥ mama tam kupitaḥ aḍasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_490:5.4",
        "original": "Dukkhābhitunno maraṇaṁ upāgā.",
        "analyzed": "dukkha abhitunnaḥ maraṇam upāgā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_490:6.1",
        "original": "Tato janā nikkhamitvāna gāmā,",
        "analyzed": "tatas janāḥ nikkhamitvāna gāmā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_490:6.2",
        "original": "Kanditvā roditvā apakkamiṁsu;",
        "analyzed": "kanditvā roditvā apakkamisu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_490:6.3",
        "original": "Tasmā ahaṁposathaṁ pālayāmi,",
        "analyzed": "tasmai ahaposatham pālayāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_490:6.4",
        "original": "Kodho mamaṁ mā punarāgamāsi.",
        "analyzed": "kodhaḥ mā punar āgamāsi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_490:7.1",
        "original": "Matāna maṁsāni bahū susāne,",
        "analyzed": "matāna masāni bahū susāne ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_490:7.2",
        "original": "Manuññarūpaṁ tava bhojane taṁ;",
        "analyzed": "manuñña rūpam tava bhojane ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_490:7.3",
        "original": "Khudaṁ pipāsaṁ adhivāsayanto,",
        "analyzed": "khudam pipāsam adhivāsayantaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_490:7.4",
        "original": "Kasmā bhavaṁposathiko siṅgāla.",
        "analyzed": "kasmai siṅgāla ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_490:8.1",
        "original": "Pavisi kucchiṁ mahato gajassa,",
        "analyzed": "pavisi kucchim mahataḥ gajassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_490:8.2",
        "original": "Kuṇape rato hatthimaṁsesu giddho;",
        "analyzed": "kuṇape rataḥ hatthi masesu giddhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_490:8.3",
        "original": "Uṇho ca vāto tikhiṇā ca rasmiyo,",
        "analyzed": "uṇhaḥ ca vātaḥ tikhiṇā ca rasmiyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_490:8.4",
        "original": "Te sosayuṁ tassa karīsamaggaṁ.",
        "analyzed": "te sosayum tassa karīsamagga ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_490:9.1",
        "original": "Kiso ca paṇḍū ca ahaṁ bhadante,",
        "analyzed": "ca ca aham bhadante ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_490:9.2",
        "original": "Na me ahū nikkhamanāya maggo;",
        "analyzed": "na me ahū nikkhamanāya maggo ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_490:9.3",
        "original": "Mahā ca megho sahasā pavassi,",
        "analyzed": "mahā ca meghaḥ sahasā pavassi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_490:9.4",
        "original": "So temayī tassa karīsamaggaṁ.",
        "analyzed": "saḥ temayī tassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_490:10.1",
        "original": "Tato ahaṁ nikkhamisaṁ bhadante,",
        "analyzed": "tatas aham nikkhamisam bhadante ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_490:10.2",
        "original": "Cando yathā rāhumukhā pamutto;",
        "analyzed": "candaḥ yathā rāhu mukhā pamuttaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_490:10.3",
        "original": "Tasmā ahaṁposathaṁ pālayāmi,",
        "analyzed": "tasmai ahaposatham pālayāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_490:10.4",
        "original": "Lobho mamaṁ mā punarāgamāsi.",
        "analyzed": "lobhaḥ mā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_490:11.1",
        "original": "Vammīkathūpasmiṁ kipillikāni,",
        "analyzed": "vammī kathūpasmim kipillikāni ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_490:11.2",
        "original": "Nippothayanto tuvaṁ pure carāsi;",
        "analyzed": "nippothayantaḥ tuvam pure carāsi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_490:11.3",
        "original": "Khudaṁ pipāsaṁ adhivāsayanto,",
        "analyzed": "khudam pipāsam adhivāsayantaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_490:11.4",
        "original": "Kasmā bhavaṁposathiko nu accha.",
        "analyzed": "kasmai bhava posathikaḥ nu accha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_490:12.1",
        "original": "Sakaṁ niketaṁ atihīḷayāno,",
        "analyzed": "sakam niketam atihīḍayānaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_490:12.2",
        "original": "Atricchatā mallagāmaṁ agacchiṁ;",
        "analyzed": "atricchatā malla gāmam agacchi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_490:12.3",
        "original": "Tato janā nikkhamitvāna gāmā,",
        "analyzed": "tatas janāḥ nikkhamitvāna gāmā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_490:12.4",
        "original": "Kodaṇḍakena paripothayiṁsu maṁ.",
        "analyzed": "kodaṇḍakena ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_490:13.1",
        "original": "So bhinnasīso ruhiramakkhitaṅgo,",
        "analyzed": "saḥ bhinna sīsaḥ ruhira makkhitaṅgaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_490:13.2",
        "original": "Paccāgamāsiṁ sakaṁ niketaṁ;",
        "analyzed": "sakam niketa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_490:13.3",
        "original": "Tasmā ahaṁposathaṁ pālayāmi,",
        "analyzed": "tasmai ahaposatham pālayāmi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_490:13.4",
        "original": "Atricchatā mā punarāgamāsi”.",
        "analyzed": "mā punar āgamāsi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_490:14.1",
        "original": "“Yaṁ no apucchittha tuvaṁ bhadante,",
        "analyzed": "yam naḥ apucchittha tuvam bhadante ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_490:14.2",
        "original": "Sabbeva byākarimha yathāpajānaṁ;",
        "analyzed": "sabbe iva byākarimha yathā apajāna ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_490:14.3",
        "original": "Mayampi pucchāma tuvaṁ bhadante,",
        "analyzed": "pucchāma bhadante ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_490:14.4",
        "original": "Kasmā bhavaṁposathiko nu brahme”.",
        "analyzed": "kasmai bhava aposathikaḥ nu brahme ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_490:15.1",
        "original": "“Anūpalitto mama assamamhi,",
        "analyzed": "an upalittaḥ mama assamamhi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_490:15.2",
        "original": "Paccekabuddho muhuttaṁ nisīdi;",
        "analyzed": "paccekabuddhaḥ muhuttam nisīdi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_490:15.3",
        "original": "So maṁ avedī gatimāgatiñca,",
        "analyzed": "saḥ mama avedī gatim āgatim ca ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_490:15.4",
        "original": "Nāmañca gottaṁ caraṇañca sabbaṁ.",
        "analyzed": "nāmañca gottam caraṇañca ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_490:16.1",
        "original": "Evampahaṁ vandi na tassa pāde,",
        "analyzed": "evampaham vandi na tassa pāde ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_490:16.2",
        "original": "Na cāpi naṁ mānagatena pucchiṁ;",
        "analyzed": "na ca api nam māna gatena pucchi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_490:16.3",
        "original": "Tasmā ahaṁposathaṁ pālayāmi,",
        "analyzed": "tasmai ahaposatham pālayāmi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_490:16.4",
        "original": "Māno mamaṁ mā punarāgamāsī”ti.",
        "analyzed": "mānaḥ mamam mā punar āgamāsi iti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_490:17.1",
        "original": "Pañcuposathikajātakaṁ sattamaṁ.",
        "analyzed": "pañcuposathika jātakam sattama ",
        "folio": "17"
    }
]