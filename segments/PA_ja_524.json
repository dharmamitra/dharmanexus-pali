[
    {
        "segmentnr": "PA_ja_524:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_524:0.2",
        "original": "Cattālīsanipāta",
        "analyzed": "cattālīsa nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_524:0.3",
        "original": "Tesakuṇavagga",
        "analyzed": "tesa kuṇa vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_524:0.4",
        "original": "4. Saṅkhapālajātaka",
        "analyzed": "saṅkhapālajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_524:1.1",
        "original": "“Ariyāvakāsosi pasannanetto,",
        "analyzed": "ariya avakāsaḥ asi pasanna nettaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_524:1.2",
        "original": "Maññe bhavaṁ pabbajito kulamhā;",
        "analyzed": "maññe bhavam pabbajitaḥ kulamhā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_524:1.3",
        "original": "Kathaṁ nu vittāni pahāya bhoge,",
        "analyzed": "katham nu vittāni pahāya bhoge ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_524:1.4",
        "original": "Pabbaji nikkhamma gharā sapañña”.",
        "analyzed": "pabbaji nikkhamma ghara sapañña ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_524:2.1",
        "original": "“Sayaṁ vimānaṁ naradeva disvā,",
        "analyzed": "sayam vimānam naradeva disvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_524:2.2",
        "original": "Mahānubhāvassa mahoragassa;",
        "analyzed": "mahā anubhāvassa mahā uragassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_524:2.3",
        "original": "Disvāna puññāna mahāvipākaṁ,",
        "analyzed": "disvāna puññāna mahā vipāka ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_524:2.4",
        "original": "Saddhāyahaṁ pabbajitomhi rāja”.",
        "analyzed": "saddhā yaham pabbajitaḥ asmi rāja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_524:3.1",
        "original": "“Na kāmakāmā na bhayā na dosā,",
        "analyzed": "na kāma kāmā na bhayā na dosā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_524:3.2",
        "original": "Vācaṁ musā pabbajitā bhaṇanti;",
        "analyzed": "vācam musā pabbajitāḥ bhaṇanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_524:3.3",
        "original": "Akkhāhi me pucchito etamatthaṁ,",
        "analyzed": "akkhāhi me pucchitaḥ etam attha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_524:3.4",
        "original": "Sutvāna me jāyihitippasādo”.",
        "analyzed": "sutvāna me jāyi hiti pasādaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_524:4.1",
        "original": "“Vāṇijja raṭṭhādhipa gacchamāno,",
        "analyzed": "vāṇijja raṭṭha adhipa gacchamāno ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_524:4.2",
        "original": "Pathe addasāsimhi bhojaputte;",
        "analyzed": "pathe addasāsimhi bhoja putte ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_524:4.3",
        "original": "Pavaddhakāyaṁ uragaṁ mahantaṁ,",
        "analyzed": "pavaddha kāyam uragam mahanta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_524:4.4",
        "original": "Ādāya gacchante pamodamāne.",
        "analyzed": "ādāya gacchante pamodamāne ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_524:5.1",
        "original": "Sohaṁ samāgamma janinda tehi,",
        "analyzed": "saḥ aham samāgamma janinda tehi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_524:5.2",
        "original": "Pahaṭṭhalomo avacamhi bhīto;",
        "analyzed": "pahaṭṭha lomaḥ avacamhi bhītaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_524:5.3",
        "original": "‘Kuhiṁ ayaṁ nīyati bhīmakāyo,",
        "analyzed": "kuhim ayam nīyati bhīma kāyaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_524:5.4",
        "original": "Nāgena kiṁ kāhatha bhojaputtā’.",
        "analyzed": "nāgena kim bhoja puttā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_524:6.1",
        "original": "‘Nāgo ayaṁ nīyati bhojanatthā,",
        "analyzed": "nāgaḥ ayam nīyati bhojanatthā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_524:6.2",
        "original": "Pavaddhakāyo urago mahanto;",
        "analyzed": "pavaddha kāyaḥ uragaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_524:6.3",
        "original": "Sāduñca thūlañca muduñca maṁsaṁ,",
        "analyzed": "sāduñca thūlañca muduñca masa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_524:6.4",
        "original": "Na tvaṁ rasaññāsi videhaputta.",
        "analyzed": "na tvam rasaññā asi videha putta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_524:7.1",
        "original": "Ito mayaṁ gantvā sakaṁ niketaṁ,",
        "analyzed": "itas mayam gantvā sakam niketa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_524:7.2",
        "original": "Ādāya satthāni vikopayitvā;",
        "analyzed": "ādāya satthāni vikopayitvā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_524:7.3",
        "original": "Maṁsāni bhokkhāma pamodamānā,",
        "analyzed": "masāni bhokkhāma pamodamānā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_524:7.4",
        "original": "Mayañhi ve sattavo pannagānaṁ’.",
        "analyzed": "mayan hi ve sattavaḥ pannagāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_524:8.1",
        "original": "‘Sace ayaṁ nīyati bhojanatthā,",
        "analyzed": "sace ayam nīyati bhojanatthā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_524:8.2",
        "original": "Pavaddhakāyo urago mahanto;",
        "analyzed": "pavaddha kāyaḥ uragaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_524:8.3",
        "original": "Dadāmi vo balibaddāni soḷasa,",
        "analyzed": "dadāmi vaḥ balibaddāni soḷasa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_524:8.4",
        "original": "Nāgaṁ imaṁ muñcatha bandhanasmā’.",
        "analyzed": "nāgam imam muñcatha bandhanasmā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_524:9.1",
        "original": "‘Addhā hi no bhakkho ayaṁ manāpo,",
        "analyzed": "addhā hi naḥ bhakkhaḥ ayam manāpaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_524:9.2",
        "original": "Bahū ca no uragā bhuttapubbā;",
        "analyzed": "bahū ca naḥ uragāḥ bhutta pubbā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_524:9.3",
        "original": "Karoma te taṁ vacanaṁ aḷāra,",
        "analyzed": "karoma te tam vacanam aḷāra ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_524:9.4",
        "original": "Mittañca no hohi videhaputta’.",
        "analyzed": "mittam ca naḥ hohi videha putta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_524:10.1",
        "original": "Tadāssu te bandhanā mocayiṁsu,",
        "analyzed": "te bandhanā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_524:10.2",
        "original": "Yaṁ natthuto paṭimokkassa pāse;",
        "analyzed": "yam paṭimokkassa pāse ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_524:10.3",
        "original": "Mutto ca so bandhanā nāgarājā,",
        "analyzed": "mutto ca saḥ nāga rājā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_524:10.4",
        "original": "Pakkāmi pācīnamukho muhuttaṁ.",
        "analyzed": "pācīna mukhaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_524:11.1",
        "original": "Gantvāna pācīnamukho muhuttaṁ,",
        "analyzed": "gantvāna pācīna mukhaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_524:11.2",
        "original": "Puṇṇehi nettehi palokayī maṁ;",
        "analyzed": "puṇṇehi nettehi palokayī ma ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_524:11.3",
        "original": "Tadāssahaṁ piṭṭhito anvagacchiṁ,",
        "analyzed": "piṭṭhitaḥ anvagacchi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_524:11.4",
        "original": "Dasaṅguliṁ añjaliṁ paggahetvā.",
        "analyzed": "daśa aṅgulim añjalim paggahetvā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_524:12.1",
        "original": "‘Gaccheva kho tvaṁ taramānarūpo,",
        "analyzed": "gaccha iva khalu tvam taramāna rūpaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_524:12.2",
        "original": "Mā taṁ amittā punaraggahesuṁ;",
        "analyzed": "mā tam amittā punar aggahesu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_524:12.3",
        "original": "Dukkho hi luddehi punā samāgamo,",
        "analyzed": "dukkhaḥ hi luddehi punā samāgamaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_524:12.4",
        "original": "Adassanaṁ bhojaputtāna gaccha’.",
        "analyzed": "adassanam bhoja puttāna gaccha ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_524:13.1",
        "original": "Agamāsi so rahadaṁ vippasannaṁ,",
        "analyzed": "agamāsi saḥ rahadam vippasanna ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_524:13.2",
        "original": "Nīlobhāsaṁ ramaṇīyaṁ sutitthaṁ;",
        "analyzed": "nīla ubhāsam ramaṇīyam sutittha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_524:13.3",
        "original": "Samotataṁ jambuhi vetasāhi,",
        "analyzed": "samotatam jambuhi vetasāhi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_524:13.4",
        "original": "Pāvekkhi nittiṇṇabhayo patīto.",
        "analyzed": "pāvekkhi nittiṇṇa bhayaḥ patītaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_524:14.1",
        "original": "So taṁ pavissa na cirassa nāgo,",
        "analyzed": "saḥ tam pavissa na cirassa nāgaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_524:14.2",
        "original": "Dibbena me pāturahuṁ janinda;",
        "analyzed": "dibbena me pātuḥ ahum janinda ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_524:14.3",
        "original": "Upaṭṭhahī maṁ pitaraṁva putto,",
        "analyzed": "upaṭṭhahī mama pitarava putto ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_524:14.4",
        "original": "Hadayaṅgamaṁ kaṇṇasukhaṁ bhaṇanto.",
        "analyzed": "hadayaṅgamam kaṇṇa sukham bhaṇantaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_524:15.1",
        "original": "‘Tvaṁ mesi mātā ca pitā aḷāra,",
        "analyzed": "tvam mā isi mātā ca pitā aḷāra ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_524:15.2",
        "original": "Abbhantaro pāṇadado sahāyo;",
        "analyzed": "abbhantaraḥ pāṇadadaḥ sahāyaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_524:15.3",
        "original": "Sakañca iddhiṁ paṭilābhakosmi,",
        "analyzed": "sakañca iddhim paṭilābhakaḥ asmi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_524:15.4",
        "original": "Aḷāra passa me nivesanāni;",
        "analyzed": "aḷāra passa me nivesanāni ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_524:15.5",
        "original": "Pahūtabhakkhaṁ bahuannapānaṁ,",
        "analyzed": "pahūta bhakkham bahu anna pāna ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_524:15.6",
        "original": "Masakkasāraṁ viya vāsavassa.",
        "analyzed": "masakka sāram viya vāsavassa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_524:16.1",
        "original": "Taṁ bhūmibhāgehi upetarūpaṁ,",
        "analyzed": "tam bhūmi bhāgehi upeta rūpa ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_524:16.2",
        "original": "Asakkharā ceva mudū subhā ca;",
        "analyzed": "asakkharā ca iva mudū subhā ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_524:16.3",
        "original": "Nīcattiṇā apparajā ca bhūmi,",
        "analyzed": "nīcattiṇā apparajā ca bhūmi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_524:16.4",
        "original": "Pāsādikā yattha jahanti sokaṁ.",
        "analyzed": "pāsādikāḥ yattha jahanti soka ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_524:17.1",
        "original": "Anāvakulā veḷuriyūpanīlā,",
        "analyzed": "an āva kulā veḷuriyū upa nīlā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_524:17.2",
        "original": "Catuddisaṁ ambavanaṁ surammaṁ;",
        "analyzed": "catuddisam ambavanam su ramma ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_524:17.3",
        "original": "Pakkā ca pesī ca phalā suphullā,",
        "analyzed": "pakkā ca pesī ca phalā suphullā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_524:17.4",
        "original": "Niccotukā dhārayantī phalāni’.",
        "analyzed": "niccotukā dhārayantī phalāni ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_524:18.1",
        "original": "Tesaṁ vanānaṁ naradeva majjhe,",
        "analyzed": "tesam vanānam naradeva majjhe ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_524:18.2",
        "original": "Nivesanaṁ bhassarasannikāsaṁ;",
        "analyzed": "nivesanam bhassara sannikāsa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_524:18.3",
        "original": "Rajataggaḷaṁ sovaṇṇamayaṁ uḷāraṁ,",
        "analyzed": "rajataggaḷam sovaṇṇa mayam uḷāra ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_524:18.4",
        "original": "Obhāsatī vijjurivantalikkhe.",
        "analyzed": "obhāsatī ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_524:19.1",
        "original": "Maṇīmayā soṇṇamayā uḷārā,",
        "analyzed": "maṇī mayā soṇṇa mayā uḷārā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_524:19.2",
        "original": "Anekacittā satataṁ sunimmitā;",
        "analyzed": "aneka cittā satatam su nimmitā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_524:19.3",
        "original": "Paripūrā kaññāhi alaṅkatābhi,",
        "analyzed": "paripūrā kaññāhi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_524:19.4",
        "original": "Suvaṇṇakāyūradharāhi rāja.",
        "analyzed": "rāja ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_524:20.1",
        "original": "So saṅkhapālo taramānarūpo,",
        "analyzed": "saḥ saṅkha pālaḥ taramāna rūpaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_524:20.2",
        "original": "Pāsādamāruyha anomavaṇṇo;",
        "analyzed": "pāsādam āruyha anoma vaṇṇaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_524:20.3",
        "original": "Sahassathambhaṁ atulānubhāvaṁ,",
        "analyzed": "sahassa thambham atula anubhāva ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_524:20.4",
        "original": "Yatthassa bhariyā mahesī ahosi.",
        "analyzed": "yatthassa bhariyā mahesī ahosi ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_524:21.1",
        "original": "Ekā ca nārī taramānarūpā,",
        "analyzed": "ekā ca nārī taramāna rūpā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_524:21.2",
        "original": "Ādāya veḷuriyamayaṁ mahagghaṁ;",
        "analyzed": "ādāya veḷuriya mayam mahat ha ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_524:21.3",
        "original": "Subhaṁ maṇiṁ jātimantūpapannaṁ,",
        "analyzed": "subham maṇim ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_524:21.4",
        "original": "Acoditā āsanamabbhihāsi.",
        "analyzed": "a coditā āsanam abbhihāsi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_524:22.1",
        "original": "Tato maṁ urago hatthe gahetvā,",
        "analyzed": "tatas mam uragaḥ hatthe gahetvā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_524:22.2",
        "original": "Nisīdayī pāmukhaāsanasmiṁ;",
        "analyzed": "nisīdayī ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_524:22.3",
        "original": "‘Idamāsanaṁ atra bhavaṁ nisīdatu,",
        "analyzed": "idam āsanam atra bhavam nisīdatu ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_524:22.4",
        "original": "Bhavañhi me aññataro garūnaṁ’.",
        "analyzed": "bhavan hi me aññataraḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_524:23.1",
        "original": "Aññā ca nārī taramānarūpā,",
        "analyzed": "aññā ca nārī taramāna rūpā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_524:23.2",
        "original": "Ādāya vāriṁ upasaṅkamitvā;",
        "analyzed": "ādāya vārim upasaṅkamitvā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_524:23.3",
        "original": "Pādāni pakkhālayī me janinda,",
        "analyzed": "pādāni pakkhālayī me janinda ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_524:23.4",
        "original": "Bhariyāva bhattū patino piyassa.",
        "analyzed": "bhariyāva bhattū patinaḥ piyassa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_524:24.1",
        "original": "Aparā ca nārī taramānarūpā,",
        "analyzed": "aparā ca nārī taramāna rūpā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_524:24.2",
        "original": "Paggayha sovaṇṇamayāya pātiyā;",
        "analyzed": "paggayha sovaṇṇa mayāya pātiyā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_524:24.3",
        "original": "Anekasūpaṁ vividhaṁ viyañjanaṁ,",
        "analyzed": "aneka sūpam vividham ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_524:24.4",
        "original": "Upanāmayī bhatta manuññarūpaṁ.",
        "analyzed": "upanāmayī bhatta manuñña rūpa ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_524:25.1",
        "original": "Turiyehi maṁ bhārata bhuttavantaṁ,",
        "analyzed": "turiyehi mama bhārata bhuttavanta ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_524:25.2",
        "original": "Upaṭṭhahuṁ bhattu mano viditvā;",
        "analyzed": "upaṭṭhahum bhattu manaḥ viditvā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_524:25.3",
        "original": "Tatuttariṁ maṁ nipatī mahantaṁ,",
        "analyzed": "tatuttarim mam nipatī mahanta ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_524:25.4",
        "original": "Dibbehi kāmehi anappakehi.",
        "analyzed": "dibbehi kāmehi anappakehi ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_524:26.1",
        "original": "‘Bhariyā mametā tisatā aḷāra,",
        "analyzed": "bhariyā mametā tisatā aḷāra ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_524:26.2",
        "original": "Sabbattamajjhā padumuttarābhā;",
        "analyzed": "padumuttara ābhā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_524:26.3",
        "original": "Aḷāra etāssu te kāmakārā,",
        "analyzed": "aḷāre etāssu te kāmakārā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_524:26.4",
        "original": "Dadāmi te tā paricārayassu’.",
        "analyzed": "dadāmi te tā paricārayassu ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_524:27.1",
        "original": "Saṁvaccharaṁ dibbarasānubhutvā,",
        "analyzed": "savat śaram ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_524:27.2",
        "original": "Tadāssuhaṁ uttarimajjhabhāsiṁ;",
        "analyzed": "uttarimajjhabhāsi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_524:27.3",
        "original": "‘Nāgassidaṁ kinti kathañca laddhaṁ,",
        "analyzed": "kinti kathañca ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_524:27.4",
        "original": "Kathajjhagamāsi vimānaseṭṭhaṁ.",
        "analyzed": "kathajjhagamāsi vimānaseṭṭha ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_524:28.1",
        "original": "Adhiccaladdhaṁ pariṇāmajaṁ te,",
        "analyzed": "pariṇāma jam te ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_524:28.2",
        "original": "Sayaṅkataṁ udāhu devehi dinnaṁ;",
        "analyzed": "sayaṅkatam udāhu devehi dinna ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_524:28.3",
        "original": "Pucchāmi taṁ nāgarājetamatthaṁ,",
        "analyzed": "pucchāmi tam nāga rāja itam attha ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_524:28.4",
        "original": "Kathajjhagamāsi vimānaseṭṭhaṁ’.",
        "analyzed": "kathajjhagamāsi vimānaseṭṭha ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_524:29.1",
        "original": "‘Nādhiccaladdhaṁ na pariṇāmajaṁ me,",
        "analyzed": "na pariṇāma jam me ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_524:29.2",
        "original": "Na sayaṅkataṁ nāpi devehi dinnaṁ;",
        "analyzed": "na sayaṅkatam na api devehi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_524:29.3",
        "original": "Sakehi kammehi apāpakehi,",
        "analyzed": "sakehi kammehi a pāpakehi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_524:29.4",
        "original": "Puññehi me laddhamidaṁ vimānaṁ’.",
        "analyzed": "puññehi me laddham idam vimāna ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_524:30.1",
        "original": "‘Kiṁ te vataṁ kiṁ pana brahmacariyaṁ,",
        "analyzed": "kim te kim pana ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_524:30.2",
        "original": "Kissa suciṇṇassa ayaṁ vipāko;",
        "analyzed": "kissa su ciṇṇassa ayam vipākaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_524:30.3",
        "original": "Akkhāhi me nāgarājetamatthaṁ,",
        "analyzed": "akkhāhi me nāga rāja itamattha ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_524:30.4",
        "original": "Kathaṁ nu te laddhamidaṁ vimānaṁ’.",
        "analyzed": "katham nu te laddham idam vimāna ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_524:31.1",
        "original": "‘Rājā ahosiṁ magadhānamissaro,",
        "analyzed": "rājā ahosim magadhānamissaraḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_524:31.2",
        "original": "Duyyodhano nāma mahānubhāvo;",
        "analyzed": "duyyodhanaḥ nāma mahā anubhāvaḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_524:31.3",
        "original": "So ittaraṁ jīvitaṁ saṁviditvā,",
        "analyzed": "saḥ ittaram jīvitam sa viditvā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_524:31.4",
        "original": "Asassataṁ vipariṇāmadhammaṁ.",
        "analyzed": "asassatam vipariṇāma dhamma ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_524:32.1",
        "original": "Annañca pānañca pasannacitto,",
        "analyzed": "annam ca pānam ca pasanna cittaḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_524:32.2",
        "original": "Sakkacca dānaṁ vipulaṁ adāsiṁ;",
        "analyzed": "sakkacca dānam vipulam adāsi ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_524:32.3",
        "original": "Opānabhūtaṁ me gharaṁ tadāsi,",
        "analyzed": "opāna bhūtam me gharam tadā asi ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_524:32.4",
        "original": "Santappitā samaṇabrāhmaṇā ca.",
        "analyzed": "santappitā samaṇa brāhmaṇā ca ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_524:33.1",
        "original": "Mālañca gandhañca vilepanañca,",
        "analyzed": "mālañca gandhañca vilepanañca ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_524:33.2",
        "original": "Padīpiyaṁ yānamupassayañca;",
        "analyzed": "padīpiyam yānam upassayam ca ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_524:33.3",
        "original": "Acchādanaṁ seyyamathannapānaṁ,",
        "analyzed": "acchādanam seyyam athannapāna ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_524:33.4",
        "original": "Sakkacca dānāni adamha tattha.",
        "analyzed": "sakkacca dānāni adamha tattha ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_524:34.1",
        "original": "Taṁ me vataṁ taṁ pana brahmacariyaṁ,",
        "analyzed": "tam me tam pana brahmacariya ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_524:34.2",
        "original": "Tassa suciṇṇassa ayaṁ vipāko;",
        "analyzed": "tassa su ciṇṇassa ayam vipākaḥ ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_524:34.3",
        "original": "Teneva me laddhamidaṁ vimānaṁ,",
        "analyzed": "tena iva me laddham idam vimāna ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_524:34.4",
        "original": "Pahūtabhakkhaṁ bahuannapānaṁ;",
        "analyzed": "pahūta bhakkham bahu anna pāna ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_524:34.5",
        "original": "Naccehi gītehi cupetarūpaṁ,",
        "analyzed": "naccehi gītehi ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_524:34.6",
        "original": "Ciraṭṭhitikaṁ na ca sassatāyaṁ’.",
        "analyzed": "cira ṭṭhitikam na ca sassatāya ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_524:35.1",
        "original": "‘Appānubhāvā taṁ mahānubhāvaṁ,",
        "analyzed": "tam mahā anubhāva ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_524:35.2",
        "original": "Tejassinaṁ hanti atejavanto;",
        "analyzed": "tejassinam hanti a tejavantaḥ ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_524:35.3",
        "original": "Kimeva dāṭhāvudha kiṁ paṭicca,",
        "analyzed": "kim eva dāṭhāvudha kim paṭicca ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_524:35.4",
        "original": "Hatthattamāgacchi vanibbakānaṁ.",
        "analyzed": "hatthattam āgacchi ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_524:36.1",
        "original": "Bhayaṁ nu te anvagataṁ mahantaṁ,",
        "analyzed": "bhayam nu te anvagatam mahanta ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_524:36.2",
        "original": "Tejo nu te nānvagaṁ dantamūlaṁ;",
        "analyzed": "tejaḥ nu te na anvagam danta mūla ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_524:36.3",
        "original": "Kimeva dāṭhāvudha kiṁ paṭicca,",
        "analyzed": "kim eva dāṭhāvudha kim paṭicca ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_524:36.4",
        "original": "Kilesamāpajji vanibbakānaṁ’.",
        "analyzed": "kilesam āpajji ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_524:37.1",
        "original": "‘Na me bhayaṁ anvagataṁ mahantaṁ,",
        "analyzed": "na me bhayam anvagatam mahanta ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_524:37.2",
        "original": "Tejo na sakkā mama tehi hantuṁ;",
        "analyzed": "tejaḥ na sakkā mama tehi hantu ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_524:37.3",
        "original": "Satañca dhammāni sukittitāni,",
        "analyzed": "satañca dhammāni su kittitāni ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_524:37.4",
        "original": "Samuddavelāva duraccayāni.",
        "analyzed": "samudda velā ava duraccayāni ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_524:38.1",
        "original": "Cātuddasiṁ pañcadasiṁ aḷāra,",
        "analyzed": "cātuddasim pañcadasim aḷāra ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_524:38.2",
        "original": "Uposathaṁ niccamupāvasāmi;",
        "analyzed": "uposatham niccam upāvasāmi ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_524:38.3",
        "original": "Athāgamuṁ soḷasa bhojaputtā,",
        "analyzed": "athā agamum soḷasa bhoja puttā ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_524:38.4",
        "original": "Rajjuṁ gahetvāna daḷhañca pāsaṁ.",
        "analyzed": "rajjum gahetvāna daḷhan ca pāsa ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_524:39.1",
        "original": "Bhetvāna nāsaṁ atikassa rajjuṁ,",
        "analyzed": "bhetvāna nāsam atikassa rajju ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_524:39.2",
        "original": "Nayiṁsu maṁ samparigayha luddā;",
        "analyzed": "mam samparigayha luddā ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_524:39.3",
        "original": "Etādisaṁ dukkhamahaṁ titikkhaṁ,",
        "analyzed": "etādisam dukkham aham titikkha ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_524:39.4",
        "original": "Uposathaṁ appaṭikopayanto’.",
        "analyzed": "uposatham a paṭikopayantaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_524:40.1",
        "original": "‘Ekāyane taṁ pathe addasaṁsu,",
        "analyzed": "ekāyane tam pathe addasasu ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_524:40.2",
        "original": "Balena vaṇṇena cupetarūpaṁ;",
        "analyzed": "balena vaṇṇena ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_524:40.3",
        "original": "Siriyā paññāya ca bhāvitosi,",
        "analyzed": "siriyā paññāya ca bhāvitosi ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_524:40.4",
        "original": "Kiṁ patthayaṁ nāga tapo karosi’.",
        "analyzed": "kim patthayam nāga tapaḥ karosi ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_524:41.1",
        "original": "‘Na puttahetū na dhanassa hetu,",
        "analyzed": "na putta hetū na dhanassa hetu ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_524:41.2",
        "original": "Na āyuno cāpi aḷāra hetu;",
        "analyzed": "na āyuno cāpi aḷāra hetu ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_524:41.3",
        "original": "Manussayoniṁ abhipatthayāno,",
        "analyzed": "manussa yonim abhipatthayānaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_524:41.4",
        "original": "Tasmā parakkamma tapo karomi’.",
        "analyzed": "tasmai parakkamma tapaḥ karomi ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_524:42.1",
        "original": "‘Tvaṁ lohitakkho vihatantaraṁso,",
        "analyzed": "tvam lohitakkhaḥ vihatantarasaḥ ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_524:42.2",
        "original": "Alaṅkato kappitakesamassu;",
        "analyzed": "alaṅkataḥ kappita kesa massu ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_524:42.3",
        "original": "Surosito lohitacandanena,",
        "analyzed": "surā usitaḥ lohita candanena ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_524:42.4",
        "original": "Gandhabbarājāva disā pabhāsasi.",
        "analyzed": "gandhabbarājā ava disā pabhāsasi ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_524:43.1",
        "original": "Deviddhipattosi mahānubhāvo,",
        "analyzed": "deviddhipatto asi mahā anubhāvaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_524:43.2",
        "original": "Sabbehi kāmehi samaṅgibhūto;",
        "analyzed": "sabbehi kāmehi samaṅgi bhūtaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_524:43.3",
        "original": "Pucchāmi taṁ nāgarājetamatthaṁ,",
        "analyzed": "pucchāmi tam nāga rāja itam attha ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_524:43.4",
        "original": "Seyyo ito kena manussaloko’.",
        "analyzed": "seyyo itas kena manussa lokaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_524:44.1",
        "original": "‘Aḷāra nāññatra manussalokā,",
        "analyzed": "aḷāra na aññatra manussa lokā ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_524:44.2",
        "original": "Suddhī va saṁvijjati saṁyamo vā;",
        "analyzed": "suddhī va savijjati sayamaḥ vā ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_524:44.3",
        "original": "Ahañca laddhāna manussayoniṁ,",
        "analyzed": "ahañca manussa yoni ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_524:44.4",
        "original": "Kāhāmi jātimaraṇassa antaṁ’.",
        "analyzed": "kāhāmi jātimaraṇassa anta ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_524:45.1",
        "original": "‘Saṁvaccharo me vasato tavantike,",
        "analyzed": "savat śaraḥ me vasataḥ tavantike ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_524:45.2",
        "original": "Annena pānena upaṭṭhitosmi;",
        "analyzed": "annena pānena upaṭṭhitaḥ asmi ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_524:45.3",
        "original": "Āmantayitvāna palemi nāga,",
        "analyzed": "āmantayitvāna nāga ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_524:45.4",
        "original": "Cirappavutthosmi ahaṁ janinda’.",
        "analyzed": "aham janinda ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_524:46.1",
        "original": "‘Puttā ca dārā anujīvino ca,",
        "analyzed": "puttā ca dārā anujīvino ca ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_524:46.2",
        "original": "Niccānusiṭṭhā upatiṭṭhathetaṁ;",
        "analyzed": "nicca anusiṭṭhāḥ upatiṭṭhatha ita ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_524:46.3",
        "original": "Kaccinnu taṁ nābhisapittha koci,",
        "analyzed": "kaccit nu tam na abhisapittha koci ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_524:46.4",
        "original": "Piyañhi me dassanaṁ tuyhaṁ aḷāra’.",
        "analyzed": "piyam hi me dassanam tuyham aḷāra ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_524:47.1",
        "original": "‘Yathāpi mātū ca pitū agāre,",
        "analyzed": "yathā api mātū ca pitū agāre ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_524:47.2",
        "original": "Putto piyo paṭivihito vaseyya;",
        "analyzed": "puttaḥ piyaḥ paṭivihitaḥ vaseyya ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_524:47.3",
        "original": "Tatopi mayhaṁ idhameva seyyo,",
        "analyzed": "tatas upi seyyaḥ ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_524:47.4",
        "original": "Cittañhi te nāga mayī pasannaṁ’.",
        "analyzed": "cittam hi te nāga mayī pasanna ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_524:48.1",
        "original": "‘Maṇī mamaṁ vijjati lohitaṅko,",
        "analyzed": "maṇī vijjati lohitaṅkaḥ ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_ja_524:48.2",
        "original": "Dhanāharo maṇiratanaṁ uḷāraṁ;",
        "analyzed": "dhana āharaḥ maṇi ratanam uḷāra ",
        "folio": "48"
    },
    {
