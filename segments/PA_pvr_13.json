    {
        "segmentnr": "PA_pvr_13:0.3",
        "original": "1. Anuvijjakaanuyoga",
        "analyzed": "anuvijjaka anuyoga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pvr_13:1.1",
        "original": "Anuvijjakena codako pucchitabbo—",
        "analyzed": "anuvijjakena codakaḥ pucchitabbo ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pvr_13:1.2",
        "original": "“yaṁ kho tvaṁ, āvuso, imaṁ bhikkhuṁ codesi, kimhi naṁ codesi, sīlavipattiyā vā codesi, ācāravipattiyā vā codesi, diṭṭhivipattiyā vā codesī”ti?",
        "analyzed": "yam kho tvaṃ āvuso imam bhikkhum codesi kimhi nam codesi sīla vipattiyā vā codesi ācāra vipattiyā vā codesi diṭṭhi vipattiyā vā codesi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pvr_13:1.3",
        "original": "So ce evaṁ vadeyya—",
        "analyzed": "sā u ced evam vadeyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pvr_13:1.4",
        "original": "“sīlavipattiyā vā codemi, ācāravipattiyā vā codemi, diṭṭhivipattiyā vā codemī”ti, so evamassa vacanīyo—",
        "analyzed": "sīla vipattiyā vā codemi ācāra vipattiyā vā codemi diṭṭhi vipattiyā vā codemi iti saḥ evam assa vacanīyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pvr_13:1.5",
        "original": "“jānāsi panāyasmā sīlavipattiṁ, jānāsi ācāravipattiṁ, jānāsi diṭṭhivipattin”ti?",
        "analyzed": "jānāsi pana ayasman sīla vipatti jānāsi ācāra vipatti jānāsi diṭṭhi vipatti nti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pvr_13:1.6",
        "original": "So ce evaṁ vadeyya—",
        "analyzed": "sā u ced evam vadeyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pvr_13:1.7",
        "original": "“jānāmi kho ahaṁ, āvuso, sīlavipattiṁ, jānāmi ācāravipattiṁ, jānāmi diṭṭhivipattin”ti, so evamassa vacanīyo—",
        "analyzed": "jānāmi kho aha āvuso sīla vipatti jānāmi ācāra vipatti jānāmi diṭṭhi vipatti ti saḥ evam assa vacanīyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pvr_13:1.8",
        "original": "“katamā panāvuso, sīlavipatti?",
