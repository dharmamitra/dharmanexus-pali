[
    {
        "segmentnr": "PA_snp_3.1:0.1",
        "original": "Sutta Nipāta 3.1",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.1:0.2",
        "original": "Pabbajjāsutta",
        "analyzed": "pabbajjāsutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.1:1.1",
        "original": "“Pabbajjaṁ kittayissāmi,",
        "analyzed": "pabbajjam kittayissāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.1:1.2",
        "original": "yathā pabbaji cakkhumā;",
        "analyzed": "yathā pabbaji cakkhumā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.1:1.3",
        "original": "Yathā vīmaṁsamāno so,",
        "analyzed": "yathā vīmasamānaḥ saḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.1:1.4",
        "original": "pabbajjaṁ samarocayi.",
        "analyzed": "pabbajjam samarocayi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.1:2.1",
        "original": "Sambādhoyaṁ gharāvāso,",
        "analyzed": "ghara āvāsaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.1:2.2",
        "original": "rajassāyatanaṁ iti;",
        "analyzed": "rajaḥ sa āyatanam iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.1:2.3",
        "original": "Abbhokāsova pabbajjā,",
        "analyzed": "abbhokāsā uva pabbajjā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.1:2.4",
        "original": "iti disvāna pabbaji.",
        "analyzed": "iti disvāna pabbaji ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.1:3.1",
        "original": "Pabbajitvāna kāyena,",
        "analyzed": "pabbajitvāna kāyena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.1:3.2",
        "original": "pāpakammaṁ vivajjayi;",
        "analyzed": "pāpa kammam vivajjayi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.1:3.3",
        "original": "Vacīduccaritaṁ hitvā,",
        "analyzed": "vacīt uccaritam hitvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.1:3.4",
        "original": "ājīvaṁ parisodhayi.",
        "analyzed": "ājīvam parisodhayi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.1:4.1",
        "original": "Agamā rājagahaṁ buddho,",
        "analyzed": "agamāḥ rājagaham buddhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.1:4.2",
        "original": "magadhānaṁ giribbajaṁ;",
        "analyzed": "magadhānam giribbaja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.1:4.3",
        "original": "Piṇḍāya abhihāresi,",
        "analyzed": "piṇḍāya abhihāresi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.1:4.4",
        "original": "ākiṇṇavaralakkhaṇo”.",
        "analyzed": "ākiṇṇa vara lakkhaṇo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.1:5.1",
        "original": "Tamaddasā bimbisāro,",
        "analyzed": "tamaddasā bimbisāraḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.1:5.2",
        "original": "pāsādasmiṁ patiṭṭhito;",
        "analyzed": "pāsādasmin patiṭṭhitaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.1:5.3",
        "original": "Disvā lakkhaṇasampannaṁ,",
        "analyzed": "disvā lakkhaṇa sampanna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.1:5.4",
        "original": "imamatthaṁ abhāsatha.",
        "analyzed": "imam attham abhāsatha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.1:6.1",
        "original": "“Imaṁ bhonto nisāmetha,",
        "analyzed": "imam bhontaḥ nisāmetha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.1:6.2",
        "original": "abhirūpo brahā suci;",
        "analyzed": "abhirūpaḥ brahā suci ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.1:6.3",
        "original": "Caraṇena ca sampanno,",
        "analyzed": "caraṇena ca sampannaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.1:6.4",
        "original": "yugamattañca pekkhati.",
        "analyzed": "yuga mattañca pekkhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.1:7.1",
        "original": "Okkhittacakkhu satimā,",
        "analyzed": "okkhitta cakkhu satimā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.1:7.2",
        "original": "nāyaṁ nīcakulāmiva;",
        "analyzed": "na ayam nīca kulām iva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.1:7.3",
        "original": "Rājadūtābhidhāvantu,",
        "analyzed": "rāja dūta abhidhāvantu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.1:7.4",
        "original": "kuhiṁ bhikkhu gamissati”.",
        "analyzed": "kuhim bhikkhu gamissati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.1:8.1",
        "original": "Te pesitā rājadūtā,",
        "analyzed": "te pesitāḥ rāja dūtā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.1:8.2",
        "original": "piṭṭhito anubandhisuṁ;",
        "analyzed": "piṭṭhitaḥ anubandhisu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.1:8.3",
        "original": "Kuhiṁ gamissati bhikkhu,",
        "analyzed": "kuhim gamissati bhikkhu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.1:8.4",
        "original": "kattha vāso bhavissati.",
        "analyzed": "kattha vāsaḥ bhavissati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.1:9.1",
        "original": "Sapadānaṁ caramāno,",
        "analyzed": "sa padānam caramānaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.1:9.2",
        "original": "guttadvāro susaṁvuto;",
        "analyzed": "gutta dvāraḥ su savutaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.1:9.3",
        "original": "Khippaṁ pattaṁ apūresi,",
        "analyzed": "khippam pattam apūresi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.1:9.4",
        "original": "sampajāno paṭissato.",
        "analyzed": "sampajānaḥ paṭissataḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.1:10.1",
        "original": "Piṇḍacāraṁ caritvāna,",
        "analyzed": "piṇḍa cāram ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.1:10.2",
        "original": "Nikkhamma nagarā muni;",
        "analyzed": "nikkhamma nagarā muni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.1:10.3",
        "original": "Paṇḍavaṁ abhihāresi,",
        "analyzed": "paṇḍavam abhihāresi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.1:10.4",
        "original": "Ettha vāso bhavissati.",
        "analyzed": "ettha vāsaḥ bhavissati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.1:11.1",
        "original": "Disvāna vāsūpagataṁ,",
        "analyzed": "disvāna vāsūpagata ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.1:11.2",
        "original": "Tayo dūtā upāvisuṁ;",
        "analyzed": "tayaḥ dūtau ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.1:11.3",
        "original": "Tesu ekova āgantvā,",
        "analyzed": "tesu ekaḥ uva āgantvā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.1:11.4",
        "original": "Rājino paṭivedayi.",
        "analyzed": "rājino paṭivedayi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.1:12.1",
        "original": "“Esa bhikkhu mahārāja,",
        "analyzed": "esa bhikkhu mahā rāja ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.1:12.2",
        "original": "Paṇḍavassa puratthato;",
        "analyzed": "paṇḍavassa puratthatas ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.1:12.3",
        "original": "Nisinno byagghusabhova,",
        "analyzed": "nisinnaḥ byagghu sabhā uva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.1:12.4",
        "original": "Sīhova girigabbhare”.",
        "analyzed": "sīhā uva giri gabbhare ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.1:13.1",
        "original": "Sutvāna dūtavacanaṁ,",
        "analyzed": "sutvāna dūta vacana ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.1:13.2",
        "original": "Bhaddayānena khattiyo;",
        "analyzed": "bhaddayānena khattiyaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.1:13.3",
        "original": "Taramānarūpo niyyāsi,",
        "analyzed": "taramāna rūpaḥ niyyāsi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.1:13.4",
        "original": "Yena paṇḍavapabbato.",
        "analyzed": "yena paṇḍavapabbataḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.1:14.1",
        "original": "Sa yānabhūmiṁ yāyitvā,",
        "analyzed": "sa yāna bhūmim yāyitvā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.1:14.2",
        "original": "Yānā oruyha khattiyo;",
        "analyzed": "yānā oruyha khattiyo ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.1:14.3",
        "original": "Pattiko upasaṅkamma,",
        "analyzed": "pattikaḥ upasaṅkamma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.1:14.4",
        "original": "Āsajja naṁ upāvisi.",
        "analyzed": "āsajja nam upāvisi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.1:15.1",
        "original": "Nisajja rājā sammodi,",
        "analyzed": "nisajja rājā sammodi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.1:15.2",
        "original": "Kathaṁ sāraṇīyaṁ tato;",
        "analyzed": "katham sāraṇīyam ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.1:15.3",
        "original": "Kathaṁ so vītisāretvā,",
        "analyzed": "katham saḥ vītisāretvā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.1:15.4",
        "original": "Imamatthaṁ abhāsatha.",
        "analyzed": "imam attham abhāsatha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.1:16.1",
        "original": "“Yuvā ca daharo cāsi,",
        "analyzed": "yuvā ca daharaḥ ca asi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.1:16.2",
        "original": "Paṭhamuppattiko susu;",
        "analyzed": "paṭhama uppattikaḥ su su ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.1:16.3",
        "original": "Vaṇṇārohena sampanno,",
        "analyzed": "vaṇṇa ārohena sampannaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.1:16.4",
        "original": "Jātimā viya khattiyo.",
        "analyzed": "jātimā viya khattiyaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.1:17.1",
        "original": "Sobhayanto anīkaggaṁ,",
        "analyzed": "sobhayantaḥ anīka gga ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.1:17.2",
        "original": "Nāgasaṅghapurakkhato;",
        "analyzed": "nāga saṅgha purakkhato ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.1:17.3",
        "original": "Dadāmi bhoge bhuñjassu,",
        "analyzed": "dadāmi bhoge bhuñjassu ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.1:17.4",
        "original": "Jātiṁ akkhāhi pucchito”.",
        "analyzed": "jātim akkhāhi pucchitaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.1:18.1",
        "original": "“Ujuṁ janapado rāja,",
        "analyzed": "ujum janapadaḥ rāja ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.1:18.2",
        "original": "Himavantassa passato;",
        "analyzed": "himavantassa passataḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.1:18.3",
        "original": "Dhanaviriyena sampanno,",
        "analyzed": "dhana viriyena sampannaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.1:18.4",
        "original": "Kosalesu niketino.",
        "analyzed": "kosalesu niketinaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.1:19.1",
        "original": "Ādiccā nāma gottena,",
        "analyzed": "ādiccā nāma gottena ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.1:19.2",
        "original": "Sākiyā nāma jātiyā;",
        "analyzed": "sākiyā nāma jātiyā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.1:19.3",
        "original": "Tamhā kulā pabbajitomhi,",
        "analyzed": "tamhā kulā pabbajitomhi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.1:19.4",
        "original": "Na kāme abhipatthayaṁ.",
        "analyzed": "na kāme abhipatthaya ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.1:20.1",
        "original": "Kāmesvādīnavaṁ disvā,",
        "analyzed": "kāmesva ādīnavam disvā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.1:20.2",
        "original": "Nekkhammaṁ daṭṭhu khemato;",
        "analyzed": "nekkhammam daṭṭhu khemataḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.1:20.3",
        "original": "Padhānāya gamissāmi,",
        "analyzed": "padhānāya gamissāmi ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.1:20.4",
        "original": "Ettha me rañjatī mano”ti.",
        "analyzed": "ettha me rañjatī manoti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.1:20.5",
        "original": "Pabbajjāsuttaṁ paṭhamaṁ.",
        "analyzed": "pabbajjā suttam paṭhama ",
        "folio": "20"
    }
]