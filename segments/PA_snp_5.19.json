[
    {
        "segmentnr": "PA_snp_5.19:0.1",
        "original": "Sutta Nipāta 5.19",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.19:0.2",
        "original": "Pārāyanānugītigāthā",
        "analyzed": "pārāyana anugīti gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.19:1.1",
        "original": "“Pārāyanamanugāyissaṁ,",
        "analyzed": "pārāyanam anugāyiḥ sa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.19:1.2",
        "original": "(iccāyasmā piṅgiyo)",
        "analyzed": "id ca ayasmā piṅgiyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.19:1.3",
        "original": "Yathāddakkhi tathākkhāsi;",
        "analyzed": "yathā addakkhi tathā akkhāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.19:1.4",
        "original": "Vimalo bhūrimedhaso,",
        "analyzed": "vimalaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.19:1.5",
        "original": "Nikkāmo nibbano nāgo;",
        "analyzed": "nikkāmaḥ nibbanaḥ nāgaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.19:1.6",
        "original": "Kissa hetu musā bhaṇe.",
        "analyzed": "kissa hetu musā bhaṇe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.19:2.1",
        "original": "Pahīnamalamohassa,",
        "analyzed": "pahīna mala mohassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.19:2.2",
        "original": "mānamakkhappahāyino;",
        "analyzed": "māna makkha ppahāyinaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.19:2.3",
        "original": "Handāhaṁ kittayissāmi,",
        "analyzed": "handa aham kittayissāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.19:2.4",
        "original": "giraṁ vaṇṇūpasañhitaṁ.",
        "analyzed": "giram vaṇṇu upasañhita ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.19:3.1",
        "original": "Tamonudo buddho samantacakkhu,",
        "analyzed": "tamonudaḥ buddhaḥ samanta cakkhu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.19:3.2",
        "original": "Lokantagū sabbabhavātivatto;",
        "analyzed": "lokantagū ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.19:3.3",
        "original": "Anāsavo sabbadukkhappahīno,",
        "analyzed": "anāsavaḥ sabba dukkha ppahīno ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.19:3.4",
        "original": "Saccavhayo brahme upāsito me.",
        "analyzed": "brahme upāsitaḥ me ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.19:4.1",
        "original": "Dijo yathā kubbanakaṁ pahāya,",
        "analyzed": "dijaḥ yathā kubbanakam pahāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.19:4.2",
        "original": "Bahupphalaṁ kānanamāvaseyya;",
        "analyzed": "bahupphalam kānanam āvaseyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.19:4.3",
        "original": "Evampahaṁ appadasse pahāya,",
        "analyzed": "evampaham appadasse pahāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.19:4.4",
        "original": "Mahodadhiṁ haṁsoriva ajjhapatto.",
        "analyzed": "mahā udadhim hasoḥ iva ajjhapattaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.19:5.1",
        "original": "Ye me pubbe viyākaṁsu,",
        "analyzed": "ye me pubbe viyākasu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.19:5.2",
        "original": "Huraṁ gotamasāsanā;",
        "analyzed": "huram gotamasāsanā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.19:5.3",
        "original": "Iccāsi iti bhavissati,",
        "analyzed": "id ca asi iti bhavissati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.19:5.4",
        "original": "Sabbaṁ taṁ itihītihaṁ;",
        "analyzed": "sabbam tam iti hi iti ha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.19:5.5",
        "original": "Sabbaṁ taṁ takkavaḍḍhanaṁ.",
        "analyzed": "sabbam tam takkavaḍḍhana ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.19:6.1",
        "original": "Eko tamanudāsino,",
        "analyzed": "ekaḥ tam anudāsinaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.19:6.2",
        "original": "jutimā so pabhaṅkaro;",
        "analyzed": "jutimā saḥ pabhaṅkaraḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.19:6.3",
        "original": "Gotamo bhūripaññāṇo,",
        "analyzed": "gotamaḥ bhūri paññāṇaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.19:6.4",
        "original": "gotamo bhūrimedhaso.",
        "analyzed": "gotamaḥ bhūri medhasaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.19:7.1",
        "original": "Yo me dhammamadesesi,",
        "analyzed": "yaḥ me dhammam adesesi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.19:7.2",
        "original": "Sandiṭṭhikamakālikaṁ;",
        "analyzed": "sandiṭṭhikam akālika ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.19:7.3",
        "original": "Taṇhakkhayamanītikaṁ,",
        "analyzed": "taṇhakkhayamanītika ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.19:7.4",
        "original": "Yassa natthi upamā kvaci”.",
        "analyzed": "yassa na atthi upamā kvaci ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.19:8.1",
        "original": "“Kiṁ nu tamhā vippavasasi,",
        "analyzed": "kim nu tamhā vippavasasi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.19:8.2",
        "original": "muhuttamapi piṅgiya;",
        "analyzed": "piṅgiya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.19:8.3",
        "original": "Gotamā bhūripaññāṇā,",
        "analyzed": "gotamā bhūri paññāṇā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.19:8.4",
        "original": "gotamā bhūrimedhasā.",
        "analyzed": "gotamā bhūri medhasā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.19:9.1",
        "original": "Yo te dhammamadesesi,",
        "analyzed": "yaḥ te dhammam adesesi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.19:9.2",
        "original": "Sandiṭṭhikamakālikaṁ;",
        "analyzed": "sandiṭṭhikam akālika ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.19:9.3",
        "original": "Taṇhakkhayamanītikaṁ,",
        "analyzed": "taṇhakkhayamanītika ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.19:9.4",
        "original": "Yassa natthi upamā kvaci”.",
        "analyzed": "yassa na atthi upamā kvaci ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.19:10.1",
        "original": "“Nāhaṁ tamhā vippavasāmi,",
        "analyzed": "na aham tamhā vippavasāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.19:10.2",
        "original": "Muhuttamapi brāhmaṇa;",
        "analyzed": "brāhmaṇa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.19:10.3",
        "original": "Gotamā bhūripaññāṇā,",
        "analyzed": "gotamā bhūri paññāṇā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.19:10.4",
        "original": "Gotamā bhūrimedhasā.",
        "analyzed": "gotamā bhūri medhasā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.19:11.1",
        "original": "Yo me dhammamadesesi,",
        "analyzed": "yaḥ me dhammam adesesi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.19:11.2",
        "original": "Sandiṭṭhikamakālikaṁ;",
        "analyzed": "sandiṭṭhikam akālika ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.19:11.3",
        "original": "Taṇhakkhayamanītikaṁ,",
        "analyzed": "taṇhakkhayamanītika ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.19:11.4",
        "original": "Yassa natthi upamā kvaci.",
        "analyzed": "yassa na atthi upamā kvaci ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.19:12.1",
        "original": "Passāmi naṁ manasā cakkhunāva,",
        "analyzed": "passāmi nam manasā cakkhunā ava ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.19:12.2",
        "original": "Rattindivaṁ brāhmaṇa appamatto;",
        "analyzed": "rattindivam brāhmaṇa appamatto ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.19:12.3",
        "original": "Namassamāno vivasemi rattiṁ,",
        "analyzed": "namassamānaḥ vivasemi ratti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.19:12.4",
        "original": "Teneva maññāmi avippavāsaṁ.",
        "analyzed": "tena iva maññāmi a vippavāsa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.19:13.1",
        "original": "Saddhā ca pīti ca mano sati ca,",
        "analyzed": "saddhā ca pīti ca manaḥ sati ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.19:13.2",
        "original": "Nāpentime gotamasāsanamhā;",
        "analyzed": "na apenti ime gotama sāsanamhā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.19:13.3",
        "original": "Yaṁ yaṁ disaṁ vajati bhūripañño,",
        "analyzed": "yam yam disam vajati bhūri paññaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.19:13.4",
        "original": "Sa tena teneva natohamasmi.",
        "analyzed": "sa tena tena iva nataḥ ham asmi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.19:14.1",
        "original": "Jiṇṇassa me dubbalathāmakassa,",
        "analyzed": "jiṇṇassa me dubbala thāmakassa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.19:14.2",
        "original": "Teneva kāyo na paleti tattha;",
        "analyzed": "tena iva kāyaḥ na ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.19:14.3",
        "original": "Saṅkappayantāya vajāmi niccaṁ,",
        "analyzed": "saṅkappayantāya vajāmi nicca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.19:14.4",
        "original": "Mano hi me brāhmaṇa tena yutto.",
        "analyzed": "manaḥ hi me brāhmaṇa tena yuttaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.19:15.1",
        "original": "Paṅke sayāno pariphandamāno,",
        "analyzed": "paṅke sa yānaḥ pariphandamānaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.19:15.2",
        "original": "Dīpā dīpaṁ upaplaviṁ;",
        "analyzed": "dīpā dīpam upaplavi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.19:15.3",
        "original": "Athaddasāsiṁ sambuddhaṁ,",
        "analyzed": "sambuddha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.19:15.4",
        "original": "Oghatiṇṇamanāsavaṁ”.",
        "analyzed": "ogha tiṇṇamanāsava ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.19:16.1",
        "original": "“Yathā ahū vakkali muttasaddho,",
        "analyzed": "yathā ahū vakkali muttasaddho ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.19:16.2",
        "original": "Bhadrāvudho āḷavigotamo ca;",
        "analyzed": "bhadrāvudhaḥ āḷavigotamaḥ ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.19:16.3",
        "original": "Evamevaṁ tvampi pamuñcassu saddhaṁ,",
        "analyzed": "evam evam tvam pi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.19:16.4",
        "original": "Gamissasi tvaṁ piṅgiya maccudheyyassa pāraṁ”.",
        "analyzed": "gamissasi tvam piṅgiya maccudheyyassa pāra ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.19:17.1",
        "original": "“Esa bhiyyo pasīdāmi,",
        "analyzed": "esa bhiyyas pasīdāmi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.19:17.2",
        "original": "Sutvāna munino vaco;",
        "analyzed": "sutvāna muninaḥ vacaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.19:17.3",
        "original": "Vivaṭṭacchado sambuddho,",
        "analyzed": "vivaṭṭacchadaḥ sambuddhaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.19:17.4",
        "original": "Akhilo paṭibhānavā.",
        "analyzed": "akhilo paṭibhānavā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.19:18.1",
        "original": "Adhideve abhiññāya,",
        "analyzed": "adhideve abhiññāya ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.19:18.2",
        "original": "Sabbaṁ vedi varovaraṁ;",
        "analyzed": "sabbam vedi varovara ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.19:18.3",
        "original": "Pañhānantakaro satthā,",
        "analyzed": "     ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.19:18.4",
        "original": "Kaṅkhīnaṁ paṭijānataṁ.",
        "analyzed": "kaṅkhīnam paṭijānata ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.19:19.1",
        "original": "Asaṁhīraṁ asaṅkuppaṁ,",
        "analyzed": "a sahīram a saṅkuppa ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.19:19.2",
        "original": "Yassa natthi upamā kvaci;",
        "analyzed": "yassa na atthi upamā kvaci ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.19:19.3",
        "original": "Addhā gamissāmi na mettha kaṅkhā,",
        "analyzed": "addhā gamissāmi na mā itthā kaṅkhā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.19:19.4",
        "original": "Evaṁ maṁ dhārehi adhimuttacittan”ti.",
        "analyzed": "evam mama dhārehi adhimutta cittan ti ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.19:19.5",
        "original": "Pārāyanavaggo pañcamo.",
        "analyzed": "pārāyana vaggaḥ pañcamaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.19:19.6",
        "original": "Suttuddānaṁ",
        "analyzed": "sutta uddāna ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.19:20.1",
        "original": "Urago dhaniyopi ca,",
        "analyzed": "uragaḥ dhaniyaḥ api ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.19:20.2",
        "original": "Khaggavisāṇo kasi ca;",
        "analyzed": "khagga visāṇaḥ kasi ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.19:20.3",
        "original": "Cundo bhavo punadeva,",
        "analyzed": "cundaḥ bhava punadeva ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.19:20.4",
        "original": "Vasalo ca karaṇīyañca;",
        "analyzed": "vasalaḥ ca karaṇīyam ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.19:20.5",
        "original": "Hemavato atha yakkho,",
        "analyzed": "hemavataḥ atha yakkhaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.19:20.6",
        "original": "Vijayasuttaṁ munisuttavaranti.",
        "analyzed": "vijaya suttam muni sutta varanti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.19:21.1",
        "original": "Paṭhamakaṭṭhavaro varavaggo,",
        "analyzed": "paṭhamaka ṭṭhavaraḥ vara vaggo ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.19:21.2",
        "original": "Dvādasasuttadharo suvibhatto;",
        "analyzed": "dvādasa sutta dharo suvibhatto ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.19:21.3",
        "original": "Desito cakkhumatā vimalena,",
        "analyzed": "desito cakkhumatā vimalena ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.19:21.4",
        "original": "Suyyati vaggavaro uragoti.",
        "analyzed": "suyyati vagga varaḥ urago ti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.19:22.1",
        "original": "Ratanāmagandho hirimaṅgalanāmo,",
        "analyzed": "rata nāma gandhaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.19:22.2",
        "original": "Sucilomakapilo ca brāhmaṇadhammo;",
        "analyzed": "sucilomakapilaḥ ca brāhmaṇadhammo ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.19:22.3",
        "original": "Nāvā kiṁsīlauṭṭhahano ca,",
        "analyzed": "nāvā kisīla oṭṭhahanaḥ ca ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.19:22.4",
        "original": "Rāhulo ca punapi vaṅgīso.",
        "analyzed": "rāhulaḥ ca vaṅgīsaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.19:23.1",
        "original": "Sammāparibbājanīyopi cettha,",
        "analyzed": "sammā paribbājanīyaḥ api ca ittha ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.19:23.2",
        "original": "Dhammikasuttavaro suvibhatto;",
        "analyzed": "dhammika sutta varo suvibhatto ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.19:23.3",
        "original": "Cuddasasuttadharo dutiyamhi,",
        "analyzed": "dutiyamhi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.19:23.4",
        "original": "Cūḷakavaggavaroti tamāhu.",
        "analyzed": "tam āhu ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.19:24.1",
        "original": "Pabbajjapadhānasubhāsitanāmo,",
        "analyzed": "pabbajja padhāna su bhāsita nāmo ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.19:24.2",
        "original": "Pūraḷāso punadeva māgho ca;",
        "analyzed": "ca ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.19:24.3",
        "original": "Sabhiyaṁ keṇiyameva sallanāmo,",
        "analyzed": "sabhiyam keṇiyam eva ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.19:24.4",
        "original": "Vāseṭṭhavaro kālikopi ca.",
        "analyzed": "vāseṭṭhavaro kāliko api ca ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.19:25.1",
        "original": "Nālakasuttavaro suvibhatto,",
        "analyzed": "nālaka sutta varo suvibhatto ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.19:25.2",
        "original": "Taṁ anupassī tathā punadeva;",
        "analyzed": "tam anupassī tathā punar eva ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.19:25.3",
        "original": "Dvādasasuttadharo tatiyamhi,",
        "analyzed": "dvādasa sutta dharo tatiyamhi ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.19:25.4",
        "original": "Suyyati vaggavaro mahānāmo.",
        "analyzed": "suyyati vagga varaḥ mahā nāmaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.19:26.1",
        "original": "Kāmaguhaṭṭhakaduṭṭhakanāmā,",
        "analyzed": "kāma guhaṭṭhaka duṭṭhaka nāmā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.19:26.2",
        "original": "Suddhavaro paramaṭṭhakanāmo;",
        "analyzed": "suddhavaro paramaṭṭhaka nāmo ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.19:26.3",
        "original": "Jarā mettiyavaro suvibhatto,",
        "analyzed": "jarā mettiya varo suvibhatto ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.19:26.4",
        "original": "Pasūramāgaṇḍiyā purābhedo.",
        "analyzed": "purābhedo ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.19:27.1",
        "original": "Kalahavivādo ubho viyuhā ca,",
        "analyzed": "kalaha vivādaḥ ubhaḥ viyuhā ca ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.19:27.2",
        "original": "Tuvaṭakaattadaṇḍasāriputtā;",
        "analyzed": "tuvaṭaka atta daṇḍa sāriputtā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.19:27.3",
        "original": "Soḷasasuttadharo catutthamhi,",
        "analyzed": "catutthamhi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.19:27.4",
        "original": "Aṭṭhakavaggavaroti tamāhu.",
        "analyzed": "aṭṭhaka vagga varaḥ ti tam āhu ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.19:28.1",
        "original": "Magadhe janapade ramaṇīye,",
        "analyzed": "magadhe janapade ramaṇīye ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.19:28.2",
        "original": "Desavare katapuññanivese;",
        "analyzed": "desavare kata puñña nivese ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.19:28.3",
        "original": "Pāsāṇakacetiyavare suvibhatte,",
        "analyzed": "pāsāṇaka cetiya vare su vibhatte ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.19:28.4",
        "original": "Vasi bhagavā gaṇaseṭṭho.",
        "analyzed": "vasi bhagavā gaṇa seṭṭhaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.19:29.1",
        "original": "Ubhayavāsamāgatiyamhi,",
        "analyzed": "ubhaya vāsamāgatiyamhi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.19:29.2",
        "original": "Dvādasayojaniyā parisāya;",
        "analyzed": "dvādasa yojaniyā parisāya ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.19:29.3",
        "original": "Soḷasabrāhmaṇānaṁ kira puṭṭho,",
        "analyzed": "kira puṭṭhaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.19:29.4",
        "original": "Pucchāya soḷasapañhakammiyā;",
        "analyzed": "pucchāya soḷasa pañha kammiyā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.19:29.5",
        "original": "Nippakāsayi dhammamadāsi.",
        "analyzed": "dhammam adāsi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.19:30.1",
        "original": "Atthapakāsakabyañjanapuṇṇaṁ,",
        "analyzed": "attha pakāsaka byañjana puṇṇa ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.19:30.2",
        "original": "Dhammamadesesi parakhemajaniyaṁ;",
        "analyzed": "dhammam adesesi para khema janiya ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.19:30.3",
        "original": "Lokahitāya jino dvipadaggo,",
        "analyzed": "loka hitāya jinaḥ dvipadaggaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.19:30.4",
        "original": "Suttavaraṁ bahudhammavicitraṁ;",
        "analyzed": "sutta varam ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.19:30.5",
        "original": "Sabbakilesapamocanahetuṁ,",
        "analyzed": "sabba kilesa pamocana hetu ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.19:30.6",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.19:31.1",
        "original": "Byañjanamatthapadaṁ samayuttaṁ,",
        "analyzed": "byañjanam attha padam ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.19:31.2",
        "original": "Akkharasaññitaopamagāḷhaṁ;",
        "analyzed": "akkhara saññita opama gāḷha ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.19:31.3",
        "original": "Lokavicāraṇañāṇapabhaggaṁ,",
        "analyzed": "loka vicāraṇa ñāṇa pabhagga ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.19:31.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.19:32.1",
        "original": "Rāgamale amalaṁ vimalaggaṁ,",
        "analyzed": "rāga male amalam ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.19:32.2",
        "original": "Dosamale amalaṁ vimalaggaṁ;",
        "analyzed": "dosa male amalam ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.19:32.3",
        "original": "Mohamale amalaṁ vimalaggaṁ,",
        "analyzed": "moha male amalam ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.19:32.4",
        "original": "Lokavicāraṇañāṇapabhaggaṁ;",
        "analyzed": "loka vicāraṇa ñāṇa pabhagga ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.19:32.5",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.19:33.1",
        "original": "Klesamale amalaṁ vimalaggaṁ,",
        "analyzed": "klesa male amalam ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_5.19:33.2",
        "original": "Duccaritamale amalaṁ vimalaggaṁ;",
        "analyzed": "duccarita male amalam ",
