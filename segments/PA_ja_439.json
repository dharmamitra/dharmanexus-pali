        "original": "kiṁ pāpaṁ pakataṁ mayā”.",
        "analyzed": "kim pāpam pakatam mayā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_439:2.1",
        "original": "“Sabbe apihitā dvārā,",
        "analyzed": "sabbe apihitā dvārā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_439:2.2",
        "original": "oruddhosmi yathā dijo;",
        "analyzed": "oruddhaḥ asmi yathā dijaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_439:2.3",
        "original": "Kimādhikaraṇaṁ yakkha,",
        "analyzed": "kimādhikaraṇam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_439:2.4",
        "original": "cakkābhinihato ahaṁ.",
        "analyzed": "cakka abhinihataḥ aha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_439:3.1",
        "original": "Laddhā satasahassāni,",
        "analyzed": "laddhā sata sahassāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_439:3.2",
        "original": "atirekāni vīsati;",
        "analyzed": "atirekāni vīsati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_439:3.3",
        "original": "Anukampakānaṁ ñātīnaṁ,",
        "analyzed": "anukampakānam ñātīna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_439:3.4",
        "original": "vacanaṁ samma nākari.",
        "analyzed": "vacanam samma na akari ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_439:4.1",
        "original": "Laṅghiṁ samuddaṁ pakkhandi,",
        "analyzed": "laṅghim samuddam pakkhandi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_439:4.2",
        "original": "sāgaraṁ appasiddhikaṁ;",
        "analyzed": "sāgaram appasiddhika ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_439:4.3",
        "original": "Catubbhi aṭṭhajjhagamā,",
        "analyzed": "catubbhi aṭṭhajjhagamā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_439:4.4",
        "original": "aṭṭhāhipi ca soḷasa.",
        "analyzed": "ca soḷasa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_439:5.1",
        "original": "Soḷasāhi ca bāttiṁsa,",
