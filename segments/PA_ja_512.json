        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_512:9.2",
        "original": "Atho sigālehipi khāditāse;",
        "analyzed": "atha u khāditāse ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_512:9.3",
        "original": "Bandhaṁ vadhaṁ bhogajāniñcupenti,",
        "analyzed": "bandham vadham ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_512:9.4",
        "original": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
        "analyzed": "tassā puṇṇam kumbham imam kiṇātha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_512:10.1",
        "original": "Yaṁ pitva bhāseyya abhāsaneyyaṁ,",
        "analyzed": "yam pitva bhāseyya a bhāsaneyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_512:10.2",
        "original": "Sabhāyamāsīno apetavattho;",
        "analyzed": "sabhāyam āsīnaḥ apetavatthaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_512:10.3",
