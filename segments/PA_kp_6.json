        "segmentnr": "PA_kp_6:11.4",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_kp_6:12.1",
        "original": "Kiñcāpi so kamma karoti pāpakaṁ,",
        "analyzed": "kiñca api saḥ kamma karoti pāpaka ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_kp_6:12.2",
        "original": "Kāyena vācā uda cetasā vā;",
        "analyzed": "kāyena vācā uda cetasā vā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_kp_6:12.3",
        "original": "Abhabba so tassa paṭicchadāya,",
