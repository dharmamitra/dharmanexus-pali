    },
    {
        "segmentnr": "PA_ja_447:0.4",
        "original": "9. Mahādhammapālajātaka",
        "analyzed": "    ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_447:1.1",
        "original": "“Kiṁ te vataṁ kiṁ pana brahmacariyaṁ,",
        "analyzed": "kim te kim pana ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_447:1.2",
        "original": "Kissa suciṇṇassa ayaṁ vipāko;",
        "analyzed": "kissa su ciṇṇassa ayam vipākaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_447:1.3",
        "original": "Akkhāhi me brāhmaṇa etamatthaṁ,",
        "analyzed": "akkhāhi me brāhmaṇa etam attha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_447:1.4",
        "original": "Kasmā nu tumhaṁ daharā na miyyare”.",
        "analyzed": "kasmai nu tumham daharāḥ na miyyare ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_447:2.1",
        "original": "“Dhammaṁ carāma na musā bhaṇāma,",
        "analyzed": "dhammam carāma na musā bhaṇāma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_447:2.2",
        "original": "Pāpāni kammāni parivajjayāma;",
        "analyzed": "pāpāni kammāni parivajjayāma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_447:2.3",
        "original": "Anariyaṁ parivajjemu sabbaṁ,",
        "analyzed": "anariyam parivajjemu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_447:2.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_447:3.1",
        "original": "Suṇoma dhammaṁ asataṁ satañca,",
        "analyzed": "suṇoma dhammam asatam satam ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_447:3.2",
        "original": "Na cāpi dhammaṁ asataṁ rocayāma;",
        "analyzed": "na ca api dhammam asatam rocayāma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_447:3.3",
        "original": "Hitvā asante na jahāma sante,",
        "analyzed": "hitvā asante na jahāma sante ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_447:3.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_447:4.1",
        "original": "Pubbeva dānā sumanā bhavāma,",
        "analyzed": "pubbā iva dānā sumanāḥ bhavāma ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_447:4.2",
        "original": "Dadampi ve attamanā bhavāma;",
        "analyzed": "dadampi ve atta manāḥ bhavāma ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_447:4.3",
        "original": "Datvāpi ve nānutappāma pacchā,",
        "analyzed": "da tvā api ve na anutappāma pacchā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_447:4.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_447:5.1",
        "original": "Samaṇe mayaṁ brāhmaṇe addhike ca,",
        "analyzed": "samaṇe mayam brāhmaṇe addhike ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_447:5.2",
        "original": "Vanibbake yācanake dalidde;",
        "analyzed": "vanibbake yācanake dalidde ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_447:5.3",
        "original": "Annena pānena abhitappayāma,",
        "analyzed": "annena pānena abhitappayāma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_447:5.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_447:6.1",
        "original": "Mayañca bhariyaṁ nātikkamāma,",
        "analyzed": "mayam ca bhariyam na atikkamāma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_447:6.2",
        "original": "Amhe ca bhariyā nātikkamanti;",
        "analyzed": "amhe ca bhariyāḥ na atikkamanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_447:6.3",
        "original": "Aññatra tāhi brahmacariyaṁ carāma,",
        "analyzed": "aññatra tāhi brahmacariyam carāma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_447:6.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_447:7.1",
        "original": "Pāṇātipātā viramāma sabbe,",
        "analyzed": "pāṇa atipātāḥ viramāma sabbe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_447:7.2",
        "original": "Loke adinnaṁ parivajjayāma;",
        "analyzed": "loke a dinnam parivajjayāma ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_447:7.3",
        "original": "Amajjapā nopi musā bhaṇāma,",
        "analyzed": "a majja pāḥ musā bhaṇāma ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_447:7.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_447:8.1",
        "original": "Etāsu ve jāyare suttamāsu,",
        "analyzed": "etāsu ve jāyare suttamāsu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_447:8.2",
        "original": "Medhāvino honti pahūtapaññā;",
        "analyzed": "medhāvinaḥ honti pahūta paññā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_447:8.3",
        "original": "Bahussutā vedaguno ca honti,",
        "analyzed": "bahussutāḥ vedagunaḥ ca honti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_447:8.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_447:9.1",
        "original": "Mātā pitā ca bhaginī bhātaro ca,",
        "analyzed": "mātā pitā ca bhaginī bhātaro ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_447:9.2",
        "original": "Puttā ca dārā ca mayañca sabbe;",
        "analyzed": "puttā ca dārā ca mayañca sabbe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_447:9.3",
        "original": "Dhammaṁ carāma paralokahetu,",
        "analyzed": "dhammam carāma para loka hetu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_447:9.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_447:10.1",
        "original": "Dāsā ca dāsyo anujīvino ca,",
        "analyzed": "dāsā ca dāsyaḥ anujīvino ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_447:10.2",
        "original": "Paricārakā kammakarā ca sabbe;",
        "analyzed": "paricārakā kammakarā ca sabbe ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_447:10.3",
        "original": "Dhammaṁ caranti paralokahetu,",
        "analyzed": "dhammam caranti para loka hetu ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_447:10.4",
        "original": "Tasmā hi amhaṁ daharā na miyyare”.",
        "analyzed": "tasmai hi amham daharāḥ na miyyare ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_447:11.1",
        "original": "“Dhammo have rakkhati dhammacāriṁ,",
        "analyzed": "dhammaḥ have rakkhati dhamma cāri ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_447:11.2",
        "original": "Dhammo suciṇṇo sukhamāvahāti;",
        "analyzed": "dhammaḥ su ciṇṇaḥ sukham āvahāti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_447:11.3",
        "original": "Esānisaṁso dhamme suciṇṇe,",
        "analyzed": "dhamme su ciṇṇe ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_447:11.4",
        "original": "Na duggatiṁ gacchati dhammacārī.",
        "analyzed": "na duggatim gacchati dhamma cārī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_447:12.1",
        "original": "Dhammo have rakkhati dhammacāriṁ,",
        "analyzed": "dhammaḥ have rakkhati dhamma cāri ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_447:12.2",
        "original": "Chattaṁ mahantaṁ viya vassakāle;",
        "analyzed": "chattam mahantam viya vassa kāle ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_447:12.3",
        "original": "Dhammena gutto mama dhammapālo,",
        "analyzed": "dhammena guttaḥ mama dhamma pālaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_447:12.4",
        "original": "Aññassa aṭṭhīni sukhī kumāro”ti.",
        "analyzed": "aññassa aṭṭhīni sukhī kumāro ti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_447:13.1",
        "original": "Mahādhammapālajātakaṁ navamaṁ.",
        "analyzed": "mahādhammapāla jātakam navama ",
        "folio": "13"
    }
]