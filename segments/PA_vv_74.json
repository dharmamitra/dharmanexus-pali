    },
    {
        "segmentnr": "PA_vv_74:0.4",
        "original": "10. (Uttara) Pāyāsivimānavatthu",
        "analyzed": "uttara pāyāsi vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_74:1.1",
        "original": "“Yā devarājassa sabhā sudhammā,",
        "analyzed": "yā devarājassa sabhā su dhammā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_74:1.2",
        "original": "Yatthacchati devasaṅgho samaggo;",
        "analyzed": "yatthacchati deva saṅghaḥ samaggaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_74:1.3",
        "original": "Tathūpamaṁ tuyhamidaṁ vimānaṁ,",
        "analyzed": "tathūpamam tuyham idam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_74:1.4",
        "original": "Obhāsayaṁ tiṭṭhati antalikkhe.",
        "analyzed": "obhāsayam tiṭṭhati antalikkhe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_74:2.1",
        "original": "Deviddhipattosi mahānubhāvo,",
        "analyzed": "deviddhipatto asi mahā anubhāvaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_74:2.2",
        "original": "Manussabhūto kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtaḥ kim akāsi puñña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_74:2.3",
        "original": "Kenāsi evaṁ jalitānubhāvo,",
        "analyzed": "kena asi evam jalita anubhāvaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_74:2.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_74:3.1",
        "original": "So devaputto attamano,",
        "analyzed": "saḥ deva puttaḥ atta mano ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_74:3.2",
        "original": "…pe…",
        "analyzed": "pe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_74:3.3",
        "original": "yassa kammassidaṁ phalaṁ.",
        "analyzed": "yassa kammassa idam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_74:4.1",
        "original": "“Ahaṁ manussesu manussabhūto,",
        "analyzed": "aham manussesu manussa bhūtaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_74:4.2",
        "original": "Rañño pāyāsissa ahosiṁ māṇavo;",
        "analyzed": "rañño pāyāsissa ahosim māṇavaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_74:4.3",
        "original": "Laddhā dhanaṁ saṁvibhāgaṁ akāsiṁ,",
        "analyzed": "laddhā dhanam sa vibhāgam akāsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_74:4.4",
        "original": "Piyā ca me sīlavanto ahesuṁ;",
        "analyzed": "piyā ca me sīlavantaḥ ahesu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_74:4.5",
        "original": "Annañca pānañca pasannacitto,",
        "analyzed": "annam ca pānam ca pasanna cittaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_74:4.6",
        "original": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
        "analyzed": "sakkacca dānam vipulam adāsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_74:5.1",
        "original": "Tena metādiso vaṇṇo,",
        "analyzed": "tena mā itādisaḥ vaṇṇaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_74:5.2",
        "original": "…pe…",
        "analyzed": "pe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_74:5.3",
        "original": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca me sabbadisā pabhāsati iti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_74:6.1",
        "original": "Pāyāsivimānaṁ dasamaṁ.",
        "analyzed": "pāyāsi vimānam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_74:7.1",
        "original": "Pāyāsivaggo chaṭṭho.",
        "analyzed": "pāyāsi vaggaḥ chaṭṭhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_74:8.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_74:9.1",
        "original": "Dve agārino phaladāyī,",
        "analyzed": "dve phala dāyī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_74:9.2",
        "original": "Dve upassayadāyī bhikkhāya dāyī;",
        "analyzed": "dve upassaya dāyī bhikkhāya dāyī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_74:9.3",
        "original": "Yavapālako ceva dve,",
        "analyzed": "yavapālako ca iva dve ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_74:9.4",
        "original": "Kuṇḍalino pāyāsīti.",
        "analyzed": "kuṇḍalinā u pāyāsi iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_74:10.1",
        "original": "Purisānaṁ dutiyo vaggo pavuccatīti.",
        "analyzed": "purisānam dutiyaḥ vaggo pavuccati iti ",
        "folio": "10"
    }
