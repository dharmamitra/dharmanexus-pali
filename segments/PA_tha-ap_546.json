        "analyzed": "paṭibhānavatam agga ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_546:9.2",
        "original": "sāvakaṁ sādhusammataṁ;",
        "analyzed": "sāvakam sādhu sammata ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_546:9.3",
        "original": "Guṇaṁ bahuṁ pakittetvā,",
        "analyzed": "guṇam bahum pakittetvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_546:9.4",
