[
    {
        "segmentnr": "PA_thig_10.1:0.1",
        "original": "Therīgāthā 10.1",
        "analyzed": "therīgāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_10.1:0.2",
        "original": "Ekādasakanipāta",
        "analyzed": "ekādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_10.1:0.3",
        "original": "Kisāgotamītherīgāthā",
        "analyzed": "kisā gotamī therī gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_10.1:1.1",
        "original": "“Kalyāṇamittatā muninā,",
        "analyzed": "kalyāṇa mitta tā muninā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_10.1:1.2",
        "original": "lokaṁ ādissa vaṇṇitā;",
        "analyzed": "lokam ādissa vaṇṇitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_10.1:1.3",
        "original": "Kalyāṇamitte bhajamāno,",
        "analyzed": "kalyāṇa mitte bhajamānaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_10.1:1.4",
        "original": "api bālo paṇḍito assa.",
        "analyzed": "api bālaḥ paṇḍitaḥ assa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_10.1:2.1",
        "original": "Bhajitabbā sappurisā,",
        "analyzed": "bhajitabbā sappurisā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_10.1:2.2",
        "original": "Paññā tathā vaḍḍhati bhajantānaṁ;",
        "analyzed": "paññā tathā vaḍḍhati bhajantāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_10.1:2.3",
        "original": "Bhajamāno sappurise,",
        "analyzed": "bhajamānaḥ sappurise ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_10.1:2.4",
        "original": "Sabbehipi dukkhehi pamucceyya.",
        "analyzed": "sabbehi pi dukkhehi pamucceyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_10.1:3.1",
        "original": "Dukkhañca vijāneyya,",
        "analyzed": "dukkhañca vijāneyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_10.1:3.2",
        "original": "Dukkhassa ca samudayaṁ nirodhaṁ;",
        "analyzed": "dukkhassa ca samudayam nirodha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_10.1:3.3",
        "original": "Aṭṭhaṅgikañca maggaṁ,",
        "analyzed": "aṭṭhaṅgikañca magga ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_10.1:3.4",
        "original": "Cattāripi ariyasaccāni”.",
        "analyzed": "cattāri pi ariya saccāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_10.1:4.1",
        "original": "“Dukkho itthibhāvo,",
        "analyzed": "dukkhaḥ itthi bhāvaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_10.1:4.2",
        "original": "Akkhāto purisadammasārathinā;",
        "analyzed": "akkhāto purisa damma sārathinā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_10.1:4.3",
        "original": "Sapattikampi hi dukkhaṁ,",
        "analyzed": "sapattikam pi hi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_10.1:4.4",
        "original": "Appekaccā sakiṁ vijātāyo.",
        "analyzed": "appā ekaccā sakim ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_10.1:5.1",
        "original": "Galake api kantanti,",
        "analyzed": "galake api kantanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_10.1:5.2",
        "original": "Sukhumāliniyo visāni khādanti;",
        "analyzed": "sukhu māliniyaḥ visāni khādanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_10.1:5.3",
        "original": "Janamārakamajjhagatā,",
        "analyzed": "janamāraka majjha gatā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_10.1:5.4",
        "original": "Ubhopi byasanāni anubhonti”.",
        "analyzed": "ubhau api byasanāni anubhonti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_10.1:6.1",
        "original": "“Upavijaññā gacchantī,",
        "analyzed": "upavijaññā gacchantī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_10.1:6.2",
        "original": "addasāhaṁ patiṁ mataṁ;",
        "analyzed": "addasā aham patim mata ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_10.1:6.3",
        "original": "Panthamhi vijāyitvāna,",
        "analyzed": "panthamhi vijāyitvāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_10.1:6.4",
        "original": "appattāva sakaṁ gharaṁ.",
        "analyzed": "appattā ava sakam ghara ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_10.1:7.1",
        "original": "Dve puttā kālakatā,",
        "analyzed": "dve puttā kālakatā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_10.1:7.2",
        "original": "Patī ca panthe mato kapaṇikāya;",
        "analyzed": "patī ca panthe mataḥ kapaṇikāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_10.1:7.3",
        "original": "Mātā pitā ca bhātā,",
        "analyzed": "mātā pitā ca bhātā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_10.1:7.4",
        "original": "Ḍayhanti ca ekacitakāyaṁ”.",
        "analyzed": "ḍayhanti ca ekacitakāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_10.1:8.1",
        "original": "“Khīṇakulīne kapaṇe,",
        "analyzed": "khīṇa kulīne kapaṇe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_10.1:8.2",
        "original": "Anubhūtaṁ te dukhaṁ aparimāṇaṁ;",
        "analyzed": "anubhūtam te dukham aparimāṇa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_10.1:8.3",
        "original": "Assū ca te pavattaṁ,",
        "analyzed": "assū ca te pavatta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_10.1:8.4",
        "original": "Bahūni ca jātisahassāni.",
        "analyzed": "bahūni ca jāti sahassāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_10.1:9.1",
        "original": "Vasitā susānamajjhe,",
        "analyzed": "vasitā su sāna majjhe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_10.1:9.2",
        "original": "Athopi khāditāni puttamaṁsāni;",
        "analyzed": "athopi khāditāni puttamasāni ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_10.1:9.3",
        "original": "Hatakulikā sabbagarahitā,",
        "analyzed": "hata kulikā sabbaga rahitā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_10.1:9.4",
        "original": "Matapatikā amatamadhigacchiṁ.",
        "analyzed": "mata patikā amatam adhigacchi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_10.1:10.1",
        "original": "Bhāvito me maggo,",
        "analyzed": "bhāvitaḥ me maggo ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_10.1:10.2",
        "original": "Ariyo aṭṭhaṅgiko amatagāmī;",
        "analyzed": "ariyaḥ aṭṭhaṅgikaḥ amata gāmī ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_10.1:10.3",
        "original": "Nibbānaṁ sacchikataṁ,",
        "analyzed": "nibbānam sacchikata ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_10.1:10.4",
        "original": "Dhammādāsaṁ avekkhiṁhaṁ.",
        "analyzed": "dhamma ādāsam avekkhiha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_10.1:11.1",
        "original": "Ahamamhi kantasallā,",
        "analyzed": "aham amhi kanta sallā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_10.1:11.2",
        "original": "Ohitabhārā katañhi karaṇīyaṁ;",
        "analyzed": "ohita bhārā katañhi karaṇīya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_10.1:11.3",
        "original": "Kisā gotamī therī,",
        "analyzed": "kisā gotamī therī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_10.1:11.4",
        "original": "Vimuttacittā imaṁ bhaṇī”ti.",
        "analyzed": "vimutta cittā imam bhaṇīti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_10.1:12.1",
        "original": "… Kisā gotamī therī ….",
        "analyzed": "kisā gotamī therī ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_10.1:13.1",
        "original": "Ekādasakanipāto niṭṭhito.",
        "analyzed": "ekādasaka nipātaḥ niṭṭhito ",
        "folio": "13"
    }
]