        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_538:9.2",
        "original": "yāvajīvaṁ mahāmune;",
        "analyzed": "yāvajīvam mahā mune ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_538:9.3",
        "original": "Kāraṁ katvā cuto saggaṁ,",
        "analyzed": "kāram katvā cutaḥ sagga ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_538:9.4",
        "original": "agaṁ sabhavanaṁ yathā.",
        "analyzed": "agam sa bhavanam yathā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_538:10.1",
        "original": "Devabhūto manusso vā,",
        "analyzed": "deva bhūtaḥ manussaḥ vā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_538:10.2",
        "original": "sukhito tassa kammuno;",
        "analyzed": "sukhitaḥ tassa kammunaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_538:10.3",
        "original": "Vāhasā saṁsaritvāna,",
        "analyzed": "vāhasā sasaritvāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_538:10.4",
        "original": "sampattimanubhomahaṁ.",
        "analyzed": "sampattim anubhomaha ",
        "folio": "10"
    },
