        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_46:3.3",
        "original": "Taṁ ve muniṁ antimadehadhāriṁ,",
        "analyzed": "tam ve munim antima deha dhāri ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_46:3.4",
        "original": "Mārañjahaṁ brūmi jarāya pāraguṁ.",
        "analyzed": "māran aham brūmi jarāya pāragu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_46:4.1",
