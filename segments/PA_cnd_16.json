    },
    {
        "segmentnr": "PA_cnd_16:3.15",
        "original": "bhadrāvudhoti tassa brāhmaṇassa nāmaṁ …pe… abhilāpoti—",
        "analyzed": "bhadrāvudhā iti tassa brāhmaṇassa nāmam pe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.16",
        "original": "iccāyasmā bhadrāvudho.",
        "analyzed": "bhadrāvudhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:4.1",
        "original": "Nandiñjahaṁ oghatiṇṇaṁ vimuttanti nandī vuccati taṇhā.",
