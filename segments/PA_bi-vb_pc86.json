    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.2",
        "original": "Tena kho pana samayena aññatarā bhikkhunī aññatarissā itthiyā kulūpikā hoti.",
        "analyzed": "tena kho pana samayena aññatarā bhikkhunī aññatarissā itthiyā kulūpikā hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.3",
        "original": "Atha kho sā itthī taṁ bhikkhuniṁ etadavoca—",
        "analyzed": "atha kho sā itthī tam bhikkhunim etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.4",
        "original": "“handāyye, imaṁ saṅghāṇiṁ amukāya nāma itthiyā dehī”ti.",
        "analyzed": "handāyye imam saṅghāṇim amukāya nāma itthiyā dehi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.5",
        "original": "Atha kho sā bhikkhunī—",
        "analyzed": "atha kho sā bhikkhunī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.6",
        "original": "“sacāhaṁ pattena ādāya gacchāmi vissaro me bhavissatī”ti paṭimuñcitvā agamāsi.",
        "analyzed": "sacā aham pattena ādāya gacchāmi vissaraḥ me bhavissati iti paṭimuñcitvā agamāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.7",
        "original": "Tassā rathikāya suttake chinne vippakiriyiṁsu.",
        "analyzed": "tassāḥ rathikāya suttake chinne ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.8",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.9",
        "original": "“kathañhi nāma bhikkhuniyo saṅghāṇiṁ dhāressanti, seyyathāpi gihiniyo kāmabhoginiyo”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ saṅghāṇim dhāressanti seyyathā api gihiniyaḥ kāmabhoginiyaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.10",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.11",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.12",
        "original": "“kathañhi nāma bhikkhunī saṅghāṇiṁ dhāressatī”ti …pe…",
        "analyzed": "katham hi nāma bhikkhunī saṅghāṇim dhāressati iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.13",
        "original": "“saccaṁ kira, bhikkhave, bhikkhunī saṅghāṇiṁ dhāretī”ti?",
        "analyzed": "saccam kira bhikkhave bhikkhunī saṅghāṇim dhāreti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.14",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc86:1.15",
        "original": "Vigarahi buddho bhagavā …pe…",
