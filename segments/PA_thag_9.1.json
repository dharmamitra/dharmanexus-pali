    },
    {
        "segmentnr": "PA_thag_9.1:0.4",
        "original": "Bhūtattheragāthā",
        "analyzed": "bhūtattheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_9.1:1.1",
        "original": "“Yadā dukkhaṁ jarāmaraṇanti paṇḍito,",
        "analyzed": "yadā dukkham jarā maraṇanti paṇḍito ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_9.1:1.2",
        "original": "Aviddasū yattha sitā puthujjanā;",
        "analyzed": "aviddasū yattha sitā puthujjanā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_9.1:1.3",
        "original": "Dukkhaṁ pariññāya satova jhāyati,",
        "analyzed": "dukkham pariññāya sataḥ iva jhāyati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_9.1:1.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_9.1:2.1",
        "original": "Yadā dukkhassāvahaniṁ visattikaṁ,",
        "analyzed": "yadā dukkhassa avahanim visattika ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_9.1:2.2",
        "original": "Papañcasaṅghātadukhādhivāhiniṁ;",
        "analyzed": "papañca saṅghāta dukha adhivāhini ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_9.1:2.3",
        "original": "Taṇhaṁ pahantvāna satova jhāyati,",
        "analyzed": "taṇham pahantvāna satas uva jhāyati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_9.1:2.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_9.1:3.1",
        "original": "Yadā sivaṁ dvecaturaṅgagāminaṁ,",
        "analyzed": "yadā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_9.1:3.2",
        "original": "Magguttamaṁ sabbakilesasodhanaṁ;",
        "analyzed": "magga uttamam sabba kilesa sodhana ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_9.1:3.3",
        "original": "Paññāya passitva satova jhāyati,",
        "analyzed": "paññāya passitvā sataḥ uva jhāyati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_9.1:3.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_9.1:4.1",
        "original": "Yadā asokaṁ virajaṁ asaṅkhataṁ,",
        "analyzed": "yadā asokam virajam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_9.1:4.2",
        "original": "Santaṁ padaṁ sabbakilesasodhanaṁ;",
        "analyzed": "santam padam sabba kilesa sodhana ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_9.1:4.3",
        "original": "Bhāveti saṁyojanabandhanacchidaṁ,",
        "analyzed": "bhāva iti sayojana bandhana chidaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_9.1:4.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_9.1:5.1",
        "original": "Yadā nabhe gajjati meghadundubhi,",
        "analyzed": "yadā nabhe gajjati megha dundubhi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_9.1:5.2",
        "original": "Dhārākulā vihagapathe samantato;",
        "analyzed": "dhārā ākulāḥ vihaga pathe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_9.1:5.3",
        "original": "Bhikkhū ca pabbhāragatova jhāyati,",
        "analyzed": "bhikkhū ca pabbhāra gataḥ uva jhāyati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_9.1:5.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_9.1:6.1",
        "original": "Yadā nadīnaṁ kusumākulānaṁ,",
        "analyzed": "yadā nadīnam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_9.1:6.2",
        "original": "Vicitta-vāneyya-vaṭaṁsakānaṁ;",
        "analyzed": "vicitta vāneyya vaṭasakāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_9.1:6.3",
        "original": "Tīre nisinno sumanova jhāyati,",
        "analyzed": "tīre nisinnaḥ sumanāḥ iva jhāyati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_9.1:6.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_9.1:7.1",
        "original": "Yadā nisīthe rahitamhi kānane,",
        "analyzed": "yadā nisīthe rahitamhi kānane ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_9.1:7.2",
        "original": "Deve gaḷantamhi nadanti dāṭhino;",
        "analyzed": "deve nadanti dāṭhinaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_9.1:7.3",
        "original": "Bhikkhū ca pabbhāragatova jhāyati,",
        "analyzed": "bhikkhū ca pabbhāra gataḥ uva jhāyati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_9.1:7.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_9.1:8.1",
        "original": "Yadā vitakke uparundhiyattano,",
        "analyzed": "yadā vitakke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_9.1:8.2",
        "original": "Nagantare nagavivaraṁ samassito;",
        "analyzed": "naga antare naga vivaram ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_9.1:8.3",
        "original": "Vītaddaro vītakhilova jhāyati,",
        "analyzed": "vīta ddaraḥ vīta khilaḥ uva jhāyati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_9.1:8.4",
        "original": "Tato ratiṁ paramataraṁ na vindati.",
        "analyzed": "tatas ratim paramataram na vindati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_9.1:9.1",
        "original": "Yadā sukhī malakhilasokanāsano,",
        "analyzed": "yadā sukhī mala khila sokana āsanaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_9.1:9.2",
        "original": "Niraggaḷo nibbanatho visallo;",
        "analyzed": "niraggaḷaḥ nibbanathaḥ visallaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_9.1:9.3",
        "original": "Sabbāsave byantikatova jhāyati,",
        "analyzed": "sabba asave byantikatas va jhāyati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_9.1:9.4",
        "original": "Tato ratiṁ paramataraṁ na vindatī”ti.",
        "analyzed": "tatas ratim paramataram na vindati iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_9.1:9.5",
        "original": "… Bhūto thero …",
        "analyzed": "bhūto theraḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_9.1:9.6",
        "original": "Navakanipāto niṭṭhito.",
        "analyzed": "navaka nipātaḥ niṭṭhito ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_9.1:10.0",
        "original": "Tatruddānaṁ",
        "analyzed": "tatra uddāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_9.1:10.1",
        "original": "Bhūto tathaddaso thero,",
        "analyzed": "bhūto thero ",
        "folio": "10"
    },
    {
