    {
        "segmentnr": "PA_ja_508:0.4",
        "original": "12. Pañcapaṇḍitajātaka",
        "analyzed": "pañcapaṇḍitajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_508:1.1",
        "original": "“Pañca paṇḍitā samāgatāttha,",
        "analyzed": "pañca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_508:1.2",
        "original": "Pañhā me paṭibhāti taṁ suṇātha;",
        "analyzed": "pañhā me paṭibhāti tam suṇātha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_508:1.3",
        "original": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
        "analyzed": "nindiyam attham pasasiyam vā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_508:1.4",
        "original": "Kassevāvikareyya guyhamatthaṁ”.",
        "analyzed": "kassa iva a vikareyya guyham attha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_508:2.1",
        "original": "“Tvaṁ āvikarohi bhūmipāla,",
        "analyzed": "tvam āvikarohi bhūmipāla ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_508:2.2",
        "original": "Bhattā bhārasaho tuvaṁ vade taṁ;",
        "analyzed": "bhattā bhāra sahaḥ tuvam vade ta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_508:2.3",
        "original": "Tava chandarucīni sammasitvā,",
        "analyzed": "tava chanda rucīni sammasitvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_508:2.4",
        "original": "Atha vakkhanti janinda pañca dhīrā”.",
        "analyzed": "atha vakkhanti janinda pañca dhīrā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_508:3.1",
        "original": "“Yā sīlavatī anaññatheyyā,",
        "analyzed": "yā sīlavatī anaññatheyyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_508:3.2",
        "original": "Bhattucchandavasānugā manāpā;",
        "analyzed": "bhattucchanda vasā anugā manāpā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_508:3.3",
        "original": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
        "analyzed": "nindiyam attham pasasiyam vā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_508:3.4",
        "original": "Bhariyāyāvikareyya guyhamatthaṁ”.",
        "analyzed": "guyham attha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_508:4.1",
        "original": "“Yo kicchagatassa āturassa,",
        "analyzed": "yaḥ kiccha gatassa āturassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_508:4.2",
        "original": "Saraṇaṁ hoti gatī parāyanañca;",
        "analyzed": "saraṇam hoti gatī parāyanam ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_508:4.3",
        "original": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
        "analyzed": "nindiyam attham pasasiyam vā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_508:4.4",
        "original": "Sakhino vāvikareyya guyhamatthaṁ”.",
        "analyzed": "sakhinaḥ guyham attha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_508:5.1",
        "original": "“Jeṭṭho atha majjhimo kaniṭṭho,",
        "analyzed": "jeṭṭhaḥ atha majjhimaḥ kaniṭṭhaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_508:5.2",
        "original": "Yo ce sīlasamāhito ṭhitatto;",
        "analyzed": "yaḥ ce sīla samāhitaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_508:5.3",
        "original": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
        "analyzed": "nindiyam attham pasasiyam vā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_508:5.4",
        "original": "Bhātu vāvīkareyya guyhamatthaṁ”.",
        "analyzed": "bhātu vāvīkareyya guyham attha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_508:6.1",
        "original": "“Yo ve pituhadayassa paddhagū,",
        "analyzed": "yaḥ ve pitu hadayassa paddhagū ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_508:6.2",
        "original": "Anujāto pitaraṁ anomapañño;",
        "analyzed": "anujātaḥ pitaram an oma paññaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_508:6.3",
        "original": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
        "analyzed": "nindiyam attham pasasiyam vā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_508:6.4",
        "original": "Puttassāvikareyya guyhamatthaṁ”.",
        "analyzed": "guyham attha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_508:7.1",
        "original": "“Mātā dvipadājanindaseṭṭha,",
        "analyzed": "mātā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_508:7.2",
        "original": "Yā naṁ poseti chandasā piyena;",
        "analyzed": "yā nam posā iti chandasā piyena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_508:7.3",
        "original": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
        "analyzed": "nindiyam attham pasasiyam vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_508:7.4",
        "original": "Mātuyāvīkareyya guyhamatthaṁ”.",
        "analyzed": "guyham attha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_508:8.1",
        "original": "“Guyhassa hi guyhameva sādhu,",
        "analyzed": "hi guyham eva sādhu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_508:8.2",
        "original": "Na hi guyhassa pasatthamāvikammaṁ;",
        "analyzed": "na hi guyhassa pasattham āvikamma ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_508:8.3",
        "original": "Anipphannatā saheyya dhīro,",
        "analyzed": "a nipphanna tā saheyya dhīraḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_508:8.4",
        "original": "Nipphannova yathāsukhaṁ bhaṇeyya”.",
        "analyzed": "nipphannaḥ uva yathāsukham bhaṇeyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_508:9.1",
        "original": "“Kiṁ tvaṁ vimanosi rājaseṭṭha,",
        "analyzed": "kim tvam vimanāḥ asi rāja seṭṭha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_508:9.2",
        "original": "Dvipadajaninda vacanaṁ suṇoma metaṁ;",
        "analyzed": "dvipada ja ninda vacanam suṇoma mā ita ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_508:9.3",
        "original": "Kiṁ cintayamāno dummanosi,",
        "analyzed": "kim cintayamānaḥ dummanāḥ asi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_508:9.4",
        "original": "Nūna deva aparādho atthi mayhaṁ”.",
        "analyzed": "nūna deva aparādhaḥ atthi mayha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_508:10.1",
        "original": "“‘Paṇhe vajjho mahosadho’ti,",
        "analyzed": "paṇhe vajjhaḥ mahosadhaḥ ti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_508:10.2",
        "original": "Āṇatto me vadhāya bhūripañño;",
        "analyzed": "āṇattaḥ me vadhāya bhūri pañño ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_508:10.3",
        "original": "Taṁ cintayamāno dummanosmi,",
        "analyzed": "tam cintayamānaḥ dummanāḥ asmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_508:10.4",
        "original": "Na hi devī aparādho atthi tuyhaṁ”.",
        "analyzed": "na hi devī aparādhaḥ atthi tuyha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_508:11.1",
        "original": "“Abhidosagato dāni ehisi,",
        "analyzed": "abhidosa gataḥ dāni ehisi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_508:11.2",
        "original": "Kiṁ sutvā kiṁ saṅkate mano te;",
        "analyzed": "kim sutvā kim saṅkate mano te ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_508:11.3",
        "original": "Ko te kimavoca bhūripañña,",
        "analyzed": "kaḥ te kim avoca bhūripañña ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_508:11.4",
        "original": "Iṅgha vacanaṁ suṇoma brūhi metaṁ”.",
        "analyzed": "iṅgha vacanam suṇoma brūhi mā ita ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_508:12.1",
        "original": "“‘Paṇhe vajjho mahosadho’ti,",
        "analyzed": "paṇhe vajjhaḥ mahosadhaḥ ti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_508:12.2",
        "original": "Yadi te mantayitaṁ janinda dosaṁ;",
