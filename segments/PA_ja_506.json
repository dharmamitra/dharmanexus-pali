        "analyzed": "muñca imu naḥ uragam ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_506:13.3",
        "original": "Ossaṭṭhakāyo urago carātu,",
        "analyzed": "ossaṭṭha kāyaḥ uragaḥ carātu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_506:13.4",
        "original": "Puññatthiko muñcatu bandhanasmā”.",
        "analyzed": "puññatthikaḥ muñcatu bandhanasmā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_506:14.1",
        "original": "Mutto campeyyako nāgo,",
        "analyzed": "muttaḥ campeyyakaḥ nāgaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_506:14.2",
        "original": "rājānaṁ etadabravi;",
        "analyzed": "rājānam etat abravi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_506:14.3",
        "original": "“Namo te kāsirājatthu,",
        "analyzed": "namaḥ te kāsirājatthu ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_506:14.4",
        "original": "namo te kāsivaḍḍhana;",
        "analyzed": "namaḥ te kāsivaḍḍhana ",
        "folio": "14"
    },
    {
