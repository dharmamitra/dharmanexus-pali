    },
    {
        "segmentnr": "PA_thag_5.12:1.1",
        "original": "“Yo ve garūnaṁ vacanaññu dhīro,",
        "analyzed": "yaḥ ve garūnam vacanaññu dhīraḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_5.12:1.2",
        "original": "Vase ca tamhi janayetha pemaṁ;",
        "analyzed": "vase ca tamhi janayetha pema ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_5.12:1.3",
        "original": "So bhattimā nāma ca hoti paṇḍito,",
        "analyzed": "saḥ bhattimā nāma ca hoti paṇḍito ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_5.12:1.4",
        "original": "Ñatvā ca dhammesu visesi assa.",
        "analyzed": "ñatvā ca dhammesu visesī assa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_5.12:2.1",
        "original": "Yaṁ āpadā uppatitā uḷārā,",
        "analyzed": "yam āpadā uppatitāḥ uḷārā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.12:2.2",
        "original": "Nakkhambhayante paṭisaṅkhayantaṁ;",
        "analyzed": "nakkhambhayante paṭisaṅkhayanta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.12:2.3",
        "original": "So thāmavā nāma ca hoti paṇḍito,",
        "analyzed": "saḥ thāmavā nāma ca hoti paṇḍito ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.12:2.4",
        "original": "Ñatvā ca dhammesu visesi assa.",
        "analyzed": "ñatvā ca dhammesu visesī assa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.12:3.1",
        "original": "Yo ve samuddova ṭhito anejo,",
        "analyzed": "yaḥ ve samuddaḥ iva ṭhitaḥ anejaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.12:3.2",
        "original": "Gambhīrapañño nipuṇatthadassī;",
        "analyzed": "         ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.12:3.3",
        "original": "Asaṁhāriyo nāma ca hoti paṇḍito,",
        "analyzed": "asahāriyaḥ nāma ca hoti paṇḍito ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.12:3.4",
        "original": "Ñatvā ca dhammesu visesi assa.",
        "analyzed": "ñatvā ca dhammesu visesī assa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.12:4.1",
        "original": "Bahussuto dhammadharo ca hoti,",
        "analyzed": "bahu sutaḥ dhamma dharaḥ ca hoti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.12:4.2",
        "original": "Dhammassa hoti anudhammacārī;",
        "analyzed": "dhammassa hoti anudhamma cārī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.12:4.3",
        "original": "So tādiso nāma ca hoti paṇḍito,",
        "analyzed": "saḥ tādisaḥ nāma ca hoti paṇḍito ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.12:4.4",
        "original": "Ñatvā ca dhammesu visesi assa.",
        "analyzed": "ñatvā ca dhammesu visesī assa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.12:5.1",
        "original": "Atthañca yo jānāti bhāsitassa,",
        "analyzed": "attham ca yaḥ jānāti bhāsitassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_5.12:5.2",
        "original": "Atthañca ñatvāna tathā karoti;",
