        "analyzed": "jina sāsana kārako ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_tha-ap_538:12.3",
        "original": "Tato cutā pañca janā,",
        "analyzed": "tatas cutā pañca janā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_tha-ap_538:12.4",
        "original": "devalokaṁ agamhase.",
        "analyzed": "deva lokam agamhase ",
        "folio": "12"
    },
