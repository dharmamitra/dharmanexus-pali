        "analyzed": "tuvam paccasi revate ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_39:14.1",
        "original": "Hatthepi chindanti athopi pāde,",
        "analyzed": "hatthepi chindanti atho pi pāde ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_39:14.2",
        "original": "Kaṇṇepi chindanti athopi nāsaṁ;",
        "analyzed": "kaṇṇepi chindanti athopi nāsa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_39:14.3",
        "original": "Athopi kākoḷagaṇā samecca,",
        "analyzed": "atha upi kākoḍagaṇā samet ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_39:14.4",
        "original": "Saṅgamma khādanti viphandamānan”ti.",
        "analyzed": "saṅgamma khādanti viphandamānanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_39:15.1",
