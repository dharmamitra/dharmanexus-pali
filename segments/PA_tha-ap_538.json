        "original": "gantvāna katipāhakaṁ;",
        "analyzed": "gantvāna kati pāhaka ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_tha-ap_538:14.3",
        "original": "Tadā bhīsanake ghore,",
        "analyzed": "tadā bhīsanake ghore ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_tha-ap_538:14.4",
        "original": "patito makarākare.",
        "analyzed": "patitaḥ makarākare ",
        "folio": "14"
    },
