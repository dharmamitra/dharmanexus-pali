        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.29",
        "original": "“ko, ayye, jānāti bhikkhunisaṅghassa dinno”ti?",
        "analyzed": "kaḥ ayye jānāti bhikkhuni saṅghassa dinnaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.30",
        "original": "Evaṁ vutte, thullanandā bhikkhunī te mahāmatte etadavoca—",
        "analyzed": "evam vutte thullanandā bhikkhunī te mahāmatte etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.31",
        "original": "“api nāyyā tumhehi diṭṭhaṁ vā sutaṁ vā sakkhiṁ ṭhapayitvā dānaṁ diyyamānan”ti?",
        "analyzed": "api nāyyā tumhehi diṭṭham vā sutam vā sakkhim ṭhapayitvā dānam diyyamānanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.32",
        "original": "Atha kho te mahāmattā—",
        "analyzed": "atha khalu te mahāmattā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.33",
        "original": "“saccaṁ kho ayyā āhā”ti taṁ udositaṁ bhikkhunisaṅghassa akaṁsu.",
        "analyzed": "saccam kho ayyā āha ati tam udositam bhikkhuni saṅghassa akasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.34",
        "original": "Atha kho so puriso parājito ujjhāyati khiyyati vipāceti—",
        "analyzed": "atha kho saḥ purisaḥ parājitaḥ ujjhāyati khiyyati vipāceti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.35",
        "original": "“assamaṇiyo imā muṇḍā bandhakiniyo.",
        "analyzed": "assamaṇiyaḥ imāḥ muṇḍāḥ bandhakiniyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.36",
        "original": "Kathañhi nāma amhākaṁ udositaṁ acchindāpessantī”ti.",
