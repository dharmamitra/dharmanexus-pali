[
    {
        "segmentnr": "PA_snp_1.5:0.1",
        "original": "Sutta Nipāta 1.5",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.5:0.2",
        "original": "Cundasutta",
        "analyzed": "cundasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.5:1.1",
        "original": "“Pucchāmi muniṁ pahūtapaññaṁ,",
        "analyzed": "pucchāmi munim pahūta pañña ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.5:1.2",
        "original": "(iti cundo kammāraputto)",
        "analyzed": "iti cundo kammāra putto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.5:1.3",
        "original": "Buddhaṁ dhammassāmiṁ vītataṇhaṁ;",
        "analyzed": "buddham dhammassāmim vītataṇha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.5:1.4",
        "original": "Dvipaduttamaṁ sārathīnaṁ pavaraṁ,",
        "analyzed": "dvipad uttamam sārathīnam pavara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.5:1.5",
        "original": "Kati loke samaṇā tadiṅgha brūhi”.",
        "analyzed": "kati loke samaṇā tadiṅgha brūhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.5:2.1",
        "original": "“Caturo samaṇā na pañcamatthi,",
        "analyzed": "caturaḥ samaṇāḥ na ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.5:2.2",
        "original": "(cundāti bhagavā)",
        "analyzed": "cundāti bhagavā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.5:2.3",
        "original": "Te te āvikaromi sakkhipuṭṭho;",
        "analyzed": "te te āvikaromi sakkhipuṭṭhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.5:2.4",
        "original": "Maggajino maggadesako ca,",
        "analyzed": "maggajinaḥ maggadesako ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.5:2.5",
        "original": "Magge jīvati yo ca maggadūsī”.",
        "analyzed": "magge jīvati yaḥ ca magga dūsī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.5:3.1",
        "original": "“Kaṁ maggajinaṁ vadanti buddhā,",
        "analyzed": "kam magga jinam vadanti buddhāḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.5:3.2",
        "original": "(iti cundo kammāraputto)",
        "analyzed": "iti cundo kammāra putto ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.5:3.3",
        "original": "Maggakkhāyī kathaṁ atulyo hoti;",
        "analyzed": "magga khāyī katham atulyaḥ hoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.5:3.4",
        "original": "Magge jīvati me brūhi puṭṭho,",
        "analyzed": "magge jīvati me brūhi puṭṭhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.5:3.5",
        "original": "Atha me āvikarohi maggadūsiṁ”.",
        "analyzed": "atha me āvikarohi maggadūsi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.5:4.1",
        "original": "“Yo tiṇṇakathaṅkatho visallo,",
        "analyzed": "yaḥ tiṇṇa kathaṅkathaḥ visallaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.5:4.2",
        "original": "Nibbānābhirato anānugiddho;",
        "analyzed": "nibbāna abhirataḥ an ānugiddhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.5:4.3",
        "original": "Lokassa sadevakassa netā,",
        "analyzed": "lokassa sa devakassa netā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.5:4.4",
        "original": "Tādiṁ maggajinaṁ vadanti buddhā.",
        "analyzed": "tādim maggajinam vadanti buddhāḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.5:5.1",
        "original": "Paramaṁ paramanti yodha ñatvā,",
        "analyzed": "paramam paramanti yodha ñatvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.5:5.2",
        "original": "Akkhāti vibhajate idheva dhammaṁ;",
        "analyzed": "akkhāti vibhajate idha iva dhamma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.5:5.3",
        "original": "Taṁ kaṅkhachidaṁ muniṁ anejaṁ,",
        "analyzed": "tam kaṅkha chidam munim aneja ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.5:5.4",
        "original": "Dutiyaṁ bhikkhunamāhu maggadesiṁ.",
        "analyzed": "dutiyam bhikkhunam āhu magga desi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.5:6.1",
        "original": "Yo dhammapade sudesite,",
        "analyzed": "yaḥ dhamma pade su desite ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.5:6.2",
        "original": "Magge jīvati saññato satīmā;",
        "analyzed": "magge jīvati saññataḥ satīmā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.5:6.3",
        "original": "Anavajjapadāni sevamāno,",
        "analyzed": "anavajja padāni sevamānaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.5:6.4",
        "original": "Tatiyaṁ bhikkhunamāhu maggajīviṁ.",
        "analyzed": "tatiyam bhikkhunam āhu magga jīvi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.5:7.1",
        "original": "Chadanaṁ katvāna subbatānaṁ,",
        "analyzed": "chadanam katvāna subbatāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.5:7.2",
        "original": "Pakkhandī kuladūsako pagabbho;",
        "analyzed": "pakkhandī kula dūsakaḥ pagabbhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.5:7.3",
        "original": "Māyāvī asaññato palāpo,",
        "analyzed": "māyāvī asaññataḥ palāpaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.5:7.4",
        "original": "Patirūpena caraṁ sa maggadūsī.",
        "analyzed": "pati rūpena caram sa maggadūsī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.5:8.1",
        "original": "Ete ca paṭivijjhi yo gahaṭṭho,",
        "analyzed": "ete ca paṭivijjhi yaḥ gahaṭṭhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.5:8.2",
        "original": "Sutavā ariyasāvako sapañño;",
        "analyzed": "sutavā ariya sāvako sa pañño ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.5:8.3",
        "original": "Sabbe netādisāti ñatvā,",
        "analyzed": "sabbe ñatvā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.5:8.4",
        "original": "Iti disvā na hāpeti tassa saddhā;",
        "analyzed": "iti disvā na ha apeti tassa saddhā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.5:8.5",
        "original": "Kathaṁ hi duṭṭhena asampaduṭṭhaṁ,",
        "analyzed": "katham hi duṭṭhena a sampaduṭṭha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.5:8.6",
        "original": "Suddhaṁ asuddhena samaṁ kareyyā”ti.",
        "analyzed": "suddham a suddhena samam kareyyāti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.5:8.7",
        "original": "Cundasuttaṁ pañcamaṁ.",
        "analyzed": "cunda suttam pañcama ",
        "folio": "8"
    }
]