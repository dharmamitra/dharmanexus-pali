        "analyzed": "anaḥ vāda kare dije ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_427:9.1",
        "original": "Evampi idha vuḍḍhānaṁ,",
        "analyzed": "evampi idha vuḍḍhāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_427:9.2",
        "original": "yo vākyaṁ nāvabujjhati;",
        "analyzed": "yaḥ vākyam na avabujjhati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_427:9.3",
        "original": "Atisīmacaro ditto,",
        "analyzed": "ati sīma caraḥ dittaḥ ",
        "folio": "9"
    },
