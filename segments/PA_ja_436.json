    },
    {
        "segmentnr": "PA_ja_436:0.4",
        "original": "10. Samuggajātaka",
        "analyzed": "samuggajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_436:1.1",
        "original": "“Kuto nu āgacchatha bho tayo janā,",
        "analyzed": "kutas nu āgacchatha bho tayaḥ janā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_436:1.2",
        "original": "Svāgatā etha nisīdathāsane;",
        "analyzed": "svāgatāḥ etha nisīdatha āsane ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_436:1.3",
        "original": "Kaccittha bhonto kusalaṁ anāmayaṁ,",
        "analyzed": "kaccit tha bhontaḥ kusalam anāmaya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_436:1.4",
        "original": "Cirassamabbhāgamanaṁ hi vo idha”.",
        "analyzed": "cirassamabbhāgamanam hi vaḥ idha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_436:2.1",
        "original": "“Ahameva eko idha majja patto,",
        "analyzed": "aham eva ekaḥ idha majja patto ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_436:2.2",
        "original": "Na cāpi me dutiyo koci vijjati;",
        "analyzed": "na ca api me dutiyaḥ koci vijjati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_436:2.3",
        "original": "Kimeva sandhāya te bhāsitaṁ ise,",
        "analyzed": "kim eva sandhāya te bhāsitam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_436:2.4",
        "original": "‘Kuto nu āgacchatha bho tayo janā’”.",
        "analyzed": "kutas nu āgacchatha bho tayaḥ janā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_436:3.1",
        "original": "“Tuvañca eko bhariyā ca te piyā,",
        "analyzed": "ekaḥ bhariyā ca te piyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_436:3.2",
        "original": "Samuggapakkhittanikiṇṇamantare;",
        "analyzed": "samugga pakkhitta nikiṇṇam antare ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_436:3.3",
        "original": "Sā rakkhitā kucchigatāva te sadā,",
        "analyzed": "sā rakkhitā kucchi gatā ava te sadā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_436:3.4",
        "original": "Vāyussa puttena sahā tahiṁ ratā”.",
        "analyzed": "vāyussa puttena sahā tahim ratā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_436:4.1",
        "original": "“Saṁviggarūpo isinā viyākato,",
        "analyzed": "sa vigga rūpaḥ isinā viyākataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_436:4.2",
        "original": "So dānavo tattha samuggamuggili;",
        "analyzed": "saḥ dānavaḥ tattha samuggamuggili ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_436:4.3",
        "original": "Addakkhi bhariyaṁ suci māladhāriniṁ,",
        "analyzed": "addakkhi bhariyam suci māla dhārini ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_436:4.4",
        "original": "Vāyussa puttena sahā tahiṁ rataṁ”.",
        "analyzed": "vāyussa puttena saha tahim ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_436:5.1",
        "original": "“Sudiṭṭharūpamuggatapānuvattinā,",
        "analyzed": "su diṭṭha rūpam ugga tapa anuvattinā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_436:5.2",
        "original": "Hīnā narā ye pamadāvasaṁ gatā;",
        "analyzed": "hīnāḥ narāḥ ye pamadā vasam gatā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_436:5.3",
        "original": "Yathā have pāṇarivettha rakkhitā,",
        "analyzed": "yathā have rakkhitā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_436:5.4",
        "original": "Duṭṭhā mayī aññamabhippamodayi.",
        "analyzed": "duṭṭhā mayī aññam abhippamodayi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_436:6.1",
        "original": "Divā ca ratto ca mayā upaṭṭhitā,",
        "analyzed": "divā ca ratto ca mayā upaṭṭhitā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_436:6.2",
        "original": "Tapassinā jotirivā vane vasaṁ;",
        "analyzed": "tapassinā jotiḥ ivā vane vasa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_436:6.3",
        "original": "Sā dhammamukkamma adhammamācari,",
        "analyzed": "sā dhammam ukkamma adhammam ācari ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_436:6.4",
        "original": "Akiriyarūpo pamadāhi santhavo.",
        "analyzed": "a kiriya rūpaḥ santhavaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_436:7.1",
        "original": "Sarīramajjhamhi ṭhitātimaññahaṁ,",
        "analyzed": "sarīra majjhamhi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_436:7.2",
        "original": "Mayhaṁ ayanti asatiṁ asaññataṁ;",
        "analyzed": "mayham ayanti asatim ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_436:7.3",
        "original": "Sā dhammamukkamma adhammamācari,",
        "analyzed": "sā dhammam ukkamma adhammam ācari ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_436:7.4",
        "original": "Akiriyarūpo pamadāhi santhavo.",
        "analyzed": "a kiriya rūpaḥ santhavaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_436:8.1",
        "original": "Surakkhitaṁ meti kathaṁ nu vissase,",
        "analyzed": "su rakkhitam mā iti katham nu vissase ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_436:8.2",
        "original": "Anekacittāsu na hatthi rakkhaṇā;",
        "analyzed": "aneka cittāsu na hatthi rakkhaṇā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_436:8.3",
        "original": "Etā hi pātālapapātasannibhā,",
        "analyzed": "etāḥ hi pātāla papāta sannibhā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_436:8.4",
        "original": "Etthappamatto byasanaṁ nigaccha”ti.",
        "analyzed": "ettha appamattaḥ byasanam nigacchati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_436:9.1",
        "original": "“Tasmā hi te sukhino vītasokā,",
        "analyzed": "tasmai hi te sukhinaḥ vīta sokā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_436:9.2",
        "original": "Ye mātugāmehi caranti nissaṭā;",
        "analyzed": "ye mātu gāmehi caranti nissaṭā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_436:9.3",
        "original": "Etaṁ sivaṁ uttamamābhipatthayaṁ,",
        "analyzed": "etam sivam uttamam ābhipatthaya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_436:9.4",
        "original": "Na mātugāmehi kareyya santhavanti”.",
        "analyzed": "na mātugāmehi kareyya santhavanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_436:10.1",
        "original": "Samuggajātakaṁ dasamaṁ.",
        "analyzed": "samugga jātakam dasama ",
        "folio": "10"
    }
]