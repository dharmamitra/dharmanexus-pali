    },
    {
        "segmentnr": "PA_snp_2.6:0.2",
        "original": "Kapilasutta (dhammacariyasutta)",
        "analyzed": "kapilasutta dhammacariyasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.6:1.1",
        "original": "Dhammacariyaṁ brahmacariyaṁ,",
        "analyzed": "dhamma cariyam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.6:1.2",
        "original": "Etadāhu vasuttamaṁ;",
        "analyzed": "etat āhuḥ vasuttama ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.6:1.3",
        "original": "Pabbajitopi ce hoti,",
        "analyzed": "pabbajitaḥ pi ce hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.6:1.4",
        "original": "Agārā anagāriyaṁ.",
        "analyzed": "agārāḥ anagāriya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.6:2.1",
        "original": "So ce mukharajātiko,",
        "analyzed": "saḥ ce mukhara jātikaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.6:2.2",
        "original": "vihesābhirato mago;",
        "analyzed": "vihesa abhirataḥ mago ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.6:2.3",
        "original": "Jīvitaṁ tassa pāpiyo,",
        "analyzed": "jīvitam tassa pāpiyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.6:2.4",
        "original": "rajaṁ vaḍḍheti attano.",
        "analyzed": "rajam vaḍḍheti attanaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.6:3.1",
        "original": "Kalahābhirato bhikkhu,",
        "analyzed": "kalaha abhirataḥ bhikkhu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.6:3.2",
        "original": "Mohadhammena āvuto;",
        "analyzed": "moha dhammena āvutaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.6:3.3",
        "original": "Akkhātampi na jānāti,",
        "analyzed": "akkhātam pi na jānāti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.6:3.4",
        "original": "Dhammaṁ buddhena desitaṁ.",
        "analyzed": "dhammam buddhena desita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.6:4.1",
        "original": "Vihesaṁ bhāvitattānaṁ,",
        "analyzed": "vihesam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.6:4.2",
        "original": "avijjāya purakkhato;",
        "analyzed": "avijjāya purakkhataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.6:4.3",
        "original": "Saṅkilesaṁ na jānāti,",
        "analyzed": "saṅkilesam na jānāti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.6:4.4",
        "original": "maggaṁ nirayagāminaṁ.",
        "analyzed": "maggam niraya gāmina ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.6:5.1",
        "original": "Vinipātaṁ samāpanno,",
        "analyzed": "vinipātam samāpannaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.6:5.2",
        "original": "gabbhā gabbhaṁ tamā tamaṁ;",
        "analyzed": "gabbhā gabbham tama tama ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.6:5.3",
        "original": "Sa ve tādisako bhikkhu,",
        "analyzed": "sa ve tādisakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.6:5.4",
        "original": "pecca dukkhaṁ nigacchati.",
        "analyzed": "pecca dukkham nigacchati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.6:6.1",
        "original": "Gūthakūpo yathā assa,",
        "analyzed": "gūtha kūpaḥ yathā assa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.6:6.2",
        "original": "sampuṇṇo gaṇavassiko;",
        "analyzed": "sampuṇṇaḥ gaṇa vassikaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.6:6.3",
        "original": "Yo ca evarūpo assa,",
        "analyzed": "yaḥ ca evarūpaḥ assa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.6:6.4",
        "original": "dubbisodho hi sāṅgaṇo.",
        "analyzed": "dubbisodhaḥ hi sa aṅgaṇaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.6:7.1",
        "original": "Yaṁ evarūpaṁ jānātha,",
        "analyzed": "yam eva rūpam jānātha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.6:7.2",
        "original": "bhikkhavo gehanissitaṁ;",
        "analyzed": "bhikkhavo gehanissita ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.6:7.3",
        "original": "Pāpicchaṁ pāpasaṅkappaṁ,",
        "analyzed": "pāpa iccham pāpa saṅkappa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.6:7.4",
        "original": "pāpaācāragocaraṁ.",
        "analyzed": "pāpa ācāra gocara ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.6:8.1",
        "original": "Sabbe samaggā hutvāna,",
        "analyzed": "sabbe samaggāḥ hutvāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.6:8.2",
        "original": "abhinibbajjiyātha naṁ;",
        "analyzed": "na ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.6:8.3",
        "original": "Kāraṇḍavaṁ niddhamatha,",
        "analyzed": "kāraṇḍavam niddham atha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.6:8.4",
        "original": "kasambuṁ apakassatha.",
        "analyzed": "kasambum apakassatha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.6:9.1",
        "original": "Tato palāpe vāhetha,",
        "analyzed": "tatas palāpe vāhetha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.6:9.2",
        "original": "Assamaṇe samaṇamānine;",
        "analyzed": "assamaṇe samaṇa mānine ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.6:9.3",
        "original": "Niddhamitvāna pāpicche,",
        "analyzed": "niddhamitvāna pāpicche ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.6:9.4",
        "original": "Pāpaācāragocare.",
        "analyzed": "pāpa ācāra gocare ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.6:10.1",
        "original": "Suddhā suddhehi saṁvāsaṁ,",
        "analyzed": "suddhā suddhehi savāsa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.6:10.2",
        "original": "Kappayavho patissatā;",
        "analyzed": "kappayavhaḥ patissatā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.6:10.3",
        "original": "Tato samaggā nipakā,",
        "analyzed": "tatas samaggā nipakā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.6:10.4",
        "original": "Dukkhassantaṁ karissathāti.",
        "analyzed": "dukkhassantam karissatha iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.6:10.5",
        "original": "Dhammacariyasuttaṁ chaṭṭhaṁ.",
        "analyzed": "dhammacariya suttam chaṭṭha ",
        "folio": "10"
    }
]