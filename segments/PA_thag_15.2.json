        "segmentnr": "PA_thag_15.2:13.2",
        "original": "buddho loke viharati;",
        "analyzed": "buddhaḥ loke viharati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_15.2:13.3",
        "original": "Nopalippati lokena,",
        "analyzed": "na upalippati lokena ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_15.2:13.4",
        "original": "toyena padumaṁ yathā.",
        "analyzed": "toyena padumam yathā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_15.2:14.1",
        "original": "Mahāgini pajjalito,",
        "analyzed": "pajjalitaḥ ",
