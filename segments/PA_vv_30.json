        "analyzed": "vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_30:0.2",
        "original": "Itthivimāna",
        "analyzed": "itthi vimāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_30:0.3",
        "original": "Pāricchattakavagga",
        "analyzed": "pāricchattaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_30:0.4",
        "original": "2. Ucchudāyikāvimānavatthu",
        "analyzed": "ucchudāyikāvimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_30:1.1",
        "original": "“Obhāsayitvā pathaviṁ sadevakaṁ,",
        "analyzed": "ā ubhāsayitvā pathavim sa devaka ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_30:1.2",
        "original": "Atirocasi candimasūriyā viya;",
        "analyzed": "atirocasi candima sūriyā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_30:1.3",
        "original": "Siriyā ca vaṇṇena yasena tejasā,",
        "analyzed": "siriyā ca vaṇṇena yasena tejasā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_30:1.4",
        "original": "Brahmāva deve tidase sahindake.",
        "analyzed": "brahma ava deve tidase sahindake ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_30:2.1",
        "original": "Pucchāmi taṁ uppalamāladhārinī,",
        "analyzed": "pucchāmi tam uppala māla dhārinī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_30:2.2",
        "original": "Āveḷinī kañcanasannibhattace;",
        "analyzed": "āveḍinī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_30:2.3",
        "original": "Alaṅkate uttamavatthadhārinī,",
        "analyzed": "alaṅkate uttamavattha dhārinī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_30:2.4",
        "original": "Kā tvaṁ subhe devate vandase mamaṁ.",
        "analyzed": "kā tvam subhe devate vandase mama ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_30:3.1",
        "original": "Kiṁ tvaṁ pure kammamakāsi attanā,",
        "analyzed": "kim tvam pure attanā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_30:3.2",
        "original": "Manussabhūtā purimāya jātiyā;",
        "analyzed": "manussabhūtā purimāya jātiyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_30:3.3",
        "original": "Dānaṁ suciṇṇaṁ atha sīlasaṁyamaṁ,",
        "analyzed": "dānam su ciṇṇam atha sīla sayama ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_30:3.4",
        "original": "Kenūpapannā sugatiṁ yasassinī;",
        "analyzed": "kena upapannā sugatim yasassinī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_30:3.5",
        "original": "Devate pucchitācikkha,",
        "analyzed": "devate pucchitā ācikkha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_30:3.6",
        "original": "Kissa kammassidaṁ phalan”ti.",
        "analyzed": "kissa kammassa idam phalanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_30:4.1",
        "original": "“Idāni bhante imameva gāmaṁ,",
        "analyzed": "idāni bhante imam eva gāma ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_30:4.2",
        "original": "Piṇḍāya amhākaṁ gharaṁ upāgami;",
        "analyzed": "piṇḍāya amhākam gharam upāgami ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_30:4.3",
        "original": "Tato te ucchussa adāsi khaṇḍikaṁ,",
        "analyzed": "tatas te ucchussa adāsi khaṇḍika ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_30:4.4",
        "original": "Pasannacittā atulāya pītiyā.",
        "analyzed": "pasanna cittāḥ atulāya pītiyā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_30:5.1",
        "original": "Sassu ca pacchā anuyuñjate mamaṁ,",
        "analyzed": "sassu ca pacchā anuyuñjate mama ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_30:5.2",
        "original": "Kahaṁ nu ucchuṁ vadhuke avākiri;",
        "analyzed": "kaham nu ucchum vadhuke avākiri ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_30:5.3",
        "original": "Na chaḍḍitaṁ no pana khāditaṁ mayā,",
        "analyzed": "na chaḍḍitam no pana khāditam mayā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_30:5.4",
        "original": "Santassa bhikkhussa sayaṁ adāsahaṁ.",
        "analyzed": "santassa bhikkhussa sayam adāsaha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_30:6.1",
        "original": "Tuyhaṁ nvidaṁ issariyaṁ atho mama,",
        "analyzed": "tuyham nu idam issariyam atha u mama ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_30:6.2",
        "original": "Itissā sassu paribhāsate mamaṁ;",
        "analyzed": "mama ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_30:6.3",
        "original": "Pīṭhaṁ gahetvā pahāraṁ adāsi me,",
        "analyzed": "pīṭham gahetvā pahāram adāsi me ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_30:6.4",
        "original": "Tato cutā kālakatāmhi devatā.",
        "analyzed": "tatas cutā devatā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_30:7.1",
        "original": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
        "analyzed": "tat eva kammam kusalam katam mayā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_30:7.2",
        "original": "Sukhañca kammaṁ anubhomi attanā;",
        "analyzed": "sukham ca kammam anubhomi attanā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_30:7.3",
        "original": "Devehi saddhiṁ paricārayāmahaṁ,",
        "analyzed": "devehi saddhim paricārayāmaha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_30:7.4",
        "original": "Modāmahaṁ kāmaguṇehi pañcahi.",
        "analyzed": "modām aham kāma guṇehi pañcahi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_30:8.1",
        "original": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
        "analyzed": "tat eva kammam kusalam katam mayā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_30:8.2",
        "original": "Sukhañca kammaṁ anubhomi attanā;",
        "analyzed": "sukham ca kammam anubhomi attanā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_30:8.3",
        "original": "Devindaguttā tidasehi rakkhitā,",
        "analyzed": "devinda guttā tidasehi rakkhitā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_30:8.4",
        "original": "Samappitā kāmaguṇehi pañcahi.",
        "analyzed": "samappitā kāma guṇehi pañcahi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_30:9.1",
        "original": "Etādisaṁ puññaphalaṁ anappakaṁ,",
        "analyzed": "etādisam puñña phalam anappaka ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_30:9.2",
        "original": "Mahāvipākā mama ucchudakkhiṇā;",
        "analyzed": "mahā vipākā mama ucchu dakkhiṇā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_30:9.3",
        "original": "Devehi saddhiṁ paricārayāmahaṁ,",
        "analyzed": "devehi saddhim paricārayāmaha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_30:9.4",
        "original": "Modāmahaṁ kāmaguṇehi pañcahi.",
        "analyzed": "modām aham kāma guṇehi pañcahi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_30:10.1",
        "original": "Etādisaṁ puññaphalaṁ anappakaṁ,",
        "analyzed": "etādisam puñña phalam anappaka ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_30:10.2",
        "original": "Mahājutikā mama ucchudakkhiṇā;",
        "analyzed": "mahā jutikā mama ucchu dakkhiṇā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_30:10.3",
        "original": "Devindaguttā tidasehi rakkhitā,",
        "analyzed": "devinda guttā tidasehi rakkhitā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_30:10.4",
        "original": "Sahassanettoriva nandane vane.",
        "analyzed": "sahassanettoḥ iva nandane vane ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_30:11.1",
        "original": "Tuvañca bhante anukampakaṁ viduṁ,",
        "analyzed": "tuvam ca bhante anukampakam vidu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_30:11.2",
        "original": "Upecca vandiṁ kusalañca pucchisaṁ;",
        "analyzed": "upa id ca vandim kusalañca pucchisa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_30:11.3",
        "original": "Tato te ucchussa adāsi khaṇḍikaṁ,",
        "analyzed": "tatas te ucchussa adāsi khaṇḍika ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_30:11.4",
        "original": "Pasannacittā atulāya pītiyā”ti.",
        "analyzed": "pasanna cittā atulāya pītiyā ati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_30:12.1",
        "original": "Ucchudāyikāvimānaṁ dutiyaṁ.",
        "analyzed": "ucchudāyikāvimānam ",
        "folio": "12"
    }
]