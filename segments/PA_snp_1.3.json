        "original": "Anavassuto apariḍayhamāno,",
        "analyzed": "an avassutaḥ a pariḍayhamānaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.3:29.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.3:30.1",
        "original": "“Ohārayitvā gihibyañjanāni,",
        "analyzed": "ohārayitvā gihi byañjanāni ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_1.3:30.2",
        "original": "Sañchannapatto yathā pārichatto;",
        "analyzed": "sañchanna pattaḥ yathā pārichattaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_1.3:30.3",
