        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_453:0.4",
        "original": "15. Mahāmaṅgalajātaka",
        "analyzed": "    ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_453:1.1",
        "original": "“Kiṁsu naro jappamadhiccakāle,",
        "analyzed": "kisu naraḥ jappam adhicca kāle ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_453:1.2",
        "original": "Kaṁ vā vijjaṁ katamaṁ vā sutānaṁ;",
        "analyzed": "kam vā vijjam katamam vā sutāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_453:1.3",
        "original": "So macco asmiñca paramhi loke,",
        "analyzed": "saḥ mat ca asmin ca paramhi loke ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_453:1.4",
        "original": "Kathaṁ karo sotthānena gutto”.",
        "analyzed": "katham sotthānena gutto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_453:2.1",
        "original": "“Yassa devā pitaro ca sabbe,",
        "analyzed": "yassa devā pitaro ca sabbe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_453:2.2",
        "original": "Sarīsapā sabbabhūtāni cāpi;",
        "analyzed": "sarīsapā sabba bhūtāni ca api ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_453:2.3",
        "original": "Mettāya niccaṁ apacitāni honti,",
        "analyzed": "mettāya niccam apacitāni honti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_453:2.4",
        "original": "Bhūtesu ve sotthānaṁ tadāhu”.",
        "analyzed": "bhūtesu sa utthānam tat āhu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_453:3.1",
        "original": "“Yo sabbalokassa nivātavutti,",
        "analyzed": "yaḥ sabba lokassa nivāta vutti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_453:3.2",
        "original": "Itthīpumānaṁ sahadārakānaṁ;",
        "analyzed": "itthī pumānam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_453:3.3",
        "original": "Khantā duruttānamappaṭikūlavādī,",
        "analyzed": "khantā duruttānam ap paṭikūla vādī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_453:3.4",
        "original": "Adhivāsanaṁ sotthānaṁ tadāhu.",
        "analyzed": "adhivāsanam sa utthānam tat āhu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_453:4.1",
        "original": "Yo nāvajānāti sahāyamitte,",
        "analyzed": "yaḥ na avajānāti sahāya mitte ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_453:4.2",
        "original": "Sippena kulyāhi dhanena jaccā;",
        "analyzed": "sippena kulyāhi dhanena jaccā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_453:4.3",
        "original": "Rucipañño atthakāle matīmā,",
        "analyzed": "ruci paññaḥ attha kāle matīmā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_453:4.4",
        "original": "Sahāyesu ve sotthānaṁ tadāhu.",
        "analyzed": "sahāyesu sa utthānam tat āhu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_453:5.1",
        "original": "Mittāni ve yassa bhavanti santo,",
        "analyzed": "mittāni ve yassa bhavanti santaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_453:5.2",
        "original": "Saṁvissatthā avisaṁvādakassa;",
        "analyzed": "sa vissatthā a visavādakassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_453:5.3",
        "original": "Na mittadubbhī saṁvibhāgī dhanena,",
        "analyzed": "na savibhāgī dhanena ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_453:5.4",
        "original": "Mittesu ve sotthānaṁ tadāhu.",
        "analyzed": "mittesu sa utthānam tat āhu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_453:6.1",
        "original": "Yassa bhariyā tulyavayā samaggā,",
        "analyzed": "yassa bhariyā tulya vayā samaggā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_453:6.2",
        "original": "Anubbatā dhammakāmā pajātā;",
        "analyzed": "anubbatā dhamma kāmā pajātā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_453:6.3",
        "original": "Koliniyā sīlavatī patibbatā,",
        "analyzed": "       ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_453:6.4",
        "original": "Dāresu ve sotthānaṁ tadāhu.",
        "analyzed": "sa utthānam tat āhu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_453:7.1",
        "original": "Yassa rājā bhūtapati yasassī,",
        "analyzed": "yassa rājā bhūta pati yasassī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_453:7.2",
        "original": "Jānāti soceyyaṁ parakkamañca;",
        "analyzed": "jānāti soceyyam parakkaman ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_453:7.3",
        "original": "Advejjhatā suhadayaṁ mamanti,",
        "analyzed": "su hadayam mamanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_453:7.4",
        "original": "Rājūsu ve sotthānaṁ tadāhu.",
        "analyzed": "rājūsu sa utthānam tat āhu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_453:8.1",
        "original": "Annañca pānañca dadāti saddho,",
        "analyzed": "annañca pānañca dadāti saddhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_453:8.2",
        "original": "Mālañca gandhañca vilepanañca;",
        "analyzed": "mālañca gandhañca vilepanañca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_453:8.3",
        "original": "Pasannacitto anumodamāno,",
        "analyzed": "pasanna cittaḥ anumodamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_453:8.4",
        "original": "Saggesu ve sotthānaṁ tadāhu.",
        "analyzed": "saggesu sotthānam tat āhu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_453:9.1",
        "original": "Yamariyadhammena punanti vuddhā,",
        "analyzed": "yam ariya dhammena punanti vuddhāḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_453:9.2",
        "original": "Ārādhitā samacariyāya santo;",
        "analyzed": "ārādhitā sama cariyāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_453:9.3",
        "original": "Bahussutā isayo sīlavanto,",
        "analyzed": "bahussutāḥ isayaḥ sīlavanto ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_453:9.4",
        "original": "Arahantamajjhe sotthānaṁ tadāhu.",
        "analyzed": "arahanta majjhe sa utthānam tat āhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_453:10.1",
        "original": "Etāni kho sotthānāni loke,",
        "analyzed": "etāni khalu sa utthānāni loke ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_453:10.2",
        "original": "Viññuppasatthāni sukhudrayāni;",
        "analyzed": "viññuppasatthāni su khudrayāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_453:10.3",
        "original": "Tānīdha sevetha naro sapañño,",
        "analyzed": "tāni idha sevetha naraḥ sa paññaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_453:10.4",
        "original": "Na hi maṅgale kiñcanamatthi saccan”ti.",
        "analyzed": "na hi maṅgale kiñcanam atthi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_453:11.1",
        "original": "Mahāmaṅgalajātakaṁ pannarasamaṁ.",
        "analyzed": "mahāmaṅgalajātakam ",
        "folio": "11"
    }
]