        "analyzed": "hisakassa pure sataḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.8:14.3",
        "original": "Ajjāhaṁ saccanāmomhi,",
        "analyzed": "ajja aham sacca nāmā umhi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.8:14.4",
        "original": "na naṁ hiṁsāmi kiñcanaṁ.",
        "analyzed": "na nam hisāmi kiñcana ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.8:15.1",
        "original": "Coro ahaṁ pure āsiṁ,",
        "analyzed": "coraḥ aham pure āsi ",
        "folio": "15"
    },
