        "analyzed": "su nikkhitta vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_85:0.4",
        "original": "11. Sunikkhittavimānavatthu",
        "analyzed": "su nikkhitta vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_85:1.1",
        "original": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
        "analyzed": "uccam idam maṇi thūṇam vimāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_85:1.2",
        "original": "Samantato dvādasa yojanāni;",
        "analyzed": "samantataḥ dvādasa yojanāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_85:1.3",
        "original": "Kūṭāgārā sattasatā uḷārā,",
        "analyzed": "kūṭāgārā satta satā uḷārā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_85:1.4",
        "original": "Veḷuriyathambhā rucakatthatā subhā.",
        "analyzed": "veḷuriya thambhā rucakatthatā subhā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_85:2.1",
        "original": "Tatthacchasi pivasi khādasi ca,",
        "analyzed": "tatthacchasi pivasi khādasi ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_85:2.2",
        "original": "Dibbā ca vīṇā pavadanti vagguṁ;",
        "analyzed": "dibbā ca vīṇā pavadanti vaggu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_85:2.3",
        "original": "Dibbā rasā kāmaguṇettha pañca,",
        "analyzed": "dibbāḥ rasāḥ kāma guṇā ittha pañca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_85:2.4",
        "original": "Nāriyo ca naccanti suvaṇṇachannā.",
        "analyzed": "nāriyaḥ ca naccanti suvaṇṇa channā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_85:3.1",
        "original": "Kena tetādiso vaṇṇo,",
        "analyzed": "kena tetādisaḥ vaṇṇaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_85:3.2",
        "original": "kena te idha mijjhati;",
        "analyzed": "kena te idha mijjhati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_85:3.3",
        "original": "Uppajjanti ca te bhogā,",
        "analyzed": "uppajjanti ca te bhogāḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_85:3.4",
        "original": "ye keci manaso piyā.",
        "analyzed": "ye keci manasaḥ piyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_85:4.1",
        "original": "Pucchāmi taṁ deva mahānubhāva,",
        "analyzed": "pucchāmi tam deva mahā anubhāva ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_85:4.2",
        "original": "Manussabhūto kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtaḥ kim akāsi puñña ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_85:4.3",
        "original": "Kenāsi evaṁ jalitānubhāvo,",
        "analyzed": "kena asi evam jalita anubhāvaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_85:4.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_85:5.1",
        "original": "So devaputto attamano,",
        "analyzed": "saḥ deva puttaḥ atta mano ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_85:5.2",
        "original": "moggallānena pucchito;",
        "analyzed": "moggallānena pucchito ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_85:5.3",
        "original": "Pañhaṁ puṭṭho viyākāsi,",
        "analyzed": "pañham puṭṭhaḥ viyākāsi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_85:5.4",
        "original": "yassa kammassidaṁ phalaṁ.",
        "analyzed": "yassa kammassa idam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_85:6.1",
        "original": "“Dunnikkhittaṁ mālaṁ sunikkhipitvā,",
        "analyzed": "dunnikkhittam mālam su nikkhipitvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_85:6.2",
        "original": "Patiṭṭhapetvā sugatassa thūpe;",
        "analyzed": "patiṭṭhapetvā sugatassa thūpe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_85:6.3",
        "original": "Mahiddhiko camhi mahānubhāvo,",
        "analyzed": "mahiddhikaḥ camhi mahā anubhāvaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_85:6.4",
        "original": "Dibbehi kāmehi samaṅgibhūto.",
        "analyzed": "dibbehi kāmehi samaṅgi bhūtaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_85:7.1",
        "original": "Tena metādiso vaṇṇo,",
        "analyzed": "tena mā itādisaḥ vaṇṇaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_85:7.2",
        "original": "tena me idha mijjhati;",
        "analyzed": "tena me idha mijjhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_85:7.3",
        "original": "Uppajjanti ca me bhogā,",
        "analyzed": "uppajjanti ca me bhogāḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_85:7.4",
        "original": "ye keci manaso piyā.",
        "analyzed": "ye keci manasaḥ piyā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_85:8.1",
        "original": "Akkhāmi te bhikkhu mahānubhāva,",
        "analyzed": "akkhāmi te bhikkhu mahā anubhāva ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_85:8.2",
        "original": "Manussabhūto yamahaṁ akāsiṁ;",
        "analyzed": "manuḥ sabhūtaḥ yam aham akāsi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_85:8.3",
        "original": "Tenamhi evaṁ jalitānubhāvo,",
        "analyzed": "evam jalita anubhāvaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_85:8.4",
        "original": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca me sabbadisā pabhāsati iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_85:9.1",
        "original": "Sunikkhittavimānaṁ ekādasamaṁ.",
        "analyzed": "su nikkhitta vimānam ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_85:9.2",
        "original": "Sunikkhittavaggo sattamo.",
        "analyzed": "sunikkhittavaggaḥ sattamaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_85:10.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_85:11.1",
        "original": "Dve daliddā vanavihārā,",
        "analyzed": "dve daliddā vana vihārā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_85:11.2",
        "original": "Bhatako gopālakaṇḍako;",
        "analyzed": "bhatakaḥ gopālakaṇḍako ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_85:11.3",
        "original": "Anekavaṇṇamaṭṭhakuṇḍalī,",
        "analyzed": "aneka vaṇṇam aṭṭha kuṇḍalī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_85:11.4",
        "original": "Serīsako sunikkhittaṁ;",
        "analyzed": "serīsako ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_85:11.5",
        "original": "Purisānaṁ tatiyo vaggo pavuccatīti.",
        "analyzed": "purisānam tatiyaḥ vaggo pavuccati iti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_85:12.1",
        "original": "Bhāṇavāraṁ catutthaṁ.",
        "analyzed": "bhāṇa vāram ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_85:13.1",
        "original": "Vimānavatthupāḷi niṭṭhitā.",
        "analyzed": "       ",
        "folio": "13"
    }
]