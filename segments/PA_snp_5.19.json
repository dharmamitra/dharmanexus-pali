        "original": "Lokavicāraṇañāṇapabhaggaṁ,",
        "analyzed": "loka vicāraṇa ñāṇa pabhagga ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_5.19:33.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_5.19:34.1",
        "original": "Āsavabandhanayogākilesaṁ,",
        "analyzed": "āsava bandhana yoga a kilesa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.19:34.2",
        "original": "Nīvaraṇāni ca tīṇi malāni;",
        "analyzed": "nīvaraṇāni ca tīṇi malāni ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.19:34.3",
        "original": "Tassa kilesapamocanahetuṁ,",
        "analyzed": "tassa kilesa pamocana hetu ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.19:34.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.19:35.1",
        "original": "Nimmalasabbakilesapanūdaṁ,",
        "analyzed": "nimmala sabba kilesa panūda ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.19:35.2",
        "original": "Rāgavirāgamanejamasokaṁ;",
        "analyzed": "rāga virāgam anejam asoka ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.19:35.3",
        "original": "Santapaṇītasududdasadhammaṁ,",
        "analyzed": "        ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.19:35.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.19:36.1",
        "original": "Rāgañca dosakamabhañjitasantaṁ,",
        "analyzed": "rāgam ca ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.19:36.2",
        "original": "Yonicatuggatipañcaviññāṇaṁ;",
        "analyzed": "yoni catur gati pañca viññāṇa ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.19:36.3",
        "original": "Taṇhāratacchadanatāṇalatāpamokkhaṁ,",
        "analyzed": "taṇhā rata chadana tāṇala tāpa mokkha ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.19:36.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.19:37.1",
        "original": "Gambhīraduddasasaṇhanipuṇaṁ,",
        "analyzed": "gambhīra duddasa saṇha nipuṇa ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.19:37.2",
        "original": "Paṇḍitavedaniyaṁ nipuṇatthaṁ;",
        "analyzed": "paṇḍita vedaniyam ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.19:37.3",
        "original": "Lokavicāraṇañāṇapabhaggaṁ,",
        "analyzed": "loka vicāraṇa ñāṇa pabhagga ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.19:37.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.19:38.1",
        "original": "Navaṅgakusumamālagīveyyaṁ,",
        "analyzed": "navaṅga kusuma māla gīveyya ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.19:38.2",
        "original": "Indriyajhānavimokkhavibhattaṁ;",
        "analyzed": "indriya jhāna vimokkha vibhatta ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.19:38.3",
        "original": "Aṭṭhaṅgamaggadharaṁ varayānaṁ,",
        "analyzed": "aṭṭhaṅga magga dharam varayāna ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.19:38.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.19:39.1",
        "original": "Somupamaṁ vimalaṁ parisuddhaṁ,",
        "analyzed": "soma upamam vimalam parisuddha ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.19:39.2",
        "original": "Aṇṇavamūpamaratanasucittaṁ;",
        "analyzed": "aṇṇavam ūpama ratana sucitta ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.19:39.3",
        "original": "Pupphasamaṁ ravimūpamatejaṁ,",
        "analyzed": "puppha samam ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.19:39.4",
        "original": "Desayi suttavaraṁ dvipadaggo.",
        "analyzed": "desayi suttavaram dvipadaggo ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.19:40.1",
        "original": "Khemasivaṁ sukhasītalasantaṁ,",
        "analyzed": "khema sivam ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_5.19:40.2",
        "original": "Maccutatāṇaparaṁ paramatthaṁ;",
        "analyzed": "param attha ",
        "folio": "40"
    },
