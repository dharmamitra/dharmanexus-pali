        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_14.1:1.1",
        "original": "“Yadā ahaṁ pabbajito,",
        "analyzed": "yadā aham pabbajitaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_14.1:1.2",
        "original": "agārasmānagāriyaṁ;",
        "analyzed": "agārasmān anagāriya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_14.1:1.3",
        "original": "Nābhijānāmi saṅkappaṁ,",
        "analyzed": "na abhijānāmi saṅkappa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_14.1:1.4",
        "original": "anariyaṁ dosasaṁhitaṁ.",
        "analyzed": "anariyam dosa sahita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_14.1:2.1",
        "original": "‘Ime haññantu vajjhantu,",
        "analyzed": "ime haññantu vajjhantu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_14.1:2.2",
        "original": "dukkhaṁ pappontu pāṇino’;",
        "analyzed": "dukkham pappontu pāṇinaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_14.1:2.3",
        "original": "Saṅkappaṁ nābhijānāmi,",
        "analyzed": "saṅkappam na abhijānāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_14.1:2.4",
        "original": "imasmiṁ dīghamantare.",
        "analyzed": "imasmin dīgham antare ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_14.1:3.1",
        "original": "Mettañca abhijānāmi,",
        "analyzed": "mā idtañca abhijānāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_14.1:3.2",
        "original": "appamāṇaṁ subhāvitaṁ;",
        "analyzed": "appamāṇam su bhāvita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_14.1:3.3",
        "original": "Anupubbaṁ paricitaṁ,",
        "analyzed": "anupubbam paricita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_14.1:3.4",
        "original": "yathā buddhena desitaṁ.",
        "analyzed": "yathā buddhena desita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_14.1:4.1",
        "original": "Sabbamitto sabbasakho,",
        "analyzed": "sabba mittaḥ sabba sakhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_14.1:4.2",
        "original": "sabbabhūtānukampako;",
        "analyzed": "sabba bhūta anukampakaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_14.1:4.3",
        "original": "Mettacittañca bhāvemi,",
        "analyzed": "metta cittam ca bhāvemi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_14.1:4.4",
        "original": "abyāpajjarato sadā.",
        "analyzed": "a byāpad jarataḥ sadā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_14.1:5.1",
        "original": "Asaṁhīraṁ asaṅkuppaṁ,",
        "analyzed": "a sahīram a saṅkuppa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_14.1:5.2",
        "original": "cittaṁ āmodayāmahaṁ;",
        "analyzed": "cittam āmodayāmaha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_14.1:5.3",
        "original": "Brahmavihāraṁ bhāvemi,",
        "analyzed": "brahmavihāram bhāvemi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_14.1:5.4",
        "original": "akāpurisasevitaṁ.",
        "analyzed": "a kāpurisa sevita ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_14.1:6.1",
        "original": "Avitakkaṁ samāpanno,",
        "analyzed": "a vitakkam samāpannaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_14.1:6.2",
        "original": "sammāsambuddhasāvako;",
        "analyzed": "sammāsambuddha sāvakaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_14.1:6.3",
        "original": "Ariyena tuṇhībhāvena,",
        "analyzed": "ariyena tuṇhībhāvena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_14.1:6.4",
        "original": "upeto hoti tāvade.",
        "analyzed": "upetaḥ tāvade ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_14.1:7.1",
        "original": "Yathāpi pabbato selo,",
        "analyzed": "yathā api pabbataḥ selo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_14.1:7.2",
        "original": "acalo suppatiṭṭhito;",
        "analyzed": "acalaḥ suppatiṭṭhitaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_14.1:7.3",
        "original": "Evaṁ mohakkhayā bhikkhu,",
        "analyzed": "evam moha kkhayā bhikkhu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_14.1:7.4",
        "original": "pabbatova na vedhati.",
        "analyzed": "pabbataḥ uva na vedhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_14.1:8.1",
        "original": "Anaṅgaṇassa posassa,",
        "analyzed": "anaṅgaṇassa posassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_14.1:8.2",
        "original": "niccaṁ sucigavesino;",
        "analyzed": "niccam suci gavesino ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_14.1:8.3",
        "original": "Vālaggamattaṁ pāpassa,",
        "analyzed": "vālagga mattam pāpassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_14.1:8.4",
        "original": "abbhamattaṁva khāyati.",
        "analyzed": "khāyati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_14.1:9.1",
        "original": "Nagaraṁ yathā paccantaṁ,",
        "analyzed": "nagaram yathā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_14.1:9.2",
        "original": "guttaṁ santarabāhiraṁ;",
        "analyzed": "guttam santarabāhira ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_14.1:9.3",
        "original": "Evaṁ gopetha attānaṁ,",
        "analyzed": "evam gopetha attāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_14.1:9.4",
        "original": "khaṇo vo mā upaccagā.",
        "analyzed": "khaṇaḥ vaḥ māḥ upaccagā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_14.1:10.1",
        "original": "Nābhinandāmi maraṇaṁ,",
        "analyzed": "na abhinandāmi maraṇa ",
        "folio": "10"
    },
