        "original": "manasā dukkaṭaṁ kataṁ;",
        "analyzed": "manasā dukkaṭam kata ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_39:11.3",
        "original": "Kena saṁsavako laddho,",
        "analyzed": "kena sa savakaḥ laddhaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_39:11.4",
        "original": "gambhīro sataporiso”ti.",
        "analyzed": "gambhīraḥ sa taporisaḥ ti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_39:12.1",
        "original": "“Samaṇe brāhmaṇe cāpi,",
        "analyzed": "samaṇe brāhmaṇe ca api ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_39:12.2",
