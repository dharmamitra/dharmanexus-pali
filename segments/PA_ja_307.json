    {
        "segmentnr": "PA_ja_307:0.4",
        "original": "7. Palāsajātaka",
        "analyzed": "    ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_307:1.1",
        "original": "“Acetanaṁ brāhmaṇa assuṇantaṁ,",
        "analyzed": "acetanam brāhmaṇa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_307:1.2",
        "original": "Jāno ajānantamimaṁ palāsaṁ;",
        "analyzed": "jāno a jānantam imam palāsa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_307:1.3",
        "original": "Āraddhaviriyo dhuvaṁ appamatto,",
        "analyzed": "āraddha viriyaḥ dhuvam appamatto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_307:1.4",
        "original": "Sukhaseyyaṁ pucchasi kissa hetu”.",
        "analyzed": "sukhaseyyam pucchasi kissa hetu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_307:2.1",
        "original": "“Dūre suto ceva brahā ca rukkho,",
        "analyzed": "dūre suto ca iva brahā ca rukkho ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_307:2.2",
        "original": "Dese ṭhito bhūtanivāsarūpo;",
        "analyzed": "dese ṭhitaḥ bhūta nivāsa rūpaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_307:2.3",
        "original": "Tasmā namassāmi imaṁ palāsaṁ,",
        "analyzed": "tasmai namassāmi imam palāsa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_307:2.4",
        "original": "Ye cettha bhūtā te dhanassa hetu”.",
        "analyzed": "ye ca ittha bhūtā te dhanassa hetu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_307:3.1",
        "original": "“So te karissāmi yathānubhāvaṁ,",
        "analyzed": "saḥ te karissāmi yathā anubhāva ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_307:3.2",
        "original": "Kataññutaṁ brāhmaṇa pekkhamāno;",
        "analyzed": "kataññutam brāhmaṇa pekkhamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_307:3.3",
        "original": "Kathañhi āgamma sataṁ sakāse,",
        "analyzed": "katham hi āgamma satam sakāse ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_307:3.4",
        "original": "Moghāni te assu pariphanditāni.",
        "analyzed": "moghāni te assu pariphanditāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_307:4.1",
        "original": "Yo tindukarukkhassa paro pilakkho,",
        "analyzed": "yaḥ tinduka rukkhassa paraḥ pilakkhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_307:4.2",
        "original": "Parivārito pubbayañño uḷāro;",
        "analyzed": "parivāritaḥ pubba yaññaḥ uḷāraḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_307:4.3",
        "original": "Tassesa mūlasmiṁ nidhi nikhāto,",
        "analyzed": "tassa isa mūlasmim nikhātaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_307:4.4",
        "original": "Adāyādo gaccha taṁ uddharāhī”ti.",
        "analyzed": "a dāyādaḥ gaccha tam uddharāhi iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_307:5.1",
        "original": "Palāsajātakaṁ sattamaṁ.",
        "analyzed": "palāsa jātakam sattama ",
        "folio": "5"
    }
]