    },
    {
        "segmentnr": "PA_thag_12.1:1.1",
        "original": "“Sīlamevidha sikkhetha,",
        "analyzed": "sīlam eva idha sikkhetha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_12.1:1.2",
