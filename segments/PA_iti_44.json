    {
        "segmentnr": "PA_iti_44:0.4",
        "original": "Nibbānadhātusutta",
        "analyzed": "nibbāna dhātu sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_44:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_44:2.1",
        "original": "“Dvemā, bhikkhave, nibbānadhātuyo.",
        "analyzed": "dve imāḥ bhikkhave nibbāna dhātuyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_44:2.2",
        "original": "Katamā dve?",
        "analyzed": "katamā dve ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_44:2.3",
        "original": "Saupādisesā ca nibbānadhātu, anupādisesā ca nibbānadhātu.",
        "analyzed": "sa aupādisesā ca nibbāna dhātu anupādisesā ca nibbāna dhātu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_44:3.1",
        "original": "Katamā ca, bhikkhave, saupādisesā nibbānadhātu?",
        "analyzed": "katamā ca bhikkhave sa aupādisesā nibbāna dhātu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_44:3.2",
        "original": "Idha, bhikkhave, bhikkhu arahaṁ hoti khīṇāsavo vusitavā katakaraṇīyo ohitabhāro anuppattasadattho parikkhīṇabhavasaṁyojano sammadaññāvimutto.",
        "analyzed": "idha bhikkhave bhikkhu araham hoti khīṇa āsavaḥ vusitavā kata karaṇīyaḥ ohita bhāraḥ an uppatta sadattho parikkhīṇa bhava sayojanaḥ sammad aññā vimutto ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_44:3.3",
        "original": "Tassa tiṭṭhanteva pañcindriyāni yesaṁ avighātattā manāpāmanāpaṁ paccanubhoti, sukhadukkhaṁ paṭisaṁvedeti.",
        "analyzed": "tassa tiṭṭhantā iva pañca indriyāni yesam a vighāta ttā manāpa amanāpam paccanubhoti sukha dukkham paṭisavedeti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_44:3.4",
        "original": "Tassa yo rāgakkhayo, dosakkhayo, mohakkhayo—",
        "analyzed": "tassa yaḥ rāga khayaḥ dosa khayaḥ moha kkhayo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_44:3.5",
        "original": "ayaṁ vuccati, bhikkhave, saupādisesā nibbānadhātu.",
        "analyzed": "ayam vuccati bhikkhave sa aupādisesā nibbāna dhātu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_44:4.1",
        "original": "Katamā ca, bhikkhave, anupādisesā nibbānadhātu?",
        "analyzed": "katamā ca bhikkhave anupādisesā nibbāna dhātu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_44:4.2",
        "original": "Idha, bhikkhave, bhikkhu arahaṁ hoti khīṇāsavo vusitavā katakaraṇīyo ohitabhāro anuppattasadattho parikkhīṇabhavasaṁyojano sammadaññāvimutto.",
        "analyzed": "idha bhikkhave bhikkhu araham hoti khīṇa āsavaḥ vusitavā kata karaṇīyaḥ ohita bhāraḥ an uppatta sadattho parikkhīṇa bhava sayojanaḥ sammad aññā vimutto ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_44:4.3",
        "original": "Tassa idheva, bhikkhave, sabbavedayitāni anabhinanditāni sīti bhavissanti.",
        "analyzed": "tassa idha iva bhikkhave sabba vedayitāni an abhinanditāni sīti bhavissanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_44:4.4",
        "original": "Ayaṁ vuccati, bhikkhave, anupādisesā nibbānadhātu.",
        "analyzed": "ayam vuccati bhikkhave anupādisesā nibbāna dhātu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_44:4.5",
        "original": "Imā kho, bhikkhave, dve nibbānadhātuyo”ti.",
        "analyzed": "imāḥ kho bhikkhave dve nibbāna dhātuyo ti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_44:4.6",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_44:4.7",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_44:5.1",
        "original": "“Duve imā cakkhumatā pakāsitā,",
        "analyzed": "duve imā cakkhumatā pakāsitā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_44:5.2",
        "original": "Nibbānadhātū anissitena tādinā;",
        "analyzed": "nibbāna dhātū a nissitena ta ādinā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_44:5.3",
        "original": "Ekā hi dhātu idha diṭṭhadhammikā,",
        "analyzed": "ekā hi dhātu idha diṭṭhadhammikā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_44:5.4",
        "original": "Saupādisesā bhavanettisaṅkhayā;",
        "analyzed": "sā aupādisesā bhavana itti saṅkhayā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_44:5.5",
        "original": "Anupādisesā pana samparāyikā,",
        "analyzed": "anupādisesā pana samparāyikā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_44:5.6",
        "original": "Yamhi nirujjhanti bhavāni sabbaso.",
        "analyzed": "yamhi nirujjhanti bhavāni sabbaso ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_44:6.1",
        "original": "Ye etadaññāya padaṁ asaṅkhataṁ,",
        "analyzed": "ye etat aññāya padam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_44:6.2",
        "original": "Vimuttacittā bhavanettisaṅkhayā;",
        "analyzed": "vimutta cittāḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_44:6.3",
        "original": "Te dhammasārādhigamā khaye ratā,",
        "analyzed": "te dhamma sāra adhigamā khaye ratā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_44:6.4",
        "original": "Pahaṁsu te sabbabhavāni tādino”ti.",
        "analyzed": "te sabba bhavāni tādinoti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_44:7.1",
        "original": "Ayampi attho vutto bhagavatā, iti me sutanti.",
        "analyzed": "ayam pi atthaḥ vuttaḥ bhagavatā iti me sutam ti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_44:7.2",
        "original": "Sattamaṁ.",
        "analyzed": "sattama ",
        "folio": "7"
    }
]