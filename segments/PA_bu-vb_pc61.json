    },
    {
        "segmentnr": "PA_bu-vb_pc61:2.1.10",
        "original": "jīvitindriyaṁ upacchindati uparodheti santatiṁ vikopeti, āpatti pācittiyassa.",
        "analyzed": "jīvitindriyam upacchindati uparodheti santatim vikopeti āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:2.2.1",
        "original": "Pāṇe pāṇasaññī jīvitā voropeti, āpatti pācittiyassa.",
        "analyzed": "pāṇe pāṇa saññī jīvitā voropeti āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:2.2.2",
        "original": "Pāṇe vematiko jīvitā voropeti, āpatti dukkaṭassa.",
