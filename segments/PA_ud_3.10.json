    },
    {
        "segmentnr": "PA_ud_3.10:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_3.10:1.2",
        "original": "ekaṁ samayaṁ bhagavā uruvelāyaṁ viharati najjā nerañjarāya tīre bodhirukkhamūle paṭhamābhisambuddho.",
        "analyzed": "ekam samayam bhagavā uruvelāyam viharati najjā nerañjarāya tīre bodhi rukkha mūle paṭhama abhisambuddhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_3.10:1.3",
        "original": "Tena kho pana samayena bhagavā sattāhaṁ ekapallaṅkena nisinno hoti vimuttisukhapaṭisaṁvedī.",
        "analyzed": "tena khalu pana samayena bhagavā sattāham eka pallaṅkena nisinnaḥ hoti vimutti sukha paṭisavedī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_3.10:2.1",
        "original": "Atha kho bhagavā tassa sattāhassa accayena tamhā samādhimhā vuṭṭhahitvā buddhacakkhunā lokaṁ volokesi.",
        "analyzed": "atha kho bhagavā tassa sattāhassa accayena tamhā samādhimhā vuṭṭhahitvā buddha cakkhunā lokam volokesi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_3.10:2.2",
        "original": "Addasā kho bhagavā buddhacakkhunā volokento satte anekehi santāpehi santappamāne, anekehi ca pariḷāhehi pariḍayhamāne—",
        "analyzed": "addasā kho bhagavā buddha cakkhunā volokentaḥ satte anekehi santāpehi santappamāne anekehi ca pariḷāhehi pariḍayhamāne ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_3.10:2.3",
        "original": "rāgajehipi, dosajehipi, mohajehipi.",
        "analyzed": "dosa jehipi moha jehipi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_3.10:3.1",
        "original": "Atha kho bhagavā etamatthaṁ viditvā tāyaṁ velāyaṁ imaṁ udānaṁ udānesi:",
        "analyzed": "atha kho bhagavā etam attham viditvā tāyam velāyam imam udānam udānesi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ud_3.10:4.1",
        "original": "“Ayaṁ loko santāpajāto,",
        "analyzed": "ayam lokaḥ santāpa jātaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_3.10:4.2",
        "original": "Phassapareto rogaṁ vadati attato;",
        "analyzed": "phassa paretaḥ rogam vadati attataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_3.10:4.3",
        "original": "Yena yena hi maññati,",
        "analyzed": "yena yena hi maññati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_3.10:4.4",
        "original": "Tato taṁ hoti aññathā.",
        "analyzed": "tatas tam hoti aññathā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_3.10:5.1",
        "original": "Aññathābhāvī bhavasatto loko,",
        "analyzed": "aññathābhāvī bhava satto loko ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_3.10:5.2",
        "original": "Bhavapareto bhavamevābhinandati;",
        "analyzed": "bhava paretaḥ bhavam eva abhinandati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_3.10:5.3",
        "original": "Yadabhinandati taṁ bhayaṁ,",
        "analyzed": "yat abhinandati tam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_3.10:5.4",
        "original": "Yassa bhāyati taṁ dukkhaṁ;",
        "analyzed": "yassa bhāyati tam dukkha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_3.10:5.5",
        "original": "Bhavavippahānāya kho,",
        "analyzed": "bhava vippahānāya kho ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_3.10:5.6",
        "original": "Panidaṁ brahmacariyaṁ vussati.",
        "analyzed": "panidam brahmacariyam vussati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_3.10:6.1",
        "original": "‘Ye hi keci samaṇā vā brāhmaṇā vā bhavena bhavassa vippamokkhamāhaṁsu, sabbe te avippamuttā bhavasmā’ti vadāmi.",
        "analyzed": "ye hi keci samaṇāḥ vā brāhmaṇāḥ vā bhavena bhavassa vippamokkham āhasu sabbe te a vippamuttāḥ bhavasma ati vadāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ud_3.10:6.2",
        "original": "‘Ye vā pana keci samaṇā vā brāhmaṇā vā vibhavena bhavassa nissaraṇamāhaṁsu, sabbe te anissaṭā bhavasmā’ti vadāmi.",
        "analyzed": "ye vā pana keci samaṇāḥ vā brāhmaṇāḥ vā vibhavena bhavassa nissaraṇam āhasu sabbe te anissaṭāḥ bhavasma ati vadāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ud_3.10:7.1",
        "original": "Upadhiñhi paṭicca dukkhamidaṁ sambhoti, sabbupādānakkhayā natthi dukkhassa sambhavo.",
        "analyzed": "upadhim hi paṭicca dukkham idam sambhoti sabba upādāna kkhayā natthi dukkhassa sambhavaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_3.10:7.2",
        "original": "Lokamimaṁ passa;",
        "analyzed": "lokam imam passa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_3.10:7.3",
        "original": "puthū avijjāya paretā bhūtā bhūtaratā aparimuttā;",
        "analyzed": "puthū avijjāya paretā bhūtā bhūta ratā a parimuttā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_3.10:7.4",
        "original": "ye hi keci bhavā sabbadhi sabbatthatāya sabbe te bhavā aniccā dukkhā vipariṇāmadhammāti.",
        "analyzed": "ye hi keci bhavā sabbadhi sabbatthatāya sabbe te bhavā aniccāḥ dukkhāḥ vipariṇāma dhammā ati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_3.10:8.1",
        "original": "Evametaṁ yathābhūtaṁ,",
        "analyzed": "evam etam yathā bhūta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_3.10:8.2",
        "original": "Sammappaññāya passato;",
        "analyzed": "sammappaññāya passato ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_3.10:8.3",
        "original": "Bhavataṇhā pahīyati,",
        "analyzed": "bhava taṇhā pahīyati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_3.10:8.4",
        "original": "Vibhavaṁ nābhinandati;",
        "analyzed": "vibhavam na abhinandati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_3.10:8.5",
        "original": "Sabbaso taṇhānaṁ khayā,",
        "analyzed": "sabbaso taṇhānam khayā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_3.10:8.6",
        "original": "Asesavirāganirodho nibbānaṁ.",
        "analyzed": "       ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_3.10:9.1",
        "original": "Tassa nibbutassa bhikkhuno,",
        "analyzed": "tassa nibbutassa bhikkhunaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_3.10:9.2",
        "original": "Anupādā punabbhavo na hoti;",
        "analyzed": "anupādā punabbhavaḥ na hoti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_3.10:9.3",
        "original": "Abhibhūto māro vijitasaṅgāmo,",
        "analyzed": "abhibhūtaḥ māraḥ vijita saṅgāmaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_3.10:9.4",
        "original": "Upaccagā sabbabhavāni tādī”ti.",
        "analyzed": "sabba bhavāni tādi iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_3.10:9.5",
        "original": "Dasamaṁ.",
        "analyzed": "dasama ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_3.10:9.6",
        "original": "Nandavaggo tatiyo.",
        "analyzed": "nanda vaggo tatiyo ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_3.10:10.0",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ud_3.10:10.1",
        "original": "Kammaṁ nando yasojo ca,",
        "analyzed": "kammam nandaḥ yasa ojaḥ ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ud_3.10:10.2",
        "original": "sāriputto ca kolito;",
        "analyzed": "sāriputto ca kolito ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ud_3.10:10.3",
        "original": "Pilindo kassapo piṇḍo,",
        "analyzed": "pilindaḥ kassapaḥ piṇḍaḥ ",
        "folio": "10"
    },
    {
