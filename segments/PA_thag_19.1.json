[
    {
        "segmentnr": "PA_thag_19.1:0.1",
        "original": "Theragāthā 19.1",
        "analyzed": "theragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_19.1:0.2",
        "original": "Paññāsanipāta",
        "analyzed": "paññā asanipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_19.1:0.3",
        "original": "Paṭhamavagga",
        "analyzed": "paṭhama vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_19.1:0.4",
        "original": "Tālapuṭattheragāthā",
        "analyzed": "tālapuṭattheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_19.1:1.1",
        "original": "“Kadā nuhaṁ pabbatakandarāsu,",
        "analyzed": "kadā nuham pabbata kandarāsu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_19.1:1.2",
        "original": "Ekākiyo addutiyo vihassaṁ;",
        "analyzed": "ekākiyaḥ addutiyaḥ vihassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_19.1:1.3",
        "original": "Aniccato sabbabhavaṁ vipassaṁ,",
        "analyzed": "aniccataḥ sabba bhavam vipassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_19.1:1.4",
        "original": "Taṁ me idaṁ taṁ nu kadā bhavissati.",
        "analyzed": "tat me idam tam nu kadā bhavissati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_19.1:2.1",
        "original": "Kadā nuhaṁ bhinnapaṭandharo muni,",
        "analyzed": "kadā bhinna paṭandharaḥ muni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_19.1:2.2",
        "original": "Kāsāvavattho amamo nirāso;",
        "analyzed": "amamaḥ nirāsaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_19.1:2.3",
        "original": "Rāgañca dosañca tatheva mohaṁ,",
        "analyzed": "rāgan ca dosan ca tathā iva moha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_19.1:2.4",
        "original": "Hantvā sukhī pavanagato vihassaṁ.",
        "analyzed": "hantvā sukhī pavana gataḥ vihassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_19.1:3.1",
        "original": "Kadā aniccaṁ vadharoganīḷaṁ,",
        "analyzed": "kadā aniccam vadha roga nīḍa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_19.1:3.2",
        "original": "Kāyaṁ imaṁ maccujarāyupaddutaṁ;",
        "analyzed": "kāyam imam maccu jarāyu padduta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_19.1:3.3",
        "original": "Vipassamāno vītabhayo vihassaṁ,",
        "analyzed": "vipassamānaḥ vīta bhayaḥ vihassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_19.1:3.4",
        "original": "Eko vane taṁ nu kadā bhavissati.",
        "analyzed": "ekaḥ vane tam nu kadā bhavissati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_19.1:4.1",
        "original": "Kadā nuhaṁ bhayajananiṁ dukhāvahaṁ,",
        "analyzed": "kadā nuham bhaya jananim dukha āvaha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_19.1:4.2",
        "original": "Taṇhālataṁ bahuvidhānuvattaniṁ;",
        "analyzed": "taṇhālatam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_19.1:4.3",
        "original": "Paññāmayaṁ tikhiṇamasiṁ gahetvā,",
        "analyzed": "paññā mayam tikhiṇam asim gahetvā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_19.1:4.4",
        "original": "Chetvā vase tampi kadā bhavissati.",
        "analyzed": "chetvā vase tam pi kadā bhavissati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_19.1:5.1",
        "original": "Kadā nu paññāmayamuggatejaṁ,",
        "analyzed": "kadā nu paññā maya mugga teja ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_19.1:5.2",
        "original": "Satthaṁ isīnaṁ sahasādiyitvā;",
        "analyzed": "sattham isīnam sahasā ādiyitvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_19.1:5.3",
        "original": "Māraṁ sasenaṁ sahasā bhañjissaṁ,",
        "analyzed": "mā aram sa senam sahasā bhañjissa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_19.1:5.4",
        "original": "Sīhāsane taṁ nu kadā bhavissati.",
        "analyzed": "sīhāsane tat nu kadā bhavissati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_19.1:6.1",
        "original": "Kadā nuhaṁ sabbhi samāgamesu,",
        "analyzed": "kadā nuham sabbhi samāgamesu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_19.1:6.2",
        "original": "Diṭṭho bhave dhammagarūhi tādibhi;",
        "analyzed": "diṭṭhaḥ bhave dhamma garūhi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_19.1:6.3",
        "original": "Yāthāvadassīhi jitindriyehi,",
        "analyzed": "yāthāvadassīhi jiti indriyehi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_19.1:6.4",
        "original": "Padhāniyo taṁ nu kadā bhavissati.",
        "analyzed": "padhāniyaḥ tat nu kadā bhavissati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_19.1:7.1",
        "original": "Kadā nu maṁ tandi khudā pipāsā,",
        "analyzed": "kadā nu mama khudā pipāsā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_19.1:7.2",
        "original": "Vātātapā kīṭasarīsapā vā;",
        "analyzed": "vāta ātapā kīṭa sarīsapā vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_19.1:7.3",
        "original": "Na bādhayissanti na taṁ giribbaje,",
        "analyzed": "na bādhayissanti na tam giribbaje ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_19.1:7.4",
        "original": "Atthatthiyaṁ taṁ nu kadā bhavissati.",
        "analyzed": "atthatthiyam tat nu kadā bhavissati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_19.1:8.1",
        "original": "Kadā nu kho yaṁ viditaṁ mahesinā,",
        "analyzed": "kadā nu khalu yam viditam mahā isinā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_19.1:8.2",
        "original": "Cattāri saccāni sududdasāni;",
        "analyzed": "cattāri saccāni su duddasāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_19.1:8.3",
        "original": "Samāhitatto satimā agacchaṁ,",
        "analyzed": "samāhitattaḥ satimāḥ agaccha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_19.1:8.4",
        "original": "Paññāya taṁ taṁ nu kadā bhavissati.",
        "analyzed": "paññāya tam tam nu kadā bhavissati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_19.1:9.1",
        "original": "Kadā nu rūpe amite ca sadde,",
        "analyzed": "kadā nu rūpe amite ca sadde ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_19.1:9.2",
        "original": "Gandhe rase phusitabbe ca dhamme;",
        "analyzed": "gandhe rase phusitabbe ca dhamme ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_19.1:9.3",
        "original": "Ādittatohaṁ samathehi yutto,",
        "analyzed": "samathehi yuttaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_19.1:9.4",
        "original": "Paññāya dacchaṁ tadidaṁ kadā me.",
        "analyzed": "paññāya daccham tat idam kadā me ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_19.1:10.1",
        "original": "Kadā nuhaṁ dubbacanena vutto,",
        "analyzed": "kadā nuham dubbacanena vuttaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_19.1:10.2",
        "original": "Tato nimittaṁ vimano na hessaṁ;",
        "analyzed": "tatas nimittam vimanaḥ na ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_19.1:10.3",
        "original": "Atho pasatthopi tato nimittaṁ,",
        "analyzed": "atho pasatthaḥ pi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_19.1:10.4",
        "original": "Tuṭṭho na hessaṁ tadidaṁ kadā me.",
        "analyzed": "tuṭṭhaḥ na hessam tat idam kadā me ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_19.1:11.1",
        "original": "Kadā nu kaṭṭhe ca tiṇe latā ca,",
        "analyzed": "kadā nu kaṭṭhe ca tiṇe latā ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_19.1:11.2",
        "original": "Khandhe imehaṁ amite ca dhamme;",
        "analyzed": "khandhe amite ca dhamme ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_19.1:11.3",
        "original": "Ajjhattikāneva ca bāhirāni ca,",
        "analyzed": "ajjhattikāni eva ca bāhirāni ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_19.1:11.4",
        "original": "Samaṁ tuleyyaṁ tadidaṁ kadā me.",
        "analyzed": "samam tuleyyam tat idam kadā me ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_19.1:12.1",
        "original": "Kadā nu maṁ pāvusakālamegho,",
        "analyzed": "kadā nu mama pāvusa kāla meghaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_19.1:12.2",
        "original": "Navena toyena sacīvaraṁ vane;",
        "analyzed": "navena toyena sa cīvaram vane ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_19.1:12.3",
        "original": "Isippayātamhi pathe vajantaṁ,",
        "analyzed": "isippayātamhi pathe vajanta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_19.1:12.4",
        "original": "Ovassate taṁ nu kadā bhavissati.",
        "analyzed": "ovassate tam nu kadā bhavissati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_19.1:13.1",
        "original": "Kadā mayūrassa sikhaṇḍino vane,",
        "analyzed": "kadā mayūrassa sikhaṇḍinaḥ vane ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_19.1:13.2",
        "original": "Dijassa sutvā girigabbhare rutaṁ;",
        "analyzed": "dijassa sutvā giri gabbhare ruta ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_19.1:13.3",
        "original": "Paccuṭṭhahitvā amatassa pattiyā,",
        "analyzed": "paccuṭṭhahitvā amatassa pattiyā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_19.1:13.4",
        "original": "Sañcintaye taṁ nu kadā bhavissati.",
        "analyzed": "sañcintaye tam nu kadā bhavissati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_19.1:14.1",
        "original": "Kadā nu gaṅgaṁ yamunaṁ sarassatiṁ,",
        "analyzed": "kadā nu gaṅgam yamunam saraḥ sati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_19.1:14.2",
        "original": "Pātālakhittaṁ vaḷavāmukhañca;",
        "analyzed": "pātālakhittam vaḷavāmukham ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_19.1:14.3",
        "original": "Asajjamāno patareyyamiddhiyā,",
        "analyzed": "a sajjamānaḥ patareyyam iddhiyā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_19.1:14.4",
        "original": "Vibhiṁsanaṁ taṁ nu kadā bhavissati.",
        "analyzed": "vibhisanam tat nu kadā bhavissati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_19.1:15.1",
        "original": "Kadā nu nāgova asaṅgacārī,",
        "analyzed": "kadā nu nāgā uva asaṅga cārī ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_19.1:15.2",
        "original": "Padālaye kāmaguṇesu chandaṁ;",
        "analyzed": "pada ālaye kāmaguṇesu chanda ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_19.1:15.3",
        "original": "Nibbajjayaṁ sabbasubhaṁ nimittaṁ,",
        "analyzed": "sabba subham ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_19.1:15.4",
        "original": "Jhāne yuto taṁ nu kadā bhavissati.",
        "analyzed": "jhāne yutaḥ tam nu kadā bhavissati ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_19.1:16.1",
        "original": "Kadā iṇaṭṭova daliddako nidhiṁ,",
        "analyzed": "kadā daliddakaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_19.1:16.2",
        "original": "Ārādhayitvā dhanikehi pīḷito;",
        "analyzed": "ārādhayitvā dhanikehi pīḍitaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_19.1:16.3",
        "original": "Tuṭṭho bhavissaṁ adhigamma sāsanaṁ,",
        "analyzed": "tuṭṭhaḥ bhavissam adhigamma sāsana ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_19.1:16.4",
        "original": "Mahesino taṁ nu kadā bhavissati.",
        "analyzed": "mahesinaḥ tat nu kadā bhavissati ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_19.1:17.1",
        "original": "Bahūni vassāni tayāmhi yācito,",
        "analyzed": "bahūni vassāni tayā amhi yācitaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_19.1:17.2",
        "original": "‘Agāravāsena alaṁ nu te idaṁ’;",
        "analyzed": "agāra vāsena alam nu te idam ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_19.1:17.3",
        "original": "Taṁ dāni maṁ pabbajitaṁ samānaṁ,",
        "analyzed": "tam dāni mam pabbajitam samāna ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_19.1:17.4",
        "original": "Kiṅkāraṇā citta tuvaṁ na yuñjasi.",
        "analyzed": "kiṅkāraṇā citta tuvam na yuñjasi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_19.1:18.1",
        "original": "Nanu ahaṁ citta tayāmhi yācito,",
        "analyzed": "nanu aham citta tayā amhi yācitaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_19.1:18.2",
        "original": "‘Giribbaje citrachadā vihaṅgamā’;",
        "analyzed": "giribbaje citrachadā vihaṅgamā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_19.1:18.3",
        "original": "Mahindaghosatthanitābhigajjino,",
        "analyzed": "mahinda ghosa tthanita abhigajjinaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_19.1:18.4",
        "original": "Te taṁ ramessanti vanamhi jhāyinaṁ.",
        "analyzed": "te tam ramessanti vanamhi jhāyina ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_19.1:19.1",
        "original": "Kulamhi mitte ca piye ca ñātake,",
        "analyzed": "kulamhi mitte ca piye ca ñātake ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_19.1:19.2",
        "original": "Khiḍḍāratiṁ kāmaguṇañca loke;",
        "analyzed": "khiḍḍa aratim kāma guṇam ca loke ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_19.1:19.3",
        "original": "Sabbaṁ pahāya imamajjhupāgato,",
        "analyzed": "sabbam pahāya imam ajjhupāgataḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_19.1:19.4",
        "original": "Athopi tvaṁ citta na mayha tussasi.",
        "analyzed": "tvam citta na tussasi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_19.1:20.1",
        "original": "Mameva etaṁ na hi tvaṁ paresaṁ,",
        "analyzed": "mama iva etam na hi tvam paresa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_19.1:20.2",
        "original": "Sannāhakāle paridevitena kiṁ;",
        "analyzed": "sannāha kāle paridevitena ki ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_19.1:20.3",
        "original": "Sabbaṁ idaṁ calamiti pekkhamāno,",
        "analyzed": "sabbam idam calam iti pekkhamāno ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_19.1:20.4",
        "original": "Abhinikkhamiṁ amatapadaṁ jigīsaṁ.",
        "analyzed": "abhinikkhamim amata padam jigīsa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_19.1:21.1",
        "original": "Suyuttavādī dvipadānamuttamo,",
        "analyzed": "su yutta vādī ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_19.1:21.2",
        "original": "Mahābhisakko naradammasārathi;",
        "analyzed": "mahā abhisakkaḥ nara damma sārathi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_19.1:21.3",
        "original": "‘Cittaṁ calaṁ makkaṭasannibhaṁ iti,",
        "analyzed": "cittam calam makkaṭa sannibham iti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_19.1:21.4",
        "original": "Avītarāgena sudunnivārayaṁ’.",
        "analyzed": "a vīta rāgena ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_19.1:22.1",
        "original": "Kāmā hi citrā madhurā manoramā,",
        "analyzed": "kāmā hi citrā madhurā manoramā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_19.1:22.2",
        "original": "Aviddasū yattha sitā puthujjanā;",
        "analyzed": "aviddasū yattha sitā puthujjanā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_19.1:22.3",
        "original": "Te dukkhamicchanti punabbhavesino,",
        "analyzed": "te dukkham icchanti punabbhavesinaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_19.1:22.4",
        "original": "Cittena nītā niraye nirākatā.",
        "analyzed": "cittena nītā niraye nirākatā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thag_19.1:23.1",
        "original": "‘Mayūrakoñcābhirutamhi kānane,",
        "analyzed": "kānane ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_19.1:23.2",
        "original": "Dīpīhi byagghehi purakkhato vasaṁ;",
        "analyzed": "dīpīhi byagghehi purakkhataḥ vasa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_19.1:23.3",
        "original": "Kāye apekkhaṁ jaha mā virādhaya’,",
        "analyzed": "kāye apekkham jaha mā virādhaya ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_19.1:23.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thag_19.1:24.1",
        "original": "‘Bhāvehi jhānāni ca indriyāni ca,",
        "analyzed": "bhāvehi jhānāni ca indriyāni ca ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thag_19.1:24.2",
        "original": "Balāni bojjhaṅgasamādhibhāvanā;",
        "analyzed": "balāni ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thag_19.1:24.3",
        "original": "Tisso ca vijjā phusa buddhasāsane’,",
        "analyzed": "tissaḥ ca vijjā phusa buddha sāsane ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thag_19.1:24.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thag_19.1:25.1",
        "original": "‘Bhāvehi maggaṁ amatassa pattiyā,",
        "analyzed": "bhāvehi maggam amatassa pattiyā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thag_19.1:25.2",
        "original": "Niyyānikaṁ sabbadukhakkhayogadhaṁ;",
        "analyzed": "niyyānikam ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thag_19.1:25.3",
        "original": "Aṭṭhaṅgikaṁ sabbakilesasodhanaṁ’,",
        "analyzed": "aṭṭhaṅgikam sabba kilesa sodhana ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thag_19.1:25.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thag_19.1:26.1",
        "original": "‘Dukkhanti khandhe paṭipassa yoniso,",
        "analyzed": "dukkhanti khandhe paṭipassa yoniso ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thag_19.1:26.2",
        "original": "Yato ca dukkhaṁ samudeti taṁ jaha;",
        "analyzed": "yatas ca dukkham samudeti tam jaha ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thag_19.1:26.3",
        "original": "Idheva dukkhassa karohi antaṁ’,",
        "analyzed": "idhā iva dukkhassa karohi anta ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thag_19.1:26.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thag_19.1:27.1",
        "original": "‘Aniccaṁ dukkhanti vipassa yoniso,",
        "analyzed": "aniccam dukkhanti vipassa yonisaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thag_19.1:27.2",
        "original": "Suññaṁ anattāti aghaṁ vadhanti ca;",
        "analyzed": "suññam anattā ati agham vadhanti ca ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thag_19.1:27.3",
        "original": "Manovicāre uparundha cetaso’,",
        "analyzed": "manaḥ vicāre uparundha cetasaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thag_19.1:27.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thag_19.1:28.1",
        "original": "‘Muṇḍo virūpo abhisāpamāgato,",
        "analyzed": "muṇḍaḥ virūpaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thag_19.1:28.2",
        "original": "Kapālahatthova kulesu bhikkhasu;",
        "analyzed": "kapāla hatthaḥ uva kulesu bhikkhasu ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thag_19.1:28.3",
        "original": "Yuñjassu satthuvacane mahesino’,",
        "analyzed": "yuñjassu satthuvacane mahesinaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thag_19.1:28.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thag_19.1:29.1",
        "original": "‘Susaṁvutatto visikhantare caraṁ,",
        "analyzed": "susavutattaḥ visikhantare cara ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thag_19.1:29.2",
        "original": "Kulesu kāmesu asaṅgamānaso;",
        "analyzed": "kulesu kāmesu asaṅga mānasaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thag_19.1:29.3",
        "original": "Cando yathā dosinapuṇṇamāsiyā’,",
        "analyzed": "candaḥ yathā dosinapuṇṇamāsiyā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thag_19.1:29.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thag_19.1:30.1",
        "original": "‘Āraññiko hohi ca piṇḍapātiko,",
        "analyzed": "āraññikaḥ ca piṇḍapātikaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thag_19.1:30.2",
        "original": "Sosāniko hohi ca paṁsukūliko;",
        "analyzed": "sosānikaḥ hohi ca pasukūlikaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thag_19.1:30.3",
        "original": "Nesajjiko hohi sadā dhute rato’,",
        "analyzed": "nesajjikaḥ hohi sadā dhute rataḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thag_19.1:30.4",
        "original": "Itissu maṁ citta pure niyuñjasi.",
        "analyzed": "itissu mam citta pure niyuñjasi ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thag_19.1:31.1",
        "original": "Ropetva rukkhāni yathā phalesī,",
        "analyzed": "ropetvā rukkhāni yathā phalesī ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thag_19.1:31.2",
        "original": "Mūle taruṁ chettu tameva icchasi;",
        "analyzed": "mūle tarum chettu tam eva icchasi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thag_19.1:31.3",
        "original": "Tathūpamaṁ cittamidaṁ karosi,",
        "analyzed": "tathā upamam cittam idam karosi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thag_19.1:31.4",
        "original": "Yaṁ maṁ aniccamhi cale niyuñjasi.",
        "analyzed": "yam mam aniccamhi cale niyuñjasi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thag_19.1:32.1",
        "original": "Arūpa dūraṅgama ekacāri,",
        "analyzed": "arūpa dūraṅgama ekacāri ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thag_19.1:32.2",
        "original": "Na te karissaṁ vacanaṁ idānihaṁ;",
        "analyzed": "na te karissam vacanam idān iha ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thag_19.1:32.3",
        "original": "Dukkhā hi kāmā kaṭukā mahabbhayā,",
        "analyzed": "dukkhā hi kāmā kaṭukā mahat bhayā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thag_19.1:32.4",
        "original": "Nibbānamevābhimano carissaṁ.",
        "analyzed": "nibbānam eva abhimanaḥ carissa ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thag_19.1:33.1",
        "original": "Nāhaṁ alakkhyā ahirikkatāya vā,",
        "analyzed": "na aham alakkhyā ahirikkatāya vā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thag_19.1:33.2",
        "original": "Na cittahetū na ca dūrakantanā;",
        "analyzed": "na citta hetū na ca dūra kantanā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thag_19.1:33.3",
        "original": "Ājīvahetū ca ahaṁ na nikkhamiṁ,",
        "analyzed": "ājīva hetū ca aham na nikkhami ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thag_19.1:33.4",
        "original": "Kato ca te citta paṭissavo mayā.",
        "analyzed": "kataḥ ca te citta paṭissavaḥ mayā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thag_19.1:34.1",
        "original": "‘Appicchatā sappurisehi vaṇṇitā,",
        "analyzed": "appiccha tā sappurisehi vaṇṇitā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thag_19.1:34.2",
        "original": "Makkhappahānaṁ vupasamo dukhassa’;",
        "analyzed": "makkha ppahānam vupasamaḥ dukhassa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thag_19.1:34.3",
        "original": "Itissu maṁ citta tadā niyuñjasi,",
        "analyzed": "itissu mam citta tadā niyuñjasi ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thag_19.1:34.4",
        "original": "Idāni tvaṁ gacchasi pubbaciṇṇaṁ.",
        "analyzed": "idāni tvam gacchasi pubbaciṇṇa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thag_19.1:35.1",
        "original": "Taṇhā avijjā ca piyāpiyañca,",
        "analyzed": "taṇhā avijjā ca piya apiyam ca ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thag_19.1:35.2",
        "original": "Subhāni rūpāni sukhā ca vedanā;",
        "analyzed": "subhāni rūpāni sukhā ca vedanā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thag_19.1:35.3",
        "original": "Manāpiyā kāmaguṇā ca vantā,",
        "analyzed": "manāpiyā kāma guṇā ca vantā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thag_19.1:35.4",
        "original": "Vante ahaṁ āvamituṁ na ussahe.",
        "analyzed": "vante aham āvamitum na ussahe ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thag_19.1:36.1",
        "original": "Sabbattha te citta vaco kataṁ mayā,",
        "analyzed": "sabbattha te citta vaco katam mayā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thag_19.1:36.2",
        "original": "Bahūsu jātīsu na mesi kopito;",
        "analyzed": "bahūsu jātīsu na mesi kopitaḥ ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thag_19.1:36.3",
        "original": "Ajjhattasambhavo kataññutāya te,",
        "analyzed": "ajjhatta sambhavaḥ kataññutāya te ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thag_19.1:36.4",
        "original": "Dukkhe ciraṁ saṁsaritaṁ tayā kate.",
        "analyzed": "dukkhe ciram sa saritam tayā kate ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thag_19.1:37.1",
        "original": "Tvaññeva no citta karosi brāhmaṇo,",
        "analyzed": "tvañc eva naḥ citta karosi brāhmaṇaḥ ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thag_19.1:37.2",
        "original": "Tvaṁ khattiyo rājadasī karosi;",
        "analyzed": "tvam khattiyaḥ rājadasī karosi ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thag_19.1:37.3",
        "original": "Vessā ca suddā ca bhavāma ekadā,",
        "analyzed": "vessā ca suddā ca bhavāmaḥ ekadā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thag_19.1:37.4",
        "original": "Devattanaṁ vāpi taveva vāhasā.",
        "analyzed": "devattanam vā api tava iva vāhasā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thag_19.1:38.1",
        "original": "Taveva hetū asurā bhavāmase,",
        "analyzed": "tava iva hetū asurāḥ bhavāmase ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thag_19.1:38.2",
        "original": "Tvaṁmūlakaṁ nerayikā bhavāmase;",
        "analyzed": "tva mūlakam na īrayikāḥ bhavāmase ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thag_19.1:38.3",
        "original": "Atho tiracchānagatāpi ekadā,",
        "analyzed": "atho tiracchāna gatā api ekadā ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thag_19.1:38.4",
        "original": "Petattanaṁ vāpi taveva vāhasā.",
        "analyzed": "vā api tava iva vāhasā ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thag_19.1:39.1",
        "original": "Nanu dubbhissasi maṁ punappunaṁ,",
        "analyzed": "nanu mama punar puna ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thag_19.1:39.2",
        "original": "Muhuṁ muhuṁ cāraṇikaṁva dassayaṁ;",
        "analyzed": "muhum muhum dassaya ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thag_19.1:39.3",
        "original": "Ummattakeneva mayā palobhasi,",
        "analyzed": "ummattakena iva mayā palobhasi ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thag_19.1:39.4",
        "original": "Kiñcāpi te citta virādhitaṁ mayā.",
        "analyzed": "kiñca api te citta virādhitam mayā ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thag_19.1:40.1",
        "original": "Idaṁ pure cittamacāri cārikaṁ,",
        "analyzed": "idam pure cittam acāri cārika ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thag_19.1:40.2",
        "original": "Yenicchakaṁ yatthakāmaṁ yathāsukhaṁ;",
        "analyzed": "yenicchakam yatthakāmam yathāsukha ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thag_19.1:40.3",
        "original": "Tadajjahaṁ niggahessāmi yoniso,",
        "analyzed": "tat ajja aham niggahessāmi yonisaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thag_19.1:40.4",
        "original": "Hatthippabhinnaṁ viya aṅkusaggaho.",
        "analyzed": "hatthi ppabhinnam viya aṅkusa gahaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thag_19.1:41.1",
        "original": "Satthā ca me lokamimaṁ adhiṭṭhahi,",
        "analyzed": "satthā ca me lokam imam adhiṭṭhahi ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thag_19.1:41.2",
        "original": "Aniccato addhuvato asārato;",
        "analyzed": "aniccataḥ a ddhuvataḥ a sārataḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thag_19.1:41.3",
        "original": "Pakkhanda maṁ citta jinassa sāsane,",
        "analyzed": "pakkhanda mama citta jinassa sāsane ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thag_19.1:41.4",
        "original": "Tārehi oghā mahatā suduttarā.",
        "analyzed": "tārehi oghā mahatā su duttarā ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thag_19.1:42.1",
        "original": "Na te idaṁ citta yathā purāṇakaṁ,",
        "analyzed": "na te idam citta yathā purāṇaka ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thag_19.1:42.2",
        "original": "Nāhaṁ alaṁ tuyha vase nivattituṁ;",
        "analyzed": "na aham alam vase ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thag_19.1:42.3",
        "original": "Mahesino pabbajitomhi sāsane,",
        "analyzed": "mahesino pabbajitomhi sāsane ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thag_19.1:42.4",
        "original": "Na mādisā honti vināsadhārino.",
        "analyzed": "na mādisāḥ honti vināsa dhārinaḥ ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thag_19.1:43.1",
        "original": "Nagā samuddā saritā vasundharā,",
        "analyzed": "nagā samuddā saritā vasundharā ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thag_19.1:43.2",
        "original": "Disā catasso vidisā adho divā;",
        "analyzed": "disā catassaḥ vidisāḥ adhas divā ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thag_19.1:43.3",
        "original": "Sabbe aniccā tibhavā upaddutā,",
        "analyzed": "sabbe aniccā ti bhavā upaddutā ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thag_19.1:43.4",
        "original": "Kuhiṁ gato citta sukhaṁ ramissasi.",
        "analyzed": "kuhim gataḥ citta sukham ramissasi ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thag_19.1:44.1",
        "original": "Dhitipparaṁ kiṁ mama citta kāhisi,",
        "analyzed": "kim mama citta kāhisi ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thag_19.1:44.2",
        "original": "Na te alaṁ citta vasānuvattako;",
        "analyzed": "na te alam citta ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thag_19.1:44.3",
        "original": "Na jātu bhastaṁ ubhatomukhaṁ chupe,",
        "analyzed": "na jātu bhastam ubhatomukham chupe ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thag_19.1:44.4",
        "original": "Dhiratthu pūraṁ nava sotasandaniṁ.",
        "analyzed": "dhiratthu pūram nava sota sandani ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thag_19.1:45.1",
        "original": "Varāhaeṇeyyavigāḷhasevite,",
        "analyzed": "varāha eṇeyya vigāḍha sevite ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thag_19.1:45.2",
        "original": "Pabbhārakuṭṭe pakateva sundare;",
        "analyzed": "pabbhāra kuṭṭe pakatā iva sundare ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thag_19.1:45.3",
        "original": "Navambunā pāvusasitthakānane,",
        "analyzed": "navambunā pāvusa sittha kānane ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thag_19.1:45.4",
        "original": "Tahiṁ guhāgehagato ramissasi.",
        "analyzed": "tahim guhā geha gataḥ ramissasi ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thag_19.1:46.1",
        "original": "Sunīlagīvā susikhā supekhunā,",
        "analyzed": "su nīla gīvā su sikhā su pekhunā ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thag_19.1:46.2",
        "original": "Sucittapattacchadanā vihaṅgamā;",
        "analyzed": "su citta patta chadanā vihaṅgamā ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thag_19.1:46.3",
        "original": "Sumañjughosatthanitābhigajjino,",
        "analyzed": "       ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thag_19.1:46.4",
        "original": "Te taṁ ramessanti vanamhi jhāyinaṁ.",
        "analyzed": "te tam ramessanti vanamhi jhāyina ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thag_19.1:47.1",
        "original": "Vuṭṭhamhi deve caturaṅgule tiṇe,",
        "analyzed": "deve catur aṅgule tiṇe ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thag_19.1:47.2",
        "original": "Sampupphite meghanibhamhi kānane;",
        "analyzed": "sampupphite megha nibhamhi kānane ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thag_19.1:47.3",
        "original": "Nagantare viṭapisamo sayissaṁ,",
        "analyzed": "na ga antare viṭapi samaḥ sayissa ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thag_19.1:47.4",
        "original": "Taṁ me mudū hehiti tūlasannibhaṁ.",
        "analyzed": "tam me mudū tūla sannibha ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thag_19.1:48.1",
        "original": "Tathā tu kassāmi yathāpi issaro,",
        "analyzed": "tathā tu kassāmi yathā api issaro ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thag_19.1:48.2",
        "original": "Yaṁ labbhati tenapi hotu me alaṁ;",
        "analyzed": "yam labbhati tena api hotu me ala ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thag_19.1:48.3",
        "original": "Na tāhaṁ kassāmi yathā atandito,",
        "analyzed": "na tāham kassāmi yathā a tanditaḥ ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thag_19.1:48.4",
        "original": "Biḷārabhastaṁva yathā sumadditaṁ.",
        "analyzed": "biḷārabhaḥ tava yathā sumaddita ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thag_19.1:49.1",
        "original": "Tathā tu kassāmi yathāpi issaro,",
        "analyzed": "tathā tu kassāmi yathā api issaro ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_thag_19.1:49.2",
        "original": "Yaṁ labbhati tenapi hotu me alaṁ;",
        "analyzed": "yam labbhati tena api hotu me ala ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_thag_19.1:49.3",
        "original": "Viriyena taṁ mayha vasānayissaṁ,",
        "analyzed": "viriyena tam mayha vasa ānayissa ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_thag_19.1:49.4",
        "original": "Gajaṁva mattaṁ kusalaṅkusaggaho.",
        "analyzed": "mattam ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_thag_19.1:50.1",
        "original": "Tayā sudantena avaṭṭhitena hi,",
        "analyzed": "tayā su dantena avaṭṭhitena hi ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_thag_19.1:50.2",
        "original": "Hayena yoggācariyova ujjunā;",
        "analyzed": "hayena ujjunā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_thag_19.1:50.3",
        "original": "Pahomi maggaṁ paṭipajjituṁ sivaṁ,",
        "analyzed": "pahomi maggam paṭipajjitum siva ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_thag_19.1:50.4",
        "original": "Cittānurakkhīhi sadā nisevitaṁ.",
        "analyzed": "citta anurakkhīhi sadā nisevita ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_thag_19.1:51.1",
        "original": "Ārammaṇe taṁ balasā nibandhisaṁ,",
        "analyzed": "ārammaṇe tam balasā ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_thag_19.1:51.2",
        "original": "Nāgaṁva thambhamhi daḷhāya rajjuyā;",
        "analyzed": "nāgava thambhamhi daḷhāya rajjuyā ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_thag_19.1:51.3",
        "original": "Taṁ me suguttaṁ satiyā subhāvitaṁ,",
        "analyzed": "tam me su guttam satiyā su bhāvita ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_thag_19.1:51.4",
        "original": "Anissitaṁ sabbabhavesu hehisi.",
        "analyzed": "a nissitam sabba bhavesu hehisi ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_thag_19.1:52.1",
        "original": "Paññāya chetvā vipathānusārinaṁ,",
        "analyzed": "paññāya chetvā vipatha anusārina ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_thag_19.1:52.2",
        "original": "Yogena niggayha pathe nivesiya;",
        "analyzed": "yogena niggayha pathe nivesiya ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_thag_19.1:52.3",
        "original": "Disvā samudayaṁ vibhavañca sambhavaṁ,",
        "analyzed": "disvā samudayam vibhavam ca sambhava ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_thag_19.1:52.4",
        "original": "Dāyādako hehisi aggavādino.",
        "analyzed": "dāyādakaḥ hehisi agga vādinaḥ ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_thag_19.1:53.1",
        "original": "Catubbipallāsavasaṁ adhiṭṭhitaṁ,",
        "analyzed": "catur bipallāsa vasam ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_thag_19.1:53.2",
        "original": "Gāmaṇḍalaṁva parinesi citta maṁ;",
        "analyzed": "parinesi citta ma ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_thag_19.1:53.3",
        "original": "Nanu saṁyojanabandhanacchidaṁ,",
        "analyzed": "nanu sayojana bandhana chidaḥ ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_thag_19.1:53.4",
        "original": "Saṁsevase kāruṇikaṁ mahāmuniṁ.",
        "analyzed": "sasevase kāruṇikam mahā muni ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_thag_19.1:54.1",
        "original": "Migo yathā seri sucittakānane,",
        "analyzed": "migaḥ yathā su citta kānane ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_thag_19.1:54.2",
        "original": "Rammaṁ giriṁ pāvusaabbhamāliniṁ;",
        "analyzed": "rammam girim pāvusa abbha mālini ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_thag_19.1:54.3",
        "original": "Anākule tattha nage ramissaṁ,",
        "analyzed": "an ākule tattha nage ramissa ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_thag_19.1:54.4",
        "original": "Asaṁsayaṁ citta parā bhavissasi.",
        "analyzed": "a sa sayam citta parā bhavissasi ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_thag_19.1:55.1",
        "original": "Ye tuyha chandena vasena vattino,",
        "analyzed": "ye tuyha chandena vasena vattino ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_thag_19.1:55.2",
        "original": "Narā ca nārī ca anubhonti yaṁ sukhaṁ;",
        "analyzed": "narā ca nārī ca anubhonti yam sukha ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_thag_19.1:55.3",
        "original": "Aviddasū māravasānuvattino,",
        "analyzed": "aviddasū māra vasā anuvattinaḥ ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_thag_19.1:55.4",
        "original": "Bhavābhinandī tava citta sāvakā”ti.",
        "analyzed": "bhava abhinandī tava ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_thag_19.1:55.5",
        "original": "… Tālapuṭo thero …",
        "analyzed": "tālapuṭaḥ theraḥ ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_thag_19.1:55.6",
        "original": "Paññāsanipāto niṭṭhito.",
        "analyzed": "paññāsa nipātaḥ niṭṭhitaḥ ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_thag_19.1:56.0",
        "original": "Tatruddānaṁ",
        "analyzed": "tatra uddāna ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_thag_19.1:56.1",
        "original": "Paññāsamhi nipātamhi,",
        "analyzed": "paññāsamhi nipātamhi ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_thag_19.1:56.2",
        "original": "eko tālapuṭo suci;",
        "analyzed": "ekaḥ tālapuṭaḥ suci ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_thag_19.1:56.3",
        "original": "Gāthāyo tattha paññāsa,",
        "analyzed": "gāthāyaḥ tattha paññāsa ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_thag_19.1:56.4",
        "original": "puna pañca ca uttarīti.",
        "analyzed": "puna pañca ca uttarī iti ",
        "folio": "56"
    }
]