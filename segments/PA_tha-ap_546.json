        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_546:10.3",
        "original": "Sabbavedavidū jāto,",
        "analyzed": "sabba veda vidū jātaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_546:10.4",
        "original": "vāgīso vādisūdano.",
        "analyzed": "vāgīsaḥ vādi sūdanaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_546:11.1",
