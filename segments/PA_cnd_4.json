    },
    {
        "segmentnr": "PA_cnd_4:16.3",
        "original": "Evameva tvampi pamuñcassu saddhaṁ,",
        "analyzed": "evam eva tvam pi pamuñcassu ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_cnd_4:16.4",
