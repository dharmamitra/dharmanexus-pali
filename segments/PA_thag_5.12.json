        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_5.12:5.3",
        "original": "Atthantaro nāma sa hoti paṇḍito,",
        "analyzed": "atthantaraḥ nāma sa hoti paṇḍito ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_5.12:5.4",
