    },
    {
        "segmentnr": "PA_ja_478:0.4",
        "original": "5. Dūtajātaka",
        "analyzed": "dūta jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_478:1.1",
        "original": "“Dūte te brahme pāhesiṁ,",
        "analyzed": "dūte te brahme pāhesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_478:1.2",
        "original": "Gaṅgātīrasmi jhāyato;",
        "analyzed": "gaṅgā tīra asmi jhāyataḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_478:1.3",
        "original": "Tesaṁ puṭṭho na byākāsi,",
        "analyzed": "tesam puṭṭhaḥ na ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_478:1.4",
        "original": "Dukkhaṁ guyhamataṁ nu te”.",
        "analyzed": "dukkham nu te ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_478:2.1",
        "original": "“Sace te dukkhamuppajje,",
        "analyzed": "sace te dukkham uppajje ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_478:2.2",
        "original": "kāsīnaṁ raṭṭhavaḍḍhana;",
        "analyzed": "kāsīnam raṭṭhavaḍḍhana ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_478:2.3",
        "original": "Mā kho naṁ tassa akkhāhi,",
        "analyzed": "mā kho nam tassa akkhāhi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_478:2.4",
        "original": "yo taṁ dukkhā na mocaye.",
        "analyzed": "yaḥ tam dukkhā na mocaye ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_478:3.1",
        "original": "Yo tassa dukkhajātassa,",
        "analyzed": "yaḥ tassa dukkha jātassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_478:3.2",
        "original": "ekaṅgamapi bhāgaso;",
        "analyzed": "ekaṅgam api bhāgaso ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_478:3.3",
        "original": "Vippamoceyya dhammena,",
        "analyzed": "vippamoceyya dhammena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_478:3.4",
        "original": "kāmaṁ tassa pavedaya.",
        "analyzed": "kāmam tassa pavedaya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_478:4.1",
        "original": "Suvijānaṁ siṅgālānaṁ,",
        "analyzed": "su vijānam siṅgālāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_478:4.2",
        "original": "sakuṇānañca vassitaṁ;",
        "analyzed": "sakuṇānam ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_478:4.3",
        "original": "Manussavassitaṁ rāja,",
        "analyzed": "rāja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_478:4.4",
        "original": "dubbijānataraṁ tato.",
        "analyzed": "tatas ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_478:5.1",
        "original": "Api ce maññatī poso,",
        "analyzed": "api ce maññati poso ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_478:5.2",
        "original": "ñāti mitto sakhāti vā;",
        "analyzed": "vā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_478:5.3",
        "original": "Yo pubbe sumano hutvā,",
        "analyzed": "yaḥ pubbe sumanaḥ hutvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_478:5.4",
        "original": "pacchā sampajjate diso.",
        "analyzed": "pacchā sampajjate disaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_478:6.1",
        "original": "Yo attano dukkhamanānupuṭṭho,",
        "analyzed": "yaḥ attanaḥ dukkha manā anupuṭṭhaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_478:6.2",
        "original": "Pavedaye jantu akālarūpe;",
        "analyzed": "pavedaye jantu akāla rūpe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_478:6.3",
        "original": "Ānandino tassa bhavantimittā,",
        "analyzed": "ānandinaḥ tassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_478:6.4",
        "original": "Hitesino tassa dukhī bhavanti.",
        "analyzed": "hita īsinaḥ tassa dukhī bhavanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_478:7.1",
        "original": "Kālañca ñatvāna tathāvidhassa,",
        "analyzed": "kālañca ñatvāna tathāvidhassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_478:7.2",
        "original": "Medhāvīnaṁ ekamanaṁ viditvā;",
        "analyzed": "medhāvīnam ekamanam viditvā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_478:7.3",
        "original": "Akkheyya tibbāni parassa dhīro,",
        "analyzed": "akkheyya tibbāni parassa dhīraḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_478:7.4",
        "original": "Saṇhaṁ giraṁ atthavatiṁ pamuñce.",
        "analyzed": "saṇham giram atthavatim pamuñce ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_478:8.1",
        "original": "Sace ca jaññā avisayhamattano,",
        "analyzed": "sace ca jaññāḥ avisayhamattano ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_478:8.2",
        "original": "Na te hi mayhaṁ sukhāgamāya;",
        "analyzed": "na te hi mayham sukha āgamāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_478:8.3",
        "original": "Ekova tibbāni saheyya dhīro,",
        "analyzed": "ekaḥ iva tibbāni saheyya dhīraḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_478:8.4",
        "original": "Saccaṁ hirottappamapekkhamāno.",
        "analyzed": "saccam hirottappam apekkhamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_478:9.1",
        "original": "Ahaṁ raṭṭhe vicaranto,",
        "analyzed": "aham raṭṭhe vicarantaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_478:9.2",
        "original": "nigame rājadhāniyo;",
        "analyzed": "nigame rājadhāniyaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_478:9.3",
        "original": "Bhikkhamāno mahārāja,",
        "analyzed": "bhikkhamānaḥ mahā rāja ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_478:9.4",
        "original": "ācariyassa dhanatthiko.",
        "analyzed": "ācariyassa dhanatthiko ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_478:10.1",
        "original": "Gahapatī rājapurise,",
        "analyzed": "gahapatiḥ rāja purise ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_478:10.2",
        "original": "mahāsāle ca brāhmaṇe;",
        "analyzed": "mahā sāle ca brāhmaṇe ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_478:10.3",
        "original": "Alatthaṁ satta nikkhāni,",
        "analyzed": "alattham satta nikkhāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_478:10.4",
        "original": "suvaṇṇassa janādhipa;",
        "analyzed": "suvaṇṇassa janādhipa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_478:10.5",
        "original": "Te me naṭṭhā mahārāja,",
        "analyzed": "te me naṭṭhāḥ mahā rāja ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_478:10.6",
        "original": "tasmā socāmahaṁ bhusaṁ.",
        "analyzed": "tasmai socāmaham bhusa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_478:11.1",
        "original": "Purisā te mahārāja,",
        "analyzed": "purisā te mahā rāja ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_478:11.2",
        "original": "manasānuvicintitā;",
        "analyzed": "manasā anuvicintitā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_478:11.3",
        "original": "Nālaṁ dukkhā pamocetuṁ,",
        "analyzed": "na alam dukkhā pamocetu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_478:11.4",
        "original": "tasmā tesaṁ na byāhariṁ.",
        "analyzed": "tasmāt na byāhari ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_478:12.1",
        "original": "Tvañca kho me mahārāja,",
        "analyzed": "tvam ca khalu me mahā rāja ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_478:12.2",
        "original": "manasānuvicintito;",
        "analyzed": "manasā anuvicintitaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_478:12.3",
        "original": "Alaṁ dukkhā pamocetuṁ,",
        "analyzed": "alam dukkhā pamocetu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_478:12.4",
        "original": "tasmā tuyhaṁ pavedayiṁ”.",
        "analyzed": "tasmai tuyham ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_478:13.1",
        "original": "“Tassādāsi pasannatto,",
        "analyzed": "pasannattaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_478:13.2",
        "original": "kāsīnaṁ raṭṭhavaḍḍhano;",
        "analyzed": "kāsīnam raṭṭhavaḍḍhano ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_478:13.3",
        "original": "Jātarūpamaye nikkhe,",
        "analyzed": "jātarūpa maye ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_478:13.4",
        "original": "suvaṇṇassa catuddasā”ti.",
        "analyzed": "suvaṇṇassa catuddasāti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_478:14.1",
        "original": "Dūtajātakaṁ pañcamaṁ.",
        "analyzed": "dūta jātakam pañcama ",
        "folio": "14"
    }
]