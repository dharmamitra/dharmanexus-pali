        "analyzed": "cakkam appaṭivattiya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.6:8.1",
        "original": "“Sambuddho paṭijānāsi,",
        "analyzed": "sambuddhaḥ paṭijānāsi ",
        "folio": "8"
    },
