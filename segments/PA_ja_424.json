    {
        "segmentnr": "PA_ja_424:0.4",
        "original": "8. Ādittajātaka",
        "analyzed": "ādittajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_424:1.1",
        "original": "“Ādittasmiṁ agārasmiṁ,",
        "analyzed": "ādittasmin ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_424:1.2",
        "original": "yaṁ nīharati bhājanaṁ;",
        "analyzed": "yam nīharati bhājana ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_424:1.3",
        "original": "Taṁ tassa hoti atthāya,",
        "analyzed": "tam tassa hoti atthāya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_424:1.4",
        "original": "no ca yaṁ tattha ḍayhati.",
        "analyzed": "na u ca yam ḍayhati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_424:2.1",
        "original": "Evamādīpito loko,",
        "analyzed": "evam ādīpitaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_424:2.2",
        "original": "jarāya maraṇena ca;",
        "analyzed": "jarāya maraṇena ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_424:2.3",
        "original": "Nīharetheva dānena,",
        "analyzed": "nīharetha iva dānena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_424:2.4",
        "original": "dinnaṁ hoti sunīhataṁ”.",
        "analyzed": "dinnam hoti sunīhata ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_424:3.1",
        "original": "“Yo dhammaladdhassa dadāti dānaṁ,",
        "analyzed": "yaḥ dhamma laddhassa dadāti dāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_424:3.2",
        "original": "Uṭṭhānaviriyādhigatassa jantu;",
        "analyzed": "uṭṭhāna viriya adhigatassa jantu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_424:3.3",
        "original": "Atikkamma so vetaraṇiṁ yamassa,",
        "analyzed": "atikkamma saḥ vetaraṇim yamassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_424:3.4",
        "original": "Dibbāni ṭhānāni upeti macco.",
        "analyzed": "dibbāni ṭhānāni upeti macco ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_424:4.1",
        "original": "Dānañca yuddhañca samānamāhu,",
        "analyzed": "dānam ca yuddhañca samānam āhu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_424:4.2",
        "original": "Appāpi santā bahuke jinanti;",
        "analyzed": "appā api santā bahuke jinanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_424:4.3",
        "original": "Appampi ce saddahāno dadāti,",
        "analyzed": "appam ce saddahānaḥ dadāti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_424:4.4",
        "original": "Teneva so hoti sukhī parattha.",
        "analyzed": "tena iva saḥ hoti sukhī parattha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_424:5.1",
        "original": "Viceyya dānaṁ sugatappasatthaṁ,",
        "analyzed": "viceyya dānam sugata ppasattha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_424:5.2",
        "original": "Ye dakkhiṇeyyā idha jīvaloke;",
        "analyzed": "ye dakkhiṇeyyāḥ idha jīva loke ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_424:5.3",
        "original": "Etesu dinnāni mahapphalāni,",
        "analyzed": "etesu dinnāni mahapphalāni ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_424:5.4",
        "original": "Bījāni vuttāni yathā sukhette.",
        "analyzed": "bījāni vuttāni yathā su khette ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_424:6.1",
        "original": "Yo pāṇabhūtāni aheṭhayaṁ caraṁ,",
        "analyzed": "yaḥ pāṇabhūtāni aheṭhayam cara ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_424:6.2",
        "original": "Parūpavādā na karoti pāpaṁ;",
        "analyzed": "parūpavādā na karoti pāpa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_424:6.3",
        "original": "Bhīruṁ pasaṁsanti na tattha sūraṁ,",
        "analyzed": "bhīrum pasasanti na tattha sūra ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_424:6.4",
        "original": "Bhayā hi santo na karonti pāpaṁ.",
        "analyzed": "bhayā hi santaḥ na karonti pāpa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_424:7.1",
        "original": "Hīnena brahmacariyena,",
        "analyzed": "hīnena brahmacariyena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_424:7.2",
        "original": "khattiye upapajjati;",
        "analyzed": "khattiye upapajjati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_424:7.3",
        "original": "Majjhimena ca devattaṁ,",
        "analyzed": "majjhimena ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_424:7.4",
        "original": "uttamena visujjhati.",
        "analyzed": "uttamena visujjhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_424:8.1",
        "original": "Addhā hi dānaṁ bahudhā pasatthaṁ,",
        "analyzed": "addhā hi dānam bahudhā pasattha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_424:8.2",
        "original": "Dānā ca kho dhammapadaṁva seyyo;",
        "analyzed": "dānā ca kho seyyo ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_424:8.3",
        "original": "Pubbeva hi pubbatareva santo,",
        "analyzed": "pubbā iva hi pubbatarā iva santaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_424:8.4",
        "original": "Nibbānamevajjhagamuṁ sapaññā”ti.",
        "analyzed": "nibbānam eva ajjhagamum sa paññā ati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_424:9.1",
        "original": "Ādittajātakaṁ aṭṭhamaṁ.",
        "analyzed": "āditta jātakam aṭṭhama ",
        "folio": "9"
    }
]