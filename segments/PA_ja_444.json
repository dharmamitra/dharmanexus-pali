        "analyzed": "catu dvāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_444:0.4",
        "original": "6. Kaṇhadīpāyanajātaka",
        "analyzed": "kaṇha dīpāyana jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_444:1.1",
        "original": "“Sattāhamevāhaṁ pasannacitto,",
        "analyzed": "sattāham eva aham pasanna cittaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_444:1.2",
        "original": "Puññatthiko ācariṁ brahmacariyaṁ;",
        "analyzed": "puññatthikaḥ ācarim brahmacariya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_444:1.3",
        "original": "Athāparaṁ yaṁ caritaṁ mamedaṁ,",
        "analyzed": "atha aparam yam caritam mama idam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_444:1.4",
        "original": "Vassāni paññāsa samādhikāni;",
        "analyzed": "vassāni paññāsa samādhikāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_444:1.5",
        "original": "Akāmakovāpi ahaṁ carāmi,",
        "analyzed": "aham carāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_444:1.6",
        "original": "Etena saccena suvatthi hotu;",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_444:1.7",
        "original": "Hataṁ visaṁ jīvatu yaññadatto”.",
        "analyzed": "hatam visam jīvatu yañña dattaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_444:2.1",
        "original": "“Yasmā dānaṁ nābhinandiṁ kadāci,",
        "analyzed": "yasmai dānam na abhinandim kadācid ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_444:2.2",
        "original": "Disvānahaṁ atithiṁ vāsakāle;",
        "analyzed": "disvān aham atithim vāsa kāle ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_444:2.3",
        "original": "Na cāpi me appiyataṁ aveduṁ,",
        "analyzed": "na ca api me appiyatam avedu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_444:2.4",
        "original": "Bahussutā samaṇabrāhmaṇā ca”.",
        "analyzed": "bahu ssutā samaṇa brāhmaṇā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_444:2.5",
        "original": "Akāmakovāpi ahaṁ dadāmi,",
        "analyzed": "aham dadāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_444:2.6",
        "original": "Etena saccena suvatthi hotu;",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_444:2.7",
        "original": "Hataṁ visaṁ jīvatu yaññadatto.",
        "analyzed": "hatam visam jīvatu yañña dattaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_444:3.1",
        "original": "“Āsīviso tāta pahūtatejo,",
        "analyzed": "āsīvisaḥ tāta pahūta tejo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_444:3.2",
        "original": "Yo taṁ adaṁsī sacarā udicca;",
        "analyzed": "yaḥ tam adasī sacarāḥ udid ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_444:3.3",
        "original": "Tasmiñca me appiyatāya ajja,",
        "analyzed": "tasmin ca me appiya tāya ajja ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_444:3.4",
        "original": "Pitari ca te natthi koci viseso;",
        "analyzed": "pitari ca te natthi koci viseso ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_444:3.5",
        "original": "Etena saccena suvatthi hotu,",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_444:3.6",
        "original": "Hataṁ visaṁ jīvatu yaññadatto”.",
        "analyzed": "hatam visam jīvatu yañña dattaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_444:4.1",
        "original": "“Santā dantāyeva paribbajanti,",
        "analyzed": "santāḥ dantāya iva paribbajanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_444:4.2",
        "original": "Aññatra kaṇhā natthākāmarūpā;",
        "analyzed": "aññatra kaṇhā nattha a kāmarūpā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_444:4.3",
        "original": "Dīpāyana kissa jigucchamāno,",
        "analyzed": "dīpāyana kissa jigucchamānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_444:4.4",
        "original": "Akāmako carasi brahmacariyaṁ”.",
        "analyzed": "akāmakaḥ carasi brahmacariya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_444:5.1",
        "original": "“Saddhāya nikkhamma punaṁ nivatto,",
        "analyzed": "saddhāya nikkhamma punar nivattaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_444:5.2",
        "original": "So eḷamūgova bālo vatāyaṁ;",
        "analyzed": "saḥ eḷamūgā uva bālaḥ vatāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_444:5.3",
        "original": "Etassa vādassa jigucchamāno,",
        "analyzed": "etassa vādassa jigucchamānaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_444:5.4",
        "original": "Akāmako carāmi brahmacariyaṁ;",
        "analyzed": "akāmakaḥ carāmi brahmacariya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_444:5.5",
        "original": "Viññuppasatthañca satañca ṭhānaṁ,",
        "analyzed": "viññuppasatthañca satañca ṭhāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_444:5.6",
        "original": "Evampahaṁ puññakaro bhavāmi”.",
        "analyzed": "evampaham puñña karaḥ bhavāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_444:6.1",
        "original": "“Samaṇe tuvaṁ brāhmaṇe addhike ca,",
        "analyzed": "samaṇe tuvam brāhmaṇe addhike ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_444:6.2",
        "original": "Santappayāsi annapānena bhikkhaṁ;",
        "analyzed": "santappayāsi anna pānena bhikkha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_444:6.3",
        "original": "Opānabhūtaṁva gharaṁ tava yidaṁ,",
        "analyzed": "opānabhūtava gharam tava yida ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_444:6.4",
        "original": "Annena pānena upetarūpaṁ;",
        "analyzed": "annena pānena upeta rūpa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_444:6.5",
        "original": "Atha kissa vādassa jigucchamāno,",
        "analyzed": "atha kissa vādassa jigucchamānaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_444:6.6",
        "original": "Akāmako dānamimaṁ dadāsi”.",
        "analyzed": "akāmakaḥ dānam imam dadāsi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_444:7.1",
        "original": "“Pitaro ca me āsuṁ pitāmahā ca,",
        "analyzed": "pitaro ca me āsum pitāmahā ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_444:7.2",
        "original": "Saddhā ahuṁ dānapatī vadaññū;",
        "analyzed": "saddhā ahum dānapatī vadaññū ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_444:7.3",
        "original": "Taṁ kullavattaṁ anuvattamāno,",
        "analyzed": "tam kullavattam anuvattamānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_444:7.4",
        "original": "Māhaṁ kule antimagandhano ahuṁ;",
        "analyzed": "mā aham kule antima gandhanaḥ ahu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_444:7.5",
        "original": "Etassa vādassa jigucchamāno,",
        "analyzed": "etassa vādassa jigucchamānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_444:7.6",
        "original": "Akāmako dānamimaṁ dadāmi”.",
        "analyzed": "akāmakaḥ dānam imam dadāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_444:8.1",
        "original": "“Dahariṁ kumāriṁ asamatthapaññaṁ,",
        "analyzed": "daharim kumārim asamattha pañña ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_444:8.2",
        "original": "Yaṁ tānayiṁ ñātikulā sugatte;",
        "analyzed": "yam tānayim ñāti kulā su gatte ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_444:8.3",
        "original": "Na cāpi me appiyataṁ avedi,",
        "analyzed": "na ca api me appiyatam avedi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_444:8.4",
        "original": "Aññatra kāmā paricārayantā;",
        "analyzed": "aññatra kāmā paricārayantā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_444:8.5",
        "original": "Atha kena vaṇṇena mayā te bhoti,",
        "analyzed": "atha kena vaṇṇena mayā te bhoti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_444:8.6",
        "original": "Saṁvāsadhammo ahu evarūpo”.",
        "analyzed": "sa vāsa dhammaḥ ahu evarūpaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_444:9.1",
        "original": "“Ārā dūre nayidha kadāci atthi,",
        "analyzed": "ārā dūre nayidha kadāci atthi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_444:9.2",
        "original": "Paramparā nāma kule imasmiṁ;",
        "analyzed": "paramparā nāma kule imasmi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_444:9.3",
        "original": "Taṁ kullavattaṁ anuvattamānā,",
        "analyzed": "tam kullavattam anuvattamānā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_444:9.4",
        "original": "Māhaṁ kule antimagandhinī ahuṁ;",
        "analyzed": "mā aham kule antima gandhinī ahu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_444:9.5",
        "original": "Etassa vādassa jigucchamānā,",
        "analyzed": "etassa vādassa jigucchamānā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_444:9.6",
        "original": "Akāmikā paddhacarāmhi tuyhaṁ”.",
        "analyzed": "akāmikā paddhacarāmhi tuyha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_444:10.1",
        "original": "“Maṇḍabya bhāsiṁ yamabhāsaneyyaṁ,",
        "analyzed": "maṇḍabya bhāsim yamabhāsaneyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_444:10.2",
        "original": "Taṁ khamyataṁ puttakahetu majja;",
        "analyzed": "tam puttaka hetu majja ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_444:10.3",
        "original": "Puttapemā na idha paratthi kiñci,",
        "analyzed": "putta pemā na idha paratthi kiñci ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_444:10.4",
        "original": "So no ayaṁ jīvati yaññadatto”ti.",
        "analyzed": "saḥ naḥ ayam jīvati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_444:11.1",
        "original": "Kaṇhadīpāyanajātakaṁ chaṭṭhaṁ.",
        "analyzed": "kaṇhadīpāyanajātakam chaṭṭha ",
        "folio": "11"
    }
]