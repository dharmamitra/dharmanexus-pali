    {
        "segmentnr": "PA_ja_509:1.2",
        "original": "brāhmaṇaṁ devavaṇṇinaṁ;",
        "analyzed": "brāhmaṇam devavaṇṇina ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_509:1.3",
        "original": "Mahājaṭaṁ khāridharaṁ,",
        "analyzed": "mahā jaṭam khāri dhara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_509:1.4",
        "original": "paṅkadantaṁ rajassiraṁ.",
        "analyzed": "paṅka dantam rajaḥ sira ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_509:2.1",
        "original": "Cirassaṁ vata passāma,",
        "analyzed": "cirassam vata passāma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_509:2.2",
        "original": "isiṁ dhammaguṇe rataṁ;",
        "analyzed": "isim dhamma guṇe rata ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_509:2.3",
        "original": "Kāsāyavatthavasanaṁ,",
        "analyzed": "kāsāyavatthavasana ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_509:2.4",
        "original": "vākacīraṁ paṭicchadaṁ.",
        "analyzed": "vāka cīram paṭicchada ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_509:3.1",
        "original": "Āsanaṁ udakaṁ pajjaṁ,",
        "analyzed": "āsanam udakam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_509:3.2",
        "original": "paṭigaṇhātu no bhavaṁ;",
        "analyzed": "paṭigaṇhātu naḥ bhava ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_509:3.3",
        "original": "Agghe bhavantaṁ pucchāma,",
        "analyzed": "agghe bhavantam pucchāma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_509:3.4",
        "original": "agghaṁ kurutu no bhavaṁ”.",
        "analyzed": "aggham kurutu naḥ bhava ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_509:4.1",
        "original": "“Adhicca vede pariyesa vittaṁ,",
        "analyzed": "adhicca vede pariyesa vitta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_509:4.2",
        "original": "Putte gahe tāta patiṭṭhapetvā;",
        "analyzed": "putte gahe tāta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_509:4.3",
        "original": "Gandhe rase paccanubhuyya sabbaṁ,",
        "analyzed": "gandhe rase paccanubhuyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_509:4.4",
        "original": "Araññaṁ sādhu muni so pasattho”.",
        "analyzed": "araññam sādhu muni saḥ pasattho ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_509:5.1",
        "original": "“Vedā na saccā na ca vittalābho,",
        "analyzed": "vedāḥ na ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_509:5.2",
        "original": "Na puttalābhena jaraṁ vihanti;",
        "analyzed": "na putta lābhena jaram vihanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_509:5.3",
        "original": "Gandhe rase muccanamāhu santo,",
        "analyzed": "gandhe rase muccanam āhu santo ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_509:5.4",
        "original": "Sakammunā hoti phalūpapatti”.",
        "analyzed": "sa kammunā hoti phala upapatti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_509:6.1",
        "original": "“Addhā hi saccaṁ vacanaṁ tavetaṁ,",
        "analyzed": "addhā hi saccam vacanam tava ita ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_509:6.2",
        "original": "Sakammunā hoti phalūpapatti;",
        "analyzed": "sa kammunā hoti phala upapatti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_509:6.3",
        "original": "Jiṇṇā ca mātāpitaro tavīme,",
        "analyzed": "jiṇṇā ca mātā pitaraḥ tavi ime ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_509:6.4",
        "original": "Passeyyuṁ taṁ vassasataṁ arogaṁ”.",
        "analyzed": "passeyyum tam vassa satam aroga ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_509:7.1",
        "original": "“Yassassa sakkhī maraṇena rāja,",
        "analyzed": "sakkhī maraṇena rāja ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_509:7.2",
        "original": "Jarāya mettī naravīraseṭṭha;",
        "analyzed": "jarāya mettī nara vīra seṭṭha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_509:7.3",
        "original": "Yo cāpi jaññā na marissaṁ kadāci,",
        "analyzed": "yo ca api jaññā na marissam kadāci ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_509:7.4",
        "original": "Passeyyuṁ taṁ vassasataṁ arogaṁ.",
        "analyzed": "passeyyum tam vassa satam aroga ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_509:8.1",
        "original": "Yathāpi nāvaṁ puriso dakamhi,",
        "analyzed": "yathā api nāvam purisaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_509:8.2",
        "original": "Ereti ce naṁ upaneti tīraṁ;",
        "analyzed": "ce naṃ upanā iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_509:8.3",
        "original": "Evampi byādhī satataṁ jarā ca,",
        "analyzed": "byādhī satatam jarā ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_509:8.4",
        "original": "Upaneti maccaṁ vasamantakassa”.",
        "analyzed": "upaneti maccam vasamantakassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_509:9.1",
        "original": "“Paṅko ca kāmā palipo ca kāmā,",
        "analyzed": "paṅko ca kāmā palipo ca kāmā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_509:9.2",
        "original": "Manoharā duttarā maccudheyyā;",
        "analyzed": "manoharā duttarā maccudheyyā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_509:9.3",
        "original": "Etasmiṁ paṅke palipe byasannā,",
        "analyzed": "etasmin paṅke palipe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_509:9.4",
        "original": "Hīnattarūpā na taranti pāraṁ.",
        "analyzed": "na taranti pāra ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_509:10.1",
        "original": "Ayaṁ pure luddamakāsi kammaṁ,",
        "analyzed": "ayam pure kamma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_509:10.2",
        "original": "Svāyaṁ gahīto na hi mokkhito me;",
        "analyzed": "su āyam gahītaḥ na hi mokkhitaḥ me ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_509:10.3",
        "original": "Orundhiyā naṁ parirakkhissāmi,",
        "analyzed": "orundhiyā nam parirakkhissāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_509:10.4",
        "original": "Māyaṁ puna luddamakāsi kammaṁ”.",
        "analyzed": "mā ayam puna luddam akāsi kamma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_509:11.1",
        "original": "“Gavaṁva naṭṭhaṁ puriso yathā vane,",
        "analyzed": "naṭṭham purisaḥ yathā vane ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_509:11.2",
        "original": "Anvesatī rāja apassamāno;",
        "analyzed": "anvesatī rāja apassamāno ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_509:11.3",
        "original": "Evaṁ naṭṭho esukārī mamattho,",
