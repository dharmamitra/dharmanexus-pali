        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_1.3:40.1",
        "original": "“Rāgañca dosañca pahāya mohaṁ,",
        "analyzed": "rāgañca dosañca pahāya moha ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_1.3:40.2",
