        "folio": "55"
    },
    {
        "segmentnr": "PA_ja_526:56.1",
        "original": "Bhūtāni hetāni caranti tāta,",
        "analyzed": "bhūtāni ha itāni caranti tāta ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_ja_526:56.2",
        "original": "Virūparūpena manussaloke;",
        "analyzed": "virūpa rūpena manussa loke ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_ja_526:56.3",
        "original": "Na tāni sevetha naro sapañño,",
