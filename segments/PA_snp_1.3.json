        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_1.3:31.3",
        "original": "Kule kule appaṭibaddhacitto,",
        "analyzed": "kule kule appaṭibaddha cittaḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_1.3:31.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_1.3:32.1",
        "original": "“Pahāya pañcāvaraṇāni cetaso,",
        "analyzed": "pahāya pañca āvaraṇāni cetasaḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_1.3:32.2",
        "original": "Upakkilese byapanujja sabbe;",
        "analyzed": "upakkilese byapanujja sabbe ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_1.3:32.3",
        "original": "Anissito chetva sinehadosaṁ,",
        "analyzed": "a nissito chetva sineha dosa ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_1.3:32.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_1.3:33.1",
        "original": "“Vipiṭṭhikatvāna sukhaṁ dukhañca,",
        "analyzed": "vipiṭṭhika tvāna sukham dukham ca ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_1.3:33.2",
        "original": "Pubbeva ca somanassadomanassaṁ;",
        "analyzed": "pubbā iva ca ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_1.3:33.3",
        "original": "Laddhānupekkhaṁ samathaṁ visuddhaṁ,",
        "analyzed": "laddha anupekkham samatham visuddha ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_1.3:33.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_1.3:34.1",
        "original": "“Āraddhaviriyo paramatthapattiyā,",
        "analyzed": "āraddha viriyaḥ paramattha pattiyā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_1.3:34.2",
        "original": "Alīnacitto akusītavutti;",
        "analyzed": "alīna cittaḥ ",
        "folio": "34"
    },
    {
