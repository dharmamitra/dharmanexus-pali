[
    {
        "segmentnr": "PA_snp_5.1:0.1",
        "original": "Sutta Nipāta 5.1",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.1:0.2",
        "original": "Vatthugāthā",
        "analyzed": "vatthu gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.1:1.1",
        "original": "Kosalānaṁ purā rammā,",
        "analyzed": "kosalānam purā rammā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.1:1.2",
        "original": "agamā dakkhiṇāpathaṁ;",
        "analyzed": "agamā dakkhiṇāpatha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.1:1.3",
        "original": "Ākiñcaññaṁ patthayāno,",
        "analyzed": "ākiñcaññam patthayānaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.1:1.4",
        "original": "brāhmaṇo mantapāragū.",
        "analyzed": "brāhmaṇaḥ manta pāragū ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.1:2.1",
        "original": "So assakassa visaye,",
        "analyzed": "saḥ assakassa visaye ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.1:2.2",
        "original": "aḷakassa samāsane;",
        "analyzed": "aḷakassa samāsane ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.1:2.3",
        "original": "Vasi godhāvarīkūle,",
        "analyzed": "vasi godhāvarī kūle ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.1:2.4",
        "original": "uñchena ca phalena ca.",
        "analyzed": "uñchena ca phalena ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.1:3.1",
        "original": "Tasseva upanissāya,",
        "analyzed": "tassa iva upanissāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.1:3.2",
        "original": "gāmo ca vipulo ahu;",
        "analyzed": "gāmaḥ ca vipulaḥ ahu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.1:3.3",
        "original": "Tato jātena āyena,",
        "analyzed": "tatas jātena āyena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.1:3.4",
        "original": "mahāyaññamakappayi.",
        "analyzed": "mahā yaññam akappayi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.1:4.1",
        "original": "Mahāyaññaṁ yajitvāna,",
        "analyzed": "mahāyaññam yajitvāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.1:4.2",
        "original": "puna pāvisi assamaṁ;",
        "analyzed": "puna pāvisi assama ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.1:4.3",
        "original": "Tasmiṁ paṭipaviṭṭhamhi,",
        "analyzed": "tasmin paṭipaviṭṭhamhi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.1:4.4",
        "original": "añño āgañchi brāhmaṇo.",
        "analyzed": "aññaḥ āgañchi brāhmaṇaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.1:5.1",
        "original": "Ugghaṭṭapādo tasito,",
        "analyzed": "ugghaṭṭa pādaḥ tasitaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.1:5.2",
        "original": "paṅkadanto rajassiro;",
        "analyzed": "paṅka dantaḥ rajaḥ siraḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.1:5.3",
        "original": "So ca naṁ upasaṅkamma,",
        "analyzed": "saḥ ca nam upasaṅkamma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.1:5.4",
        "original": "satāni pañca yācati.",
        "analyzed": "satāni pañca yācati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.1:6.1",
        "original": "Tamenaṁ bāvarī disvā,",
        "analyzed": "tam enam bāvarī disvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.1:6.2",
        "original": "āsanena nimantayi;",
        "analyzed": "āsanena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.1:6.3",
        "original": "Sukhañca kusalaṁ pucchi,",
        "analyzed": "sukham ca kusalam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.1:6.4",
        "original": "idaṁ vacanamabravi.",
        "analyzed": "idam vacanam abravi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.1:7.1",
        "original": "“Yaṁ kho mama deyyadhammaṁ,",
        "analyzed": "yam kho mama ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.1:7.2",
        "original": "Sabbaṁ visajjitaṁ mayā;",
        "analyzed": "sabbam visajjitam mayā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.1:7.3",
        "original": "Anujānāhi me brahme,",
        "analyzed": "anujānāhi me brahme ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.1:7.4",
        "original": "Natthi pañcasatāni me”.",
        "analyzed": "natthi pañca satāni me ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.1:8.1",
        "original": "“Sace me yācamānassa,",
        "analyzed": "sace me yācamānassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.1:8.2",
        "original": "bhavaṁ nānupadassati;",
        "analyzed": "bhavam na an upadassati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.1:8.3",
        "original": "Sattame divase tuyhaṁ,",
        "analyzed": "sattame divase tuyha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.1:8.4",
        "original": "muddhā phalatu sattadhā”.",
        "analyzed": "muddhā phalatu sattadhā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.1:9.1",
        "original": "Abhisaṅkharitvā kuhako,",
        "analyzed": "abhisaṅkharitvā kuhakaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.1:9.2",
        "original": "bheravaṁ so akittayi;",
        "analyzed": "bheravam saḥ akittayi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.1:9.3",
        "original": "Tassa taṁ vacanaṁ sutvā,",
        "analyzed": "tassa tam vacanam sutvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.1:9.4",
        "original": "bāvarī dukkhito ahu.",
        "analyzed": "bāvarī dukkhitaḥ ahu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.1:10.1",
        "original": "Ussussati anāhāro,",
        "analyzed": "ussussati anāhāraḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.1:10.2",
        "original": "Sokasallasamappito;",
        "analyzed": "soka salla samappito ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.1:10.3",
        "original": "Athopi evaṁ cittassa,",
        "analyzed": "evam cittassa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.1:10.4",
        "original": "Jhāne na ramatī mano.",
        "analyzed": "jhāne na ramatī mano ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.1:11.1",
        "original": "Utrastaṁ dukkhitaṁ disvā,",
        "analyzed": "utrastam dukkhitam disvā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.1:11.2",
        "original": "Devatā atthakāminī;",
        "analyzed": "devatā attha kāminī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.1:11.3",
        "original": "Bāvariṁ upasaṅkamma,",
        "analyzed": "bāvarim upasaṅkamma ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.1:11.4",
        "original": "Idaṁ vacanamabravi.",
        "analyzed": "idam vacanam abravi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.1:12.1",
        "original": "“Na so muddhaṁ pajānāti,",
        "analyzed": "na saḥ muddham pajānāti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.1:12.2",
        "original": "Kuhako so dhanatthiko;",
        "analyzed": "kuhakaḥ so dhanatthiko ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.1:12.3",
        "original": "Muddhani muddhapāte vā,",
        "analyzed": "muddhani muddha pāte vā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.1:12.4",
        "original": "Ñāṇaṁ tassa na vijjati”.",
        "analyzed": "ñāṇam tassa na vijjati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.1:13.1",
        "original": "“Bhotī carahi jānāsi,",
        "analyzed": "bhotī carahi jānāsi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.1:13.2",
        "original": "Taṁ me akkhāhi pucchitā;",
        "analyzed": "tam me akkhāhi pucchitā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.1:13.3",
        "original": "Muddhaṁ muddhādhipātañca,",
        "analyzed": "muddham muddha adhipātam ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.1:13.4",
        "original": "Taṁ suṇoma vaco tava”.",
        "analyzed": "tam suṇoma vacaḥ tava ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_5.1:14.1",
        "original": "“Ahampetaṁ na jānāmi,",
        "analyzed": "aham petam na jānāmi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.1:14.2",
        "original": "Ñāṇamettha na vijjati;",
        "analyzed": "ñāṇam ettha na vijjati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.1:14.3",
        "original": "Muddhani muddhādhipāte ca,",
        "analyzed": "muddhani muddha adhipāte ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.1:14.4",
        "original": "Jinānaṁ hettha dassanaṁ”.",
        "analyzed": "jinānam ha ittha dassana ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_5.1:15.1",
        "original": "“Atha ko carahi jānāti,",
        "analyzed": "atha kaḥ carahi jānāti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.1:15.2",
        "original": "Asmiṁ pathavimaṇḍale;",
        "analyzed": "asmin pathavi maṇḍale ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.1:15.3",
        "original": "Muddhaṁ muddhādhipātañca,",
        "analyzed": "muddham muddha adhipātam ca ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.1:15.4",
        "original": "Taṁ me akkhāhi devate”.",
        "analyzed": "tam me akkhāhi devate ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_5.1:16.1",
        "original": "“Purā kapilavatthumhā,",
        "analyzed": "purā kapilavatthumhā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.1:16.2",
        "original": "Nikkhanto lokanāyako;",
        "analyzed": "nikkhantaḥ loka nāyako ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.1:16.3",
        "original": "Apacco okkākarājassa,",
        "analyzed": "a paccaḥ okkāka rājassa ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.1:16.4",
        "original": "Sakyaputto pabhaṅkaro.",
        "analyzed": "sakya putto pabhaṅkaro ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_5.1:17.1",
        "original": "So hi brāhmaṇa sambuddho,",
        "analyzed": "saḥ hi brāhmaṇa sambuddhaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.1:17.2",
        "original": "Sabbadhammāna pāragū;",
        "analyzed": "sabba dhammāna pāragū ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.1:17.3",
        "original": "Sabbābhiññābalappatto,",
        "analyzed": "sabba abhiññā bala ppatto ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.1:17.4",
        "original": "Sabbadhammesu cakkhumā;",
        "analyzed": "sabba dhammesu cakkhumā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.1:17.5",
        "original": "Sabbakammakkhayaṁ patto,",
        "analyzed": "sabba kamma kkhayam patto ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.1:17.6",
        "original": "Vimutto upadhikkhaye.",
        "analyzed": "vimuttaḥ upadhi khaye ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_5.1:18.1",
        "original": "Buddho so bhagavā loke,",
        "analyzed": "buddhaḥ saḥ bhagavā loke ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.1:18.2",
        "original": "Dhammaṁ deseti cakkhumā;",
        "analyzed": "dhammaṃ deseti cakkhumā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.1:18.3",
        "original": "Taṁ tvaṁ gantvāna pucchassu,",
        "analyzed": "tam tvam gantvāna pucchassu ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.1:18.4",
        "original": "So te taṁ byākarissati”.",
        "analyzed": "saḥ te tam byākarissati ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_5.1:19.1",
        "original": "Sambuddhoti vaco sutvā,",
        "analyzed": "sambuddhā iti vacaḥ sutvā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.1:19.2",
        "original": "Udaggo bāvarī ahu;",
        "analyzed": "udaggaḥ bāvarī ahu ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.1:19.3",
        "original": "Sokassa tanuko āsi,",
        "analyzed": "sokassa tanukaḥ āsi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.1:19.4",
        "original": "Pītiñca vipulaṁ labhi.",
        "analyzed": "pītim ca vipulam ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_5.1:20.1",
        "original": "So bāvarī attamano udaggo,",
        "analyzed": "saḥ bāvarī atta manaḥ udaggaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.1:20.2",
        "original": "Taṁ devataṁ pucchati vedajāto;",
        "analyzed": "tam devatam pucchati veda jātaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.1:20.3",
        "original": "“Katamamhi gāme nigamamhi vā pana,",
        "analyzed": "katamamhi gāme nigamamhi vā pana ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.1:20.4",
        "original": "Katamamhi vā janapade lokanātho;",
        "analyzed": "katamamhi vā janapade loka nāthaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.1:20.5",
        "original": "Yattha gantvāna passemu,",
        "analyzed": "yattha gantvāna passemu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.1:20.6",
        "original": "Sambuddhaṁ dvipaduttamaṁ”.",
        "analyzed": "sambuddham dvipad uttama ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_5.1:21.1",
        "original": "“Sāvatthiyaṁ kosalamandire jino,",
        "analyzed": "sāvatthiyam kosala mandire jinaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.1:21.2",
        "original": "Pahūtapañño varabhūrimedhaso;",
        "analyzed": "pahūta paññaḥ vara bhūri medhasaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.1:21.3",
        "original": "So sakyaputto vidhuro anāsavo,",
        "analyzed": "saḥ sakya puttaḥ vidhuraḥ anāsavaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.1:21.4",
        "original": "Muddhādhipātassa vidū narāsabho”.",
        "analyzed": "muddha adhipātassa vidū narāsabhaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_5.1:22.1",
        "original": "Tato āmantayī sisse,",
        "analyzed": "tatas āmantayī sisse ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.1:22.2",
        "original": "Brāhmaṇe mantapārage;",
        "analyzed": "brāhmaṇe mantapārage ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.1:22.3",
        "original": "“Etha māṇavā akkhissaṁ,",
        "analyzed": "etha māṇavā akkhissa ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.1:22.4",
        "original": "Suṇātha vacanaṁ mama.",
        "analyzed": "suṇātha vacanam mama ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_5.1:23.1",
        "original": "Yasseso dullabho loke,",
        "analyzed": "yaḥ sesaḥ dullabhaḥ loke ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.1:23.2",
        "original": "Pātubhāvo abhiṇhaso;",
        "analyzed": "pātu bhāvaḥ abhiṇhasaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.1:23.3",
        "original": "Svājja lokamhi uppanno,",
        "analyzed": "svājja lokamhi uppannaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.1:23.4",
        "original": "Sambuddho iti vissuto;",
        "analyzed": "sambuddhaḥ iti vissutaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.1:23.5",
        "original": "Khippaṁ gantvāna sāvatthiṁ,",
        "analyzed": "khippam gantvāna sāvatthi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.1:23.6",
        "original": "Passavho dvipaduttamaṁ”.",
        "analyzed": "passavhaḥ dvipad uttama ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_5.1:24.1",
        "original": "“Kathaṁ carahi jānemu,",
        "analyzed": "katham carahi jānemu ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.1:24.2",
        "original": "Disvā buddhoti brāhmaṇa;",
        "analyzed": "disvā buddha uti brāhmaṇa ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.1:24.3",
        "original": "Ajānataṁ no pabrūhi,",
        "analyzed": "a jānatam naḥ upabrūhi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.1:24.4",
        "original": "Yathā jānemu taṁ mayaṁ”.",
        "analyzed": "yathā tam maya ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_5.1:25.1",
        "original": "“Āgatāni hi mantesu,",
        "analyzed": "āgatāni hi mantesu ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.1:25.2",
        "original": "Mahāpurisalakkhaṇā;",
        "analyzed": "mahāpurisa lakkhaṇā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.1:25.3",
        "original": "Dvattiṁsāni ca byākkhātā,",
        "analyzed": "dvattisāni ca byākkhātā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.1:25.4",
        "original": "Samattā anupubbaso.",
        "analyzed": "samattāḥ anupubbaso ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_5.1:26.1",
        "original": "Yassete honti gattesu,",
        "analyzed": "yassa ete honti gattesu ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.1:26.2",
        "original": "Mahāpurisalakkhaṇā;",
        "analyzed": "mahāpurisa lakkhaṇā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.1:26.3",
        "original": "Dveyeva tassa gatiyo,",
        "analyzed": "dveyā iva tassa gatiyaḥ ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.1:26.4",
        "original": "Tatiyā hi na vijjati.",
        "analyzed": "tatiyā hi na vijjati ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_5.1:27.1",
        "original": "Sace agāraṁ āvasati,",
        "analyzed": "sace agāram āvasati ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.1:27.2",
        "original": "Vijeyya pathaviṁ imaṁ;",
        "analyzed": "vijeyya pathavim ima ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.1:27.3",
        "original": "Adaṇḍena asatthena,",
        "analyzed": "a daṇḍena a satthena ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.1:27.4",
        "original": "Dhammena manusāsati.",
        "analyzed": "dhammena manusāsati ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_5.1:28.1",
        "original": "Sace ca so pabbajati,",
        "analyzed": "sace ca so pabbajati ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.1:28.2",
        "original": "Agārā anagāriyaṁ;",
        "analyzed": "agārāḥ anagāriya ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.1:28.3",
        "original": "Vivaṭṭacchado sambuddho,",
        "analyzed": "vivaṭṭacchadaḥ sambuddhaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.1:28.4",
        "original": "Arahā bhavati anuttaro.",
        "analyzed": "arahā bhavati anuttaraḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_5.1:29.1",
        "original": "Jātiṁ gottañca lakkhaṇaṁ,",
        "analyzed": "jātim gottam ca lakkhaṇa ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.1:29.2",
        "original": "Mante sisse punāpare;",
        "analyzed": "mante sisse punā apare ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.1:29.3",
        "original": "Muddhaṁ muddhādhipātañca,",
        "analyzed": "muddham muddha adhipātam ca ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.1:29.4",
        "original": "Manasāyeva pucchatha.",
        "analyzed": "manasāya iva pucchatha ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_5.1:30.1",
        "original": "Anāvaraṇadassāvī,",
        "analyzed": "an āvaraṇa dassāvī ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.1:30.2",
        "original": "Yadi buddho bhavissati;",
        "analyzed": "yadi buddhaḥ bhavissati ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.1:30.3",
        "original": "Manasā pucchite pañhe,",
        "analyzed": "manasā pucchite pañhe ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.1:30.4",
        "original": "Vācāya vissajessati”.",
        "analyzed": "vācāya vissajessati ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_5.1:31.1",
        "original": "Bāvarissa vaco sutvā,",
        "analyzed": "bāvarissa vacaḥ sutvā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.1:31.2",
        "original": "Sissā soḷasa brāhmaṇā;",
        "analyzed": "sissā soḷasa brāhmaṇā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.1:31.3",
        "original": "Ajito tissametteyyo,",
        "analyzed": "ajitaḥ tissametteyyaḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.1:31.4",
        "original": "Puṇṇako atha mettagū.",
        "analyzed": "puṇṇakaḥ atha mettagū ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_5.1:32.1",
        "original": "Dhotako upasīvo ca,",
        "analyzed": "dhotakaḥ upasīvaḥ ca ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.1:32.2",
        "original": "Nando ca atha hemako;",
        "analyzed": "nandaḥ ca atha hemakaḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.1:32.3",
        "original": "Todeyya-kappā dubhayo,",
        "analyzed": "todeyya kappā dubhayaḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.1:32.4",
        "original": "Jatukaṇṇī ca paṇḍito.",
        "analyzed": "jatukaṇṇī ca paṇḍitaḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_5.1:33.1",
        "original": "Bhadrāvudho udayo ca,",
        "analyzed": "bhadrāvudhaḥ udayaḥ ca ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_5.1:33.2",
        "original": "Posālo cāpi brāhmaṇo;",
        "analyzed": "posālo ca api brāhmaṇaḥ ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_5.1:33.3",
        "original": "Mogharājā ca medhāvī,",
        "analyzed": "mogha rājā ca medhāvī ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_5.1:33.4",
        "original": "Piṅgiyo ca mahāisi.",
        "analyzed": "piṅgiyaḥ ca mahā aisi ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_5.1:34.1",
        "original": "Paccekagaṇino sabbe,",
        "analyzed": "paccekagaṇinaḥ sabbe ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.1:34.2",
        "original": "Sabbalokassa vissutā;",
        "analyzed": "sabba lokassa vissutā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.1:34.3",
        "original": "Jhāyī jhānaratā dhīrā,",
        "analyzed": "jhāyī jhāna ratā dhīrā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.1:34.4",
        "original": "Pubbavāsanavāsitā.",
        "analyzed": "pubbavāsana vāsi tā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_5.1:35.1",
        "original": "Bāvariṁ abhivādetvā,",
        "analyzed": "bāvarim abhivādetvā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.1:35.2",
        "original": "Katvā ca naṁ padakkhiṇaṁ;",
        "analyzed": "katvā ca nam padakkhiṇa ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.1:35.3",
        "original": "Jaṭājinadharā sabbe,",
        "analyzed": "jaṭā ajina dharā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.1:35.4",
        "original": "Pakkāmuṁ uttarāmukhā.",
        "analyzed": "uttarā mukhā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_5.1:36.1",
        "original": "Aḷakassa patiṭṭhānaṁ,",
        "analyzed": "aḷakassa patiṭṭhāna ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.1:36.2",
        "original": "Purimāhissatiṁ tadā;",
        "analyzed": "tadā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.1:36.3",
        "original": "Ujjeniñcāpi gonaddhaṁ,",
        "analyzed": "ujjeniñca api gonaddha ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.1:36.4",
        "original": "Vedisaṁ vanasavhayaṁ.",
        "analyzed": "vedisam vanasavhaya ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_5.1:37.1",
        "original": "Kosambiñcāpi sāketaṁ,",
        "analyzed": "kosambiñca api sāketa ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.1:37.2",
        "original": "Sāvatthiñca puruttamaṁ;",
        "analyzed": "sāvatthiñca puruttama ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.1:37.3",
        "original": "Setabyaṁ kapilavatthuṁ,",
        "analyzed": "setabyam kapilavatthu ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.1:37.4",
        "original": "Kusinārañca mandiraṁ.",
        "analyzed": "kusinārañca mandira ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_5.1:38.1",
        "original": "Pāvañca bhoganagaraṁ,",
        "analyzed": "bhoganagara ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.1:38.2",
        "original": "Vesāliṁ māgadhaṁ puraṁ;",
        "analyzed": "vesālim māgadham pura ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.1:38.3",
        "original": "Pāsāṇakaṁ cetiyañca,",
        "analyzed": "pāsāṇakam cetiyat ca ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.1:38.4",
        "original": "Ramaṇīyaṁ manoramaṁ.",
        "analyzed": "ramaṇīyam manorama ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_5.1:39.1",
        "original": "Tasitovudakaṁ sītaṁ,",
        "analyzed": "sīta ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.1:39.2",
        "original": "Mahālābhaṁva vāṇijo;",
        "analyzed": "vāṇijo ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.1:39.3",
        "original": "Chāyaṁ ghammābhitattova,",
        "analyzed": "chāyam ghamma abhitattaḥ uva ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.1:39.4",
        "original": "Turitā pabbatamāruhuṁ.",
        "analyzed": "turitā pabbatam āruhu ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_5.1:40.1",
        "original": "Bhagavā tamhi samaye,",
        "analyzed": "bhagavā tamhi samaye ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_5.1:40.2",
        "original": "Bhikkhusaṅghapurakkhato;",
        "analyzed": "bhikkhu saṅgha purakkhato ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_5.1:40.3",
        "original": "Bhikkhūnaṁ dhammaṁ deseti,",
        "analyzed": "bhikkhūnam dhammaṃ deseti ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_5.1:40.4",
        "original": "Sīhova nadatī vane.",
        "analyzed": "sīhā uva nadatī vane ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_5.1:41.1",
        "original": "Ajito addasa buddhaṁ,",
        "analyzed": "ajitaḥ addasa buddha ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_5.1:41.2",
        "original": "Sataraṁsiṁva bhāṇumaṁ;",
        "analyzed": "satarasiva ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_5.1:41.3",
        "original": "Candaṁ yathā pannarase,",
        "analyzed": "candam yathā pannarase ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_5.1:41.4",
        "original": "Pāripūriṁ upāgataṁ.",
        "analyzed": "pāripūrim upāgata ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_5.1:42.1",
        "original": "Athassa gatte disvāna,",
        "analyzed": "athassa gatte disvāna ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_5.1:42.2",
        "original": "Paripūrañca byañjanaṁ;",
        "analyzed": "paripūran ca byañjana ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_5.1:42.3",
        "original": "Ekamantaṁ ṭhito haṭṭho,",
        "analyzed": "eka mantam ṭhitaḥ haṭṭhaḥ ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_5.1:42.4",
        "original": "Manopañhe apucchatha.",
        "analyzed": "manaḥ pañhe apucchatha ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_5.1:43.1",
        "original": "“Ādissa jammanaṁ brūhi,",
        "analyzed": "ādissa jammanam brūhi ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_5.1:43.2",
        "original": "Gottaṁ brūhi salakkhaṇaṁ;",
        "analyzed": "gottam brūhi salakkhaṇa ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_5.1:43.3",
        "original": "Mantesu pāramiṁ brūhi,",
        "analyzed": "mantesu pāramim brūhi ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_5.1:43.4",
        "original": "Kati vāceti brāhmaṇo”.",
        "analyzed": "kati vācā iti brāhmaṇaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_5.1:44.1",
        "original": "“Vīsaṁ vassasataṁ āyu,",
        "analyzed": "vīsam vassa satam āyu ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_5.1:44.2",
        "original": "So ca gottena bāvarī;",
        "analyzed": "sā u ca gottena bāvarī ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_5.1:44.3",
        "original": "Tīṇissa lakkhaṇā gatte,",
        "analyzed": "tīṇissa lakkhaṇā gatte ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_5.1:44.4",
        "original": "Tiṇṇaṁ vedāna pāragū.",
        "analyzed": "tiṇṇam pāragū ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_5.1:45.1",
        "original": "Lakkhaṇe itihāse ca,",
        "analyzed": "lakkhaṇe itihāse ca ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_5.1:45.2",
        "original": "Sanighaṇḍusakeṭubhe;",
        "analyzed": "sa nighaṇḍu sa keṭubhe ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_5.1:45.3",
        "original": "Pañcasatāni vāceti,",
        "analyzed": "pañca satāni vācā iti ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_5.1:45.4",
        "original": "Sadhamme pāramiṁ gato”.",
        "analyzed": "sadhamme pāramim gataḥ ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_5.1:46.1",
        "original": "“Lakkhaṇānaṁ pavicayaṁ,",
        "analyzed": "lakkhaṇānam pavicaya ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_5.1:46.2",
        "original": "Bāvarissa naruttama;",
        "analyzed": "bāvarissa naruttama ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_5.1:46.3",
        "original": "Kaṅkhacchida pakāsehi,",
        "analyzed": "kaṅkha chida pakāsehi ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_5.1:46.4",
        "original": "Mā no kaṅkhāyitaṁ ahu”.",
        "analyzed": "mā naḥ kaṅkhāyitam ahu ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_5.1:47.1",
        "original": "“Mukhaṁ jivhāya chādeti,",
        "analyzed": "mukham jivhāya chāda iti ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_5.1:47.2",
        "original": "Uṇṇassa bhamukantare;",
        "analyzed": "uṇṇassa bha muka antare ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_5.1:47.3",
        "original": "Kosohitaṁ vatthaguyhaṁ,",
        "analyzed": "kosohitam ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_5.1:47.4",
        "original": "Evaṁ jānāhi māṇava”.",
        "analyzed": "evam jānāhi māṇava ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_5.1:48.1",
        "original": "Pucchañhi kiñci asuṇanto,",
        "analyzed": "pucchan hi kiñci a suṇanto ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_5.1:48.2",
        "original": "Sutvā pañhe viyākate;",
        "analyzed": "sutvā pañhe viyākate ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_5.1:48.3",
        "original": "Vicinteti jano sabbo,",
        "analyzed": "vicinteti jano sabbo ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_5.1:48.4",
        "original": "Vedajāto katañjalī.",
        "analyzed": "veda jātaḥ katañjalī ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_5.1:49.1",
        "original": "“Ko nu devo vā brahmā vā,",
        "analyzed": "kaḥ nu devaḥ vā brahmā vā ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_5.1:49.2",
        "original": "Indo vāpi sujampati;",
        "analyzed": "indo vā api su jampati ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_5.1:49.3",
        "original": "Manasā pucchite pañhe,",
        "analyzed": "manasā pucchite pañhe ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_5.1:49.4",
        "original": "Kametaṁ paṭibhāsati”.",
        "analyzed": "kam etam paṭibhāsati ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_5.1:50.1",
        "original": "“Muddhaṁ muddhādhipātañca,",
        "analyzed": "muddham muddha adhipātam ca ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_5.1:50.2",
        "original": "Bāvarī paripucchati;",
        "analyzed": "bāvarī paripucchati ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_5.1:50.3",
        "original": "Taṁ byākarohi bhagavā,",
        "analyzed": "tam byākarohi bhagavā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_5.1:50.4",
        "original": "Kaṅkhaṁ vinaya no ise”.",
        "analyzed": "kaṅkham vinaya naḥ ise ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_5.1:51.1",
        "original": "“Avijjā muddhāti jānāhi,",
        "analyzed": "avijjā muddhā ati jānāhi ",
        "folio": "51"
    },
