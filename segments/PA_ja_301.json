    },
    {
        "segmentnr": "PA_ja_301:1.1",
        "original": "“Vivarathimāsaṁ dvāraṁ,",
        "analyzed": "vivarathi māsam dvāra ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_301:1.2",
        "original": "Nagaraṁ pavisantu aruṇarājassa;",
        "analyzed": "nagaram pavisantu aruṇa rājassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_301:1.3",
        "original": "Sīhena susiṭṭhena,",
        "analyzed": "sīhena susiṭṭhena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_301:1.4",
        "original": "Surakkhitaṁ nandisenena”.",
        "analyzed": "su rakkhitam nandisenena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_301:2.1",
        "original": "“Jayo kaliṅgānamasayhasāhinaṁ,",
        "analyzed": "jayo kaliṅgān amasayha sāhina ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_301:2.2",
        "original": "Parājayo anayo assakānaṁ;",
        "analyzed": "parājayaḥ anayaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_301:2.3",
        "original": "Icceva te bhāsitaṁ brahmacāri,",
        "analyzed": "id ca iva te bhāsitam brahmacāri ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_301:2.4",
        "original": "Na ujjubhūtā vitathaṁ bhaṇanti”.",
        "analyzed": "na ujjubhūtāḥ vitatham bhaṇanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_301:3.1",
        "original": "“Devā musāvādamupātivattā,",
        "analyzed": "devāḥ musāvādam upātivattā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_301:3.2",
        "original": "Saccaṁ dhanaṁ paramaṁ tesu sakka;",
        "analyzed": "saccam dhanam paramam tesu sakka ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_301:3.3",
        "original": "Taṁ te musā bhāsitaṁ devarāja,",
        "analyzed": "tam te musā bhāsitam devarāja ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_301:3.4",
        "original": "Kiṁ vā paṭicca maghavā mahinda”.",
        "analyzed": "kim vā paṭicca maghavā mahinda ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_301:4.1",
        "original": "“Nanu te sutaṁ brāhmaṇa bhaññamāne,",
        "analyzed": "nanu te sutam brāhmaṇa bhaññamāne ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_301:4.2",
        "original": "Devā na issanti purisaparakkamassa;",
        "analyzed": "devāḥ naḥ issanti purisa parakkamassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_301:4.3",
        "original": "Damo samādhi manaso abhejjo,",
        "analyzed": "damaḥ manasaḥ abhejjaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_301:4.4",
        "original": "Abyaggatā nikkamanañca kāle;",
        "analyzed": "a byagga tā nikkamanañca kāle ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_301:4.5",
        "original": "Daḷhañca viriyaṁ purisaparakkamo ca,",
        "analyzed": "daḷham ca viriyam purisa parakkamo ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_301:4.6",
        "original": "Teneva āsi vijayo assakānan”ti.",
        "analyzed": "tena iva āsi vijayaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_301:5.1",
        "original": "Cūḷakāliṅgajātakaṁ paṭhamaṁ.",
