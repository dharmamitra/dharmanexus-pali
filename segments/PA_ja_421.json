        "analyzed": "na tam tapati ātapo ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_421:3.1",
        "original": "“Na maṁ tapati ātapo,",
        "analyzed": "na mama tapati ātapo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_421:3.2",
        "original": "ātapā tapayanti maṁ;",
        "analyzed": "ātapā tapayanti ma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_421:3.3",
        "original": "Atthā hi vividhā rāja,",
        "analyzed": "atthā hi vividhā rāja ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_421:3.4",
        "original": "te tapanti na ātapo”.",
        "analyzed": "te tapanti naḥ ātapo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_421:4.1",
        "original": "“Addasaṁ kāma te mūlaṁ,",
        "analyzed": "addasam kāma te mūla ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_421:4.2",
        "original": "saṅkappā kāma jāyasi;",
        "analyzed": "saṅkappā kāma jāyasi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_421:4.3",
        "original": "Na taṁ saṅkappayissāmi,",
        "analyzed": "na tam saṅkappayissāmi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_421:4.4",
