    },
    {
        "segmentnr": "PA_ja_386:1.1",
        "original": "“Saccaṁ kirevamāhaṁsu,",
        "analyzed": "sat cam kira ivam āhasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_386:1.2",
        "original": "vastaṁ bāloti paṇḍitā;",
        "analyzed": "paṇḍitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_386:1.3",
        "original": "Passa bālo rahokammaṁ,",
        "analyzed": "passa bālaḥ rahokamma ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_386:1.4",
        "original": "āvikubbaṁ na bujjhati”.",
        "analyzed": "āvikubbam na bujjhati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_386:2.1",
        "original": "“Tvaṁ khopi samma bālosi,",
        "analyzed": "tvam kho upi samma bālā asi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_386:2.2",
        "original": "kharaputta vijānahi;",
        "analyzed": "kharaputta vijānahi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_386:2.3",
        "original": "Rajjuyā hi parikkhitto,",
        "analyzed": "rajjuyā hi parikkhittaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_386:2.4",
        "original": "vaṅkoṭṭho ohitomukho.",
        "analyzed": "vaṅkoṭṭhaḥ ohitomukhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_386:3.1",
        "original": "Aparampi samma te bālyaṁ,",
        "analyzed": "samma te bālya ",
