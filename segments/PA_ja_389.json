    },
    {
        "segmentnr": "PA_ja_389:0.4",
        "original": "4. Suvaṇṇakakkaṭajātaka",
        "analyzed": "      ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_389:1.1",
        "original": "“Siṅgīmigo āyatacakkhunetto,",
        "analyzed": "siṅgīmigaḥ āyata cakkhu nettaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_389:1.2",
        "original": "Aṭṭhittaco vārisayo alomo;",
        "analyzed": "aṭṭhittacaḥ vārisayaḥ alomo ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_389:1.3",
        "original": "Tenābhibhūto kapaṇaṁ rudāmi,",
        "analyzed": "tena abhibhūtaḥ kapaṇam rudāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_389:1.4",
        "original": "Hare sakhā kissa nu maṁ jahāsi”.",
        "analyzed": "hare sakhā kissa nu mama jahāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_389:2.1",
        "original": "So passasanto mahatā phaṇena,",
        "analyzed": "saḥ passasantaḥ mahatā phaṇena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_389:2.2",
        "original": "Bhujaṅgamo kakkaṭamajjhapatto;",
        "analyzed": "bhujaṅgamaḥ kakkaṭa majjha patto ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_389:2.3",
        "original": "Sakhā sakhāraṁ paritāyamāno,",
        "analyzed": "sakhā sakhāram paritāyamānaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_389:2.4",
        "original": "Bhujaṅgamaṁ kakkaṭako gahesi.",
        "analyzed": "bhujaṅgamam kakkaṭakaḥ gahesi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_389:3.1",
        "original": "“Na vāyasaṁ no pana kaṇhasappaṁ,",
        "analyzed": "na vāyasam no pana kaṇhasappa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_389:3.2",
        "original": "Ghāsatthiko kakkaṭako adeyya;",
        "analyzed": "ghāsatthikaḥ kakkaṭakaḥ a deyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_389:3.3",
        "original": "Pucchāmi taṁ āyatacakkhunetta,",
        "analyzed": "pucchāmi tam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_389:3.4",
        "original": "Atha kissa hetumha ubho gahītā”.",
        "analyzed": "atha kissa ubhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_389:4.1",
        "original": "“Ayaṁ puriso mama atthakāmo,",
        "analyzed": "ayam purisaḥ mama attha kāmo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_389:4.2",
        "original": "Yo maṁ gahetvāna dakāya neti;",
        "analyzed": "yaḥ mam dakāya na iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_389:4.3",
        "original": "Tasmiṁ mate dukkhamanappakaṁ me,",
        "analyzed": "tasmim mate dukkham anappakam me ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_389:4.4",
        "original": "Ahañca eso ca ubho na homa.",
        "analyzed": "ahan ca eso ca ubhau na ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_389:5.1",
        "original": "Mamañca disvāna pavaddhakāyaṁ,",
        "analyzed": "mamañca disvāna pavaddhakāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_389:5.2",
        "original": "Sabbo jano hiṁsitumeva micche;",
        "analyzed": "sabbaḥ janaḥ hisitum eva micche ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_389:5.3",
        "original": "Sāduñca thūlañca muduñca maṁsaṁ,",
        "analyzed": "sāduñca thūlañca muduñca masa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_389:5.4",
        "original": "Kākāpi maṁ disvāna viheṭhayeyyuṁ”.",
        "analyzed": "kākā api mam disvāna viheṭhayeyyu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_389:6.1",
        "original": "“Sacetassa hetumha ubho gahītā,",
        "analyzed": "saced itassa ubhaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_389:6.2",
        "original": "Uṭṭhātu poso visamāvamāmi;",
        "analyzed": "uṭṭhātu posaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_389:6.3",
        "original": "Mamañca kākañca pamuñca khippaṁ,",
        "analyzed": "mamañca kākañca pamuñca khippa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_389:6.4",
        "original": "Pure visaṁ gāḷhamupeti maccaṁ”.",
        "analyzed": "pure visam gāḍham upeti mat ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_389:7.1",
        "original": "“Sappaṁ pamokkhāmi na tāva kākaṁ,",
        "analyzed": "sappam pamokkhāmi na tāva kāka ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_389:7.2",
        "original": "Paṭibandhako hohiti tāva kāko;",
        "analyzed": "paṭibandhakaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_389:7.3",
        "original": "Purisañca disvāna sukhiṁ arogaṁ,",
        "analyzed": "purisañca disvāna sukhim aroga ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_389:7.4",
        "original": "Kākaṁ pamokkhāmi yatheva sappaṁ”.",
        "analyzed": "kākam pamokkhāmi yathā iva sappa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_389:8.1",
        "original": "“Kāko tadā devadatto ahosi,",
        "analyzed": "kāko tadā devadattaḥ ahosi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_389:8.2",
        "original": "Māro pana kaṇhasappo ahosi;",
        "analyzed": "māraḥ pana kaṇhasappaḥ ahosi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_389:8.3",
        "original": "Ānandabhaddo kakkaṭako ahosi,",
        "analyzed": "ānandabhaddaḥ kakkaṭakaḥ ahosi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_389:8.4",
        "original": "Ahaṁ tadā brāhmaṇo homi satthā”ti.",
        "analyzed": "aham tadā brāhmaṇaḥ homi satthā ati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_389:9.1",
        "original": "Suvaṇṇakakkaṭajātakaṁ catutthaṁ.",
        "analyzed": "suvaṇṇakakkaṭajātakam ",
        "folio": "9"
    }
]