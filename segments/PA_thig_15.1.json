[
    {
        "segmentnr": "PA_thig_15.1:0.1",
        "original": "Therīgāthā 15.1",
        "analyzed": "therīgāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_15.1:0.2",
        "original": "Cattālīsanipāta",
        "analyzed": "cattālīsa nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_15.1:0.3",
        "original": "Isidāsītherīgāthā",
        "analyzed": "isi dāsī therī gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_15.1:1.1",
        "original": "Nagaramhi kusumanāme,",
        "analyzed": "nagaramhi kusumanāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_15.1:1.2",
        "original": "Pāṭaliputtamhi pathaviyā maṇḍe;",
        "analyzed": "pāṭaliputtamhi pathaviyāḥ maṇḍe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_15.1:1.3",
        "original": "Sakyakulakulīnāyo,",
        "analyzed": "sakya kula kulīnāyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_15.1:1.4",
        "original": "Dve bhikkhuniyo hi guṇavatiyo.",
        "analyzed": "dve bhikkhuniyaḥ hi guṇavatiyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_15.1:2.1",
        "original": "Isidāsī tattha ekā,",
        "analyzed": "isi dāsī tattha ekā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_15.1:2.2",
        "original": "Dutiyā bodhīti sīlasampannā ca;",
        "analyzed": "dutiyā bodhī iti sīla sampannā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_15.1:2.3",
        "original": "Jhānajjhāyanaratāyo,",
        "analyzed": "jhāna ajjhāyana rata ayaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_15.1:2.4",
        "original": "Bahussutāyo dhutakilesāyo.",
        "analyzed": "bahu ssutāyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_15.1:3.1",
        "original": "Tā piṇḍāya caritvā,",
        "analyzed": "tā piṇḍāya caritvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_15.1:3.2",
        "original": "Bhattatthaṁ kariya dhotapattāyo;",
        "analyzed": "bhattattham kariya dhota pattāyo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_15.1:3.3",
        "original": "Rahitamhi sukhanisinnā,",
        "analyzed": "rahitamhi sukha nisinnā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_15.1:3.4",
        "original": "Imā girā abbhudīresuṁ.",
        "analyzed": "imāḥ girāḥ abbhudīresu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_15.1:4.1",
        "original": "“Pāsādikāsi ayye,",
        "analyzed": "pāsādikā asi ayye ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_15.1:4.2",
        "original": "Isidāsi vayopi te aparihīno;",
        "analyzed": "isi dāsi te a parihīnaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_15.1:4.3",
        "original": "Kiṁ disvāna byālikaṁ,",
        "analyzed": "kim disvāna byālika ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_15.1:4.4",
        "original": "Athāsi nekkhammamanuyuttā”.",
        "analyzed": "atha asi nekkhammam anuyuttā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_15.1:5.1",
        "original": "Evamanuyuñjiyamānā sā,",
        "analyzed": "evam anuyuñjiyamānā sā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_15.1:5.2",
        "original": "Rahite dhammadesanākusalā;",
        "analyzed": "rahite dhamma desanā kusalā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_15.1:5.3",
        "original": "Isidāsī vacanamabravi,",
        "analyzed": "isi dāsī vacanam abravi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_15.1:5.4",
        "original": "“Suṇa bodhi yathāmhi pabbajitā”.",
        "analyzed": "suṇa bodhi yathā amhi pabbajitā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_15.1:6.1",
        "original": "“Ujjeniyā puravare,",
        "analyzed": "ujjeniyā puravare ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_15.1:6.2",
        "original": "Mayhaṁ pitā sīlasaṁvuto seṭṭhi;",
        "analyzed": "mayham pitā sīla savutaḥ seṭṭhi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_15.1:6.3",
        "original": "Tassamhi ekadhītā,",
        "analyzed": "tassamhi ekadhītā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_15.1:6.4",
        "original": "Piyā manāpā ca dayitā ca.",
        "analyzed": "piyā manāpā ca dayitā ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_15.1:7.1",
        "original": "Atha me sāketato varakā,",
        "analyzed": "atha me sāketataḥ varakā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_15.1:7.2",
        "original": "Āgacchumuttamakulīnā;",
        "analyzed": "āgacchum uttama kulīnā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_15.1:7.3",
        "original": "Seṭṭhī pahūtaratano,",
        "analyzed": "seṭṭhī pahūta ratano ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_15.1:7.4",
        "original": "Tassa mamaṁ suṇhamadāsi tāto.",
        "analyzed": "tassa suṇham adāsi tāto ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_15.1:8.1",
        "original": "Sassuyā sasurassa ca,",
        "analyzed": "sassuyā sasurassa ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_15.1:8.2",
        "original": "Sāyaṁ pātaṁ paṇāmamupagamma;",
        "analyzed": "sāyam pātam paṇāmam upagamma ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_15.1:8.3",
        "original": "Sirasā karomi pāde,",
        "analyzed": "sirasā karomi pāde ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_15.1:8.4",
        "original": "Vandāmi yathāmhi anusiṭṭhā.",
        "analyzed": "vandāmi yathā amhi anusiṭṭhā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_15.1:9.1",
        "original": "Yā mayhaṁ sāmikassa,",
        "analyzed": "yā mayham sāmikassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_15.1:9.2",
        "original": "Bhaginiyo bhātuno parijano vā;",
        "analyzed": "bhaginiyaḥ parijanaḥ vā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_15.1:9.3",
        "original": "Tamekavarakampi disvā,",
        "analyzed": "tam eka vara kampi disvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_15.1:9.4",
        "original": "Ubbiggā āsanaṁ demi.",
        "analyzed": "ubbiggā āsanam demi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_15.1:10.1",
        "original": "Annena ca pānena ca,",
        "analyzed": "annena ca pānena ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_15.1:10.2",
        "original": "Khajjena ca yañca tattha sannihitaṁ;",
        "analyzed": "khajjena ca yat ca tattha sannihita ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_15.1:10.3",
        "original": "Chādemi upanayāmi ca,",
        "analyzed": "chādemi upanayāmi ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_15.1:10.4",
        "original": "Demi ca yaṁ yassa patirūpaṁ.",
        "analyzed": "demi ca yam yassa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_15.1:11.1",
        "original": "Kālena upaṭṭhahitvā,",
        "analyzed": "kālena upaṭṭhahitvā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_15.1:11.2",
        "original": "Gharaṁ samupagamāmi ummāre;",
        "analyzed": "gharam samupagamāmi ummāre ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_15.1:11.3",
        "original": "Dhovantī hatthapāde,",
        "analyzed": "dhovantī hattha pāde ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_15.1:11.4",
        "original": "Pañjalikā sāmikamupemi.",
        "analyzed": "pañjalikā sāmikam upemi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_15.1:12.1",
        "original": "Kocchaṁ pasādaṁ añjaniñca,",
        "analyzed": "koccham pasādam añjaniñca ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_15.1:12.2",
        "original": "Ādāsakañca gaṇhitvā;",
        "analyzed": "ādāsakam ca gaṇhitvā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_15.1:12.3",
        "original": "Parikammakārikā viya,",
        "analyzed": "parikamma kārikāḥ viya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_15.1:12.4",
        "original": "Sayameva patiṁ vibhūsemi.",
        "analyzed": "sayam eva patim vibhūsemi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_15.1:13.1",
        "original": "Sayameva odanaṁ sādhayāmi,",
        "analyzed": "sayam eva odanam sādhayāmi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_15.1:13.2",
        "original": "Sayameva bhājanaṁ dhovantī;",
        "analyzed": "sayam eva bhājanam dhovantī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_15.1:13.3",
        "original": "Mātāva ekaputtakaṁ,",
        "analyzed": "mātā avaḥ eka puttaka ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_15.1:13.4",
        "original": "Tathā bhattāraṁ paricarāmi.",
        "analyzed": "tathā bhattāram paricarāmi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_15.1:14.1",
        "original": "Evaṁ maṁ bhattikataṁ,",
        "analyzed": "evam mama ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_15.1:14.2",
        "original": "Anurattaṁ kārikaṁ nihatamānaṁ;",
        "analyzed": "anurattam kārikam nihata māna ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_15.1:14.3",
        "original": "Uṭṭhāyikaṁ analasaṁ,",
        "analyzed": "uṭṭhāyikam analasa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_15.1:14.4",
        "original": "Sīlavatiṁ dussate bhattā.",
        "analyzed": "sīlavatim dussate bhattā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_15.1:15.1",
        "original": "So mātarañca pitarañca,",
        "analyzed": "saḥ mātarañca pitarañca ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_15.1:15.2",
        "original": "Bhaṇati ‘āpucchahaṁ gamissāmi;",
        "analyzed": "bhaṇati āpuccha aham gamissāmi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_15.1:15.3",
        "original": "Isidāsiyā na saha vacchaṁ,",
        "analyzed": "isi dāsiyā na saha vaccha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_15.1:15.4",
        "original": "Ekāgārehaṁ saha vatthuṁ’.",
        "analyzed": "ekāgāra iham saha vatthu ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_15.1:16.1",
        "original": "‘Mā evaṁ putta avaca,",
        "analyzed": "mā evam putta avaca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_15.1:16.2",
        "original": "Isidāsī paṇḍitā paribyattā;",
        "analyzed": "isi dāsī paṇḍitā paribyattā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_15.1:16.3",
        "original": "Uṭṭhāyikā analasā,",
        "analyzed": "uṭṭhāyikā analasā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_15.1:16.4",
        "original": "Kiṁ tuyhaṁ na rocate putta’.",
        "analyzed": "kim tuyham na rocate putta ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_15.1:17.1",
        "original": "‘Na ca me hiṁsati kiñci,",
        "analyzed": "na ca me hisati kiñci ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_15.1:17.2",
        "original": "Na cahaṁ isidāsiyā saha vacchaṁ;",
        "analyzed": "na caham isi dāsiyā saha vaccha ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_15.1:17.3",
        "original": "Dessāva me alaṁ me,",
        "analyzed": "me alam me ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_15.1:17.4",
        "original": "Apucchāhaṁ gamissāmi’.",
        "analyzed": "apucchā aham gamissāmi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_15.1:18.1",
        "original": "Tassa vacanaṁ suṇitvā,",
        "analyzed": "tassa vacanam suṇitvā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_15.1:18.2",
        "original": "Sassu sasuro ca maṁ apucchiṁsu;",
        "analyzed": "ca mama apucchisu ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_15.1:18.3",
        "original": "‘Kissa tayā aparaddhaṁ,",
        "analyzed": "kissa tayā aparaddha ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_15.1:18.4",
        "original": "Bhaṇa vissaṭṭhā yathābhūtaṁ’.",
        "analyzed": "bhaṇa vissaṭṭhā yathābhūta ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_15.1:19.1",
        "original": "‘Napihaṁ aparajjhaṁ kiñci,",
        "analyzed": "na piham a parajjham kiñci ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_15.1:19.2",
        "original": "Napi hiṁsemi na bhaṇāmi dubbacanaṁ;",
        "analyzed": "na pi hisemi na bhaṇāmi dubbacana ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_15.1:19.3",
        "original": "Kiṁ sakkā kātuyye,",
        "analyzed": "kim sakkā kātuyye ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_15.1:19.4",
        "original": "Yaṁ maṁ viddessate bhattā’.",
        "analyzed": "yam mama viddessate bhattā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_15.1:20.1",
        "original": "Te maṁ pitugharaṁ paṭinayiṁsu,",
        "analyzed": "te mam pitu gharam ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_15.1:20.2",
        "original": "Vimanā dukhena adhibhūtā;",
        "analyzed": "vimanā dukhena adhibhūtā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_15.1:20.3",
        "original": "Puttamanurakkhamānā,",
        "analyzed": "puttam anurakkhamānā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_15.1:20.4",
        "original": "‘Jitāmhase rūpiniṁ lakkhiṁ’.",
        "analyzed": "rūpinim lakkhi ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_15.1:21.1",
        "original": "Atha maṁ adāsi tāto,",
        "analyzed": "atha mama adāsi tāto ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_15.1:21.2",
        "original": "Aḍḍhassa gharamhi dutiyakulikassa;",
        "analyzed": "aḍḍhassa gharamhi dutiya kulikassa ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_15.1:21.3",
        "original": "Tato upaḍḍhasuṅkena,",
        "analyzed": "tatas upaḍḍhasuṅkena ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_15.1:21.4",
        "original": "Yena maṁ vindatha seṭṭhi.",
        "analyzed": "yena mama vindatha seṭṭhi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_15.1:22.1",
        "original": "Tassapi gharamhi māsaṁ,",
        "analyzed": "tassa pi gharamhi māsa ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_15.1:22.2",
        "original": "Avasiṁ atha sopi maṁ paṭiccharayi;",
        "analyzed": "avasim atha saḥ upi mam paṭiccharayi ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_15.1:22.3",
        "original": "Dāsīva upaṭṭhahantiṁ,",
        "analyzed": "dāsī vaḥ upaṭṭhahanti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_15.1:22.4",
        "original": "Adūsikaṁ sīlasampannaṁ.",
        "analyzed": "a dūsikam sīla sampanna ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_15.1:23.1",
        "original": "Bhikkhāya ca vicarantaṁ,",
        "analyzed": "bhikkhāya ca vicaranta ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_15.1:23.2",
        "original": "Damakaṁ dantaṁ me pitā bhaṇati;",
        "analyzed": "damakam dantam me pitā bhaṇati ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_15.1:23.3",
        "original": "‘Hohisi me jāmātā,",
        "analyzed": "hohisi me jāmātā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_15.1:23.4",
        "original": "Nikkhipa poṭṭhiñca ghaṭikañca’.",
        "analyzed": "nikkhipa poṭṭhiñca ghaṭikañca ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_15.1:24.1",
        "original": "Sopi vasitvā pakkhaṁ,",
        "analyzed": "saḥ upi vasitvā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_15.1:24.2",
        "original": "Atha tātaṁ bhaṇati ‘dehi me poṭṭhiṁ;",
        "analyzed": "atha tātam bhaṇati dehi me poṭṭhi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_15.1:24.3",
        "original": "Ghaṭikañca mallakañca,",
        "analyzed": "ghaṭikañca mallakañca ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_15.1:24.4",
        "original": "Punapi bhikkhaṁ carissāmi’.",
        "analyzed": "puna api bhikkham carissāmi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_15.1:25.1",
        "original": "Atha naṁ bhaṇatī tāto,",
        "analyzed": "atha nam bhaṇatī tāto ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_15.1:25.2",
        "original": "Ammā sabbo ca me ñātigaṇavaggo;",
        "analyzed": "ammā sabbaḥ ca me ñāti gaṇa vaggo ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_15.1:25.3",
        "original": "‘Kiṁ te na kīrati idha,",
        "analyzed": "kim te na kīrati idha ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_15.1:25.4",
        "original": "Bhaṇa khippaṁ taṁ te karihi’ti.",
        "analyzed": "bhaṇa khippam tam te ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_15.1:26.1",
        "original": "Evaṁ bhaṇito bhaṇati,",
        "analyzed": "evam bhaṇitaḥ bhaṇati ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_15.1:26.2",
        "original": "‘Yadi me attā sakkoti alaṁ mayhaṁ;",
        "analyzed": "yadi me attā sakkoti alam mayha ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_15.1:26.3",
        "original": "Isidāsiyā na saha vacchaṁ,",
        "analyzed": "isi dāsiyā na saha vaccha ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_15.1:26.4",
        "original": "Ekagharehaṁ saha vatthuṁ’.",
        "analyzed": "saha vatthu ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_15.1:27.1",
        "original": "Vissajjito gato so,",
        "analyzed": "vissajjitaḥ gataḥ saḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_15.1:27.2",
        "original": "Ahampi ekākinī vicintemi;",
        "analyzed": "aham pi ekākinī vicintemi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_15.1:27.3",
        "original": "‘Āpucchitūna gacchaṁ,",
        "analyzed": "āpucchitūna gaccha ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_15.1:27.4",
        "original": "Marituye vā pabbajissaṁ vā’.",
        "analyzed": "vā pabbajissam vā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_15.1:28.1",
        "original": "Atha ayyā jinadattā,",
        "analyzed": "atha ayyā jinadattā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_15.1:28.2",
        "original": "Āgacchī gocarāya caramānā;",
        "analyzed": "āgacchī gocarāya caramānā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_15.1:28.3",
        "original": "Tātakulaṁ vinayadharī,",
        "analyzed": "tāta kulam vinaya dharī ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_15.1:28.4",
        "original": "Bahussutā sīlasampannā.",
        "analyzed": "bahu ssutā sīla sampannā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_15.1:29.1",
        "original": "Taṁ disvāna amhākaṁ,",
        "analyzed": "tam disvāna amhāka ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_15.1:29.2",
        "original": "Uṭṭhāyāsanaṁ tassā paññāpayiṁ;",
        "analyzed": "uṭṭhāya āsanam tassā paññāpayi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_15.1:29.3",
        "original": "Nisinnāya ca pāde,",
        "analyzed": "nisinnāya ca pāde ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_15.1:29.4",
        "original": "Vanditvā bhojanamadāsiṁ.",
        "analyzed": "vanditvā bhojanam adāsi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_15.1:30.1",
        "original": "Annena ca pānena ca,",
        "analyzed": "annena ca pānena ca ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_15.1:30.2",
        "original": "Khajjena ca yañca tattha sannihitaṁ;",
        "analyzed": "khajjena ca yat ca tattha sannihita ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_15.1:30.3",
        "original": "Santappayitvā avacaṁ,",
        "analyzed": "santappayitvā avaca ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_15.1:30.4",
        "original": "‘Ayye icchāmi pabbajituṁ’.",
        "analyzed": "ayye icchāmi pabbajitu ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_15.1:31.1",
        "original": "Atha maṁ bhaṇatī tāto,",
        "analyzed": "atha mama bhaṇatī tāto ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_15.1:31.2",
        "original": "‘Idheva puttaka carāhi tvaṁ dhammaṁ;",
        "analyzed": "idha iva puttaka carāhi tvam dhamma ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_15.1:31.3",
        "original": "Annena ca pānena ca,",
        "analyzed": "annena ca pānena ca ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_15.1:31.4",
        "original": "Tappaya samaṇe dvijātī ca’.",
        "analyzed": "samaṇe dvijātī ca ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_15.1:32.1",
        "original": "Athahaṁ bhaṇāmi tātaṁ,",
        "analyzed": "atha aham bhaṇāmi tāta ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_15.1:32.2",
        "original": "Rodantī añjaliṁ paṇāmetvā;",
        "analyzed": "rodantī añjalim paṇāma itvā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_15.1:32.3",
        "original": "‘Pāpañhi mayā pakataṁ,",
        "analyzed": "pāpañhi mayā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_15.1:32.4",
        "original": "Kammaṁ taṁ nijjaressāmi’.",
        "analyzed": "kammam tam nijjaressāmi ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_15.1:33.1",
        "original": "Atha maṁ bhaṇatī tāto,",
        "analyzed": "atha mama bhaṇatī tāto ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_15.1:33.2",
        "original": "‘Pāpuṇa bodhiñca aggadhammañca;",
        "analyzed": "pāpuṇa bodhiñca agga dhammañca ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_15.1:33.3",
        "original": "Nibbānañca labhassu,",
        "analyzed": "nibbānañca labhassu ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_15.1:33.4",
        "original": "Yaṁ sacchikarī dvipadaseṭṭho’.",
        "analyzed": "yam sacchikarī dvipada seṭṭhaḥ ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_15.1:34.1",
        "original": "Mātāpitū abhivādayitvā,",
        "analyzed": "mātā pitū abhivādayitvā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_15.1:34.2",
        "original": "Sabbañca ñātigaṇavaggaṁ;",
        "analyzed": "sabbañca ñāti gaṇa vagga ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_15.1:34.3",
        "original": "Sattāhaṁ pabbajitā,",
        "analyzed": "satta aham pabbajitā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_15.1:34.4",
        "original": "Tisso vijjā aphassayiṁ.",
        "analyzed": "tissaḥ vijjāḥ aphassayi ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_15.1:35.1",
        "original": "Jānāmi attano satta,",
        "analyzed": "jānāmi attanaḥ satta ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thig_15.1:35.2",
        "original": "Jātiyo yassayaṁ phalavipāko;",
        "analyzed": "jātiyaḥ yaḥ sayam phala vipākaḥ ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thig_15.1:35.3",
        "original": "Taṁ tava ācikkhissaṁ,",
        "analyzed": "tam tava ācikkhissa ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thig_15.1:35.4",
        "original": "Taṁ ekamanā nisāmehi.",
        "analyzed": "tam ekamanāḥ nisāma ihi ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thig_15.1:36.1",
        "original": "Nagaramhi erakacche,",
        "analyzed": "nagaramhi erakacche ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thig_15.1:36.2",
        "original": "Suvaṇṇakāro ahaṁ pahūtadhano;",
        "analyzed": "suvaṇṇa kāraḥ aham pahūta dhanaḥ ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thig_15.1:36.3",
        "original": "Yobbanamadena matto,",
        "analyzed": "yobbana madena mattaḥ ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thig_15.1:36.4",
        "original": "So paradāraṁ asevihaṁ.",
        "analyzed": "saḥ para dāram ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_thig_15.1:37.1",
        "original": "Sohaṁ tato cavitvā,",
        "analyzed": "saḥ ham tatas ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thig_15.1:37.2",
        "original": "Nirayamhi apaccisaṁ ciraṁ;",
        "analyzed": "nirayamhi apaccisam cira ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thig_15.1:37.3",
        "original": "Pakko tato ca uṭṭhahitvā,",
        "analyzed": "pakkaḥ tatas ca uṭṭhahitvā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thig_15.1:37.4",
        "original": "Makkaṭiyā kucchimokkamiṁ.",
        "analyzed": "makkaṭiyā kucchimokkami ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_thig_15.1:38.1",
        "original": "Sattāhajātakaṁ maṁ,",
        "analyzed": "satta aha jātakam ma ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thig_15.1:38.2",
        "original": "Mahākapi yūthapo nillacchesi;",
        "analyzed": "mahā kapi yūthapaḥ ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thig_15.1:38.3",
        "original": "Tassetaṁ kammaphalaṁ,",
        "analyzed": "tassa itam kamma phala ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thig_15.1:38.4",
        "original": "Yathāpi gantvāna paradāraṁ.",
        "analyzed": "yathā api gantvāna paradāra ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_thig_15.1:39.1",
        "original": "Sohaṁ tato cavitvā,",
        "analyzed": "saḥ ham tatas ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thig_15.1:39.2",
        "original": "Kālaṁ karitvā sindhavāraññe;",
        "analyzed": "kālam karitvā sindhavāraññe ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thig_15.1:39.3",
        "original": "Kāṇāya ca khañjāya ca,",
        "analyzed": "kāṇāya ca khañjāya ca ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thig_15.1:39.4",
        "original": "Eḷakiyā kucchimokkamiṁ.",
        "analyzed": "eḷakiyā kucchimokkami ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_thig_15.1:40.1",
        "original": "Dvādasa vassāni ahaṁ,",
        "analyzed": "dvādasa vassāni aha ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thig_15.1:40.2",
        "original": "Nillacchito dārake parivahitvā;",
        "analyzed": "nillacchitaḥ dārake parivahitvā ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thig_15.1:40.3",
        "original": "Kimināvaṭṭo akallo,",
        "analyzed": "kimina avaṭṭaḥ a kallaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thig_15.1:40.4",
        "original": "Yathāpi gantvāna paradāraṁ.",
        "analyzed": "yathā api gantvāna paradāra ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_thig_15.1:41.1",
        "original": "Sohaṁ tato cavitvā,",
        "analyzed": "saḥ ham tatas ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thig_15.1:41.2",
        "original": "Govāṇijakassa gāviyā jāto;",
        "analyzed": "go vāṇijakassa gāviyā jātaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thig_15.1:41.3",
        "original": "Vaccho lākhātambo,",
        "analyzed": "vacchaḥ lākhā tambaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thig_15.1:41.4",
        "original": "Nillacchito dvādase māse.",
        "analyzed": "nillacchitaḥ dvādase māse ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_thig_15.1:42.1",
        "original": "Voḍhūna naṅgalamahaṁ,",
        "analyzed": "naṅgalam aha ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thig_15.1:42.2",
        "original": "Sakaṭañca dhārayāmi;",
        "analyzed": "sakaṭam ca dhārayāmi ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thig_15.1:42.3",
        "original": "Andhovaṭṭo akallo,",
        "analyzed": "andhovaṭṭaḥ a kallaḥ ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thig_15.1:42.4",
        "original": "Yathāpi gantvāna paradāraṁ.",
        "analyzed": "yathā api gantvāna paradāra ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_thig_15.1:43.1",
        "original": "Sohaṁ tato cavitvā,",
        "analyzed": "saḥ ham tatas ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thig_15.1:43.2",
        "original": "Vīthiyā dāsiyā ghare jāto;",
        "analyzed": "vīthiyā dāsiyā ghare jātaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thig_15.1:43.3",
        "original": "Neva mahilā na puriso,",
        "analyzed": "na iva mahilā na purisaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thig_15.1:43.4",
        "original": "Yathāpi gantvāna paradāraṁ.",
        "analyzed": "yathā api gantvāna paradāra ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_thig_15.1:44.1",
        "original": "Tiṁsativassamhi mato,",
        "analyzed": "tisa tivassamhi mato ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thig_15.1:44.2",
        "original": "Sākaṭikakulamhi dārikā jātā;",
        "analyzed": "sākaṭika kulamhi dārikā jātā ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thig_15.1:44.3",
        "original": "Kapaṇamhi appabhoge,",
        "analyzed": "kapaṇamhi appabhoge ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thig_15.1:44.4",
        "original": "Dhanika purisapātabahulamhi.",
        "analyzed": "dhanika purisa pāta bahulamhi ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_thig_15.1:45.1",
        "original": "Taṁ maṁ tato satthavāho,",
        "analyzed": "tam mam tatas satthavāho ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thig_15.1:45.2",
        "original": "Ussannāya vipulāya vaḍḍhiyā;",
        "analyzed": "ussannāya vipulāya vaḍḍhiyā ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thig_15.1:45.3",
        "original": "Okaḍḍhati vilapantiṁ,",
        "analyzed": "okaḍḍhati vilapanti ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thig_15.1:45.4",
        "original": "Acchinditvā kulagharasmā.",
        "analyzed": "a chinditvā kulagharasmai ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_thig_15.1:46.1",
        "original": "Atha soḷasame vasse,",
        "analyzed": "atha soḍasame vasse ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thig_15.1:46.2",
        "original": "Disvā maṁ pattayobbanaṁ kaññaṁ;",
        "analyzed": "disvā mam patta yobbanam kañña ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thig_15.1:46.3",
        "original": "Orundhatassa putto,",
        "analyzed": "orundhatassa putto ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thig_15.1:46.4",
        "original": "Giridāso nāma nāmena.",
        "analyzed": "giridāsaḥ nāma nāmena ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_thig_15.1:47.1",
        "original": "Tassapi aññā bhariyā,",
        "analyzed": "tassa api aññā bhariyā ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thig_15.1:47.2",
        "original": "Sīlavatī guṇavatī yasavatī ca;",
        "analyzed": "sīlavatī guṇavatī yasavatī ca ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thig_15.1:47.3",
        "original": "Anurattā bhattāraṁ,",
        "analyzed": "anurattāḥ bhattāra ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thig_15.1:47.4",
        "original": "Tassāhaṁ viddesanamakāsiṁ.",
        "analyzed": "tassa aham viddesanam akāsi ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_thig_15.1:48.1",
        "original": "Tassetaṁ kammaphalaṁ,",
        "analyzed": "tassa itam kamma phala ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thig_15.1:48.2",
        "original": "Yaṁ maṁ apakīritūna gacchanti;",
        "analyzed": "yam mama gacchanti ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thig_15.1:48.3",
        "original": "Dāsīva upaṭṭhahantiṁ,",
        "analyzed": "dāsī vaḥ upaṭṭhahanti ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thig_15.1:48.4",
        "original": "Tassapi anto kato mayā”ti.",
        "analyzed": "tassa api antaḥ kataḥ mayā ati ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_thig_15.1:49.1",
        "original": "… Isidāsī therī ….",
        "analyzed": "isidāsī therī ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_thig_15.1:50.1",
        "original": "Cattālīsanipāto niṭṭhito.",
        "analyzed": "cattālīsa nipātaḥ niṭṭhito ",
        "folio": "50"
    }
]