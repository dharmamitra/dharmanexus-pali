        "analyzed": "adhas tapati vālukā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_421:2.3",
        "original": "Atha gāyasi vattāni,",
        "analyzed": "atha gāyasi vattāni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_421:2.4",
