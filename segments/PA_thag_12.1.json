        "analyzed": "asmin loke su sikkhita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_12.1:1.3",
        "original": "Sīlañhi sabbasampattiṁ,",
        "analyzed": "sīlam hi sabba sampatti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_12.1:1.4",
        "original": "upanāmeti sevitaṁ.",
        "analyzed": "upanāmā iti sevita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_12.1:2.1",
        "original": "Sīlaṁ rakkheyya medhāvī,",
        "analyzed": "sīlam rakkheyya medhāvī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_12.1:2.2",
        "original": "patthayāno tayo sukhe;",
        "analyzed": "patthayānaḥ tayo sukhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_12.1:2.3",
        "original": "Pasaṁsaṁ vittilābhañca,",
        "analyzed": "pasasam vitti lābham ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_12.1:2.4",
        "original": "pecca sagge pamodanaṁ.",
        "analyzed": "pecca sagge pamodana ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_12.1:3.1",
        "original": "Sīlavā hi bahū mitte,",
        "analyzed": "sīlavā hi bahū mitte ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_12.1:3.2",
        "original": "saññamenādhigacchati;",
        "analyzed": "saññamena adhigacchati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_12.1:3.3",
        "original": "Dussīlo pana mittehi,",
        "analyzed": "dussīlaḥ pana mittehi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_12.1:3.4",
        "original": "dhaṁsate pāpamācaraṁ.",
        "analyzed": "dhasate pāpam ācara ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_12.1:4.1",
        "original": "Avaṇṇañca akittiñca,",
        "analyzed": "a vaṇṇam ca a kittim ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_12.1:4.2",
        "original": "dussīlo labhate naro;",
        "analyzed": "dussīlaḥ labhate naraḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_12.1:4.3",
        "original": "Vaṇṇaṁ kittiṁ pasaṁsañca,",
        "analyzed": "vaṇṇam kittim pasasam ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_12.1:4.4",
        "original": "sadā labhati sīlavā.",
        "analyzed": "sadā labhati sīlavā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_12.1:5.1",
        "original": "Ādi sīlaṁ patiṭṭhā ca,",
        "analyzed": "ādi sīlam patiṭṭhā ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_12.1:5.2",
        "original": "kalyāṇānañca mātukaṁ;",
        "analyzed": "mātuka ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_12.1:5.3",
        "original": "Pamukhaṁ sabbadhammānaṁ,",
        "analyzed": "pamukham sabba dhammāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_12.1:5.4",
        "original": "tasmā sīlaṁ visodhaye.",
        "analyzed": "tasmai sīlam visodhaye ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_12.1:6.1",
        "original": "Velā ca saṁvaraṁ sīlaṁ,",
        "analyzed": "ca sa varam sīla ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_12.1:6.2",
        "original": "cittassa abhihāsanaṁ;",
        "analyzed": "cittassa abhihāsana ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_12.1:6.3",
        "original": "Titthañca sabbabuddhānaṁ,",
        "analyzed": "tittham ca sabbabuddhāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_12.1:6.4",
        "original": "tasmā sīlaṁ visodhaye.",
        "analyzed": "tasmai sīlam visodhaye ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_12.1:7.1",
        "original": "Sīlaṁ balaṁ appaṭimaṁ,",
        "analyzed": "sīlam balam appaṭima ",
        "folio": "7"
    },
