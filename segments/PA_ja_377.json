        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_377:1.3",
        "original": "Mātā pitā disatā setaketu,",
        "analyzed": "mātā pitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_377:1.4",
        "original": "Ācariyamāhu disataṁ pasatthā.",
