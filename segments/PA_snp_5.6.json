    },
    {
        "segmentnr": "PA_snp_5.6:0.2",
        "original": "Dhotakamāṇavapucchā",
        "analyzed": "dhotaka māṇava pucchā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.6:1.1",
        "original": "“Pucchāmi taṁ bhagavā brūhi metaṁ,",
        "analyzed": "pucchāmi tam bhagavā brūhi mā ita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.6:1.2",
        "original": "(iccāyasmā dhotako)",
        "analyzed": "id ca ayasmai dhotako ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.6:1.3",
        "original": "Vācābhikaṅkhāmi mahesi tuyhaṁ;",
        "analyzed": "vācā abhikaṅkhāmi mahā isi tuyha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.6:1.4",
        "original": "Tava sutvāna nigghosaṁ,",
        "analyzed": "tava sutvāna nigghosa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.6:1.5",
        "original": "Sikkhe nibbānamattano”.",
        "analyzed": "sikkhe nibbāna mattano ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.6:2.1",
        "original": "“Tenahātappaṁ karohi,",
        "analyzed": "tenahā ātappam karohi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.6:2.2",
        "original": "(dhotakāti bhagavā)",
        "analyzed": "bhagavā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.6:2.3",
        "original": "Idheva nipako sato;",
        "analyzed": "idhā iva nipakaḥ sato ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.6:2.4",
        "original": "Ito sutvāna nigghosaṁ,",
        "analyzed": "itas sutvāna nigghosa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.6:2.5",
        "original": "Sikkhe nibbānamattano”.",
        "analyzed": "sikkhe nibbāna mattano ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.6:3.1",
        "original": "“Passāmahaṁ devamanussaloke,",
        "analyzed": "passāmaham deva manussa loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.6:3.2",
        "original": "Akiñcanaṁ brāhmaṇamiriyamānaṁ;",
        "analyzed": "akiñcanam brāhmaṇam iriyamāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.6:3.3",
        "original": "Taṁ taṁ namassāmi samantacakkhu,",
        "analyzed": "tam tam namassāmi samanta cakkhu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.6:3.4",
        "original": "Pamuñca maṁ sakka kathaṅkathāhi”.",
        "analyzed": "pamuñca mama sakka kathaṅkathāhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.6:4.1",
        "original": "“Nāhaṁ sahissāmi pamocanāya,",
        "analyzed": "na aham sahissāmi pamocanāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.6:4.2",
        "original": "Kathaṅkathiṁ dhotaka kañci loke;",
        "analyzed": "kathaṅkathim dhotaka kañci loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.6:4.3",
        "original": "Dhammañca seṭṭhaṁ abhijānamāno,",
        "analyzed": "dhammañca seṭṭham abhijānamānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.6:4.4",
        "original": "Evaṁ tuvaṁ oghamimaṁ taresi”.",
        "analyzed": "evam tuvam ogham imam taresi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.6:5.1",
        "original": "“Anusāsa brahme karuṇāyamāno,",
        "analyzed": "anusāsa brahme karuṇāyamānaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.6:5.2",
        "original": "Vivekadhammaṁ yamahaṁ vijaññaṁ;",
        "analyzed": "viveka dhammam yam aham vijañña ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.6:5.3",
        "original": "Yathāhaṁ ākāsova abyāpajjamāno,",
        "analyzed": "yathā aham ākāsaḥ uva a byāpajjamānaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.6:5.4",
        "original": "Idheva santo asito careyyaṁ”.",
        "analyzed": "idhā iva santaḥ asitaḥ careyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.6:6.1",
        "original": "“Kittayissāmi te santiṁ,",
        "analyzed": "kittayissāmi te santi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.6:6.2",
        "original": "(dhotakāti bhagavā)",
        "analyzed": "bhagavā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.6:6.3",
        "original": "Diṭṭhe dhamme anītihaṁ;",
        "analyzed": "diṭṭhe dhamme anīti ha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.6:6.4",
        "original": "Yaṁ viditvā sato caraṁ,",
        "analyzed": "yam viditvā satas cara ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.6:6.5",
        "original": "Tare loke visattikaṁ”.",
        "analyzed": "tare loke visattika ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.6:7.1",
        "original": "“Tañcāhaṁ abhinandāmi,",
        "analyzed": "tat ca aham abhinandāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.6:7.2",
        "original": "mahesi santimuttamaṁ;",
        "analyzed": "mahesi santim uttama ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.6:7.3",
        "original": "Yaṁ viditvā sato caraṁ,",
        "analyzed": "yam viditvā satas cara ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.6:7.4",
        "original": "tare loke visattikaṁ”.",
        "analyzed": "tare loke visattika ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.6:8.1",
        "original": "“Yaṁ kiñci sampajānāsi,",
        "analyzed": "yam kiñci sampajānāsi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.6:8.2",
        "original": "(dhotakāti bhagavā)",
        "analyzed": "bhagavā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.6:8.3",
        "original": "Uddhaṁ adho tiriyañcāpi majjhe;",
        "analyzed": "uddham adhas majjhe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.6:8.4",
        "original": "Etaṁ viditvā saṅgoti loke,",
        "analyzed": "etam viditvā saṅgoti loke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.6:8.5",
        "original": "Bhavābhavāya mākāsi taṇhan”ti.",
        "analyzed": "bhava abhavāya mākā asi taṇhanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.6:8.6",
        "original": "Dhotakamāṇavapucchā pañcamī.",
        "analyzed": "dhotakamāṇavapucchā pañcamī ",
        "folio": "8"
    }
