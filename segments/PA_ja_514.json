    },
    {
        "segmentnr": "PA_ja_514:1.1",
        "original": "“Kiṁ nu socasinuccaṅgi,",
        "analyzed": "kim nu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_514:1.2",
        "original": "paṇḍūsi varavaṇṇini;",
        "analyzed": "paṇḍū asi varavaṇṇini ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_514:1.3",
        "original": "Milāyasi visālakkhi,",
        "analyzed": "milāyasi visālakkhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_514:1.4",
        "original": "mālāva parimadditā”.",
        "analyzed": "parimadditā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_514:2.1",
        "original": "“Dohaḷo me mahārāja,",
        "analyzed": "dohaḷaḥ me mahā rāja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_514:2.2",
        "original": "supinantenupajjhagā;",
        "analyzed": "supin antena upajjhagā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_514:2.3",
        "original": "Na so sulabharūpova,",
        "analyzed": "na saḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_514:2.4",
        "original": "yādiso mama dohaḷo”.",
        "analyzed": "yādisaḥ mama dohaḷaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_514:3.1",
        "original": "“Ye keci mānusā kāmā,",
        "analyzed": "ye keci mānusā kāmā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_514:3.2",
        "original": "idha lokasmi nandane;",
        "analyzed": "idha nandane ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_514:3.3",
        "original": "Sabbe te pacurā mayhaṁ,",
        "analyzed": "sabbe te pacurāḥ mayha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_514:3.4",
        "original": "ahaṁ te dammi dohaḷaṁ”.",
        "analyzed": "aham te dammi dohaḷa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_514:4.1",
        "original": "“Luddā deva samāyantu,",
        "analyzed": "luddāḥ deva samāyantu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_514:4.2",
        "original": "ye keci vijite tava;",
        "analyzed": "ye keci vijite tava ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_514:4.3",
        "original": "Etesaṁ ahamakkhissaṁ,",
        "analyzed": "etesam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_514:4.4",
        "original": "yādiso mama dohaḷo”.",
        "analyzed": "yādisaḥ mama dohaḷaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_514:5.1",
        "original": "“Ime te luddakā devi,",
        "analyzed": "ime te luddakāḥ devi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_514:5.2",
        "original": "katahatthā visāradā;",
        "analyzed": "katahatthā visāradā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_514:5.3",
        "original": "Vanaññū ca migaññū ca,",
        "analyzed": "vanaññū ca migaññū ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_514:5.4",
        "original": "mamatthe cattajīvitā”.",
        "analyzed": "mamatthe catta jīvitā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_514:6.1",
        "original": "“Luddaputtā nisāmetha,",
        "analyzed": "ludda puttā nisāmetha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_514:6.2",
        "original": "yāvantettha samāgatā;",
        "analyzed": "yāvantā itthā samāgatā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_514:6.3",
        "original": "Chabbisāṇaṁ gajaṁ setaṁ,",
        "analyzed": "chat bisānam gajam seta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_514:6.4",
        "original": "addasaṁ supine ahaṁ;",
        "analyzed": "addasam supine aha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_514:6.5",
        "original": "Tassa dantehi me attho,",
        "analyzed": "tassa dantehi me atthaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_514:6.6",
        "original": "alābhe natthi jīvitaṁ”.",
        "analyzed": "alābhe natthi jīvita ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_514:7.1",
        "original": "“Na no pitūnaṁ na pitāmahānaṁ,",
        "analyzed": "na naḥ pitūnam na pitāmahāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_514:7.2",
        "original": "Diṭṭho suto kuñjaro chabbisāṇo;",
        "analyzed": "diṭṭhaḥ sutaḥ kuñjaraḥ chabbisāṇaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_514:7.3",
        "original": "Yamaddasā supine rājaputtī,",
        "analyzed": "rāja puttī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_514:7.4",
        "original": "Akkhāhi no yādiso hatthināgo”.",
        "analyzed": "akkhāhi naḥ yādisaḥ hatthi nāgaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_514:8.1",
        "original": "“Disā catasso vidisā catasso,",
        "analyzed": "disā catassaḥ vidisā catassaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_514:8.2",
        "original": "Uddhaṁ adho dasa disā imāyo;",
        "analyzed": "uddham adhas dasa disāḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_514:8.3",
        "original": "Katamaṁ disaṁ tiṭṭhati nāgarājā,",
        "analyzed": "katamam disam tiṭṭhati nāga rājā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_514:8.4",
        "original": "Yamaddasā supine chabbisāṇaṁ”.",
        "analyzed": "yamaddasā supine chabbisāṇa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_514:9.1",
        "original": "“Ito ujuṁ uttariyaṁ disāyaṁ,",
        "analyzed": "itas ujum uttariyam disāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_514:9.2",
        "original": "Atikkamma so sattagirī brahante;",
        "analyzed": "atikkamma saḥ satta girī brahante ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_514:9.3",
        "original": "Suvaṇṇapasso nāma girī uḷāro,",
        "analyzed": "suvaṇṇapassaḥ nāma girī uḷāraḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_514:9.4",
        "original": "Supupphito kimpurisānuciṇṇo.",
        "analyzed": "      ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_514:10.1",
        "original": "Āruyha selaṁ bhavanaṁ kinnarānaṁ,",
        "analyzed": "āruyha selam bhavanam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_514:10.2",
        "original": "Olokaya pabbatapādamūlaṁ;",
        "analyzed": "olokaya pabbata pāda mūla ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_514:10.3",
        "original": "Atha dakkhasī meghasamānavaṇṇaṁ,",
        "analyzed": "atha dakkhasī megha samāna vaṇṇa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_514:10.4",
        "original": "Nigrodharājaṁ aṭṭhasahassapādaṁ.",
        "analyzed": "nigrodha rājam aṭṭha sahassapāda ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_514:11.1",
        "original": "Tatthacchatī kuñjaro chabbisāṇo,",
        "analyzed": "kuñjaraḥ chabbisāṇaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_514:11.2",
        "original": "Sabbaseto duppasaho parebhi;",
        "analyzed": "sabba seto duppasahaḥ parebhi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_514:11.3",
        "original": "Rakkhanti naṁ aṭṭhasahassanāgā,",
        "analyzed": "rakkhanti nam aṭṭha sahassa nāgā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_514:11.4",
        "original": "Īsādantā vātajavappahārino.",
        "analyzed": "īsādantā vāta java ppahārino ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_514:12.1",
        "original": "Tiṭṭhanti te tumūlaṁ passasantā,",
        "analyzed": "tiṭṭhanti te tumūlam passasantā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_514:12.2",
        "original": "Kuppanti vātassapi eritassa;",
        "analyzed": "kuppanti vātassa api eritassa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_514:12.3",
        "original": "Manussabhūtaṁ pana tattha disvā,",
        "analyzed": "manussa bhūtam pana tattha disvā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_514:12.4",
        "original": "Bhasmaṁ kareyyuṁ nāssa rajopi tassa”.",
        "analyzed": "bhasmam kareyyum na assa rajaḥ api tassa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_514:13.1",
        "original": "“Bahū hime rājakulamhi santi,",
        "analyzed": "bahū hime rājakulamhi santi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_514:13.2",
        "original": "Piḷandhanā jātarūpassa devī;",
        "analyzed": "piḷandhanā jātarūpassa devī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_514:13.3",
        "original": "Muttā maṇī veḷuriyāmayā ca,",
        "analyzed": "muttā maṇī ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_514:13.4",
        "original": "Kiṁ kāhasi dantapiḷandhanena;",
        "analyzed": "kim danta piḍandhanena ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_514:13.5",
        "original": "Māretukāmā kuñjaraṁ chabbisāṇaṁ,",
        "analyzed": "mā āretu kāmā kuñjaram chabbisāṇa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_514:13.6",
        "original": "Udāhu ghātessasi luddaputte”.",
        "analyzed": "udāhu ghātessasi ludda putte ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_514:14.1",
        "original": "“Sā issitā dukkhitā casmi ludda,",
        "analyzed": "sā issitā dukkhitā casmi ludda ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_514:14.2",
        "original": "Uddhañca sussāmi anussarantī;",
        "analyzed": "uddhañca sussāmi anussarantī ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_514:14.3",
        "original": "Karohi me luddaka etamatthaṁ,",
        "analyzed": "karohi me luddakaḥ etam attha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_514:14.4",
        "original": "Dassāmi te gāmavarāni pañca”.",
        "analyzed": "dassāmi te gām avarāni pañca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_514:15.1",
        "original": "“Katthacchatī kattha mupeti ṭhānaṁ,",
        "analyzed": "katthacchatī kattha mupeti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_514:15.2",
        "original": "Vīthissa kā nhāna gatassa hoti;",
        "analyzed": "vīthissa kā nhāna gatassa hoti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_514:15.3",
        "original": "Kathañhi so nhāyati nāgarājā,",
        "analyzed": "kathañhi saḥ nhāyati nāga rājā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_514:15.4",
        "original": "Kathaṁ vijānemu gatiṁ gajassa”.",
        "analyzed": "katham vijānemu gatim gajassa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_514:16.1",
        "original": "“Tattheva sā pokkharaṇī adūre,",
        "analyzed": "tattha iva sā pokkharaṇī adūre ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_514:16.2",
        "original": "Rammā sutitthā ca mahodikā ca;",
        "analyzed": "rammā sutitthā ca mahodikā ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_514:16.3",
        "original": "Sampupphitā bhamaragaṇānuciṇṇā,",
        "analyzed": "sampupphitā bhamara gaṇa anuciṇṇā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_514:16.4",
        "original": "Ettha hi so nhāyati nāgarājā.",
        "analyzed": "ettha hi saḥ nhāyati nāga rājā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_514:17.1",
        "original": "Sīsaṁ nahātuppalamālabhārī,",
        "analyzed": "sīsam nahā tuppala māla bhārī ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_514:17.2",
        "original": "Sabbaseto puṇḍarīkattacaṅgī;",
        "analyzed": "sabbaseto ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_514:17.3",
        "original": "Āmodamāno gacchati sanniketaṁ,",
        "analyzed": "āmodamānaḥ gacchati sanniketa ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_514:17.4",
        "original": "Purakkhatvā mahesiṁ sabbabhaddaṁ”.",
        "analyzed": "purakkhatvā mahesim sabbabhadda ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_514:18.1",
        "original": "Tattheva so uggahetvāna vākyaṁ,",
        "analyzed": "tattha iva saḥ uggahetvāna vākya ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_514:18.2",
        "original": "Ādāya tūṇiñca dhanuñca luddo;",
        "analyzed": "ādāya tūṇiñca dhanuñca luddaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_514:18.3",
        "original": "Vituriyati sattagirī brahante,",
        "analyzed": "vituriyati satta girī brahante ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_514:18.4",
        "original": "Suvaṇṇapassaṁ nāma giriṁ uḷāraṁ.",
        "analyzed": "suvaṇṇapassam nāma girim uḷāra ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_514:19.1",
        "original": "Āruyha selaṁ bhavanaṁ kinnarānaṁ,",
        "analyzed": "āruyha selam bhavanam ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_514:19.2",
        "original": "Olokayī pabbatapādamūlaṁ;",
        "analyzed": "olokayī pabbata pāda mūla ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_514:19.3",
        "original": "Tatthaddasā meghasamānavaṇṇaṁ,",
        "analyzed": "megha samāna vaṇṇa ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_514:19.4",
        "original": "Nigrodharājaṁ aṭṭhasahassapādaṁ.",
        "analyzed": "nigrodha rājam aṭṭha sahassapāda ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_514:20.1",
        "original": "Tatthaddasā kuñjaraṁ chabbisāṇaṁ,",
        "analyzed": "kuñjaram chabbisāṇa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_514:20.2",
        "original": "Sabbasetaṁ duppasahaṁ parebhi;",
        "analyzed": "sabbasetam duppasaham parebhi ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_514:20.3",
        "original": "Rakkhanti naṁ aṭṭhasahassanāgā,",
        "analyzed": "rakkhanti nam aṭṭha sahassa nāgā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_514:20.4",
        "original": "Īsādantā vātajavappahārino.",
        "analyzed": "īsādantā vāta java ppahārino ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_514:21.1",
        "original": "Tatthaddasā pokkharaṇiṁ adūre,",
        "analyzed": "pokkharaṇim adūre ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_514:21.2",
        "original": "Rammaṁ sutitthañca mahodikañca;",
        "analyzed": "rammam sutittham ca mahodikam ca ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_514:21.3",
        "original": "Sampupphitaṁ bhamaragaṇānuciṇṇaṁ,",
        "analyzed": "sampupphitam ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_514:21.4",
        "original": "Yattha hi so nhāyati nāgarājā.",
        "analyzed": "yattha hi saḥ nhāyati nāga rājā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_514:22.1",
        "original": "Disvāna nāgassa gatiṁ ṭhitiñca,",
        "analyzed": "disvāna nāgassa gatim ṭhitiñca ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_514:22.2",
        "original": "Vīthissa yā nhānagatassa hoti;",
        "analyzed": "vīthissa yā nhāna gatassa hoti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_514:22.3",
        "original": "Opātamāgacchi anariyarūpo,",
        "analyzed": "opātam āgacchi an ariya rūpaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_514:22.4",
        "original": "Payojito cittavasānugāya.",
        "analyzed": "citta vasā anugāya ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_514:23.1",
        "original": "Khaṇitvāna kāsuṁ phalakehi chādayi,",
        "analyzed": "khaṇitvāna kāsum phalakehi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_514:23.2",
        "original": "Attānamodhāya dhanuñca luddo;",
        "analyzed": "attānam odhāya dhanuṃ ca luddaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_514:23.3",
        "original": "Passāgataṁ puthusallena nāgaṁ,",
        "analyzed": "passāgatam puthusallena nāga ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_514:23.4",
        "original": "Samappayī dukkaṭakammakārī.",
        "analyzed": "samappayī dukkaṭa kamma kārī ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_514:24.1",
        "original": "Viddho ca nāgo koñcamanādi ghoraṁ,",
        "analyzed": "viddhaḥ ca nāgaḥ ghora ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_514:24.2",
        "original": "Sabbe ca nāgā ninnaduṁ ghorarūpaṁ;",
        "analyzed": "sabbe ca nāgāḥ ghora rūpa ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_514:24.3",
        "original": "Tiṇañca kaṭṭhañca raṇaṁ karontā,",
        "analyzed": "tiṇam ca kaṭṭham ca raṇam karontā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_514:24.4",
        "original": "Dhāviṁsu te aṭṭha disā samantato.",
        "analyzed": "te aṭṭha disā samantataḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_514:25.1",
        "original": "Vadhissametanti parāmasanto,",
        "analyzed": "vadhiḥ sametanti parāmasantaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_514:25.2",
        "original": "Kāsāvamaddakkhi dhajaṁ isīnaṁ;",
        "analyzed": "dhajam isīna ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_514:25.3",
        "original": "Dukkhena phuṭṭhassudapādi saññā,",
        "analyzed": "dukkhena phuṭṭhassa udapādi saññā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_514:25.4",
        "original": "Arahaddhajo sabbhi avajjharūpo.",
        "analyzed": "arahat hajaḥ sabbhi avajjha rūpaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_514:26.1",
        "original": "“Anikkasāvo kāsāvaṁ,",
        "analyzed": "a nikkasāvaḥ kāsāva ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_514:26.2",
        "original": "yo vatthaṁ paridahissati;",
        "analyzed": "yaḥ vattham paridahissati ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_514:26.3",
        "original": "Apeto damasaccena,",
        "analyzed": "apa itas dama saccena ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_514:26.4",
        "original": "na so kāsāvamarahati.",
        "analyzed": "na sā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_514:27.1",
        "original": "Yo ca vantakasāvassa,",
        "analyzed": "yaḥ ca vantakasāvassa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_514:27.2",
        "original": "sīlesu susamāhito;",
        "analyzed": "sīlesu su samāhitaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_514:27.3",
        "original": "Upeto damasaccena,",
        "analyzed": "upetaḥ dama saccena ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_514:27.4",
        "original": "sa ve kāsāvamarahati”.",
        "analyzed": "sa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_514:28.1",
        "original": "Samappito puthusallena nāgo,",
        "analyzed": "samappitaḥ puthusallena nāgaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_514:28.2",
        "original": "Aduṭṭhacitto luddakamajjhabhāsi;",
        "analyzed": "aduṭṭha cittaḥ luddakam ajjhabhāsi ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_514:28.3",
        "original": "“Kimatthayaṁ kissa vā samma hetu,",
        "analyzed": "kimatthayam kissa vā samma hetu ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_514:28.4",
        "original": "Mamaṁ vadhī kassa vāyaṁ payogo”.",
        "analyzed": "mamam vadhī kassa vā ayam payogaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_514:29.1",
        "original": "“Kāsissa rañño mahesī bhadante,",
        "analyzed": "kāsissa raññaḥ mahesī bhadante ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_514:29.2",
        "original": "Sā pūjitā rājakule subhaddā;",
        "analyzed": "sā pūjitā rāja kule subhaddā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_514:29.3",
        "original": "Taṁ addasā sā ca mamaṁ asaṁsi,",
        "analyzed": "tam addasā sā ca asasi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_514:29.4",
        "original": "Dantehi atthoti ca maṁ avoca”.",
        "analyzed": "dantehi attha iti ca mama avoca ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_514:30.1",
        "original": "“Bahū hi me dantayugā uḷārā,",
        "analyzed": "bahū hi me danta yugā uḷārā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_514:30.2",
        "original": "Ye me pitūnañca pitāmahānaṁ;",
        "analyzed": "ye me pitūnam ca pitāmahāna ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_514:30.3",
        "original": "Jānāti sā kodhanā rājaputtī,",
        "analyzed": "jānāti sā kodhanā rāja puttī ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_514:30.4",
        "original": "Vadhatthikā veramakāsi bālā.",
        "analyzed": "vadhatthikā veramakā asi bālā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_514:31.1",
        "original": "Uṭṭhehi tvaṁ ludda kharaṁ gahetvā,",
        "analyzed": "uṭṭhehi tvam ludda kharam gahetvā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_514:31.2",
        "original": "Dante ime chinda purā marāmi;",
        "analyzed": "dante ime chinda purā marāmi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_514:31.3",
        "original": "Vajjāsi taṁ kodhanaṁ rājaputtiṁ,",
        "analyzed": "vajjāsi tam kodhanam rāja putti ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_514:31.4",
        "original": "‘Nāgo hato handa imassa dantā’”.",
        "analyzed": "nāgaḥ hataḥ handa imassa dantā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_514:32.1",
        "original": "Uṭṭhāya so luddo kharaṁ gahetvā,",
        "analyzed": "uṭṭhāya saḥ luddaḥ kharam gahetvā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_514:32.2",
        "original": "Chetvāna dantāni gajuttamassa;",
        "analyzed": "chetvāna dantāni gajuttamassa ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_514:32.3",
        "original": "Vaggū subhe appaṭime pathabyā,",
        "analyzed": "vaggū subhe appaṭime pathabyā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_514:32.4",
        "original": "Ādāya pakkāmi tato hi khippaṁ.",
        "analyzed": "ādāya pakkāmi tatas hi khippa ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_514:33.1",
        "original": "Bhayaṭṭitā nāgavadhena aṭṭā,",
        "analyzed": "bhaya ṭṭitā nāga vadhena aṭṭā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_514:33.2",
        "original": "Ye te nāgā aṭṭha disā vidhāvuṁ;",
        "analyzed": "ye te nāgāḥ aṭṭha disā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_514:33.3",
        "original": "Adisvāna posaṁ gajapaccamittaṁ,",
        "analyzed": "a disvāna posam gaja pacca mitta ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_514:33.4",
        "original": "Paccāgamuṁ yena so nāgarājā.",
        "analyzed": "pacca āgamum yena saḥ nāga rājā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_514:34.1",
        "original": "Te tattha kanditvā roditvāna nāgā,",
        "analyzed": "te tattha kanditvā roditvāna nāgā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_514:34.2",
        "original": "Sīse sake paṁsukaṁ okiritvā;",
        "analyzed": "sīse sake pasukam okiritvā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_514:34.3",
        "original": "Agamaṁsu te sabbe sakaṁ niketaṁ,",
        "analyzed": "agamasu te sabbe sakam niketa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_514:34.4",
        "original": "Purakkhatvā mahesiṁ sabbabhaddaṁ.",
        "analyzed": "purakkhatvā mahesim sabbabhadda ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_514:35.1",
        "original": "Ādāya dantāni gajuttamassa,",
        "analyzed": "ādāya dantāni gajuttamassa ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_514:35.2",
        "original": "Vaggū subhe appaṭime pathabyā;",
        "analyzed": "vaggū subhe appaṭime pathabyā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_514:35.3",
        "original": "Suvaṇṇarājīhi samantamodare,",
        "analyzed": "suvaṇṇa rājīhi samantama udare ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_514:35.4",
        "original": "So luddako kāsipuraṁ upāgami;",
        "analyzed": "saḥ luddakaḥ kāsipuram upāgami ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_514:35.5",
        "original": "Upanesi so rājakaññāya dante,",
        "analyzed": "upanesi saḥ rāja kaññāya dante ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_514:35.6",
        "original": "Nāgo hato handa imassa dantā.",
        "analyzed": "nāgaḥ hataḥ handa imassa dantā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_514:36.1",
        "original": "Disvāna dantāni gajuttamassa,",
        "analyzed": "disvāna dantāni gajuttamassa ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_514:36.2",
        "original": "Bhattuppiyassa purimāya jātiyā;",
        "analyzed": "bhattuppiyassa purimāya jātiyā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_514:36.3",
        "original": "Tattheva tassā hadayaṁ aphāli,",
        "analyzed": "tattha iva tassāḥ hadayam aphāli ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_514:36.4",
        "original": "Teneva sā kālamakāsi bālā.",
        "analyzed": "tena iva sā bālā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_514:37.1",
        "original": "Sambodhipatto sa mahānubhāvo,",
        "analyzed": "sambodhi pattaḥ sa mahā anubhāvaḥ ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_514:37.2",
        "original": "Sitaṁ akāsi parisāya majjhe;",
        "analyzed": "sitam akāsi parisāya majjhe ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_514:37.3",
        "original": "Pucchiṁsu bhikkhū suvimuttacittā,",
        "analyzed": "bhikkhū su vimutta cittā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_514:37.4",
        "original": "“Nākāraṇe pātukaronti buddhā.",
        "analyzed": "na akāraṇe pātukaronti buddhāḥ ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_514:38.1",
        "original": "Yamaddasātha dahariṁ kumāriṁ,",
        "analyzed": "daharim kumāri ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_514:38.2",
        "original": "Kāsāyavatthaṁ anagāriyaṁ carantiṁ;",
        "analyzed": "kāsāyavattham anagāriyam caranti ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_514:38.3",
        "original": "Sā kho tadā rājakaññā ahosi,",
        "analyzed": "sā kho tadā rāja kaññā ahosi ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_514:38.4",
        "original": "Ahaṁ tadā nāgarājā ahosiṁ.",
        "analyzed": "aham tadā nāga rājā ahosi ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_514:39.1",
        "original": "Ādāya dantāni gajuttamassa,",
        "analyzed": "ādāya dantāni gajuttamassa ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_514:39.2",
        "original": "Vaggū subhe appaṭime pathabyā;",
        "analyzed": "vaggū subhe appaṭime pathabyā ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_514:39.3",
        "original": "Yo luddako kāsipuraṁ upāgami,",
        "analyzed": "yaḥ luddakaḥ kāsipuram upāgami ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_514:39.4",
        "original": "So kho tadā devadatto ahosi”.",
        "analyzed": "saḥ khalu tadā devadattaḥ ahosi ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_514:40.1",
        "original": "Anāvasūraṁ cirarattasaṁsitaṁ,",
        "analyzed": "cira ratta sa sita ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_514:40.2",
        "original": "Uccāvacaṁ caritamidaṁ purāṇaṁ;",
        "analyzed": "uccāvacam caritam idam purāṇa ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_514:40.3",
        "original": "Vītaddaro vītasoko visallo,",
        "analyzed": "vīta ddaraḥ vīta sokaḥ visallaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_514:40.4",
        "original": "Sayaṁ abhiññāya abhāsi buddho.",
        "analyzed": "sayam abhiññāya abhāsi buddhaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_514:41.1",
        "original": "“Ahaṁ vo tena kālena,",
        "analyzed": "aham vaḥ tena kālena ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_514:41.2",
        "original": "ahosiṁ tattha bhikkhavo;",
        "analyzed": "ahosim tattha bhikkhavaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_514:41.3",
        "original": "Nāgarājā tadā homi,",
        "analyzed": "nāga rājā tadā ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_514:41.4",
        "original": "evaṁ dhāretha jātakan”ti.",
        "analyzed": "evam dhāretha jātakanti ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_514:42.1",
        "original": "Chaddantajātakaṁ catutthaṁ.",
        "analyzed": "chad danta jātakam ",
        "folio": "42"
    }
]