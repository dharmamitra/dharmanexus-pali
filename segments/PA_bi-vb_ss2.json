        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.2",
        "original": "Tena kho pana samayena vesāliyaṁ aññatarassa licchavissa pajāpati aticārinī hoti.",
        "analyzed": "tena kho pana samayena vesāliyam aññatarassa licchavissa pajāpati aticārinī hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.3",
        "original": "Atha kho so licchavi taṁ itthiṁ etadavoca—",
        "analyzed": "atha khalu saḥ licchavi tam itthim etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.4",
        "original": "“sādhu viramāhi, anatthaṁ kho te karissāmī”ti.",
        "analyzed": "sādhu viramāhi anattham kho te karissāmi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.5",
        "original": "Evampi vuccamānā nādiyi.",
        "analyzed": "evam pi vuccamānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.6",
        "original": "Tena kho pana samayena vesāliyaṁ licchavigaṇo sannipatito hoti kenacideva karaṇīyena.",
        "analyzed": "tena kho pana samayena vesāliyam licchavi gaṇaḥ sannipatitaḥ hoti kenacid eva karaṇīyena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.7",
        "original": "Atha kho so licchavi te licchavayo etadavoca—",
        "analyzed": "atha khalu saḥ licchavi te licchavayaḥ etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.8",
        "original": "“ekaṁ me, ayyo, itthiṁ anujānāthā”ti.",
        "analyzed": "ekam me ayyaḥ itthim anujānātha iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.9",
        "original": "“Kā nāma sā”ti?",
        "analyzed": "kā nāma sāti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.10",
        "original": "“Mayhaṁ pajāpati aticarati, taṁ ghātessāmī”ti.",
        "analyzed": "mayham pajāpatiḥ aticarati tam ghātessāmi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.11",
        "original": "“Jānāhī”ti.",
        "analyzed": "jānāhi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.12",
        "original": "Assosi kho sā itthī—",
        "analyzed": "assosi kho sā itthī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.13",
        "original": "“sāmiko kira maṁ ghātetukāmo”ti.",
        "analyzed": "sāmikaḥ kira mam ghātetu kāmā iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.14",
        "original": "Varabhaṇḍaṁ ādāya sāvatthiṁ gantvā titthiye upasaṅkamitvā pabbajjaṁ yāci.",
        "analyzed": "varabhaṇḍam ādāya sāvatthim gantvā titthiye upasaṅkamitvā pabbajjam yāci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.15",
        "original": "Titthiyā na icchiṁsu pabbājetuṁ.",
        "analyzed": "titthiyā naḥ icchisu pabbājetu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.16",
        "original": "Bhikkhuniyo upasaṅkamitvā pabbajjaṁ yāci.",
        "analyzed": "bhikkhuniyaḥ upasaṅkamitvā pabbajjam yāci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.17",
        "original": "Bhikkhuniyopi na icchiṁsu pabbājetuṁ.",
        "analyzed": "bhikkhuniyaḥ api naḥ icchisu pabbājetu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.18",
        "original": "Thullanandaṁ bhikkhuniṁ upasaṅkamitvā bhaṇḍakaṁ dassetvā pabbajjaṁ yāci.",
        "analyzed": "thullanandam bhikkhunim upasaṅkamitvā bhaṇḍakam dassetvā pabbajjam yāci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.19",
        "original": "Thullanandā bhikkhunī bhaṇḍakaṁ gahetvā pabbājesi.",
        "analyzed": "thullanandā bhikkhunī bhaṇḍakam gahetvā pabbājesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.20",
        "original": "Atha kho so licchavi taṁ itthiṁ gavesanto sāvatthiṁ gantvā bhikkhunīsu pabbajitaṁ disvāna yena rājā pasenadi kosalo tenupasaṅkami;",
        "analyzed": "atha kho saḥ licchavi tam itthim gavesantaḥ sāvatthim gantvā bhikkhunīsu pabbajitam disvāna yena rājā pasenadi kosalaḥ tena upasaṅkami ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.21",
        "original": "upasaṅkamitvā rājānaṁ pasenadiṁ kosalaṁ etadavoca—",
        "analyzed": "upasaṅkamitvā rājānam pasenadim kosalam etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.22",
        "original": "“pajāpati me, deva, varabhaṇḍaṁ ādāya sāvatthiṁ anuppattā.",
        "analyzed": "pajāpati me deva varabhaṇḍam ādāya sāvatthim an uppattā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.23",
        "original": "Taṁ devo anujānātū”ti.",
        "analyzed": "tam devaḥ anujānātu uti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.24",
        "original": "“Tena hi, bhaṇe, vicinitvā ācikkhā”ti.",
        "analyzed": "tena hi bhaṇe vicinitvā ācikkhāti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.25",
        "original": "“Diṭṭhā, deva, bhikkhunīsu pabbajitā”ti.",
        "analyzed": "diṭṭhā deva bhikkhunīsu pabbajitā ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.26",
        "original": "“Sace, bhaṇe, bhikkhunīsu pabbajitā, na sā labbhā kiñci kātuṁ.",
        "analyzed": "sace bhaṇe bhikkhunīsu pabbajitā na sā labbhā kiñci kātu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.27",
        "original": "Svākkhāto bhagavatā dhammo, caratu brahmacariyaṁ sammā dukkhassa antakiriyāyā”ti.",
        "analyzed": "su ākkhātaḥ bhagavatā dhammaḥ caratu brahmacariyam sammā dukkhassa anta kiriyāya ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.28",
        "original": "Atha kho so licchavi ujjhāyati khiyyati vipāceti—",
        "analyzed": "atha kho saḥ licchavi ujjhāyati khiyyati vipāceti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.29",
        "original": "“kathañhi nāma bhikkhuniyo coriṁ pabbājessantī”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ corim pabbājessanti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.30",
        "original": "Assosuṁ kho bhikkhuniyo tassa licchavissa ujjhāyantassa khiyyantassa vipācentassa.",
        "analyzed": "assosum kho bhikkhuniyaḥ tassa licchavissa ujjhāyantassa khiyyantassa vipācentassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.31",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.32",
        "original": "“kathañhi nāma ayyā thullanandā coriṁ pabbājessatī”ti.",
        "analyzed": "katham hi nāma ayyā thullanandā corim pabbājessati iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.33",
        "original": "Atha kho tā bhikkhuniyo bhikkhūnaṁ etamatthaṁ ārocesuṁ …pe…",
        "analyzed": "atha kho tāḥ bhikkhuniyaḥ bhikkhūnam etam attham ārocesuṃ pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.34",
        "original": "“saccaṁ kira, bhikkhave, thullanandā bhikkhunī coriṁ pabbājetī”ti?",
        "analyzed": "saccam kira bhikkhave thullanandā bhikkhunī corim pabbājeti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.35",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.36",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.37",
        "original": "kathañhi nāma, bhikkhave, thullanandā bhikkhunī coriṁ pabbājessati.",
        "analyzed": "katham hi nāma bhikkhave thullanandā bhikkhunī corim pabbājessati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss2:1.38",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
