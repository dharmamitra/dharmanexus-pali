        "analyzed": "anāhāra upasammati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_15.2:14.3",
        "original": "Aṅgāresu ca santesu,",
        "analyzed": "aṅgāresu ca santesu ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_15.2:14.4",
        "original": "nibbutoti pavuccati.",
        "analyzed": "nibbuto ti pavuccati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_15.2:15.1",
        "original": "Atthassāyaṁ viññāpanī,",
        "analyzed": "atthassa ayam viññāpanī ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_15.2:15.2",
        "original": "upamā viññūhi desitā;",
        "analyzed": "upamā viññūhi desitā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_15.2:15.3",
        "original": "Viññissanti mahānāgā,",
        "analyzed": "viññissanti mahā nāgā ",
        "folio": "15"
    },
