[
    {
        "segmentnr": "PA_snp_4.10:0.1",
        "original": "Sutta Nipāta 4.10",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.10:0.2",
        "original": "Purābhedasutta",
        "analyzed": "purābhedasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.10:1.1",
        "original": "“Kathaṁdassī kathaṁsīlo,",
        "analyzed": "katha dassī katha sīlaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.10:1.2",
        "original": "upasantoti vuccati;",
        "analyzed": "upasanto ti vuccati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.10:1.3",
        "original": "Taṁ me gotama pabrūhi,",
        "analyzed": "tam me gotama pabrūhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.10:1.4",
        "original": "pucchito uttamaṁ naraṁ”.",
        "analyzed": "pucchitaḥ uttamam nara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.10:2.1",
        "original": "“Vītataṇho purā bhedā,",
        "analyzed": "vītataṇho purā bhedā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.10:2.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.10:2.3",
        "original": "Pubbamantamanissito;",
        "analyzed": "pubbam antam anissitaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.10:2.4",
        "original": "Vemajjhe nupasaṅkheyyo,",
        "analyzed": "vemajjhe n upasaṅkheyyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.10:2.5",
        "original": "Tassa natthi purakkhataṁ.",
        "analyzed": "tassa natthi purakkhata ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.10:3.1",
        "original": "Akkodhano asantāsī,",
        "analyzed": "akkodhanaḥ asantāsī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.10:3.2",
        "original": "avikatthī akukkuco;",
        "analyzed": "a vikatthī a kukkucaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.10:3.3",
        "original": "Mantabhāṇī anuddhato,",
        "analyzed": "manta bhāṇī anuddhataḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.10:3.4",
        "original": "sa ve vācāyato muni.",
        "analyzed": "sa veḥ vācāyataḥ muni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.10:4.1",
        "original": "Nirāsatti anāgate,",
        "analyzed": "nirāsatti anāgate ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.10:4.2",
        "original": "atītaṁ nānusocati;",
        "analyzed": "atītam na anusocati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.10:4.3",
        "original": "Vivekadassī phassesu,",
        "analyzed": "viveka dassī phassesu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.10:4.4",
        "original": "diṭṭhīsu ca na nīyati.",
        "analyzed": "diṭṭhīsu ca na ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.10:5.1",
        "original": "Patilīno akuhako,",
        "analyzed": "pati līnaḥ a kuhakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.10:5.2",
        "original": "apihālu amaccharī;",
        "analyzed": "apihālu amaccharī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.10:5.3",
        "original": "Appagabbho ajeguccho,",
        "analyzed": "appagabbhaḥ ajegucchaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.10:5.4",
        "original": "pesuṇeyye ca no yuto.",
        "analyzed": "pesuṇeyye ca naḥ yutaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.10:6.1",
        "original": "Sātiyesu anassāvī,",
        "analyzed": "sātiyesu anassāvī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.10:6.2",
        "original": "atimāne ca no yuto;",
        "analyzed": "atimāne ca naḥ yutaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.10:6.3",
        "original": "Saṇho ca paṭibhānavā,",
        "analyzed": "saṇhaḥ ca paṭibhānavā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.10:6.4",
        "original": "na saddho na virajjati.",
        "analyzed": "na saddhaḥ na virajjati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.10:7.1",
        "original": "Lābhakamyā na sikkhati,",
        "analyzed": "lābha kamyā na sikkhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.10:7.2",
        "original": "alābhe ca na kuppati;",
        "analyzed": "alābhe ca na kuppati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.10:7.3",
        "original": "Aviruddho ca taṇhāya,",
        "analyzed": "a viruddhaḥ ca taṇhāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.10:7.4",
        "original": "rasesu nānugijjhati.",
        "analyzed": "rasesu na anugijjhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.10:8.1",
        "original": "Upekkhako sadā sato,",
        "analyzed": "upekkhakaḥ sadā sato ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.10:8.2",
        "original": "na loke maññate samaṁ;",
        "analyzed": "na loke maññate sama ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.10:8.3",
        "original": "Na visesī na nīceyyo,",
        "analyzed": "na visesī na nīceyyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.10:8.4",
        "original": "tassa no santi ussadā.",
        "analyzed": "tassa no santi ussadā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.10:9.1",
        "original": "Yassa nissayanā natthi,",
        "analyzed": "yassa nissayanā natthi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.10:9.2",
        "original": "ñatvā dhammaṁ anissito;",
        "analyzed": "ñatvā dhammaṃ anissitaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.10:9.3",
        "original": "Bhavāya vibhavāya vā,",
        "analyzed": "bhavāya vibhavāya vā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.10:9.4",
        "original": "taṇhā yassa na vijjati.",
        "analyzed": "taṇhā yassa na vijjati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.10:10.1",
        "original": "Taṁ brūmi upasantoti,",
        "analyzed": "tam brūmi upasantā iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.10:10.2",
        "original": "Kāmesu anapekkhinaṁ;",
        "analyzed": "kāmesu anapekkhina ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.10:10.3",
        "original": "Ganthā tassa na vijjanti,",
        "analyzed": "ganthā tassa na vijjanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.10:10.4",
        "original": "Atarī so visattikaṁ.",
        "analyzed": "atarī saḥ visattika ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.10:11.1",
        "original": "Na tassa puttā pasavo,",
        "analyzed": "na tassa puttā pasavaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.10:11.2",
        "original": "Khettaṁ vatthuñca vijjati;",
        "analyzed": "khettam vatthuñca vijjati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.10:11.3",
        "original": "Attā vāpi nirattā vā,",
        "analyzed": "attā vā api nirattā vā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.10:11.4",
        "original": "Na tasmiṁ upalabbhati.",
        "analyzed": "na tasmin upalabbhati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.10:12.1",
        "original": "Yena naṁ vajjuṁ puthujjanā,",
        "analyzed": "yena nam vajjum puthujjanā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.10:12.2",
        "original": "Atho samaṇabrāhmaṇā;",
        "analyzed": "atha u samaṇa brāhmaṇā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.10:12.3",
        "original": "Taṁ tassa apurakkhataṁ,",
        "analyzed": "tam tassa apurakkhata ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.10:12.4",
        "original": "Tasmā vādesu nejati.",
        "analyzed": "tasmai vādesu nejati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.10:13.1",
        "original": "Vītagedho amaccharī,",
        "analyzed": "vīta gedhaḥ amaccharī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.10:13.2",
        "original": "Na ussesu vadate muni;",
        "analyzed": "na ussesu vadate muni ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.10:13.3",
        "original": "Na samesu na omesu,",
        "analyzed": "na samesu na omesu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.10:13.4",
        "original": "Kappaṁ neti akappiyo.",
        "analyzed": "kappam neti akappiyaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.10:14.1",
        "original": "Yassa loke sakaṁ natthi,",
        "analyzed": "yassa loke sakam na atthi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.10:14.2",
        "original": "Asatā ca na socati;",
        "analyzed": "asatā ca na socati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.10:14.3",
        "original": "Dhammesu ca na gacchati,",
        "analyzed": "dhammesu ca na gacchati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.10:14.4",
        "original": "Sa ve santoti vuccatī”ti.",
        "analyzed": "sa ve santo ti vuccati iti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.10:14.5",
        "original": "Purābhedasuttaṁ dasamaṁ.",
        "analyzed": "purābheda suttam dasama ",
        "folio": "14"
    }
]