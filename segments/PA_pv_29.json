        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_29:2.3",
        "original": "Ayaṁ maṁsāni khāditvā,",
        "analyzed": "ayam masāni khāditvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_29:2.4",
        "original": "Musāvādena vañceti.",
        "analyzed": "musā vādena vañca iti ",
        "folio": "2"
    },
    {
