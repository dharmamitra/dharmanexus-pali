[
    {
        "segmentnr": "PA_snp_4.12:0.1",
        "original": "Sutta Nipāta 4.12",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.12:0.2",
        "original": "Cūḷabyūhasutta",
        "analyzed": "cūḍa byūha sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.12:1.1",
        "original": "“Sakaṁsakaṁdiṭṭhiparibbasānā,",
        "analyzed": "saka saka diṭṭhi paribbasānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.12:1.2",
        "original": "Viggayha nānā kusalā vadanti;",
        "analyzed": "viggayha nānā kusalāḥ vadanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.12:1.3",
        "original": "Yo evaṁ jānāti sa vedi dhammaṁ,",
        "analyzed": "yaḥ evam jānāti sa vedi dhamma ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.12:1.4",
        "original": "Idaṁ paṭikkosamakevalī so.",
        "analyzed": "idam paṭikkosama kevalī saḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.12:2.1",
        "original": "Evampi viggayha vivādayanti,",
        "analyzed": "evam pi viggayha vivādayanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.12:2.2",
        "original": "Bālo paro akkusaloti cāhu;",
        "analyzed": "bālaḥ paraḥ akkusalaḥ iti ca āhu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.12:2.3",
        "original": "Sacco nu vādo katamo imesaṁ,",
        "analyzed": "saccaḥ nu vādaḥ katamaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.12:2.4",
        "original": "Sabbeva hīme kusalāvadānā”.",
        "analyzed": "sabbe iva hi ime kusala avadānā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.12:3.1",
        "original": "“Parassa ce dhammamanānujānaṁ,",
        "analyzed": "parassa ce dhammam anā anujāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.12:3.2",
        "original": "Bālomako hoti nihīnapañño;",
        "analyzed": "bālomakaḥ hoti nihīna paññaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.12:3.3",
        "original": "Sabbeva bālā sunihīnapaññā,",
        "analyzed": "sabbā iva bālā su nihīna paññā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.12:3.4",
        "original": "Sabbevime diṭṭhiparibbasānā.",
        "analyzed": "sabbe iva ime diṭṭhi paribbasānā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.12:4.1",
        "original": "Sandiṭṭhiyā ceva na vīvadātā,",
        "analyzed": "sandiṭṭhiyā ca iva na vīvadātā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.12:4.2",
        "original": "Saṁsuddhapaññā kusalā mutīmā;",
        "analyzed": "sa suddha paññā kusalā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.12:4.3",
        "original": "Na tesaṁ koci parihīnapañño,",
        "analyzed": "na tesam koci parihīna pañño ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.12:4.4",
        "original": "Diṭṭhī hi tesampi tathā samattā.",
        "analyzed": "diṭṭhī hi tesam pi tathā samattā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.12:5.1",
        "original": "Na vāhametaṁ tathiyanti brūmi,",
        "analyzed": "na vā aham etam tathiyanti brūmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.12:5.2",
        "original": "Yamāhu bālā mithu aññamaññaṁ;",
        "analyzed": "yam āhu bālāḥ mithu aññamañña ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.12:5.3",
        "original": "Sakaṁ sakaṁ diṭṭhimakaṁsu saccaṁ,",
        "analyzed": "sakam sakam diṭṭhi maka sat ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.12:5.4",
        "original": "Tasmā hi bāloti paraṁ dahanti”.",
        "analyzed": "tasmai hi param dahanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.12:6.1",
        "original": "“Yamāhu saccaṁ tathiyanti eke,",
        "analyzed": "yam āhu saccam eke ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.12:6.2",
        "original": "Tamāhu aññe tucchaṁ musāti;",
        "analyzed": "tam āhu aññe tuccham musā ati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.12:6.3",
        "original": "Evampi vigayha vivādayanti,",
        "analyzed": "evampi vigayha vivādayanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.12:6.4",
        "original": "Kasmā na ekaṁ samaṇā vadanti”.",
        "analyzed": "kasmai naḥ ekam samaṇāḥ vadanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.12:7.1",
        "original": "“Ekañhi saccaṁ na dutīyamatthi,",
        "analyzed": "ekam hi saccam na dutīyam atthi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.12:7.2",
        "original": "Yasmiṁ pajā no vivade pajānaṁ;",
        "analyzed": "yasmin pajāḥ naḥ vivade pajāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.12:7.3",
        "original": "Nānā te saccāni sayaṁ thunanti,",
        "analyzed": "nānā te saccāni sayam thunanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.12:7.4",
        "original": "Tasmā na ekaṁ samaṇā vadanti”.",
        "analyzed": "tasmai naḥ ekam samaṇāḥ vadanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.12:8.1",
        "original": "“Kasmā nu saccāni vadanti nānā,",
        "analyzed": "kasmai nu satcāni vadanti nānā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.12:8.2",
        "original": "Pavādiyāse kusalāvadānā;",
        "analyzed": "kusala avadānā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.12:8.3",
        "original": "Saccāni sutāni bahūni nānā,",
        "analyzed": "saccāni sutāni bahūni nānā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.12:8.4",
        "original": "Udāhu te takkamanussaranti”.",
        "analyzed": "uda ahu te takkam anussaranti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.12:9.1",
        "original": "“Na heva saccāni bahūni nānā,",
        "analyzed": "na ha iva saccāni bahūni nānā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.12:9.2",
        "original": "Aññatra saññāya niccāni loke;",
        "analyzed": "aññatra saññāya niccāni loke ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.12:9.3",
        "original": "Takkañca diṭṭhīsu pakappayitvā,",
        "analyzed": "takkañca diṭṭhīsu pakappayitvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.12:9.4",
        "original": "Saccaṁ musāti dvayadhammamāhu.",
        "analyzed": "saccam musā ati dvaya dhammam āhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.12:10.1",
        "original": "Diṭṭhe sute sīlavate mute vā,",
        "analyzed": "diṭṭhe sute sīlavate mute vā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.12:10.2",
        "original": "Ete ca nissāya vimānadassī;",
        "analyzed": "ete ca nissāya vimāna dassī ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.12:10.3",
        "original": "Vinicchaye ṭhatvā pahassamāno,",
        "analyzed": "vinicchaye ṭhatvā pahassamānaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.12:10.4",
        "original": "Bālo paro akkusaloti cāha.",
        "analyzed": "bālaḥ paraḥ akkusalaḥ iti ca āha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.12:11.1",
        "original": "Yeneva bāloti paraṁ dahāti,",
        "analyzed": "yena iva param dahāti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.12:11.2",
        "original": "Tenātumānaṁ kusaloti cāha;",
        "analyzed": "tena atumānam kusala iti ca āha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.12:11.3",
        "original": "Sayamattanā so kusalāvadāno,",
        "analyzed": "sayam attanā saḥ kusala avadānaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.12:11.4",
        "original": "Aññaṁ vimāneti tadeva pāva.",
        "analyzed": "aññam vimāneti tat eva pāva ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.12:12.1",
        "original": "Atisāradiṭṭhiyā so samatto,",
        "analyzed": "atisāra diṭṭhiyā so samatto ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.12:12.2",
        "original": "Mānena matto paripuṇṇamānī;",
        "analyzed": "mānena mattaḥ paripuṇṇa mānī ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.12:12.3",
        "original": "Sayameva sāmaṁ manasābhisitto,",
        "analyzed": "sayam eva sāmam manasā abhisittaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.12:12.4",
        "original": "Diṭṭhī hi sā tassa tathā samattā.",
        "analyzed": "diṭṭhī hi sā tassa tathā samattā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.12:13.1",
        "original": "Parassa ce hi vacasā nihīno,",
        "analyzed": "parassa ce hi vacasā nihīnaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.12:13.2",
        "original": "Tumo sahā hoti nihīnapañño;",
        "analyzed": "tumaḥ sahā hoti nihīna paññaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.12:13.3",
        "original": "Atha ce sayaṁ vedagū hoti dhīro,",
        "analyzed": "atha ce sayam vedagū hoti dhīraḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.12:13.4",
        "original": "Na koci bālo samaṇesu atthi.",
        "analyzed": "na koci bālaḥ samaṇesu atthi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.12:14.1",
        "original": "Aññaṁ ito yābhivadanti dhammaṁ,",
        "analyzed": "aññam itas yā abhivadanti dhamma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.12:14.2",
        "original": "Aparaddhā suddhimakevalī te;",
        "analyzed": "aparaddhā suddhim akevalī te ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.12:14.3",
        "original": "Evampi titthyā puthuso vadanti,",
        "analyzed": "evam pi titthyā puthusaḥ vadanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.12:14.4",
        "original": "Sandiṭṭhirāgena hi tebhirattā.",
        "analyzed": "sandiṭṭhi rāgena hi tebhiḥ attā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.12:15.1",
        "original": "Idheva suddhiṁ iti vādayanti,",
        "analyzed": "idhā iva suddhim iti vādayanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.12:15.2",
        "original": "Nāññesu dhammesu visuddhimāhu;",
        "analyzed": "na aññesu dhammesu visuddhim āhu ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.12:15.3",
        "original": "Evampi titthyā puthuso niviṭṭhā,",
        "analyzed": "evam pi titthyā puthusas niviṭṭhā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.12:15.4",
        "original": "Sakāyane tattha daḷhaṁ vadānā.",
        "analyzed": "sa kāyane tattha daḷham vadānā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.12:16.1",
        "original": "Sakāyane vāpi daḷhaṁ vadāno,",
        "analyzed": "sa kāyane vā api daḷham vadānaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.12:16.2",
        "original": "Kamettha bāloti paraṁ daheyya;",
        "analyzed": "kam ettha bāla iti param daheyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.12:16.3",
        "original": "Sayaṁva so medhagamāvaheyya,",
        "analyzed": "sa yava saḥ medha gam āvaheyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.12:16.4",
        "original": "Paraṁ vadaṁ bālamasuddhidhammaṁ.",
        "analyzed": "param vadam bālam asuddhi dhamma ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.12:17.1",
        "original": "Vinicchaye ṭhatvā sayaṁ pamāya,",
        "analyzed": "vinicchaye sayam pamāya ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.12:17.2",
        "original": "Uddhaṁsa lokasmiṁ vivādameti;",
        "analyzed": "uddhasa lokasmin vivādam eti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.12:17.3",
        "original": "Hitvāna sabbāni vinicchayāni,",
        "analyzed": "hitvāna sabbāni vinicchayāni ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.12:17.4",
        "original": "Na medhagaṁ kubbati jantu loke”ti.",
        "analyzed": "na medha gam kubbati jantu loka iti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.12:17.5",
        "original": "Cūḷabyūhasuttaṁ dvādasamaṁ.",
        "analyzed": "cūḷabyūhasuttam dvādasama ",
        "folio": "17"
    }
]