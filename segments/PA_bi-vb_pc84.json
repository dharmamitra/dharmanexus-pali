    {
        "segmentnr": "PA_bi-vb_pc84:0.5",
        "original": "84. Chattupāhanasikkhāpada",
        "analyzed": "chattupāhana sikkhāpada ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.2",
        "original": "Tena kho pana samayena chabbaggiyā bhikkhuniyo chattupāhanaṁ dhārenti.",
        "analyzed": "tena kho pana samayena chabbaggiyāḥ bhikkhuniyaḥ chattupāhanam dhārenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.3",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.4",
        "original": "“kathañhi nāma bhikkhuniyo chattupāhanaṁ dhāressanti, seyyathāpi gihiniyo kāmabhoginiyo”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ chattupāhanam dhāressanti seyyathā api gihiniyaḥ kāmabhoginiyaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.5",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.6",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.7",
        "original": "“kathañhi nāma chabbaggiyā bhikkhuniyo chattupāhanaṁ dhāressantī”ti …pe…",
        "analyzed": "katham hi nāma chabbaggiyāḥ bhikkhuniyaḥ chattupāhanam dhāressanti iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.8",
        "original": "“saccaṁ kira, bhikkhave, chabbaggiyā bhikkhuniyo chattupāhanaṁ dhārentī”ti?",
        "analyzed": "saccam kira bhikkhave chabbaggiyāḥ bhikkhuniyaḥ chattupāhanam dhārenti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.9",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.10",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.11",
        "original": "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo chattupāhanaṁ dhāressanti.",
        "analyzed": "katham hi nāma bhikkhave chabbaggiyāḥ bhikkhuniyaḥ chattupāhanam dhāressanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.12",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
        "analyzed": "na ita bhikkhave appasannānam vā pasādāya pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.13",
        "original": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
        "analyzed": "evam ca pana bhikkhave bhikkhuniyaḥ imam sikkhāpadam uddisantu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.14.1",
        "original": "“Yā pana bhikkhunī chattupāhanaṁ dhāreyya, pācittiyan”ti.",
        "analyzed": "yā pana bhikkhunī chattu pāhanam dhāreyya pācittiyanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.1.15",
        "original": "Evañcidaṁ bhagavatā bhikkhunīnaṁ sikkhāpadaṁ paññattaṁ hoti.",
        "analyzed": "evañca idam bhagavatā bhikkhunīnam sikkhāpadam paññattam hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.2.1",
        "original": "Tena kho pana samayena aññatarā bhikkhunī gilānā hoti.",
        "analyzed": "tena kho pana samayena aññatarā bhikkhunī gilānā hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.2.2",
        "original": "Tassā vinā chattupāhanaṁ na phāsu hoti …pe…",
        "analyzed": "vinā chattupāhanam na phāsu hoti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.2.3",
        "original": "bhagavato etamatthaṁ ārocesuṁ …pe…",
        "analyzed": "bhagavato etam attham ārocesuṃ pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.2.4",
        "original": "anujānāmi, bhikkhave, gilānāya bhikkhuniyā chattupāhanaṁ.",
        "analyzed": "anujānāmi bhikkhave gilānāya bhikkhuniyā chattupāhana ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc84:1.2.5",
        "original": "Evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
