[
    {
        "segmentnr": "PA_vv_16:0.1",
        "original": "Vimānavatthu",
        "analyzed": "vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_16:0.2",
        "original": "Itthivimāna",
        "analyzed": "itthi vimāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_16:0.3",
        "original": "Pīṭhavagga",
        "analyzed": "pīṭha vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_16:0.4",
        "original": "16. Sirimāvimānavatthu",
        "analyzed": "    ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_16:1.1",
        "original": "“Yuttā ca te paramaalaṅkatā hayā,",
        "analyzed": "yuttā ca te parama alaṅkatā hayā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_16:1.2",
        "original": "Adhomukhā aghasigamā balī javā;",
        "analyzed": "adhomukhāḥ balī javā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_16:1.3",
        "original": "Abhinimmitā pañcarathāsatā ca te,",
        "analyzed": "abhinimmitā pañca rathā asatā ca te ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_16:1.4",
        "original": "Anventi taṁ sārathicoditā hayā.",
        "analyzed": "anventi tam sārathi coditāḥ hayā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_16:2.1",
        "original": "Sā tiṭṭhasi rathavare alaṅkatā,",
        "analyzed": "sā tiṭṭhasi rathavare alaṅkatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_16:2.2",
        "original": "Obhāsayaṁ jalamiva joti pāvako;",
        "analyzed": "obhāsayam jalam iva pāvako ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_16:2.3",
        "original": "Pucchāmi taṁ varatanu anomadassane,",
        "analyzed": "pucchāmi tam vara tanu anomadassane ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_16:2.4",
        "original": "Kasmā nu kāyā anadhivaraṁ upāgami”.",
        "analyzed": "kasmai nu kāyai an adhivaram upāgami ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_16:3.1",
        "original": "“Kāmaggapattānaṁ yamāhunuttaraṁ,",
        "analyzed": "kāmagga pattānam yam āhuḥ uttara ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_16:3.2",
        "original": "Nimmāya nimmāya ramanti devatā;",
        "analyzed": "nimmāya nimmāya ramanti devatā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_16:3.3",
        "original": "Tasmā kāyā accharā kāmavaṇṇinī,",
        "analyzed": "tasmai kāyā accharā kāma vaṇṇinī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_16:3.4",
        "original": "Idhāgatā anadhivaraṁ namassituṁ”.",
        "analyzed": "idha āgatāḥ an adhivaram ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_16:4.1",
        "original": "“Kiṁ tvaṁ pure sucaritamācarīdha,",
        "analyzed": "kim tvam pure su caritam ācarīdha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_16:4.2",
        "original": "Kenacchasi tvaṁ amitayasā sukhedhitā;",
        "analyzed": "kenacchasi tvam amita yasā su khedhitā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_16:4.3",
        "original": "Iddhī ca te anadhivarā vihaṅgamā,",
        "analyzed": "iddhī ca te anadhivarā vihaṅgamā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_16:4.4",
        "original": "Vaṇṇo ca te dasa disā virocati.",
        "analyzed": "vaṇṇo ca te dasa disā virocati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_16:5.1",
        "original": "Devehi tvaṁ parivutā sakkatā casi,",
        "analyzed": "devehi tvam parivutā sakkatā casi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_16:5.2",
        "original": "Kuto cutā sugatigatāsi devate;",
        "analyzed": "kutas cutā sugati gatā asi devate ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_16:5.3",
        "original": "Kassa vā tvaṁ vacanakarānusāsaniṁ,",
        "analyzed": "kassa vā tvam vacana kara anusāsani ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_16:5.4",
        "original": "Ācikkha me tvaṁ yadi buddhasāvikā”ti.",
        "analyzed": "ācikkha me tvam yadi buddha sāvikā ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_16:6.1",
        "original": "“Nagantare nagaravare sumāpite,",
        "analyzed": "nagantare nagara vare su māpite ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_16:6.2",
        "original": "Paricārikā rājavarassa sirimato;",
        "analyzed": "paricārikā rājavarassa sirimato ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_16:6.3",
        "original": "Nacce gīte paramasusikkhitā ahuṁ,",
        "analyzed": "nacce gīte parama su sikkhitā ahu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_16:6.4",
        "original": "Sirimāti maṁ rājagahe avediṁsu.",
        "analyzed": "sirimāti mam rājagahe avedisu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_16:7.1",
        "original": "Buddho ca me isinisabho vināyako,",
        "analyzed": "buddhaḥ ca me isinisabhaḥ vināyako ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_16:7.2",
        "original": "Adesayī samudayadukkhaniccataṁ;",
        "analyzed": "samudaya dukkha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_16:7.3",
        "original": "Asaṅkhataṁ dukkhanirodhasassataṁ,",
        "analyzed": "asaṅkhatam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_16:7.4",
        "original": "Maggañcimaṁ akuṭilamañjasaṁ sivaṁ.",
        "analyzed": "akuṭilam añjasam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_16:8.1",
        "original": "Sutvānahaṁ amatapadaṁ asaṅkhataṁ,",
        "analyzed": "sutvānaham amata padam ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_16:8.2",
        "original": "Tathāgatassanadhivarassa sāsanaṁ;",
        "analyzed": "sāsana ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_16:8.3",
        "original": "Sīlesvahaṁ paramasusaṁvutā ahuṁ,",
        "analyzed": "sīlesu aham ahu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_16:8.4",
        "original": "Dhamme ṭhitā naravarabuddhadesite.",
        "analyzed": "dhamme ṭhitāḥ nara vara buddha desite ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_16:9.1",
        "original": "Ñatvānahaṁ virajapadaṁ asaṅkhataṁ,",
        "analyzed": "viraja padam ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_16:9.2",
        "original": "Tathāgatenanadhivarena desitaṁ;",
        "analyzed": "tathāgatena anadhivarena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_16:9.3",
        "original": "Tatthevahaṁ samathasamādhimāphusiṁ,",
        "analyzed": "samatha samādhim āphusi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_16:9.4",
        "original": "Sāyeva me paramaniyāmatā ahu.",
        "analyzed": "sāyā iva me parama niyāma tā ahu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_16:10.1",
        "original": "Laddhānahaṁ amatavaraṁ visesanaṁ,",
        "analyzed": "amata varam visesana ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_16:10.2",
        "original": "Ekaṁsikā abhisamaye visesiya;",
        "analyzed": "ekasikāḥ abhisamaye ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_16:10.3",
        "original": "Asaṁsayā bahujanapūjitā ahaṁ,",
        "analyzed": "a sasayā bahu jana pūjitā aha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_16:10.4",
        "original": "Khiḍḍāratiṁ paccanubhomanappakaṁ.",
        "analyzed": "khiḍḍa aratim ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_16:11.1",
        "original": "Evaṁ ahaṁ amatadasamhi devatā,",
        "analyzed": "evam aham devatā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_16:11.2",
        "original": "Tathāgatassanadhivarassa sāvikā;",
        "analyzed": "sāvikā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_16:11.3",
        "original": "Dhammaddasā paṭhamaphale patiṭṭhitā,",
        "analyzed": "dhammaddasā paṭhama phale patiṭṭhitā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_16:11.4",
        "original": "Sotāpannā na ca pana matthi duggati.",
        "analyzed": "sotāpannāḥ na ca pana matthi duggati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_16:12.1",
        "original": "Sā vandituṁ anadhivaraṁ upāgamiṁ,",
        "analyzed": "sā vanditum an adhivaram upāgami ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_16:12.2",
        "original": "Pāsādike kusalarate ca bhikkhavo;",
        "analyzed": "pāsādike kusala rate ca bhikkhavo ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_16:12.3",
        "original": "Namassituṁ samaṇasamāgamaṁ sivaṁ,",
        "analyzed": "namassitum samaṇa samāgamam siva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_16:12.4",
        "original": "Sagāravā sirimato dhammarājino.",
        "analyzed": "sa gāravā sirimataḥ dhammarājino ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_16:13.1",
        "original": "Disvā muniṁ muditamanamhi pīṇitā,",
        "analyzed": "disvā munim pīṇitā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_16:13.2",
        "original": "Tathāgataṁ naravaradammasārathiṁ;",
        "analyzed": "tathāgatam nara vara damma sārathi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_16:13.3",
        "original": "Taṇhacchidaṁ kusalarataṁ vināyakaṁ,",
        "analyzed": "taṇha chidam kusala ratam vināyaka ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_16:13.4",
        "original": "Vandāmahaṁ paramahitānukampakan”ti.",
        "analyzed": "vandāmaham parama hita anukampakam ti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_16:14.1",
        "original": "Sirimāvimānaṁ soḷasamaṁ.",
        "analyzed": "sirimāvimānam ",
        "folio": "14"
    }
]