        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_439:5.2",
        "original": "atricchaṁ cakkamāsado;",
        "analyzed": "atriccham cakkam āsadaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_439:5.3",
        "original": "Icchāhatassa posassa,",
        "analyzed": "icchā hatassa posassa ",
