    },
    {
        "segmentnr": "PA_ja_434:0.4",
        "original": "8. Cakkavākajātaka",
        "analyzed": "cakkavākajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_434:1.1",
        "original": "“Kāsāyavatthe sakuṇe vadāmi,",
        "analyzed": "kāsāyavatthe sakuṇe vadāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_434:1.2",
        "original": "Duve duve nandamane carante;",
        "analyzed": "duve duve nandamane carante ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_434:1.3",
        "original": "Kaṁ aṇḍajaṁ aṇḍajā mānusesu,",
        "analyzed": "kam aṇḍa jam aṇḍa jāḥ mānusesu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_434:1.4",
        "original": "Jātiṁ pasaṁsanti tadiṅgha brūtha”.",
        "analyzed": "jātim pasasanti tat iṅgha brūtha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_434:2.1",
        "original": "“Amhe manussesu manussahiṁsa,",
        "analyzed": "amhe manussesu manussa hisa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_434:2.2",
        "original": "Anubbate cakkavāke vadanti;",
        "analyzed": "anubbate cakkavāke vadanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_434:2.3",
        "original": "Kalyāṇabhāvamhe dijesu sammatā,",
        "analyzed": "dijesu sammatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_434:2.4",
        "original": "Abhirūpā vicarāma aṇṇave”.",
        "analyzed": "abhirūpāḥ vicarāma aṇṇave ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_434:3.1",
        "original": "“Kiṁ aṇṇave kāni phalāni bhuñje,",
        "analyzed": "kim kāni phalāni bhuñje ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_434:3.2",
        "original": "Maṁsaṁ kuto khādatha cakkavākā;",
        "analyzed": "masam kutas khādatha cakkavākā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_434:3.3",
        "original": "Kiṁ bhojanaṁ bhuñjatha vo anomā,",
        "analyzed": "kim bhojanam bhuñjatha vaḥ anomā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_434:3.4",
        "original": "Balañca vaṇṇo ca anapparūpā”.",
        "analyzed": "balañca vaṇṇo ca anapparūpā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_434:4.1",
        "original": "“Na aṇṇave santi phalāni dhaṅka,",
        "analyzed": "na aṇṇave santi phalāni dhaṅka ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_434:4.2",
        "original": "Maṁsaṁ kuto khādituṁ cakkavāke;",
        "analyzed": "masam kutas khāditum cakkavāke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_434:4.3",
        "original": "Sevālabhakkhamha apāṇabhojanā,",
        "analyzed": "a pāṇa bhojanā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_434:4.4",
        "original": "Na ghāsahetūpi karoma pāpaṁ”.",
        "analyzed": "na ghāsa hetūpi karoma pāpa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_434:5.1",
        "original": "“Na me idaṁ ruccati cakkavāka,",
        "analyzed": "na me idam ruccati cakkavāka ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_434:5.2",
        "original": "Asmiṁ bhave bhojanasannikāso;",
        "analyzed": "asmin bhave bhojana sannikāsaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_434:5.3",
        "original": "Ahosi pubbe tato me aññathā,",
        "analyzed": "ahosi pubbe tatas me aññathā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_434:5.4",
        "original": "Icceva me vimati ettha jātā.",
        "analyzed": "id ca iva me vimati ettha jātā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_434:6.1",
        "original": "Ahampi maṁsāni phalāni bhuñje,",
        "analyzed": "aham pi masāni phalāni bhuñje ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_434:6.2",
        "original": "Annāni ca loṇiyateliyāni;",
        "analyzed": "annāni ca loṇiya teliyāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_434:6.3",
        "original": "Rasaṁ manussesu labhāmi bhottuṁ,",
        "analyzed": "rasam manussesu labhāmi bhottu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_434:6.4",
        "original": "Sūrova saṅgāmamukhaṁ vijetvā;",
        "analyzed": "sūraḥ uva saṅgāma mukham vijetvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_434:6.5",
        "original": "Na ca me tādiso vaṇṇo,",
        "analyzed": "na ca me tādisaḥ vaṇṇaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_434:6.6",
        "original": "Cakkavāka yathā tava”.",
        "analyzed": "cakkavāka yathā tava ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_434:7.1",
        "original": "“Asuddhabhakkhosi khaṇānupātī,",
        "analyzed": "a suddha bhakkhaḥ asi khaṇa anupātī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_434:7.2",
        "original": "Kicchena te labbhati annapānaṁ;",
        "analyzed": "kicchena te labbhati annapāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_434:7.3",
        "original": "Na tussasī rukkhaphalehi dhaṅka,",
        "analyzed": "na tussasī rukkha phalehi dhaṅka ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_434:7.4",
        "original": "Maṁsāni vā yāni susānamajjhe.",
        "analyzed": "masāni vā yāni susāna majjhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_434:8.1",
        "original": "Yo sāhasena adhigamma bhoge,",
        "analyzed": "yaḥ sāhasena adhigamma bhoge ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_434:8.2",
        "original": "Paribhuñjati dhaṅka khaṇānupātī;",
        "analyzed": "paribhuñjati dhaṅka khaṇa anupātī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_434:8.3",
        "original": "Tato upakkosati naṁ sabhāvo,",
        "analyzed": "tatas upakkosati nam sabhāvaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_434:8.4",
        "original": "Upakkuṭṭho vaṇṇabalaṁ jahāti.",
        "analyzed": "upakkuṭṭhaḥ vaṇṇa balam jahāti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_434:9.1",
        "original": "Appampi ce nibbutiṁ bhuñjatī yadi,",
        "analyzed": "appam pi ce nibbutim bhuñjatī yadi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_434:9.2",
        "original": "Asāhasena aparūpaghātī;",
        "analyzed": "asāhasena aparūpa ghātī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_434:9.3",
        "original": "Balañca vaṇṇo ca tadassa hoti,",
        "analyzed": "balam ca vaṇṇo ca tat assa hoti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_434:9.4",
        "original": "Na hi sabbo āhāramayena vaṇṇo”ti.",
        "analyzed": "na hi sabbaḥ āhāra mayena vaṇṇoti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_434:10.1",
        "original": "Cakkavākajātakaṁ aṭṭhamaṁ.",
        "analyzed": "cakkavāka jātakam aṭṭhama ",
        "folio": "10"
    }
]