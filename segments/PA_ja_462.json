        "analyzed": "tava sīlam janādhipo ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_462:1.3",
        "original": "Ime kumāre pūjento,",
        "analyzed": "ime kumāre ",
        "folio": "1"
    },
    {
