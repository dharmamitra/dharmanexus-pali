        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_519:1.3",
        "original": "Puṭṭhāsi me pāṇipameyyamajjhe,",
        "analyzed": "puṭṭhā asi me pāṇi pameyya majjhe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_519:1.4",
