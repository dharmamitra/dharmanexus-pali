    {
        "segmentnr": "PA_iti_75:0.4",
        "original": "Avuṭṭhikasutta",
        "analyzed": "avuṭṭhika sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_75:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_75:2.1",
        "original": "“Tayome, bhikkhave, puggalā santo saṁvijjamānā lokasmiṁ.",
        "analyzed": "tayo me bhikkhave puggalā santaḥ savijjamānāḥ lokasmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_75:2.2",
        "original": "Katame tayo?",
        "analyzed": "katame tayaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_75:2.3",
        "original": "Avuṭṭhikasamo, padesavassī, sabbatthābhivassī.",
        "analyzed": "avuṭṭhika samaḥ padesa vassī sabbattha abhivassī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_75:3.1",
        "original": "Kathañca, bhikkhave, puggalo avuṭṭhikasamo hoti?",
        "analyzed": "katham ca bhikkhave puggalaḥ avuṭṭhika samaḥ hoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_75:3.2",
        "original": "Idha, bhikkhave, ekacco puggalo sabbesaññeva na dātā hoti, samaṇabrāhmaṇakapaṇaddhikavanibbakayācakānaṁ annaṁ pānaṁ vatthaṁ yānaṁ mālāgandhavilepanaṁ seyyāvasathapadīpeyyaṁ.",
        "analyzed": "idha bhikkhave ekaccaḥ puggalo sabbesam eva na dātā hoti samaṇa brāhmaṇaka paṇaddhika vanibbaka yācakānam annam pānam vattham yānam mālā gandha vilepanam seyyā āvasatha padīpeyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_75:3.3",
        "original": "Evaṁ kho, bhikkhave, puggalo avuṭṭhikasamo hoti.",
        "analyzed": "evam kho bhikkhave puggalaḥ avuṭṭhika samaḥ hoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_75:4.1",
        "original": "Kathañca, bhikkhave, puggalo padesavassī hoti?",
        "analyzed": "katham ca bhikkhave puggalaḥ padesa vassī hoti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_75:4.2_0",
        "original": "Idha, bhikkhave, ekacco puggalo ekaccānaṁ dātā hoti, ekaccānaṁ na dātā hoti samaṇabrāhmaṇakapaṇaddhikavanibbakayācakānaṁ",
        "analyzed": "idha bhikkhave ekaccaḥ puggalaḥ ekaccānam dātā hoti ekaccānam na dātā hoti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_75:4.2_1",
        "original": "annaṁ pānaṁ vatthaṁ yānaṁ mālāgandhavilepanaṁ seyyāvasathapadīpeyyaṁ.",
        "analyzed": "annam pānam vattham yānam mālā gandha vilepanam seyya āvasatha padīpeyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_75:4.3",
        "original": "Evaṁ kho, bhikkhave, puggalo padesavassī hoti.",
        "analyzed": "evam kho bhikkhave puggalaḥ padesa vassī hoti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_75:5.1",
        "original": "Kathañca, bhikkhave, puggalo sabbatthābhivassī hoti?",
        "analyzed": "katham ca bhikkhave puggalaḥ sabbattha abhivassī hoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_75:5.2",
        "original": "Idha, bhikkhave, ekacco puggalo sabbesaṁva deti, samaṇabrāhmaṇakapaṇaddhikavanibbakayācakānaṁ annaṁ pānaṁ vatthaṁ yānaṁ mālāgandhavilepanaṁ seyyāvasathapadīpeyyaṁ.",
        "analyzed": "idha bhikkhave ekaccaḥ puggalaḥ deti samaṇa brāhmaṇaka paṇaddhika vanibbaka yācakānam annam pānam vattham yānam mālā gandha vilepanam seyyā āvasatha padīpeyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_75:5.3",
        "original": "Evaṁ kho, bhikkhave, puggalo sabbatthābhivassī hoti.",
        "analyzed": "evam kho bhikkhave puggalaḥ sabbattha abhivassī hoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_75:5.4",
        "original": "Ime kho, bhikkhave, tayo puggalā santo saṁvijjamānā lokasmin”ti.",
        "analyzed": "ime kho bhikkhave tayo puggalā santaḥ sa vijjamānāḥ lokasmin ti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_75:5.5",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_75:5.6",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_75:6.1",
        "original": "“Na samaṇe na brāhmaṇe,",
        "analyzed": "na samaṇe na brāhmaṇe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_75:6.2",
        "original": "Na kapaṇaddhikavanibbake;",
        "analyzed": "na kapaṇaddhika vanibbake ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_75:6.3",
        "original": "Laddhāna saṁvibhājeti,",
        "analyzed": "savibhājā iti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_75:6.4",
        "original": "Annaṁ pānañca bhojanaṁ;",
        "analyzed": "annam pānañca bhojana ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_75:6.5",
        "original": "Taṁ ve avuṭṭhikasamoti,",
        "analyzed": "tam ve avuṭṭhika samaḥ ti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_75:6.6",
        "original": "Āhu naṁ purisādhamaṁ.",
        "analyzed": "āhuḥ nam purisa adhama ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_75:7.1",
        "original": "Ekaccānaṁ na dadāti,",
        "analyzed": "ekaccānam na dadāti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_75:7.2",
        "original": "ekaccānaṁ pavecchati;",
        "analyzed": "ekaccānam pavā icchati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_75:7.3",
        "original": "Taṁ ve padesavassīti,",
        "analyzed": "tam ve padesavassī iti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_75:7.4",
        "original": "āhu medhāvino janā.",
        "analyzed": "āhu medhāvinaḥ janā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_75:8.1",
        "original": "Subhikkhavāco puriso,",
        "analyzed": "subhikkha vācaḥ puriso ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_75:8.2",
        "original": "sabbabhūtānukampako;",
        "analyzed": "sabba bhūta anukampakaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_75:8.3",
        "original": "Āmodamāno pakireti,",
        "analyzed": "āmodamānaḥ pakira iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_75:8.4",
        "original": "detha dethāti bhāsati.",
        "analyzed": "detha detha iti bhāsati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_75:9.1",
        "original": "Yathāpi megho thanayitvā,",
        "analyzed": "yathā api meghaḥ thanayitvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_75:9.2",
        "original": "gajjayitvā pavassati;",
        "analyzed": "gajjayitvā pavassati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_75:9.3",
        "original": "Thalaṁ ninnañca pūreti,",
        "analyzed": "thalam ninnañca pūra iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_75:9.4",
        "original": "abhisandantova vārinā.",
        "analyzed": "abhisandantaḥ uva vārinā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_75:10.1",
        "original": "Evamevaṁ idhekacco,",
        "analyzed": "evam evam idha ikacco ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_75:10.2",
        "original": "puggalo hoti tādiso;",
        "analyzed": "puggalaḥ hoti tādisaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_75:10.3",
        "original": "Dhammena saṁharitvāna,",
        "analyzed": "dhammena saharitvāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_75:10.4",
        "original": "uṭṭhānādhigataṁ dhanaṁ;",
        "analyzed": "uṭṭhāna adhigatam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_75:10.5",
        "original": "Tappeti annapānena,",
        "analyzed": "anna pānena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_75:10.6",
        "original": "sammā patte vanibbake”ti.",
        "analyzed": "sammā patte vanibbaketi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_75:11.1",
        "original": "Ayampi attho vutto bhagavatā, iti me sutanti.",
        "analyzed": "ayam pi atthaḥ vuttaḥ bhagavatā iti me sutam ti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_iti_75:11.2",
        "original": "Chaṭṭhaṁ.",
        "analyzed": "chaṭṭha ",
        "folio": "11"
    }
]