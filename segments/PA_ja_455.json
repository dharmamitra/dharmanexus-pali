    },
    {
        "segmentnr": "PA_ja_455:1.1",
        "original": "“Tassa nāgassa vippavāsena,",
        "analyzed": "tassa nāgassa vippavāsena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_455:1.2",
        "original": "Virūḷhā sallakī ca kuṭajā ca;",
        "analyzed": "virūḍhā sallakī ca kuṭajā ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_455:1.3",
        "original": "Kuruvindakaravīrā bhisasāmā ca,",
        "analyzed": "kuruvinda karavīrāḥ bhisa sāmā ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_455:1.4",
        "original": "Nivāte pupphitā ca kaṇikārā.",
        "analyzed": "nivāte pupphitā ca kaṇikārā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_455:2.1",
        "original": "Kocideva suvaṇṇakāyurā,",
        "analyzed": "kocid eva suvaṇṇaka āyurā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_455:2.2",
        "original": "Nāgarājaṁ bharanti piṇḍena;",
        "analyzed": "nāga rājam bharanti piṇḍena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_455:2.3",
        "original": "Yattha rājā rājakumāro vā,",
        "analyzed": "yattha rājā rāja kumāraḥ vā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_455:2.4",
        "original": "Kavacamabhihessati achambhito”.",
        "analyzed": "kavacam abhihessati a chambhitaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_455:3.1",
        "original": "“Gaṇhāhi nāga kabaḷaṁ,",
        "analyzed": "gaṇhāhi nāga kabaḷa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_455:3.2",
        "original": "mā nāga kisako bhava;",
        "analyzed": "mā nāga kisakaḥ bhava ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_455:3.3",
        "original": "Bahūni rājakiccāni,",
        "analyzed": "bahūni rāja kiccāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_455:3.4",
        "original": "tāni nāga karissasi”.",
        "analyzed": "tāni nāga karissasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_455:4.1",
        "original": "“Sā nūnasā kapaṇikā,",
        "analyzed": "sā nūnasā kapaṇikā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_455:4.2",
        "original": "andhā apariṇāyikā;",
        "analyzed": "andhā apariṇāyikā ",
        "folio": "4"
    },
