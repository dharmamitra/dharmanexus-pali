    },
    {
        "segmentnr": "PA_ps_3.5:1.2",
        "original": "iriyāpathacariyā, āyatanacariyā, saticariyā, samādhicariyā, ñāṇacariyā, maggacariyā, patticariyā, lokatthacariyāti.",
        "analyzed": "iriyāpatha cariyā āyatana cariyā sati cariyā samādhi cariyā ñāṇa cariyā magga cariyā patti cariyā lokattha cariyā ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ps_3.5:2.1",
        "original": "Iriyāpathacariyāti catūsu iriyāpathesu.",
        "analyzed": "iriyāpatha cariyā ati catūsu iriyāpathesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:2.2",
        "original": "Āyatanacariyāti chasu ajjhattikabāhiresu āyatanesu.",
        "analyzed": "āyatana cariyā ti chasu ajjhattika bāhiresu āyatanesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:2.3",
        "original": "Saticariyāti catūsu satipaṭṭhānesu.",
        "analyzed": "sati cariyā ati catūsu sati paṭṭhānesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:2.4",
        "original": "Samādhicariyāti catūsu jhānesu.",
        "analyzed": "samādhi cariyā ati catūsu jhānesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:2.5",
        "original": "Ñāṇacariyāti catūsu ariyasaccesu.",
        "analyzed": "ñāṇa cariyā ati catūsu ariya saccesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:2.6",
        "original": "Maggacariyāti catūsu ariyamaggesu.",
        "analyzed": "magga cariyā ati catūsu ariya maggesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:2.7",
        "original": "Patticariyāti catūsu sāmaññaphalesu.",
        "analyzed": "patti cariyā ati catūsu sāmañña phalesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:2.8",
        "original": "Lokatthacariyāti tathāgatesu arahantesu sammāsambuddhesu padese paccekabuddhesu padese sāvakesu.",
        "analyzed": "lokattha cariyā ati tathāgatesu arahantesu sammāsambuddhesu padese paccekabuddhesu padese sāvakesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_3.5:3.1",
        "original": "Iriyāpathacariyā ca paṇidhisampannānaṁ.",
