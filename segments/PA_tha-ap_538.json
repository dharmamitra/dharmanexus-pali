        "original": "‘arahāyamidhāgato.",
        "analyzed": "arahā ayam idha āgataḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_tha-ap_538:17.1",
        "original": "Imaṁ annena pānena,",
        "analyzed": "imam annena pānena ",
        "folio": "17"
    },
