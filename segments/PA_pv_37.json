[
    {
        "segmentnr": "PA_pv_37:0.1",
        "original": "Petavatthu",
        "analyzed": "petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_37:0.2",
        "original": "Mahāvagga",
        "analyzed": "mahāvagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_37:0.3",
        "original": "2. Serīsakapetavatthu",
        "analyzed": "sa irīsaka petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_37:1.1",
        "original": "“Suṇotha yakkhassa vāṇijāna ca,",
        "analyzed": "suṇotha yakkhassa vāṇijāna ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_37:1.2",
        "original": "Samāgamo yattha tadā ahosi;",
        "analyzed": "samāgamaḥ yattha tadā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_37:1.3",
        "original": "Yathā kathaṁ itaritarena cāpi,",
        "analyzed": "yathā katham itaritarena ca api ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_37:1.4",
        "original": "Subhāsitaṁ tañca suṇātha sabbe.",
        "analyzed": "su bhāsitam tat ca suṇātha sabbe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_37:2.1",
        "original": "Yo so ahu rājā pāyāsi nāma,",
        "analyzed": "yaḥ saḥ ahu rājā pāyāsi nāma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_37:2.2",
        "original": "Bhummānaṁ sahabyagato yasassī;",
        "analyzed": "bhummānam sahabya gataḥ yasassī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_37:2.3",
        "original": "So modamānova sake vimāne,",
        "analyzed": "saḥ modamānā iva sake vimāne ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_37:2.4",
        "original": "Amānuso mānuse ajjhabhāsī”ti.",
        "analyzed": "amānusaḥ mānuse ajjhabhāsi iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_37:3.1",
        "original": "“Vaṅke araññe amanussaṭṭhāne,",
        "analyzed": "vaṅke araññe a manussa ṭhāne ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_37:3.2",
        "original": "Kantāre appodake appabhakkhe;",
        "analyzed": "kantāre appodake appabhakkhe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_37:3.3",
        "original": "Suduggame vaṇṇupathassa majjhe,",
        "analyzed": "su duggame vaṇṇu pathassa majjhe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_37:3.4",
        "original": "Vaṅkambhayā naṭṭhamanā manussā.",
        "analyzed": "vaṅkambhayā naṭṭha manā manussā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_37:4.1",
        "original": "Nayidha phalā mūlamayā ca santi,",
        "analyzed": "phalā mūla mayā ca santi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_37:4.2",
        "original": "Upādānaṁ natthi kutodha bhakkho;",
        "analyzed": "upādānam na atthi kutas dha bhakkho ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_37:4.3",
        "original": "Aññatra paṁsūhi ca vālukāhi ca,",
        "analyzed": "aññatra pasūhi ca vālukāhi ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_37:4.4",
        "original": "Tatāhi uṇhāhi ca dāruṇāhi ca.",
        "analyzed": "tatāhi uṇhāhi ca dāruṇāhi ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_37:5.1",
        "original": "Ujjaṅgalaṁ tattamivaṁ kapālaṁ,",
        "analyzed": "ujjaṅgalam kapāla ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_37:5.2",
        "original": "Anāyasaṁ paralokena tulyaṁ;",
        "analyzed": "anāyasam para lokena tulya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_37:5.3",
        "original": "Luddānamāvāsamidaṁ purāṇaṁ,",
        "analyzed": "purāṇa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_37:5.4",
        "original": "Bhūmippadeso abhisattarūpo.",
        "analyzed": "bhūmippadeso abhisatta rūpo ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_37:6.1",
        "original": "Atha tumhe kena vaṇṇena,",
        "analyzed": "atha tumhe kena vaṇṇena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_37:6.2",
        "original": "Kimāsamānā imaṁ padesaṁ hi;",
        "analyzed": "imam padesam hi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_37:6.3",
        "original": "Anupaviṭṭhā sahasā samacca,",
        "analyzed": "an upaviṭṭhā sahasā samat ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_37:6.4",
        "original": "Lobhā bhayā atha vā sampamūḷhā”ti.",
        "analyzed": "lobhā bhayā atha vā sampamūḍhāti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_37:7.1",
        "original": "“Magadhesu aṅgesu ca satthavāhā,",
        "analyzed": "magadhesu aṅgesu ca satthavāhā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_37:7.2",
        "original": "Āropayitvā paṇiyaṁ puthuttaṁ;",
        "analyzed": "āropayitvā paṇiyam puthutta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_37:7.3",
        "original": "Te yāmase sindhusovīrabhūmiṁ,",
        "analyzed": "te yāmase sindhusovīra bhūmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_37:7.4",
        "original": "Dhanatthikā uddayaṁ patthayānā.",
        "analyzed": "dhanatthikā uddayam patthayānā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_37:8.1",
        "original": "Divā pipāsaṁ nadhivāsayantā,",
        "analyzed": "divā pipāsam na adhivāsayantā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_37:8.2",
        "original": "Yoggānukampañca samekkhamānā;",
        "analyzed": "yoggā anukampañca samekkhamānā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_37:8.3",
        "original": "Etena vegena āyāma sabbe,",
        "analyzed": "etena vegena āyāma sabbe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_37:8.4",
        "original": "Rattiṁ maggaṁ paṭipannā vikāle.",
        "analyzed": "rattim maggam paṭipannāḥ vikāle ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_37:9.1",
        "original": "Te duppayātā aparaddhamaggā,",
        "analyzed": "te duppayātāḥ aparaddha maggā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_37:9.2",
        "original": "Andhākulā vippanaṭṭhā araññe;",
        "analyzed": "andha ākulāḥ vippanaṭṭhāḥ araññe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_37:9.3",
        "original": "Suduggame vaṇṇupathassa majjhe,",
        "analyzed": "su duggame vaṇṇu pathassa majjhe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_37:9.4",
        "original": "Disaṁ na jānāma pamūḷhacittā.",
        "analyzed": "disam na jānāma pamūḍha cittā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_37:10.1",
        "original": "Idañca disvāna adiṭṭhapubbaṁ,",
        "analyzed": "idam ca disvāna a diṭṭha pubba ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_37:10.2",
        "original": "Vimānaseṭṭhañca tavañca yakkha;",
        "analyzed": "vimānaseṭṭham ca tavañca yakkha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_37:10.3",
        "original": "Tatuttariṁ jīvitamāsamānā,",
        "analyzed": "tat uttarim jīvitam āsamānā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_37:10.4",
        "original": "Disvā patītā sumanā udaggā”ti.",
        "analyzed": "disvā patītā sumanāḥ udaggāti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_37:11.1",
        "original": "“Pāraṁ samuddassa imañca vaṇṇuṁ,",
        "analyzed": "pāram samuddasse imañca vaṇṇu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_37:11.2",
        "original": "Vettācaraṁ saṅkupathañca maggaṁ;",
        "analyzed": "vettā caram saṅku patham ca magga ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_37:11.3",
        "original": "Nadiyo pana pabbatānañca duggā,",
        "analyzed": "nadiyaḥ pana pabbatānam ca duggā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_37:11.4",
        "original": "Puthuddisā gacchatha bhogahetu.",
        "analyzed": "puthuddisā gacchatha bhoga hetu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_37:12.1",
        "original": "Pakkhandiyāna vijitaṁ paresaṁ,",
        "analyzed": "pakkhandiyāna vijitam paresa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_37:12.2",
        "original": "Verajjake mānuse pekkhamānā;",
        "analyzed": "verajjake mānuse pekkhamānā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_37:12.3",
        "original": "Yaṁ vo sutaṁ vā atha vāpi diṭṭhaṁ,",
        "analyzed": "yam vaḥ sutam vai atha vā api ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_37:12.4",
        "original": "Accherakaṁ taṁ vo suṇoma tātā”ti.",
        "analyzed": "accherakam tam vaḥ suṇoma tāta ati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_37:13.1",
        "original": "“Itopi accherataraṁ kumāra,",
        "analyzed": "itas upi accherataram kumāra ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_37:13.2",
        "original": "Na no sutaṁ vā atha vāpi diṭṭhaṁ;",
        "analyzed": "na naḥ sutam vai atha vā api diṭṭha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_37:13.3",
        "original": "Atītamānussakameva sabbaṁ,",
        "analyzed": "atīta mānussakam eva ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_37:13.4",
        "original": "Disvā na tappāma anomavaṇṇaṁ.",
        "analyzed": "disvā na tappāma anomavaṇṇa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_37:14.1",
        "original": "Vehāyasaṁ pokkharañño savanti,",
        "analyzed": "vehāyasam pokkharaññaḥ savanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_37:14.2",
        "original": "Pahūtamalyā bahupuṇḍarīkā;",
        "analyzed": "bahu puṇḍarīkā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_37:14.3",
        "original": "Dumā cime niccaphalūpapannā,",
        "analyzed": "dumā cime nicca phala upapannā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_37:14.4",
        "original": "Atīva gandhā surabhiṁ pavāyanti.",
        "analyzed": "atīva gandhā surabhim pavāyanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_37:15.1",
        "original": "Veḷūriyathambhā satamussitāse,",
        "analyzed": "veḷūriya thambhā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_37:15.2",
        "original": "Silāpavāḷassa ca āyataṁsā;",
        "analyzed": "silāpavāḷassa ca āya tasā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_37:15.3",
        "original": "Masāragallā sahalohitaṅgā,",
        "analyzed": "masāragallā saha lohitaṅgā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_37:15.4",
        "original": "Thambhā ime jotirasāmayāse.",
        "analyzed": "thambhāḥ ime jotiḥ asāmayāse ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_37:16.1",
        "original": "Sahassathambhaṁ atulānubhāvaṁ,",
        "analyzed": "sahassa thambham atula anubhāva ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_37:16.2",
        "original": "Tesūpari sādhumidaṁ vimānaṁ;",
        "analyzed": "sādhum idam ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_37:16.3",
        "original": "Ratanantaraṁ kañcanavedimissaṁ,",
        "analyzed": "rata anantaram kañcana vedimiḥ sa ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_37:16.4",
        "original": "Tapanīyapaṭṭehi ca sādhuchannaṁ.",
        "analyzed": "ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_37:17.1",
        "original": "Jambonaduttattamidaṁ sumaṭṭho,",
        "analyzed": "sumaṭṭhaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_37:17.2",
        "original": "Pāsādasopānaphalūpapanno;",
        "analyzed": "pāsāda sopāna phala upapannaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_37:17.3",
        "original": "Daḷho ca vaggu ca susaṅgato ca,",
        "analyzed": "ca vaggu ca su saṅgataḥ ca ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_37:17.4",
        "original": "Atīva nijjhānakhamo manuñño.",
        "analyzed": "atīva nijjhāna khamaḥ manuññaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_37:18.1",
        "original": "Ratanantarasmiṁ bahuannapānaṁ,",
        "analyzed": "bahu anna pāna ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_37:18.2",
        "original": "Parivārito accharāsaṅgaṇena;",
        "analyzed": "parivāritaḥ accha rāsaṅgaṇena ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_37:18.3",
        "original": "Murajaālambaratūriyaghuṭṭho,",
        "analyzed": "muraja ālambara tūriya ghuṭṭhaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_37:18.4",
        "original": "Abhivanditosi thutivandanāya.",
        "analyzed": "abhivanditaḥ asi thuti vandanāya ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_37:19.1",
        "original": "So modasi nārigaṇappabodhano,",
        "analyzed": "saḥ modasi nāri gaṇa ppabodhano ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_37:19.2",
        "original": "Vimānapāsādavare manorame;",
        "analyzed": "vimāna pāsāda vare manorame ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_37:19.3",
        "original": "Acintiyo sabbaguṇūpapanno,",
        "analyzed": "acintiyaḥ sabba guṇa upapannaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_37:19.4",
        "original": "Rājā yathā vessavaṇo naḷinyā.",
        "analyzed": "rājā yathā vessavaṇaḥ naḷinyā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_37:20.1",
        "original": "Devo nu āsi udavāsi yakkho,",
        "analyzed": "devaḥ nu āsi udavāsi yakkhaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_37:20.2",
        "original": "Udāhu devindo manussabhūto;",
        "analyzed": "uda ahu devindaḥ manussa bhūtaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_37:20.3",
        "original": "Pucchanti taṁ vāṇijā satthavāhā,",
        "analyzed": "pucchanti tam vāṇijāḥ satthavāhāḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_37:20.4",
        "original": "Ācikkha ko nāma tuvaṁsi yakkho”ti.",
        "analyzed": "ācikkha kaḥ nāma tuvasi yakkho ti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_37:21.1",
        "original": "“Serīsako nāma ahamhi yakkho,",
        "analyzed": "serīsakaḥ nāma ahamhi yakkho ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_37:21.2",
        "original": "Kantāriyo vaṇṇupathamhi gutto;",
        "analyzed": "kantāriyaḥ vaṇṇu pathamhi gutto ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_37:21.3",
        "original": "Imaṁ padesaṁ abhipālayāmi,",
        "analyzed": "imam padesam abhipālayāmi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_37:21.4",
        "original": "Vacanakaro vessavaṇassa rañño”ti.",
        "analyzed": "vacana karaḥ vessavaṇassa rañño ti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_37:22.1",
        "original": "“Adhiccaladdhaṁ pariṇāmajaṁ te,",
        "analyzed": "pariṇāma jam te ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_pv_37:22.2",
        "original": "Sayaṅkataṁ udāhu devehi dinnaṁ;",
        "analyzed": "sayaṅkatam udāhu devehi dinna ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_pv_37:22.3",
        "original": "Pucchanti taṁ vāṇijā satthavāhā,",
        "analyzed": "pucchanti tam vāṇijāḥ satthavāhāḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_pv_37:22.4",
        "original": "Kathaṁ tayā laddhamidaṁ manuññan”ti.",
        "analyzed": "katham tayā laddham idam ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_pv_37:23.1",
        "original": "“Nādhiccaladdhaṁ na pariṇāmajaṁ me,",
        "analyzed": "na pariṇāma jam me ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_pv_37:23.2",
        "original": "Na sayaṅkataṁ na hi devehi dinnaṁ;",
        "analyzed": "na sayaṅkatam na hi devehi dinna ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_pv_37:23.3",
        "original": "Sakehi kammehi apāpakehi,",
        "analyzed": "sakehi kammehi a pāpakehi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_pv_37:23.4",
        "original": "Puññehi me laddhamidaṁ manuññan”ti.",
        "analyzed": "puññehi me laddham idam manuññanti ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_pv_37:24.1",
        "original": "“Kiṁ te vataṁ kiṁ pana brahmacariyaṁ,",
        "analyzed": "kim te kim pana ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_pv_37:24.2",
        "original": "Kissa suciṇṇassa ayaṁ vipāko;",
        "analyzed": "kissa su ciṇṇassa ayam vipākaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_pv_37:24.3",
        "original": "Pucchanti taṁ vāṇijā satthavāhā,",
        "analyzed": "pucchanti tam vāṇijāḥ satthavāhāḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_pv_37:24.4",
        "original": "Kathaṁ tayā laddhamidaṁ vimānan”ti.",
        "analyzed": "katham tayā laddham idam vimānanti ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_pv_37:25.1",
        "original": "“Mamaṁ pāyāsīti ahu samaññā,",
        "analyzed": "mamam pāyāsi iti ahu samaññā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_pv_37:25.2",
        "original": "Rajjaṁ yadā kārayiṁ kosalānaṁ;",
        "analyzed": "rajjam yadā kārayim kosalāna ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_pv_37:25.3",
        "original": "Natthikadiṭṭhi kadariyo pāpadhammo,",
        "analyzed": "natthika diṭṭhi kadariyaḥ pāpa dhammo ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_pv_37:25.4",
        "original": "Ucchedavādī ca tadā ahosiṁ.",
        "analyzed": "uccheda vādī ca tadā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_pv_37:26.1",
        "original": "Samaṇo ca kho āsi kumārakassapo,",
        "analyzed": "samaṇaḥ ca kho āsi kumārakassapaḥ ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_pv_37:26.2",
        "original": "Bahussuto cittakathī uḷāro;",
        "analyzed": "bahu sutaḥ citta kathī uḷāro ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_pv_37:26.3",
        "original": "So me tadā dhammakathaṁ abhāsi,",
        "analyzed": "saḥ me tadā dhamma katham abhāsi ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_pv_37:26.4",
        "original": "Diṭṭhivisūkāni vinodayī me.",
        "analyzed": "diṭṭhi visūkāni me ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_pv_37:27.1",
        "original": "Tāhaṁ tassa dhammakathaṁ suṇitvā,",
        "analyzed": "tāham tassa dhamma katham suṇitvā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_pv_37:27.2",
        "original": "Upāsakattaṁ paṭivedayissaṁ;",
        "analyzed": "upāsaka ttam paṭivedayissa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_pv_37:27.3",
        "original": "Pāṇātipātā virato ahosiṁ,",
        "analyzed": "pāṇa atipātā virataḥ ahosi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_pv_37:27.4",
        "original": "Loke adinnaṁ parivajjayissaṁ;",
        "analyzed": "loke adinnam parivajjayissa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_pv_37:27.5",
        "original": "Amajjapo no ca musā abhāṇiṁ,",
        "analyzed": "na u ca musāḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_pv_37:27.6",
        "original": "Sakena dārena ca ahosi tuṭṭho.",
        "analyzed": "sakena dārena ca ahosi tuṭṭho ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_pv_37:28.1",
        "original": "Taṁ me vataṁ taṁ pana brahmacariyaṁ,",
        "analyzed": "tam me tam pana brahmacariya ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_pv_37:28.2",
        "original": "Tassa suciṇṇassa ayaṁ vipāko;",
        "analyzed": "tassa su ciṇṇassa ayam vipākaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_pv_37:28.3",
        "original": "Teheva kammehi apāpakehi,",
        "analyzed": "tehi iva kammehi a pāpakehi ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_pv_37:28.4",
        "original": "Puññehi me laddhamidaṁ vimānan”ti.",
        "analyzed": "puññehi me laddham idam vimānanti ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_pv_37:29.1",
        "original": "“Saccaṁ kirāhaṁsu narā sapaññā,",
        "analyzed": "saccam narā sa paññā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_pv_37:29.2",
        "original": "Anaññathā vacanaṁ paṇḍitānaṁ;",
        "analyzed": "an aññathā vacanam ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_pv_37:29.3",
        "original": "Yahiṁ yahiṁ gacchati puññakammo,",
        "analyzed": "yahim yahim gacchati puñña kammaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_pv_37:29.4",
        "original": "Tahiṁ tahiṁ modati kāmakāmī.",
        "analyzed": "tahim tahim modati kāma kāmī ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_pv_37:30.1",
        "original": "Yahiṁ yahiṁ sokapariddavo ca,",
        "analyzed": "yahim yahim soka pariddavaḥ ca ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_pv_37:30.2",
        "original": "Vadho ca bandho ca parikkileso;",
        "analyzed": "vadho ca bandho ca parikkileso ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_pv_37:30.3",
        "original": "Tahiṁ tahiṁ gacchati pāpakammo,",
        "analyzed": "tahim tahim gacchati pāpa kammaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_pv_37:30.4",
        "original": "Na muccati duggatiyā kadācī”ti.",
        "analyzed": "na muccati duggatiyā kadāci iti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_pv_37:31.1",
        "original": "“Sammūḷharūpova jano ahosi,",
        "analyzed": "sammūḍha rūpaḥ uva janaḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_pv_37:31.2",
        "original": "Asmiṁ muhutte kalalīkatova;",
        "analyzed": "asmin muhutte kalalīkataḥ uva ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_pv_37:31.3",
        "original": "Janassimassa tuyhañca kumāra,",
        "analyzed": "janassimassa tuyhañca kumāra ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_pv_37:31.4",
        "original": "Appaccayo kena nu kho ahosī”ti.",
        "analyzed": "appaccayaḥ kena nu kho ahosi iti ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_pv_37:32.1",
        "original": "“Ime ca sirīsavanā tātā,",
        "analyzed": "ime ca sirīsavanā tātā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_pv_37:32.2",
        "original": "Dibbā gandhā surabhī sampavanti;",
        "analyzed": "dibbāḥ gandhāḥ surabhī sampavanti ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_pv_37:32.3",
        "original": "Te sampavāyanti imaṁ vimānaṁ,",
        "analyzed": "te sampavāyanti imam ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_pv_37:32.4",
        "original": "Divā ca ratto ca tamaṁ nihantvā.",
        "analyzed": "divā ca ca ca tamam nihantvā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_pv_37:33.1",
        "original": "Imesañca kho vassasataccayena,",
        "analyzed": "imesam ca khaḥ vassa sata cayena ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_pv_37:33.2",
        "original": "Sipāṭikā phalati ekamekā;",
        "analyzed": "sipāṭikā phalati ekam ekā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_pv_37:33.3",
        "original": "Mānussakaṁ vassasataṁ atītaṁ,",
        "analyzed": "mānussakam vassa satam atīta ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_pv_37:33.4",
        "original": "Yadagge kāyamhi idhūpapanno.",
        "analyzed": "yad agge kāyamhi idhu upapannaḥ ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_pv_37:34.1",
        "original": "Disvānahaṁ vassasatāni pañca,",
        "analyzed": "disvān aham vassa satāni pañca ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_pv_37:34.2",
        "original": "Asmiṁ vimāne ṭhatvāna tātā;",
        "analyzed": "asmin vimāne tātā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_pv_37:34.3",
        "original": "Āyukkhayā puññakkhayā cavissaṁ,",
        "analyzed": "āyu khayā puñña khayā cavissa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_pv_37:34.4",
        "original": "Teneva sokena pamucchitosmī”ti.",
        "analyzed": "tena iva sokena pamucchitaḥ asmi iti ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_pv_37:35.1",
        "original": "“Kathaṁ nu soceyya tathāvidho so,",
        "analyzed": "katham nu tathāvidhaḥ saḥ ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_pv_37:35.2",
        "original": "Laddhā vimānaṁ atulaṁ cirāya;",
        "analyzed": "laddhā vimānam atulam cirāya ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_pv_37:35.3",
        "original": "Ye cāpi kho ittaramupapannā,",
        "analyzed": "ye ca api kho ittaram upapannā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_pv_37:35.4",
        "original": "Te nūna soceyyuṁ parittapuññā”ti.",
        "analyzed": "te nūna soceyyum paritta puññā iti ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_pv_37:36.1",
        "original": "“Anucchaviṁ ovadiyañca me taṁ,",
        "analyzed": "anucchavim me te ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_pv_37:36.2",
        "original": "Yaṁ maṁ tumhe peyyavācaṁ vadetha;",
        "analyzed": "yam mama tumhe peyya vācam vadetha ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_pv_37:36.3",
        "original": "Tumhe ca kho tātā mayānuguttā,",
        "analyzed": "tumhe ca kho tātā mayā anuguttā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_pv_37:36.4",
        "original": "Yenicchakaṁ tena paletha sotthin”ti.",
        "analyzed": "tena ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_pv_37:37.1",
        "original": "“Gantvā mayaṁ sindhusovīrabhūmiṁ,",
        "analyzed": "gantvā mayam sindhusovīra bhūmi ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_pv_37:37.2",
        "original": "Dhanatthikā uddayaṁ patthayānā;",
        "analyzed": "dhanatthikā uddayam patthayānā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_pv_37:37.3",
        "original": "Yathāpayogā paripuṇṇacāgā,",
        "analyzed": "yathā apayogā paripuṇṇa cāgā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_pv_37:37.4",
        "original": "Kāhāma serīsamahaṁ uḷāran”ti.",
        "analyzed": "uḷāranti ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_pv_37:38.1",
        "original": "“Mā ceva serīsamahaṁ akattha,",
        "analyzed": "mā ca iva akattha ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_pv_37:38.2",
        "original": "Sabbañca vo bhavissati yaṁ vadetha;",
        "analyzed": "sabbam ca vaḥ bhavissati yam vadetha ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_pv_37:38.3",
        "original": "Pāpāni kammāni vivajjayātha,",
        "analyzed": "pāpāni kammāni vivajjayātha ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_pv_37:38.4",
        "original": "Dhammānuyogañca adhiṭṭhahātha.",
        "analyzed": "dhamma anuyogañca adhiṭṭhahātha ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_pv_37:39.1",
        "original": "Upāsako atthi imamhi saṅghe,",
        "analyzed": "upāsakaḥ atthi imamhi saṅghe ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_pv_37:39.2",
        "original": "Bahussuto sīlavatūpapanno;",
        "analyzed": "bahu sutaḥ sīlavatu upapannaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_pv_37:39.3",
        "original": "Saddho ca cāgī ca supesalo ca,",
        "analyzed": "saddhaḥ ca cāgī ca su pesalaḥ ca ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_pv_37:39.4",
        "original": "Vicakkhaṇo santusito mutīmā.",
        "analyzed": "vicakkhaṇaḥ santusitaḥ mutīmā ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_pv_37:40.1",
        "original": "Sañjānamāno na musā bhaṇeyya,",
        "analyzed": "sañjānamānaḥ na musā bhaṇeyya ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_pv_37:40.2",
        "original": "Parūpaghātāya na cetayeyya;",
        "analyzed": "parūpa ghātāya na cetayeyya ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_pv_37:40.3",
        "original": "Vebhūtikaṁ pesuṇaṁ no kareyya,",
        "analyzed": "vebhūtikam pesuṇam no kareyya ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_pv_37:40.4",
        "original": "Saṇhañca vācaṁ sakhilaṁ bhaṇeyya.",
        "analyzed": "saṇhan ca vācam sa khilam bhaṇeyya ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_pv_37:41.1",
        "original": "Sagāravo sappatisso vinīto,",
        "analyzed": "sa gāravaḥ sa patissaḥ vinītaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_pv_37:41.2",
        "original": "Apāpako adhisīle visuddho;",
        "analyzed": "a pāpakaḥ adhi sīle visuddhaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_pv_37:41.3",
        "original": "So mātaraṁ pitarañcāpi jantu,",
        "analyzed": "saḥ mātaram pitaram ca api ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_pv_37:41.4",
        "original": "Dhammena poseti ariyavutti.",
        "analyzed": "dhammena poseti ariya vutti ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_pv_37:42.1",
        "original": "Maññe so mātāpitūnaṁ kāraṇā,",
        "analyzed": "maññe saḥ mātā pitūnam kāraṇā ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_pv_37:42.2",
        "original": "Bhogāni pariyesati na attahetu;",
        "analyzed": "bhogāni pariyesati na atta hetu ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_pv_37:42.3",
        "original": "Mātāpitūnañca yo accayena,",
        "analyzed": "mātā pitūnam ca yaḥ a cayena ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_pv_37:42.4",
        "original": "Nekkhammapoṇo carissati brahmacariyaṁ.",
        "analyzed": "nekkhamma poṇaḥ carissati brahmacariya ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_pv_37:43.1",
        "original": "Ujū avaṅko asaṭho amāyo,",
        "analyzed": "ujū avaṅkaḥ asaṭhaḥ amāyaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_pv_37:43.2",
        "original": "Na lesakappena ca vohareyya;",
        "analyzed": "na lesa kappena ca vohareyya ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_pv_37:43.3",
        "original": "So tādiso sukatakammakārī,",
        "analyzed": "so tādiso su kata kamma kārī ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_pv_37:43.4",
        "original": "Dhamme ṭhito kinti labhetha dukkhaṁ.",
        "analyzed": "dhamme ṭhitaḥ kinti labhetha dukkha ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_pv_37:44.1",
        "original": "Taṁ kāraṇā pātukatomhi attanā,",
        "analyzed": "tam kāraṇā pātukataḥ amhi attanā ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_pv_37:44.2",
        "original": "Tasmā dhammaṁ passatha vāṇijāse;",
        "analyzed": "tasmā dhammam passatha vāṇijāse ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_pv_37:44.3",
        "original": "Aññatra teniha bhasmī bhavetha,",
        "analyzed": "aññatra tena iha bhasmī bhavetha ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_pv_37:44.4",
        "original": "Andhākulā vippanaṭṭhā araññe;",
        "analyzed": "andha ākulāḥ vippanaṭṭhāḥ araññe ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_pv_37:44.5",
        "original": "Taṁ khippamānena lahuṁ parena,",
        "analyzed": "tam khippamānena lahum parena ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_pv_37:44.6",
        "original": "Sukho have sappurisena saṅgamo”ti.",
        "analyzed": "sukhaḥ have sappurisena saṅgamaḥ uti ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_pv_37:45.1",
        "original": "“Kiṁ nāma so kiñca karoti kammaṁ,",
        "analyzed": "kim nāma saḥ kiñca karoti kamma ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_pv_37:45.2",
        "original": "Kiṁ nāmadheyyaṁ kiṁ pana tassa gottaṁ;",
        "analyzed": "kim nāmadheyyam kim pana tassa gotta ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_pv_37:45.3",
        "original": "Mayampi naṁ daṭṭhukāmamha yakkha,",
        "analyzed": "mayam pi nam daṭṭhu kāmamha yakkha ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_pv_37:45.4",
        "original": "Yassānukampāya idhāgatosi;",
        "analyzed": "yassa anukampāyai idha āgataḥ asi ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_pv_37:45.5",
        "original": "Lābhā hi tassa yassa tuvaṁ pihesī”ti.",
        "analyzed": "lābhā hi tassa yassa tuvam pihesi iti ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_pv_37:46.1",
        "original": "“Yo kappako sambhavanāmadheyyo,",
        "analyzed": "yaḥ kappako sambhava nāmadheyyaḥ ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_pv_37:46.2",
        "original": "Upāsako kocchaphalūpajīvī;",
        "analyzed": "upāsakaḥ ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_pv_37:46.3",
        "original": "Jānātha naṁ tumhākaṁ pesiyo so,",
        "analyzed": "jānātha nam tumhākam pesiyaḥ saḥ ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_pv_37:46.4",
        "original": "Mā kho naṁ hīḷittha supesalo so”ti.",
        "analyzed": "mā khalu nam hīḍittha su pesalaḥ sa uti ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_pv_37:47.1",
        "original": "“Jānāmase yaṁ tvaṁ pavadesi yakkha,",
        "analyzed": "jānāmase yam tvam pavadesi yakkha ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_pv_37:47.2",
        "original": "Na kho naṁ jānāma sa edisoti;",
        "analyzed": "na khalu nam jānāma saḥ edisaḥ iti ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_pv_37:47.3",
        "original": "Mayampi naṁ pūjayissāma yakkha,",
        "analyzed": "mayam pi nam pūjayissāma yakkha ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_pv_37:47.4",
        "original": "Sutvāna tuyhaṁ vacanaṁ uḷāran”ti.",
        "analyzed": "sutvāna tuyham vacanam uḷāranti ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_pv_37:48.1",
        "original": "“Ye keci imasmiṁ satthe manussā,",
        "analyzed": "ye keci imasmin satthe manussā ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_pv_37:48.2",
        "original": "Daharā mahantā athavāpi majjhimā;",
