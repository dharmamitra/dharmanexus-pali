        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.2",
        "original": "Tena kho pana samayena aññatarā bhikkhunī asaṅkaccikā gāmaṁ piṇḍāya pāvisi.",
        "analyzed": "tena kho pana samayena aññatarā bhikkhunī a saṅkaccikā gāmam piṇḍāya pāvisi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.3",
        "original": "Tassā rathikāya vātamaṇḍalikā saṅghāṭiyo ukkhipiṁsu.",
        "analyzed": "tassāḥ rathikāya vātamaṇḍalikā saṅghāṭiyaḥ ukkhipisu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.4",
        "original": "Manussā ukkuṭṭhiṁ akaṁsu—",
        "analyzed": "manussāḥ ukkuṭṭhim akasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.5",
        "original": "“sundarā ayyāya thanudarā”ti.",
        "analyzed": "sundarā ayyāya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.6",
        "original": "Sā bhikkhunī tehi manussehi uppaṇḍiyamānā maṅku ahosi.",
        "analyzed": "sā bhikkhunī tehi manussehi uppaṇḍiyamānā maṅku ahosi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.7",
        "original": "Atha kho sā bhikkhunī upassayaṁ gantvā bhikkhunīnaṁ etamatthaṁ ārocesi.",
        "analyzed": "atha kho sā bhikkhunī upassayam gantvā bhikkhunīnam etam attham ārocesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.8",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.9",
        "original": "“kathañhi nāma bhikkhunī asaṅkaccikā gāmaṁ pavisissatī”ti …pe…",
        "analyzed": "katham hi nāma bhikkhunī a saṅkaccikā gāmam pavisissati iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.10",
        "original": "“saccaṁ kira, bhikkhave, bhikkhunī asaṅkaccikā gāmaṁ pāvisī”ti?",
        "analyzed": "saccam kira bhikkhave bhikkhunī a saṅkaccikā gāmam pāvisi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc96:1.11",
        "original": "“Saccaṁ, bhagavā”ti.",
