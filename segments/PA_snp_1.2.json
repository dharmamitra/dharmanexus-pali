[
    {
        "segmentnr": "PA_snp_1.2:0.1",
        "original": "Sutta Nipāta 1.2",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.2:0.2",
        "original": "Dhaniyasutta",
        "analyzed": "dhaniyasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.2:1.1",
        "original": "“Pakkodano duddhakhīrohamasmi,",
        "analyzed": "pakkodanaḥ duddha khīraḥ ham asmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.2:1.2",
        "original": "(iti dhaniyo gopo)",
        "analyzed": "iti dhaniyaḥ gopaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.2:1.3",
        "original": "Anutīre mahiyā samānavāso;",
        "analyzed": "anu tīre mahiyā samāna vāsaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.2:1.4",
        "original": "Channā kuṭi āhito gini,",
        "analyzed": "channā kuṭi ā hi taḥ gini ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.2:1.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.2:2.1",
        "original": "“Akkodhano vigatakhilohamasmi,",
        "analyzed": "akkodhanaḥ vigata khilaḥ ham asmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.2:2.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.2:2.3",
        "original": "Anutīre mahiyekarattivāso;",
        "analyzed": "anu tīre mahiyā ika ratti vāso ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.2:2.4",
        "original": "Vivaṭā kuṭi nibbuto gini,",
        "analyzed": "vivaṭā kuṭi nibbutaḥ gini ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.2:2.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.2:3.1",
        "original": "“Andhakamakasā na vijjare,",
        "analyzed": "andhakamakasā na vijjare ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.2:3.2",
        "original": "(iti dhaniyo gopo)",
        "analyzed": "iti dhaniyaḥ gopaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.2:3.3",
        "original": "Kacche rūḷhatiṇe caranti gāvo;",
        "analyzed": "kacche rūḍha tiṇe caranti gāvaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.2:3.4",
        "original": "Vuṭṭhimpi saheyyumāgataṁ,",
        "analyzed": "vuṭṭhim pi saheyyum āgata ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.2:3.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.2:4.1",
        "original": "“Baddhāsi bhisī susaṅkhatā,",
        "analyzed": "baddhā asi bhisī su saṅkhatā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.2:4.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.2:4.3",
        "original": "Tiṇṇo pāragato vineyya oghaṁ;",
        "analyzed": "tiṇṇaḥ pāra gataḥ vineyyaḥ ogha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.2:4.4",
        "original": "Attho bhisiyā na vijjati,",
        "analyzed": "atthaḥ bhisiyā na vijjati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.2:4.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.2:5.1",
        "original": "“Gopī mama assavā alolā,",
        "analyzed": "gopī mama assavā alolā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.2:5.2",
        "original": "(iti dhaniyo gopo)",
        "analyzed": "iti dhaniyaḥ gopaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.2:5.3",
        "original": "Dīgharattaṁ saṁvāsiyā manāpā;",
        "analyzed": "dīgharattam sa vāsiyā manāpā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.2:5.4",
        "original": "Tassā na suṇāmi kiñci pāpaṁ,",
        "analyzed": "tassā na suṇāmi kiñci pāpa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.2:5.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.2:6.1",
        "original": "“Cittaṁ mama assavaṁ vimuttaṁ,",
        "analyzed": "cittam mama assavam vimutta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.2:6.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.2:6.3",
        "original": "Dīgharattaṁ paribhāvitaṁ sudantaṁ;",
        "analyzed": "dīgharattam paribhāvitam su danta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.2:6.4",
        "original": "Pāpaṁ pana me na vijjati,",
        "analyzed": "pāpam pana me na vijjati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.2:6.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.2:7.1",
        "original": "“Attavetanabhatohamasmi,",
        "analyzed": "atta veta nabhataḥ ham asmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.2:7.2",
        "original": "(iti dhaniyo gopo)",
        "analyzed": "iti dhaniyaḥ gopaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.2:7.3",
        "original": "Puttā ca me samāniyā arogā;",
        "analyzed": "puttā ca me samāniyā arogā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.2:7.4",
        "original": "Tesaṁ na suṇāmi kiñci pāpaṁ,",
        "analyzed": "tesam na suṇāmi kiñci pāpa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.2:7.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.2:8.1",
        "original": "“Nāhaṁ bhatakosmi kassaci,",
        "analyzed": "na aham bhatakaḥ asmi kassaci ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.2:8.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.2:8.3",
        "original": "Nibbiṭṭhena carāmi sabbaloke;",
        "analyzed": "nibbiṭṭhena carāmi sabba loke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.2:8.4",
        "original": "Attho bhatiyā na vijjati,",
        "analyzed": "atthaḥ bhatiyā na vijjati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.2:8.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.2:9.1",
        "original": "“Atthi vasā atthi dhenupā,",
        "analyzed": "atthi vasā atthi dhenupā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.2:9.2",
        "original": "(iti dhaniyo gopo)",
        "analyzed": "iti dhaniyaḥ gopaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.2:9.3",
        "original": "Godharaṇiyo paveṇiyopi atthi;",
        "analyzed": "godharaṇiyo paveṇiyo pi atthi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.2:9.4",
        "original": "Usabhopi gavampatīdha atthi,",
        "analyzed": "usabhaḥ upi gavampatī idha atthi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.2:9.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.2:10.1",
        "original": "“Natthi vasā natthi dhenupā,",
        "analyzed": "natthi vasā natthi dhenupā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.2:10.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.2:10.3",
        "original": "Godharaṇiyo paveṇiyopi natthi;",
        "analyzed": "godharaṇiyo paveṇiyo pi natthi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.2:10.4",
        "original": "Usabhopi gavampatīdha natthi,",
        "analyzed": "usabhaḥ pi gavampati idha natthi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.2:10.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.2:11.1",
        "original": "“Khilā nikhātā asampavedhī,",
        "analyzed": "khilā nikhātā a sampavedhī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.2:11.2",
        "original": "(iti dhaniyo gopo)",
        "analyzed": "iti dhaniyaḥ gopaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.2:11.3",
        "original": "Dāmā muñjamayā navā susaṇṭhānā;",
        "analyzed": "dāmā muñja mayā navā su saṇṭhānā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.2:11.4",
        "original": "Na hi sakkhinti dhenupāpi chettuṁ,",
        "analyzed": "na hi sakkhinti dhenu pāpi chettu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.2:11.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.2:12.1",
        "original": "“Usabhoriva chetva bandhanāni,",
        "analyzed": "usabhoḥ iva bandhanāni ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.2:12.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.2:12.3",
        "original": "Nāgo pūtilataṁva dālayitvā;",
        "analyzed": "dālayitvā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.2:12.4",
        "original": "Nāhaṁ punupessaṁ gabbhaseyyaṁ,",
        "analyzed": "na aham punupessam gabbhaseyya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.2:12.5",
        "original": "Atha ce patthayasī pavassa deva”.",
        "analyzed": "atha ce patthayasi pavassa deva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.2:13.1",
        "original": "Ninnañca thalañca pūrayanto,",
        "analyzed": "ninnañca thalañca pūrayantaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.2:13.2",
        "original": "Mahāmegho pavassi tāvadeva;",
        "analyzed": "mahā meghaḥ pavassi tāvat eva ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.2:13.3",
        "original": "Sutvā devassa vassato,",
        "analyzed": "sutvā devassa vassato ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.2:13.4",
        "original": "Imamatthaṁ dhaniyo abhāsatha.",
        "analyzed": "imam attham dhaniyaḥ abhāsatha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.2:14.1",
        "original": "“Lābhā vata no anappakā,",
        "analyzed": "lābhā vata naḥ anappakā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.2:14.2",
        "original": "Ye mayaṁ bhagavantaṁ addasāma;",
        "analyzed": "ye mayam bhagavantam addasāma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.2:14.3",
        "original": "Saraṇaṁ taṁ upema cakkhuma,",
        "analyzed": "saraṇam tam upema cakkhuma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.2:14.4",
        "original": "Satthā no hohi tuvaṁ mahāmuni.",
        "analyzed": "satthā naḥ hohi tuvam mahā muni ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.2:15.1",
        "original": "Gopī ca ahañca assavā,",
        "analyzed": "gopī ca aham ca assavā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.2:15.2",
        "original": "Brahmacariyaṁ sugate carāmase;",
        "analyzed": "brahmacariyam sugate carāmase ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.2:15.3",
        "original": "Jātimaraṇassa pāragū,",
        "analyzed": "jāti maraṇassa pāragū ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.2:15.4",
        "original": "Dukkhassantakarā bhavāmase”.",
        "analyzed": "dukkhassanta karāḥ bhavāmase ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.2:16.1",
        "original": "“Nandati puttehi puttimā,",
        "analyzed": "nandati puttehi puttimā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.2:16.2",
        "original": "(iti māro pāpimā)",
        "analyzed": "iti māro pāpimā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.2:16.3",
        "original": "Gomā gohi tatheva nandati;",
        "analyzed": "gomā gohi tathā iva nandati ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.2:16.4",
        "original": "Upadhī hi narassa nandanā,",
        "analyzed": "upadhī hi narassa nandanā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.2:16.5",
        "original": "Na hi so nandati yo nirūpadhi”.",
        "analyzed": "na hi saḥ nandati yaḥ nirūpadhi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.2:17.1",
        "original": "“Socati puttehi puttimā,",
        "analyzed": "socati puttehi puttimā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.2:17.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.2:17.3",
        "original": "Gomā gohi tatheva socati;",
        "analyzed": "gomā tathā iva socati ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.2:17.4",
        "original": "Upadhī hi narassa socanā,",
        "analyzed": "upadhī hi narassa socanā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.2:17.5",
        "original": "Na hi so socati yo nirūpadhī”ti.",
        "analyzed": "na hi saḥ socati yaḥ nirūpadhī iti ",
        "folio": "17"
    },
    {
