[
    {
        "segmentnr": "PA_vv_35:0.1",
        "original": "Vimānavatthu",
        "analyzed": "vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_35:0.2",
        "original": "Itthivimāna",
        "analyzed": "itthi vimāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_35:0.3",
        "original": "Pāricchattakavagga",
        "analyzed": "pāricchattaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_35:0.4",
        "original": "7. Pesavatīvimānavatthu",
        "analyzed": "pesavatī vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_35:1.1",
        "original": "“Phalikarajatahemajālachannaṁ,",
        "analyzed": "phalika rajata hema jāla channa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_35:1.2",
        "original": "Vividhacitratalamaddasaṁ surammaṁ;",
        "analyzed": "vividha citra tala maddasam su ramma ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_35:1.3",
        "original": "Byamhaṁ sunimmitaṁ toraṇūpapannaṁ,",
        "analyzed": "byamham su nimmitam toraṇūpapanna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_35:1.4",
        "original": "Rucakupakiṇṇamidaṁ subhaṁ vimānaṁ.",
        "analyzed": "rucaka upakiṇṇam idam subham vimāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_35:2.1",
        "original": "Bhāti ca dasa disā nabheva suriyo,",
        "analyzed": "bhāti ca dasa disā nabhā iva suriyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_35:2.2",
        "original": "Sarade tamonudo sahassaraṁsī;",
        "analyzed": "sarade tamonudaḥ sahas sarasī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_35:2.3",
        "original": "Tathā tapati midaṁ tava vimānaṁ,",
        "analyzed": "tathā tapati midam tava vimāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_35:2.4",
        "original": "Jalamiva dhūmasikho nise nabhagge.",
        "analyzed": "jalam iva nise ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_35:3.1",
        "original": "Musatīva nayanaṁ sateratāva,",
        "analyzed": "nayanam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_35:3.2",
        "original": "Ākāse ṭhapitamidaṁ manuññaṁ;",
        "analyzed": "ākāse ṭhapitam idam manuñña ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_35:3.3",
        "original": "Vīṇāmurajasammatāḷaghuṭṭhaṁ,",
        "analyzed": "vīṇā muraja samma tāḷa ghuṭṭha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_35:3.4",
        "original": "Iddhaṁ indapuraṁ yathā tavedaṁ.",
        "analyzed": "iddham inda puram yathā tava īda ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_35:4.1",
        "original": "Padumakumuduppalakuvalayaṁ,",
        "analyzed": "paduma kumuda uppala kuvalaya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_35:4.2",
        "original": "Yodhika bandhukanojakā ca santi;",
        "analyzed": "ca santi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_35:4.3",
        "original": "Sālakusumitapupphitā asokā,",
        "analyzed": "sāla kusumita pupphitā asokā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_35:4.4",
        "original": "Vividhadumaggasugandhasevitamidaṁ.",
        "analyzed": "vividha dumagga sugandha sevita ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_35:5.1",
        "original": "Saḷalalabujabhujaka saṁyuttā,",
        "analyzed": "sa ḷa la la buja bhujaka sa yuttā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_35:5.2",
        "original": "Kusakasuphullitalatāvalambinīhi;",
        "analyzed": "kusaka su phullita latā avalambinī hi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_35:5.3",
        "original": "Maṇijālasadisā yasassinī,",
        "analyzed": "maṇi jāla sadisā yasassinī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_35:5.4",
        "original": "Rammā pokkharaṇī upaṭṭhitā te.",
        "analyzed": "rammā pokkharaṇī upaṭṭhitā te ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_35:6.1",
        "original": "Udakaruhā ca yetthi pupphajātā,",
        "analyzed": "udakaruhā ca yā itthi puppha jātā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_35:6.2",
        "original": "Thalajā ye ca santi rukkhajātā;",
        "analyzed": "thala jāḥ ye ca santi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_35:6.3",
        "original": "Mānusakāmānussakā ca dibbā,",
        "analyzed": "mānusakā amānussakā ca dibbā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_35:6.4",
        "original": "Sabbe tuyhaṁ nivesanamhi jātā.",
        "analyzed": "sabbe tuyham nivesanamhi jātā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_35:7.1",
        "original": "Kissa saṁyamadamassayaṁ vipāko,",
        "analyzed": "kissa sayama damaḥ sayam vipākaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_35:7.2",
        "original": "Kenāsi kammaphalenidhūpapannā;",
        "analyzed": "kena asi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_35:7.3",
        "original": "Yathā ca te adhigatamidaṁ vimānaṁ,",
        "analyzed": "yathā ca te adhigatam idam vimāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_35:7.4",
        "original": "Tadanupadaṁ avacāsiḷārapamhe”ti.",
        "analyzed": "tad anupadam avacā siḷāra pamhā iti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_35:8.1",
        "original": "“Yathā ca me adhigatamidaṁ vimānaṁ,",
        "analyzed": "yathā ca me adhigatam idam vimāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_35:8.2",
        "original": "Koñcamayūracakorasaṅghacaritaṁ;",
        "analyzed": "           ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_35:8.3",
        "original": "Dibyapilavahaṁsarājaciṇṇaṁ,",
        "analyzed": "di bi api lava hasa rāja ciṇ ṇa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_35:8.4",
        "original": "Dijakāraṇḍavakokilābhinaditaṁ.",
        "analyzed": "dija kāraṇḍava kokila abhinadita ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_35:9.1",
        "original": "Nānāsantānakapuppharukkhavividhā,",
        "analyzed": "nānā santānaka puppha rukkha vividhā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_35:9.2",
        "original": "Pāṭalijambuasokarukkhavantaṁ;",
        "analyzed": "pāṭali jambu asoka rukkhavanta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_35:9.3",
        "original": "Yathā ca me adhigatamidaṁ vimānaṁ,",
        "analyzed": "yathā ca me adhigatam idam vimāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_35:9.4",
        "original": "Taṁ te pavedayāmi suṇohi bhante.",
        "analyzed": "tam te pavedayāmi suṇohi bhante ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_35:10.1",
        "original": "Magadhavarapuratthimena,",
        "analyzed": "magadhavarapuratthimena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_35:10.2",
        "original": "Nāḷakagāmo nāma atthi bhante;",
        "analyzed": "nāḷakagāmaḥ nāma atthi bhante ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_35:10.3",
        "original": "Tattha ahosiṁ pure suṇisā,",
        "analyzed": "tattha ahosim pure suṇisā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_35:10.4",
        "original": "Pesavatīti tattha jāniṁsu mamaṁ.",
        "analyzed": "pesavatī iti tattha jānisu mama ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_35:11.1",
        "original": "Sāhamapacitatthadhammakusalaṁ,",
        "analyzed": "sā aham apacitattha dhamma kusala ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_35:11.2",
        "original": "Devamanussapūjitaṁ mahantaṁ;",
        "analyzed": "deva manuḥ sa pūjitam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_35:11.3",
        "original": "Upatissaṁ nibbutamappameyyaṁ,",
        "analyzed": "upatissam nibbutam appameyya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_35:11.4",
        "original": "Muditamanā kusumehi abbhukiriṁ.",
        "analyzed": "mudita manāḥ kusumehi abbhukiri ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_35:12.1",
        "original": "Paramagatigatañca pūjayitvā,",
        "analyzed": "parama gati gatam ca pūjayitvā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_35:12.2",
        "original": "Antimadehadharaṁ isiṁ uḷāraṁ;",
        "analyzed": "antima deha dharam isim uḷāra ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_35:12.3",
        "original": "Pahāya mānusakaṁ samussayaṁ,",
        "analyzed": "pahāya mānusakam samussaya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_35:12.4",
        "original": "Tidasagatā idha māvasāmi ṭhānan”ti.",
        "analyzed": "tidasa gatāḥ idha mā avasāmi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_35:13.1",
        "original": "Pesavatīvimānaṁ sattamaṁ.",
        "analyzed": "pesavatī vimānam sattama ",
        "folio": "13"
    }
]