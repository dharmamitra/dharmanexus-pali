        "analyzed": "codayī pubbadevatā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_tha-ap_538:19.3",
        "original": "‘Na tvaṁ upāyamaggaññū,",
        "analyzed": "na tvam upāya maggaññū ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_tha-ap_538:19.4",
        "original": "kuto tvaṁ arahā bhave’.",
        "analyzed": "kutas tvam ara hā bhave ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_tha-ap_538:20.1",
        "original": "Codito tāya saṁviggo,",
        "analyzed": "coditaḥ tāya sa viggaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_538:20.2",
        "original": "tadāhaṁ paripucchi taṁ;",
        "analyzed": "tadā aham paripucchi te ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_538:20.3",
        "original": "‘Ke vā ete kuhiṁ loke,",
        "analyzed": "ke vai ete kuhim loke ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_538:20.4",
        "original": "arahanto naruttamā’.",
        "analyzed": "arahantaḥ naruttamā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_538:21.1",
        "original": "‘Sāvatthiyaṁ kosalamandire jino,",
        "analyzed": "sāvatthiyam kosala mandire jinaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_538:21.2",
        "original": "Pahūtapañño varabhūrimedhaso;",
        "analyzed": "pahūta paññaḥ vara bhūri medhasaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_538:21.3",
        "original": "So sakyaputto arahā anāsavo,",
        "analyzed": "so sakya puttaḥ arahā anāsavaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_538:21.4",
        "original": "Deseti dhammaṁ arahattapattiyā’.",
        "analyzed": "deseti dhammam arahatta pattiyā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_538:22.1",
        "original": "Tadassa sutvā vacanaṁ supīṇito,",
        "analyzed": "tat assa sutvā vacanam su pīṇitaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_538:22.2",
        "original": "Nidhiṁva laddhā kapaṇoti vimhito;",
        "analyzed": "nidhiva laddhā kapaṇoti vimhitaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_538:22.3",
        "original": "Udaggacitto arahattamuttamaṁ,",
        "analyzed": "udagga cittaḥ arahattam uttama ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_538:22.4",
        "original": "Sudassanaṁ daṭṭhumanantagocaraṁ.",
        "analyzed": "su dassanam daṭṭhum ananta gocara ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_538:23.1",
        "original": "Tadā tato nikkhamitvāna ‘satthuno,",
        "analyzed": "tadā tatas nikkhamitvāna satthunaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_538:23.2",
        "original": "Sadā jinaṁ passāmi vimalānanaṁ’;",
        "analyzed": "sadā jinam passāmi vimala ānana ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_538:23.3",
        "original": "Upecca rammaṁ vijitavhayaṁ vanaṁ,",
        "analyzed": "upecca rammam vijita vhayam vana ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_538:23.4",
        "original": "Dije apucchiṁ ‘kuhiṁ lokanandano’.",
        "analyzed": "dije apucchim kuhim loka nandanaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_538:24.1",
        "original": "‘Tato avocuṁ naradevavandito,",
        "analyzed": "tatas avocum naradeva vanditaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_538:24.2",
        "original": "Puraṁ paviṭṭho asanesanāya so;",
        "analyzed": "puram paviṭṭhaḥ asanesanāya saḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_538:24.3",
        "original": "Sasova khippaṁ munidassanussuko,",
        "analyzed": "khippam muni dassana ussukaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_538:24.4",
        "original": "Upecca vandāhi tamaggapuggalaṁ’.",
        "analyzed": "upecca vandāhi tam agga puggala ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_538:25.1",
        "original": "Tatohaṁ tuvaṭaṁ gantvā,",
        "analyzed": "tatoham tuvaṭam gantvā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_tha-ap_538:25.2",
        "original": "sāvatthiṁ puramuttamaṁ;",
        "analyzed": "sāvatthim puram uttama ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_tha-ap_538:25.3",
        "original": "Vicarantaṁ tamaddakkhiṁ,",
        "analyzed": "vicarantam tam addakkhi ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_tha-ap_538:25.4",
        "original": "piṇḍatthaṁ apihāgidhaṁ.",
        "analyzed": "piṇḍattham ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_tha-ap_538:26.1",
        "original": "Pattapāṇiṁ alolakkhaṁ,",
        "analyzed": "patta pāṇim a lolakkha ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_tha-ap_538:26.2",
        "original": "pācayantaṁ pītākaraṁ;",
        "analyzed": "pācayantam pīta ākara ",
        "folio": "26"
