        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc48:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc48:1.2",
        "original": "Tena kho pana samayena thullanandā bhikkhunī āvasathaṁ anissajjitvā cārikaṁ pakkāmi.",
        "analyzed": "tena kho pana samayena thullanandā bhikkhunī āvasatham a nissajjitvā cārikam pakkāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc48:1.3",
        "original": "Tena kho pana samayena thullanandāya bhikkhuniyā āvasatho ḍayhati.",
        "analyzed": "tena kho pana samayena thullanandāya bhikkhuniyāḥ āvasathaḥ ḍayhati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc48:1.4",
        "original": "Bhikkhuniyo evamāhaṁsu—",
        "analyzed": "bhikkhuniyaḥ evam āhasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc48:1.5",
        "original": "“handāyye, bhaṇḍakaṁ nīharāmā”ti.",
