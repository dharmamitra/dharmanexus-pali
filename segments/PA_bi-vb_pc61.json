        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.2",
        "original": "Tena kho pana samayena bhikkhuniyo gabbhiniṁ vuṭṭhāpenti.",
        "analyzed": "tena kho pana samayena bhikkhuniyaḥ gabbhinim vuṭṭhāpenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.3",
        "original": "Sā piṇḍāya carati.",
        "analyzed": "sā piṇḍāya carati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.4",
        "original": "Manussā evamāhaṁsu—",
        "analyzed": "manussāḥ evam āhasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.5",
        "original": "“dethāyyāya bhikkhaṁ, garubhārā ayyā”ti.",
        "analyzed": "detha ayyāya bhikkha garu bhārā ayyā ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.6",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.7",
        "original": "“kathañhi nāma bhikkhuniyo gabbhiniṁ vuṭṭhāpessantī”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ gabbhinim vuṭṭhāpessanti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.8",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.9",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.10",
        "original": "“kathañhi nāma bhikkhuniyo gabbhiniṁ vuṭṭhāpessantī”ti …pe…",
        "analyzed": "katham hi nāma bhikkhuniyaḥ gabbhinim vuṭṭhāpessanti iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc61:1.11",
        "original": "“saccaṁ kira, bhikkhave, bhikkhuniyo gabbhiniṁ vuṭṭhāpentī”ti?",
