    },
    {
        "segmentnr": "PA_ja_405:0.4",
        "original": "10. Bakajātaka",
        "analyzed": "bakajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_405:1.1",
        "original": "“Dvāsattati gotama puññakammā,",
        "analyzed": "dvāsattati gotama puñña kammā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_405:1.2",
        "original": "Vasavattino jātijaraṁ atītā;",
        "analyzed": "vasa vattinaḥ jāti jaram atītā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_405:1.3",
        "original": "Ayamantimā vedagū brahmapatti,",
        "analyzed": "ayam antimā vedagū ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_405:1.4",
        "original": "Asmābhijappanti janā anekā”.",
        "analyzed": "asmā abhijappanti janāḥ anekā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_405:2.1",
        "original": "“Appañhi etaṁ na hi dīghamāyu,",
        "analyzed": "etam na hi dīgham āyu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_405:2.2",
        "original": "Yaṁ tvaṁ baka maññasi dīghamāyuṁ;",
        "analyzed": "yam tvam baka maññasi dīgham āyu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_405:2.3",
        "original": "Sataṁ sahassāni nirabbudānaṁ,",
        "analyzed": "satam sahassāni nirabbudāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_405:2.4",
        "original": "Āyuṁ pajānāmi tavāha brahme”.",
        "analyzed": "āyum tava āha brahme ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_405:3.1",
        "original": "“Anantadassī bhagavāhamasmi,",
        "analyzed": "ananta dassī bhagavān aham asmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_405:3.2",
        "original": "Jātijjaraṁ sokamupātivatto;",
        "analyzed": "jātijjaram sokam upātivattaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_405:3.3",
        "original": "Kiṁ me purāṇaṁ vatasīlavattaṁ,",
        "analyzed": "kim me purāṇam vatasīlavatta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_405:3.4",
        "original": "Ācikkha me taṁ yamahaṁ vijaññaṁ”.",
        "analyzed": "ācikkha me tam yam aham vijañña ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_405:4.1",
        "original": "“Yaṁ tvaṁ apāyesi bahū manusse,",
        "analyzed": "yam tvam apāyesi bahū manusse ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_405:4.2",
        "original": "Pipāsite ghammani samparete;",
        "analyzed": "pipāsite ghammani samparete ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_405:4.3",
        "original": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
        "analyzed": "tam te purāṇam vatasīlavatta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_405:4.4",
        "original": "Suttappabuddhova anussarāmi.",
        "analyzed": "suttappabuddhaḥ uva anussarāmi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_405:5.1",
        "original": "Yaṁ eṇikūlasmi janaṁ gahītaṁ,",
        "analyzed": "yam janam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_405:5.2",
        "original": "Amocayī gayhaka niyyamānaṁ;",
        "analyzed": "amocayī gayhaka niyyamāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_405:5.3",
        "original": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
        "analyzed": "tam te purāṇam vatasīlavatta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_405:5.4",
        "original": "Suttappabuddhova anussarāmi.",
        "analyzed": "suttappabuddhaḥ uva anussarāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_405:6.1",
        "original": "Gaṅgāya sotasmiṁ gahītanāvaṁ,",
        "analyzed": "gaṅgāya sotasmin gahītana ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_405:6.2",
        "original": "Luddena nāgena manussakappā;",
        "analyzed": "luddena nāgena manussa kappā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_405:6.3",
        "original": "Amocayi tvaṁ balasā pasayha,",
        "analyzed": "amocayi tvam balasā pasayha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_405:6.4",
        "original": "Taṁ te purāṇaṁ vatasīlavattaṁ;",
        "analyzed": "tam te purāṇam vatasīlavatta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_405:6.5",
        "original": "Suttappabuddhova anussarāmi.",
        "analyzed": "suttappabuddhaḥ uva anussarāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_405:7.1",
        "original": "Kappo ca te baddhacaro ahosiṁ,",
        "analyzed": "kappaḥ ca te baddha caraḥ ahosi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_405:7.2",
        "original": "Sambuddhimantaṁ vatinaṁ amaññaṁ;",
        "analyzed": "sambuddhimantam vatinam amañña ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_405:7.3",
        "original": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
        "analyzed": "tam te purāṇam vatasīlavatta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_405:7.4",
        "original": "Suttappabuddhova anussarāmi”.",
        "analyzed": "suttappabuddhaḥ uva anussarāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_405:8.1",
        "original": "“Addhā pajānāsi mametamāyuṁ,",
        "analyzed": "addhā pajānāsi mama etam āyu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_405:8.2",
        "original": "Aññampi jānāsi tathā hi buddho;",
        "analyzed": "aññam pi jānāsi tathā hi buddhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_405:8.3",
        "original": "Tathā hi tāyaṁ jalitānubhāvo,",
        "analyzed": "tathā hi jalita anubhāvaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_405:8.4",
        "original": "Obhāsayaṁ tiṭṭhati brahmalokan”ti.",
        "analyzed": "obhāsayam tiṭṭhati brahma lokam ti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_405:9.1",
        "original": "Bakajātakaṁ dasamaṁ.",
        "analyzed": "baka jātakam dasama ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_405:9.2",
        "original": "Kukkuvaggo paṭhamo.",
        "analyzed": "kukku vaggaḥ paṭhamaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_405:10.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_405:11.1",
        "original": "Varakaṇṇika cāpavaro sutano,",
        "analyzed": "varakaṇṇika su tano ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_405:11.2",
        "original": "Atha gijjha sarohitamacchavaro;",
        "analyzed": "atha gijjha sa rohita maccha varaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_405:11.3",
        "original": "Puna paṇṇaka senaka yācanako,",
        "analyzed": "puna paṇṇaka senaka yācanako ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_405:11.4",
        "original": "Atha veri sabrahmabakena dasāti.",
        "analyzed": "atha veri sa brahma bakena dasāti ",
        "folio": "11"
    }
]