    {
        "segmentnr": "PA_ja_477:0.4",
        "original": "4. Cūḷanāradajātaka",
        "analyzed": "cūḍa nārada jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_477:1.1",
        "original": "“Na te kaṭṭhāni bhinnāni,",
        "analyzed": "na te kaṭṭhāni bhinnāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_477:1.2",
        "original": "na te udakamābhataṁ;",
        "analyzed": "na te udakam ābhata ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_477:1.3",
        "original": "Aggīpi te na hāpito,",
        "analyzed": "aggīpi te na hāpitaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_477:1.4",
        "original": "kiṁ nu mandova jhāyasi”.",
        "analyzed": "kim nu mandaḥ uva jhāyasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_477:2.1",
        "original": "“Na ussahe vane vatthuṁ,",
        "analyzed": "na ussahe vane vatthu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_477:2.2",
        "original": "kassapāmantayāmi taṁ;",
        "analyzed": "kassapāmantayāmi ta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_477:2.3",
        "original": "Dukkho vāso araññasmiṁ,",
        "analyzed": "dukkhaḥ vāsaḥ araññasmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_477:2.4",
        "original": "raṭṭhaṁ icchāmi gantave.",
        "analyzed": "raṭṭham icchāmi gantave ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_477:3.1",
        "original": "Yathā ahaṁ ito gantvā,",
        "analyzed": "yathā aham itas gantvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_477:3.2",
        "original": "Yasmiṁ janapade vasaṁ;",
        "analyzed": "yasmin janapade vasa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_477:3.3",
        "original": "Ācāraṁ brahme sikkheyyaṁ,",
        "analyzed": "ācāram brahme sikkheyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_477:3.4",
        "original": "Taṁ dhammaṁ anusāsa maṁ”.",
        "analyzed": "tam dhammam anusāsa ma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_477:4.1",
        "original": "“Sace araññaṁ hitvāna,",
        "analyzed": "sace araññam hitvāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_477:4.2",
        "original": "vanamūlaphalāni ca;",
        "analyzed": "vana mūla phalāni ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_477:4.3",
        "original": "Raṭṭhe rocayase vāsaṁ,",
        "analyzed": "raṭṭhe rocayase vāsa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_477:4.4",
        "original": "taṁ dhammaṁ nisāmehi me.",
        "analyzed": "tam dhammam nisāmehi me ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_477:5.1",
        "original": "Visaṁ mā paṭisevittho,",
        "analyzed": "visam mā paṭisevitthaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_477:5.2",
        "original": "papātaṁ parivajjaya;",
        "analyzed": "papātam parivajjaya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_477:5.3",
        "original": "Paṅke ca mā visīdittho,",
        "analyzed": "paṅke ca mā visīditthaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_477:5.4",
        "original": "yatto cāsīvise care”.",
        "analyzed": "yattaḥ cāsīvise care ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_477:6.1",
        "original": "“Kiṁ nu visaṁ papāto vā,",
        "analyzed": "kim nu visam papātaḥ vā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_477:6.2",
        "original": "paṅko vā brahmacārinaṁ;",
        "analyzed": "paṅkaḥ vā brahmacārina ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_477:6.3",
        "original": "Kaṁ tvaṁ āsīvisaṁ brūsi,",
        "analyzed": "kam tvam āsīvisam brūsi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_477:6.4",
        "original": "taṁ me akkhāhi pucchito”.",
        "analyzed": "tam me akkhāhi pucchitaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_477:7.1",
        "original": "“Āsavo tāta lokasmiṁ,",
        "analyzed": "āsavaḥ tāta lokasmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_477:7.2",
        "original": "surā nāma pavuccati;",
        "analyzed": "surā nāma pavuccati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_477:7.3",
        "original": "Manuñño surabhī vaggu,",
        "analyzed": "manuññaḥ surabhī vaggu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_477:7.4",
        "original": "sādu khuddarasūpamo;",
        "analyzed": "sādu khuddara sūpamaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_477:7.5",
        "original": "Visaṁ tadāhu ariyā se,",
        "analyzed": "visam tat āhuḥ ariyā se ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_477:7.6",
        "original": "brahmacariyassa nārada.",
        "analyzed": "brahmacariyassa nārada ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_477:8.1",
        "original": "Itthiyo tāta lokasmiṁ,",
        "analyzed": "itthiyo tāta lokasmi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_477:8.2",
        "original": "pamattaṁ pamathenti tā;",
        "analyzed": "pamattam pamathenti tā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_477:8.3",
        "original": "Haranti yuvino cittaṁ,",
        "analyzed": "haranti yuvinaḥ citta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_477:8.4",
        "original": "tūlaṁ bhaṭṭhaṁva māluto;",
        "analyzed": "tūlam bhaṭṭhava māluto ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_477:8.5",
        "original": "Papāto eso akkhāto,",
        "analyzed": "papātaḥ eso akkhāto ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_477:8.6",
        "original": "brahmacariyassa nārada.",
        "analyzed": "brahmacariyassa nārada ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_477:9.1",
        "original": "Lābho siloko sakkāro,",
        "analyzed": "lābhaḥ silokaḥ sakkāro ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_477:9.2",
        "original": "pūjā parakulesu ca;",
        "analyzed": "pūjā para kulesu ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_477:9.3",
        "original": "Paṅko eso ca akkhāto,",
        "analyzed": "paṅkaḥ eso ca akkhāto ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_477:9.4",
        "original": "brahmacariyassa nārada.",
        "analyzed": "brahmacariyassa nārada ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_477:10.1",
        "original": "Sasatthā tāta rājāno,",
        "analyzed": "sasatthāḥ tāta rājānaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_477:10.2",
        "original": "āvasanti mahiṁ imaṁ;",
        "analyzed": "āvasanti mahim ima ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_477:10.3",
        "original": "Te tādise manussinde,",
        "analyzed": "te tādise manussinde ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_477:10.4",
        "original": "mahante tāta nārada.",
        "analyzed": "mahante tāta nārada ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_477:11.1",
        "original": "Issarānaṁ adhipatīnaṁ,",
        "analyzed": "issarānam adhipatīna ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_477:11.2",
        "original": "na tesaṁ pādato care;",
        "analyzed": "na tesam pādataḥ care ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_477:11.3",
        "original": "Āsīvisoti akkhāto,",
        "analyzed": "āsīviso ti akkhāto ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_477:11.4",
        "original": "brahmacariyassa nārada.",
        "analyzed": "brahmacariyassa nārada ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_477:12.1",
        "original": "Bhattattho bhattakāle ca,",
        "analyzed": "bhattatthaḥ bhatta kāle ca ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_477:12.2",
        "original": "yaṁ gehamupasaṅkame;",
        "analyzed": "yam geham upasaṅkame ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_477:12.3",
        "original": "Yadettha kusalaṁ jaññā,",
        "analyzed": "yat ettha kusalam jaññā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_477:12.4",
        "original": "tattha ghāsesanaṁ care.",
        "analyzed": "tattha ghāsesanam care ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_477:13.1",
        "original": "Pavisitvā parakulaṁ,",
        "analyzed": "pavisitvā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_477:13.2",
        "original": "pānatthaṁ bhojanāya vā;",
        "analyzed": "pānattham bhojanāya vā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_477:13.3",
        "original": "Mitaṁ khāde mitaṁ bhuñje,",
        "analyzed": "mitam khāde mitam bhuñje ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_477:13.4",
        "original": "na ca rūpe manaṁ kare.",
        "analyzed": "na ca rūpe manam kare ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_477:14.1",
        "original": "Goṭṭhaṁ majjaṁ kirāṭañca,",
        "analyzed": "goṭṭham majjam kirāṭam ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_477:14.2",
        "original": "sabhā nikiraṇāni ca;",
        "analyzed": "sabhā nikiraṇāni ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_477:14.3",
        "original": "Ārakā parivajjehi,",
        "analyzed": "ārakā parivajjehi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_477:14.4",
        "original": "yānīva visamaṁ pathan”ti.",
        "analyzed": "yāni iva visamam pathanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_477:15.1",
        "original": "Cūḷanāradajātakaṁ catutthaṁ.",
        "analyzed": "cūḍa nārada jātakam ",
        "folio": "15"
    }
]