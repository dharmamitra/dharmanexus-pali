[
    {
        "segmentnr": "PA_snp_4.14:0.1",
        "original": "Sutta Nipāta 4.14",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.14:0.2",
        "original": "Tuvaṭakasutta",
        "analyzed": "tuvaṭaka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.14:1.1",
        "original": "“Pucchāmi taṁ ādiccabandhu,",
        "analyzed": "pucchāmi tam ādiccabandhu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.14:1.2",
        "original": "Vivekaṁ santipadañca mahesi;",
        "analyzed": "vivekam santipadam ca mahā isi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.14:1.3",
        "original": "Kathaṁ disvā nibbāti bhikkhu,",
        "analyzed": "katham disvā nibbāti bhikkhu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.14:1.4",
        "original": "Anupādiyāno lokasmiṁ kiñci”.",
        "analyzed": "an upādiyānaḥ lokasmin kiñci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.14:2.1",
        "original": "“Mūlaṁ papañcasaṅkhāya,",
        "analyzed": "mūlam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.14:2.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.14:2.3",
        "original": "Mantā asmīti sabbamuparundhe;",
        "analyzed": "mantā asmi iti sabbam uparundhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.14:2.4",
        "original": "Yā kāci taṇhā ajjhattaṁ,",
        "analyzed": "yā kāci taṇhā ajjhatta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.14:2.5",
        "original": "Tāsaṁ vinayā sadā sato sikkhe.",
        "analyzed": "tāsam vinayā sadā sataḥ sikkhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.14:3.1",
        "original": "Yaṁ kiñci dhammamabhijaññā,",
        "analyzed": "yam kiñci dhammam abhijaññā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.14:3.2",
        "original": "Ajjhattaṁ atha vāpi bahiddhā;",
        "analyzed": "ajjhattam atha vā api bahiddhā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.14:3.3",
        "original": "Na tena thāmaṁ kubbetha,",
        "analyzed": "na tena thāmam kubbetha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.14:3.4",
        "original": "Na hi sā nibbuti sataṁ vuttā.",
        "analyzed": "na hi sā nibbuti satam vuttā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.14:4.1",
        "original": "Seyyo na tena maññeyya,",
        "analyzed": "seyyaḥ na tena maññeyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.14:4.2",
        "original": "Nīceyyo atha vāpi sarikkho;",
        "analyzed": "nīceyyaḥ atha vā api sa rikkhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.14:4.3",
        "original": "Phuṭṭho anekarūpehi,",
        "analyzed": "phuṭṭhaḥ aneka rūpehi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.14:4.4",
        "original": "Nātumānaṁ vikappayaṁ tiṭṭhe.",
        "analyzed": "na atumānam vikappayan tiṭṭhe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.14:5.1",
        "original": "Ajjhattamevupasame,",
        "analyzed": "ajjhattame iva upasame ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.14:5.2",
        "original": "Na aññato bhikkhu santimeseyya;",
        "analyzed": "na aññatas bhikkhu santimeseyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.14:5.3",
        "original": "Ajjhattaṁ upasantassa,",
        "analyzed": "ajjhattam upasantassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.14:5.4",
        "original": "Natthi attā kuto nirattā vā.",
        "analyzed": "natthi attā kutas nirattā vā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.14:6.1",
        "original": "Majjhe yathā samuddassa,",
        "analyzed": "majjhe yathā samuddassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.14:6.2",
        "original": "Ūmi no jāyatī ṭhito hoti;",
        "analyzed": "naḥ jāyati ṭhitaḥ hoti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.14:6.3",
        "original": "Evaṁ ṭhito anejassa,",
        "analyzed": "evam ṭhitaḥ anejassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.14:6.4",
        "original": "Ussadaṁ bhikkhu na kareyya kuhiñci”.",
        "analyzed": "ussadam bhikkhu na kareyya kuhiñci ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.14:7.1",
        "original": "“Akittayī vivaṭacakkhu,",
        "analyzed": "akittayī vivaṭa cakkhu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.14:7.2",
        "original": "Sakkhidhammaṁ parissayavinayaṁ;",
        "analyzed": "sakkhi dhammam parissaya vinaya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.14:7.3",
        "original": "Paṭipadaṁ vadehi bhaddante,",
        "analyzed": "paṭipadam vadehi bhaddante ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.14:7.4",
        "original": "Pātimokkhaṁ atha vāpi samādhiṁ”.",
        "analyzed": "pātimokkham atha vā api samādhi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.14:8.1",
        "original": "“Cakkhūhi neva lolassa,",
        "analyzed": "cakkhūhi na iva lolassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.14:8.2",
        "original": "Gāmakathāya āvaraye sotaṁ;",
        "analyzed": "āvaraye sota ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.14:8.3",
        "original": "Rase ca nānugijjheyya,",
        "analyzed": "rase ca na anugijjheyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.14:8.4",
        "original": "Na ca mamāyetha kiñci lokasmiṁ.",
        "analyzed": "na ca mamāyetha kiñci ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.14:9.1",
        "original": "Phassena yadā phuṭṭhassa,",
        "analyzed": "phassena yadā phuṭṭhassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.14:9.2",
        "original": "Paridevaṁ bhikkhu na kareyya kuhiñci;",
        "analyzed": "paridevam bhikkhu na kareyya kuhiñci ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.14:9.3",
        "original": "Bhavañca nābhijappeyya,",
        "analyzed": "bhavan ca na abhijappeyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.14:9.4",
        "original": "Bheravesu ca na sampavedheyya.",
        "analyzed": "bheravesu ca na sampavedheyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.14:10.1",
        "original": "Annānamatho pānānaṁ,",
        "analyzed": "annānam atho pānāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.14:10.2",
        "original": "Khādanīyānaṁ athopi vatthānaṁ;",
        "analyzed": "khādanīyānam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.14:10.3",
        "original": "Laddhā na sannidhiṁ kayirā,",
        "analyzed": "laddhā na sannidhim kayirā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.14:10.4",
        "original": "Na ca parittase tāni alabhamāno.",
        "analyzed": "na ca parittase tāni a labhamānaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.14:11.1",
        "original": "Jhāyī na pādalolassa,",
        "analyzed": "na pāda lolassa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.14:11.2",
        "original": "Virame kukkuccā nappamajjeyya;",
        "analyzed": "virame kukkuccā na ppamajjeyya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.14:11.3",
        "original": "Athāsanesu sayanesu,",
        "analyzed": "atha āsanesu sayanesu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.14:11.4",
        "original": "Appasaddesu bhikkhu vihareyya.",
        "analyzed": "appasaddesu bhikkhu vihareyya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.14:12.1",
        "original": "Niddaṁ na bahulīkareyya,",
        "analyzed": "niddam na bahulīkareyya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.14:12.2",
        "original": "Jāgariyaṁ bhajeyya ātāpī;",
        "analyzed": "jāgariyam bhajeyyaḥ ātāpī ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.14:12.3",
        "original": "Tandiṁ māyaṁ hassaṁ khiḍḍaṁ,",
        "analyzed": "tandim māyam hassam khiḍḍa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.14:12.4",
        "original": "Methunaṁ vippajahe savibhūsaṁ.",
        "analyzed": "methunam vippajahe savibhūsa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.14:13.1",
        "original": "Āthabbaṇaṁ supinaṁ lakkhaṇaṁ,",
        "analyzed": "āthabbaṇam supinam lakkhaṇa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.14:13.2",
        "original": "No vidahe athopi nakkhattaṁ;",
        "analyzed": "no vidahe athopi nakkhatta ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.14:13.3",
        "original": "Virutañca gabbhakaraṇaṁ,",
        "analyzed": "virutam ca gabbhakaraṇa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.14:13.4",
        "original": "Tikicchaṁ māmako na seveyya.",
        "analyzed": "tikiccham māmakaḥ na seveyya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.14:14.1",
        "original": "Nindāya nappavedheyya,",
        "analyzed": "nindāya na ppavedheyya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.14:14.2",
        "original": "Na uṇṇameyya pasaṁsito bhikkhu;",
        "analyzed": "na uṇṇameyya pasasitaḥ bhikkhu ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.14:14.3",
        "original": "Lobhaṁ saha macchariyena,",
        "analyzed": "lobham saha mad śariyena ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.14:14.4",
        "original": "Kodhaṁ pesuṇiyañca panudeyya.",
        "analyzed": "kodham pesuṇiyam ca panudeyya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.14:15.1",
        "original": "Kayavikkaye na tiṭṭheyya,",
        "analyzed": "kaya vikkaye na tiṭṭheyya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.14:15.2",
        "original": "Upavādaṁ bhikkhu na kareyya kuhiñci;",
        "analyzed": "upavādam bhikkhu na kareyya kuhiñci ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.14:15.3",
        "original": "Gāme ca nābhisajjeyya,",
        "analyzed": "gāme ca na abhisajjeyya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.14:15.4",
        "original": "Lābhakamyā janaṁ na lapayeyya.",
        "analyzed": "lābha kamyā janam na lapayeyya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.14:16.1",
        "original": "Na ca katthitā siyā bhikkhu,",
        "analyzed": "na ca katthitā siyā bhikkhu ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.14:16.2",
        "original": "Na ca vācaṁ payuttaṁ bhāseyya;",
        "analyzed": "na ca vācam payuttam bhāseyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.14:16.3",
        "original": "Pāgabbhiyaṁ na sikkheyya,",
        "analyzed": "pāgabbhiyam na sikkheyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.14:16.4",
        "original": "Kathaṁ viggāhikaṁ na kathayeyya.",
        "analyzed": "katham viggāhikam na kathayeyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.14:17.1",
        "original": "Mosavajje na nīyetha,",
        "analyzed": "mosavajje na nīyetha ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.14:17.2",
        "original": "Sampajāno saṭhāni na kayirā;",
        "analyzed": "sampajāno saṭhāni na kayirā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.14:17.3",
        "original": "Atha jīvitena paññāya,",
        "analyzed": "atha jīvitena paññāya ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.14:17.4",
        "original": "Sīlabbatena nāññamatimaññe.",
        "analyzed": "sīlabbatena na aññamatimaññe ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.14:18.1",
        "original": "Sutvā rusito bahuṁ vācaṁ,",
        "analyzed": "sutvā rusitaḥ bahum vāca ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.14:18.2",
        "original": "Samaṇānaṁ vā puthujanānaṁ;",
        "analyzed": "vā puthujanāna ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.14:18.3",
        "original": "Pharusena ne na paṭivajjā,",
        "analyzed": "pharusena ne na paṭivajjā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.14:18.4",
        "original": "Na hi santo paṭisenikaronti.",
        "analyzed": "na hi santaḥ paṭisenikaronti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.14:19.1",
        "original": "Etañca dhammamaññāya,",
        "analyzed": "etat ca dhammam aññāya ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.14:19.2",
        "original": "Vicinaṁ bhikkhu sadā sato sikkhe;",
        "analyzed": "vicinam bhikkhu sadā sataḥ sikkhe ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.14:19.3",
        "original": "Santīti nibbutiṁ ñatvā,",
        "analyzed": "santi iti nibbutim ñatvā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.14:19.4",
        "original": "Sāsane gotamassa na pamajjeyya.",
        "analyzed": "sāsane gotamassa na pamajjeyya ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.14:20.1",
        "original": "Abhibhū hi so anabhibhūto,",
        "analyzed": "hi saḥ an abhibhūtaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.14:20.2",
        "original": "Sakkhidhammamanītihamadassī;",
        "analyzed": "sakkhi dhamma manīti hama dassī ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.14:20.3",
        "original": "Tasmā hi tassa bhagavato sāsane,",
        "analyzed": "tasmā hi tassa bhagavataḥ sāsane ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.14:20.4",
        "original": "Appamatto sadā namassamanusikkhe”ti.",
        "analyzed": "appamattaḥ sadā namassam anusikkha iti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.14:20.5",
        "original": "Tuvaṭakasuttaṁ cuddasamaṁ.",
        "analyzed": "tuvaṭaka suttam cuddasama ",
        "folio": "20"
    }
]