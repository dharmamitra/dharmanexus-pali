        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_467:0.3",
        "original": "Cūḷakuṇālavagga",
        "analyzed": "cūḷa kuṇāla vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_467:0.4",
        "original": "4. Kāmajātaka",
        "analyzed": "kāmajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_467:1.1",
        "original": "“Kāmaṁ kāmayamānassa,",
        "analyzed": "kāmam kāmayamānassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_467:1.2",
        "original": "tassa ce taṁ samijjhati;",
        "analyzed": "tassa ce tam samijjhati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_467:1.3",
        "original": "Addhā pītimano hoti,",
        "analyzed": "addhā pītimanaḥ hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_467:1.4",
        "original": "laddhā macco yadicchati.",
        "analyzed": "laddhā mad caḥ yat icchati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_467:2.1",
        "original": "Kāmaṁ kāmayamānassa,",
        "analyzed": "kāmam kāmayamānassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_467:2.2",
        "original": "tassa ce taṁ samijjhati;",
        "analyzed": "tassa ce tam samijjhati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_467:2.3",
        "original": "Tato naṁ aparaṁ kāme,",
        "analyzed": "tatas naṃ aparam kāme ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_467:2.4",
        "original": "ghamme taṇhaṁva vindati.",
        "analyzed": "ghamme vindati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_467:3.1",
        "original": "Gavaṁva siṅgino siṅgaṁ,",
        "analyzed": "gavava siṅgino siṅga ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_467:3.2",
        "original": "vaḍḍhamānassa vaḍḍhati;",
        "analyzed": "vaḍḍhamānassa vaḍḍhati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_467:3.3",
        "original": "Evaṁ mandassa posassa,",
        "analyzed": "evam mandassa posassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_467:3.4",
        "original": "bālassa avijānato;",
        "analyzed": "bālassa a vijānataḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_467:3.5",
        "original": "Bhiyyo taṇhā pipāsā ca,",
        "analyzed": "bhiyyaḥ taṇhā pipāsā ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_467:3.6",
        "original": "vaḍḍhamānassa vaḍḍhati.",
        "analyzed": "vaḍḍhamānassa vaḍḍhati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_467:4.1",
        "original": "Pathabyā sāliyavakaṁ,",
        "analyzed": "pathabyā sāliyavaka ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_467:4.2",
        "original": "gavāssaṁ dāsaporisaṁ;",
        "analyzed": "gavāssam dāsa porisa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_467:4.3",
        "original": "Datvā ca nālamekassa,",
        "analyzed": "da tvā ca na alam ekassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_467:4.4",
        "original": "iti vidvā samaṁ care.",
        "analyzed": "iti vidvā samam care ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_467:5.1",
        "original": "Rājā pasayha pathaviṁ vijitvā,",
        "analyzed": "rājā pasayha pathavim vijitvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_467:5.2",
        "original": "Sasāgarantaṁ mahimāvasanto;",
        "analyzed": "sa sāgarantam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_467:5.3",
        "original": "Oraṁ samuddassa atittarūpo,",
        "analyzed": "oram samuddassa a titta rūpaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_467:5.4",
        "original": "Pāraṁ samuddassapi patthayetha.",
        "analyzed": "pāram samuddassa api patthayetha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_467:6.1",
        "original": "Yāva anussaraṁ kāme,",
        "analyzed": "yāva anussaram kāme ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_467:6.2",
        "original": "Manasā titti nājjhagā;",
        "analyzed": "manasā titti na ajjhagā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_467:6.3",
        "original": "Tato nivattā paṭikkamma disvā,",
        "analyzed": "tatas nivattā paṭikkamma disvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_467:6.4",
        "original": "Te ve sutittā ye paññāya tittā.",
        "analyzed": "te ve su tittā ye paññāya tittā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_467:7.1",
        "original": "Paññāya tittinaṁ seṭṭhaṁ,",
        "analyzed": "paññāya tittinam seṭṭha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_467:7.2",
        "original": "na so kāmehi tappati;",
        "analyzed": "na saḥ kāmehi tappati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_467:7.3",
        "original": "Paññāya tittaṁ purisaṁ,",
        "analyzed": "paññāya tittam purisa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_467:7.4",
        "original": "taṇhā na kurute vasaṁ.",
        "analyzed": "taṇhā na kurute vasa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_467:8.1",
        "original": "Apacinetheva kāmānaṁ,",
        "analyzed": "apacinetha iva kāmāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_467:8.2",
        "original": "appicchassa alolupo;",
        "analyzed": "appicchassa a lolupaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_467:8.3",
        "original": "Samuddamatto puriso,",
        "analyzed": "samudda mattaḥ purisaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_467:8.4",
        "original": "na so kāmehi tappati.",
        "analyzed": "na saḥ kāmehi tappati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_467:9.1",
        "original": "Rathakārova cammassa,",
        "analyzed": "rathakāraḥ uva cammassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_467:9.2",
        "original": "parikantaṁ upāhanaṁ;",
        "analyzed": "parikantam upāhana ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_467:9.3",
        "original": "Yaṁ yaṁ cajati kāmānaṁ,",
        "analyzed": "yam yam cajati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_467:9.4",
        "original": "taṁ taṁ sampajjate sukhaṁ;",
        "analyzed": "tam tam sampajjate sukha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_467:9.5",
        "original": "Sabbañce sukhamiccheyya,",
        "analyzed": "sabbañce sukham iccheyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_467:9.6",
        "original": "sabbe kāme pariccaje”.",
        "analyzed": "sabbe kāme pariccaje ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_467:10.1",
        "original": "“Aṭṭha te bhāsitā gāthā,",
        "analyzed": "aṭṭha te bhāsitā gāthā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_467:10.2",
        "original": "sabbā honti sahassiyo;",
        "analyzed": "sabbāḥ honti sahassiyaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_467:10.3",
        "original": "Paṭigaṇha mahābrahme,",
        "analyzed": "paṭigaṇha mahābrahme ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_467:10.4",
        "original": "sādhetaṁ tava bhāsitaṁ”.",
        "analyzed": "sādha itam tava bhāsita ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_467:11.1",
        "original": "“Na me attho sahassehi,",
        "analyzed": "na me atthaḥ sahassehi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_467:11.2",
        "original": "satehi nahutehi vā;",
        "analyzed": "satehi nahutehi vā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_467:11.3",
        "original": "Pacchimaṁ bhāsato gāthaṁ,",
        "analyzed": "pacchimam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_467:11.4",
        "original": "kāme me na rato mano”.",
        "analyzed": "kāme me na rataḥ mano ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_467:12.1",
        "original": "“Bhadrako vatāyaṁ māṇavako,",
        "analyzed": "bhadrakaḥ vata ayam māṇavakaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_467:12.2",
        "original": "Sabbalokavidū muni;",
        "analyzed": "muni ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_467:12.3",
        "original": "Yo imaṁ taṇhaṁ dukkhajananiṁ,",
        "analyzed": "yaḥ imam taṇham dukkha janani ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_467:12.4",
        "original": "Parijānāti paṇḍito”ti.",
        "analyzed": "parijānāti paṇḍitā iti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_467:13.1",
        "original": "Kāmajātakaṁ catutthaṁ.",
        "analyzed": "kāma jātakam ",
        "folio": "13"
    }
]