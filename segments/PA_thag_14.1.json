        "segmentnr": "PA_thag_14.1:10.2",
        "original": "nābhinandāmi jīvitaṁ;",
        "analyzed": "na abhinandāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_14.1:10.3",
        "original": "Kālañca paṭikaṅkhāmi,",
        "analyzed": "kālañca paṭikaṅkhāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_14.1:10.4",
        "original": "nibbisaṁ bhatako yathā.",
        "analyzed": "nibbisam bhatakaḥ yathā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_14.1:11.1",
        "original": "Nābhinandāmi maraṇaṁ,",
        "analyzed": "na abhinandāmi maraṇa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_14.1:11.2",
