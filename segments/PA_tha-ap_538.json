        "segmentnr": "PA_tha-ap_538:16.2",
        "original": "gāmaṁ piṇḍāya pāvisiṁ;",
        "analyzed": "gāmam piṇḍāya pāvisi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_tha-ap_538:16.3",
        "original": "Tadāha so jano tuṭṭho,",
        "analyzed": "tat āha saḥ janaḥ tuṭṭhaḥ ",
        "folio": "16"
    },
