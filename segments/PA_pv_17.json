[
    {
        "segmentnr": "PA_pv_17:0.1",
        "original": "Petavatthu",
        "analyzed": "petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_17:0.2",
        "original": "Ubbarivagga",
        "analyzed": "ubbari vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_17:0.3",
        "original": "5. Maṭṭhakuṇḍalīpetavatthu",
        "analyzed": "maṭṭha kuṇḍalī peta vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_17:1.1",
        "original": "“Alaṅkato maṭṭhakuṇḍalī,",
        "analyzed": "alaṅkataḥ maṭṭha kuṇḍalī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_17:1.2",
        "original": "Māladhārī haricandanussado;",
        "analyzed": "      ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_17:1.3",
        "original": "Bāhā paggayha kandasi,",
        "analyzed": "bāhā paggayha kandasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_17:1.4",
        "original": "Vanamajjhe kiṁ dukkhito tuvan”ti.",
        "analyzed": "vana majjhe kim dukkhitaḥ tuvanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_17:2.1",
        "original": "“Sovaṇṇamayo pabhassaro,",
        "analyzed": "sovaṇṇa mayaḥ pabhassaraḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_17:2.2",
        "original": "Uppanno rathapañjaro mama;",
        "analyzed": "uppannaḥ ratha pañjaraḥ mama ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_17:2.3",
        "original": "Tassa cakkayugaṁ na vindāmi,",
        "analyzed": "tassa cakka yugam na vindāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_17:2.4",
        "original": "Tena dukkhena jahāmi jīvitan”ti.",
        "analyzed": "tena dukkhena jahāmi jīvitanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_17:3.1",
        "original": "“Sovaṇṇamayaṁ maṇimayaṁ,",
        "analyzed": "sovaṇṇa mayam maṇi maya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_17:3.2",
        "original": "Lohitakamayaṁ atha rūpiyamayaṁ;",
        "analyzed": "lohitaka mayam atha rūpiya maya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_17:3.3",
        "original": "Ācikkha me bhaddamāṇava,",
        "analyzed": "ācikkha me bhadda māṇava ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_17:3.4",
        "original": "Cakkayugaṁ paṭipādayāmi te”ti.",
        "analyzed": "cakka yugam paṭipādayāmi te iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_17:4.1",
        "original": "So māṇavo tassa pāvadi,",
        "analyzed": "saḥ māṇavaḥ tassa pāvadi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_17:4.2",
        "original": "“Candasūriyā ubhayettha dissare;",
        "analyzed": "canda sūriyāḥ ubhayā ittha dissare ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_17:4.3",
        "original": "Sovaṇṇamayo ratho mama,",
        "analyzed": "sovaṇṇa mayaḥ rathaḥ mama ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_17:4.4",
        "original": "Tena cakkayugena sobhatī”ti.",
        "analyzed": "tena cakka yugena sobhati iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_17:5.1",
        "original": "“Bālo kho tvaṁ asi māṇava,",
        "analyzed": "bālaḥ kho tvam asi māṇava ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_17:5.2",
        "original": "Yo tvaṁ patthayase apatthiyaṁ;",
        "analyzed": "yaḥ tvam patthayase a patthiya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_17:5.3",
        "original": "Maññāmi tuvaṁ marissasi,",
        "analyzed": "maññāmi tuvam marissasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_17:5.4",
        "original": "Na hi tvaṁ lacchasi candasūriye”ti.",
        "analyzed": "na hi tvam lacchasi canda sūriya iti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_17:6.1",
        "original": "“Gamanāgamanampi dissati,",
        "analyzed": "gamana āgamanam pi dissati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_17:6.2",
        "original": "Vaṇṇadhātu ubhayattha vīthiyā;",
        "analyzed": "vaṇṇa dhātu ubhayattha vīthiyā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_17:6.3",
        "original": "Peto kālakato na dissati,",
        "analyzed": "petaḥ kālakataḥ na dissati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_17:6.4",
        "original": "Ko nidha kandataṁ bālyataro”ti.",
        "analyzed": "kaḥ nidha kandatam bālyataroti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_17:7.1",
        "original": "“Saccaṁ kho vadesi māṇava,",
        "analyzed": "saccam kho vadesi māṇava ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_17:7.2",
        "original": "Ahameva kandataṁ bālyataro;",
        "analyzed": "aham eva bālyataro ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_17:7.3",
        "original": "Candaṁ viya dārako rudaṁ,",
        "analyzed": "candam dārakaḥ ruda ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_17:7.4",
        "original": "Petaṁ kālakatābhipatthayinti.",
        "analyzed": "petam kālakata abhipatthayinti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_17:8.1",
        "original": "Ādittaṁ vata maṁ santaṁ,",
        "analyzed": "ādittam vata mam santa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_17:8.2",
        "original": "ghatasittaṁva pāvakaṁ;",
        "analyzed": "pāvaka ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_17:8.3",
        "original": "Vārinā viya osiñcaṁ,",
        "analyzed": "vārinā viyaḥ osiñca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_17:8.4",
        "original": "sabbaṁ nibbāpaye daraṁ.",
        "analyzed": "sabbam nibbāpaye dara ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_17:9.1",
        "original": "Abbahī vata me sallaṁ,",
        "analyzed": "abbahī vata me salla ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_17:9.2",
        "original": "sokaṁ hadayanissitaṁ;",
        "analyzed": "sokam hadaya nissita ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_17:9.3",
        "original": "Yo me sokaparetassa,",
        "analyzed": "yaḥ me soka paretassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_17:9.4",
        "original": "puttasokaṁ apānudi.",
        "analyzed": "putta sokam apānudi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_17:10.1",
        "original": "Svāhaṁ abbūḷhasallosmi,",
        "analyzed": "svāham abbūḍha sallaḥ asmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_17:10.2",
        "original": "sītibhūtosmi nibbuto;",
        "analyzed": "sīti bhūtaḥ asmi nibbutaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_17:10.3",
        "original": "Na socāmi na rodāmi,",
        "analyzed": "na socāmi na rodāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_17:10.4",
        "original": "tava sutvāna māṇavā”ti.",
        "analyzed": "tava sutvāna māṇavāti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_17:11.1",
        "original": "“Devatā nusi gandhabbo,",
        "analyzed": "devatā nusi gandhabbo ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_17:11.2",
        "original": "adu sakko purindado;",
        "analyzed": "adu sakko purindadaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_17:11.3",
        "original": "Ko vā tvaṁ kassa vā putto,",
        "analyzed": "kaḥ vā tvam kassa vā putto ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_17:11.4",
        "original": "kathaṁ jānemu taṁ mayan”ti.",
        "analyzed": "katham jānemu tam mayanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_17:12.1",
        "original": "“Yañca kandasi yañca rodasi,",
        "analyzed": "yat ca kandasi yat ca rodasi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_17:12.2",
        "original": "Puttaṁ āḷāhane sayaṁ dahitvā;",
        "analyzed": "puttam āḷāhane sayam dahitvā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_17:12.3",
        "original": "Svāhaṁ kusalaṁ karitvā kammaṁ,",
        "analyzed": "svāham kusalam karitvā kamma ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_17:12.4",
        "original": "Tidasānaṁ sahabyataṁ gato”ti.",
        "analyzed": "tidasānam sahabyatam gata uti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_17:13.1",
        "original": "“Appaṁ vā bahuṁ vā nāddasāma,",
        "analyzed": "appam vā bahum vā na addasāma ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_17:13.2",
        "original": "Dānaṁ dadantassa sake agāre;",
        "analyzed": "dānam dadantassa sake agāre ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_17:13.3",
        "original": "Uposathakammaṁ vā tādisaṁ,",
        "analyzed": "uposatha kammam vā tādisa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_17:13.4",
        "original": "Kena kammena gatosi devalokan”ti.",
        "analyzed": "kena kammena gataḥ si deva lokanti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pv_17:14.1",
        "original": "“Ābādhikohaṁ dukkhito gilāno,",
        "analyzed": "ābādhikaḥ uham dukkhitaḥ gilānaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_17:14.2",
        "original": "Āturarūpomhi sake nivesane;",
        "analyzed": "ātura rūpaḥ umhi sake nivesane ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_17:14.3",
        "original": "Buddhaṁ vigatarajaṁ vitiṇṇakaṅkhaṁ,",
        "analyzed": "buddham vigata rajam vitiṇṇa kaṅkha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_17:14.4",
        "original": "Addakkhiṁ sugataṁ anomapaññaṁ.",
        "analyzed": "addakkhim sugatam anomapañña ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pv_17:15.1",
        "original": "Svāhaṁ muditamano pasannacitto,",
        "analyzed": "svāham mudita manaḥ pasanna cittaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_17:15.2",
        "original": "Añjaliṁ akariṁ tathāgatassa;",
        "analyzed": "añjalim akarim tathāgatassa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_17:15.3",
        "original": "Tāhaṁ kusalaṁ karitvāna kammaṁ,",
        "analyzed": "tāham kusalam karitvāna kamma ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_17:15.4",
        "original": "Tidasānaṁ sahabyataṁ gato”ti.",
        "analyzed": "tidasānam sahabyatam gata uti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pv_17:16.1",
        "original": "“Acchariyaṁ vata abbhutaṁ vata,",
        "analyzed": "acchariyam vata abbhutam vata ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_17:16.2",
        "original": "Añjalikammassa ayamīdiso vipāko;",
        "analyzed": "añjali kammassa ayam īdisaḥ vipākaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_17:16.3",
        "original": "Ahampi muditamano pasannacitto,",
        "analyzed": "aham pi mudita manaḥ pasanna cittaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_17:16.4",
        "original": "Ajjeva buddhaṁ saraṇaṁ vajāmī”ti.",
        "analyzed": "ajjā iva buddham saraṇam vajāmi iti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pv_17:17.1",
        "original": "“Ajjeva buddhaṁ saraṇaṁ vajāhi,",
        "analyzed": "ajjā iva buddham saraṇam vajāhi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_17:17.2",
        "original": "Dhammañca saṅghañca pasannacitto;",
        "analyzed": "dhammañca saṅghañca pasanna cittaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_17:17.3",
        "original": "Tatheva sikkhāya padāni pañca,",
        "analyzed": "tathā iva sikkhāya padāni pañca ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_17:17.4",
        "original": "Akhaṇḍaphullāni samādiyassu.",
        "analyzed": "akhaṇḍa phullāni ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pv_17:18.1",
        "original": "Pāṇātipātā viramassu khippaṁ,",
        "analyzed": "pāṇa atipātāḥ viramassu khippa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_17:18.2",
        "original": "Loke adinnaṁ parivajjayassu;",
        "analyzed": "loke a dinnam parivajjayassu ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_17:18.3",
        "original": "Amajjapo mā ca musā bhaṇāhi,",
        "analyzed": "a majja paḥ mā ca musā bhaṇāhi ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_17:18.4",
        "original": "Sakena dārena ca hohi tuṭṭho”ti.",
        "analyzed": "sakena dārena ca hohi tuṭṭha uti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pv_17:19.1",
        "original": "“Atthakāmosi me yakkha,",
        "analyzed": "attha kāmaḥ asi me yakkha ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_17:19.2",
        "original": "hitakāmosi devate;",
        "analyzed": "hita kāmaḥ asi devate ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_17:19.3",
        "original": "Karomi tuyhaṁ vacanaṁ,",
        "analyzed": "karomi tuyham vacana ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_17:19.4",
        "original": "tvaṁsi ācariyo mamāti.",
        "analyzed": "tvasi ācariyaḥ mama ati ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pv_17:20.1",
        "original": "Upemi saraṇaṁ buddhaṁ,",
        "analyzed": "upemi saraṇam buddha ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_17:20.2",
        "original": "dhammañcāpi anuttaraṁ;",
        "analyzed": "dhammañca api anuttara ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_17:20.3",
        "original": "Saṅghañca naradevassa,",
        "analyzed": "saṅghañca naradevassa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_17:20.4",
        "original": "gacchāmi saraṇaṁ ahaṁ.",
        "analyzed": "gacchāmi saraṇam aha ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pv_17:21.1",
        "original": "Pāṇātipātā viramāmi khippaṁ,",
        "analyzed": "pāṇa atipātā viramāmi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_17:21.2",
        "original": "Loke adinnaṁ parivajjayāmi;",
        "analyzed": "loke a dinnam parivajjayāmi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_17:21.3",
        "original": "Amajjapo no ca musā bhaṇāmi,",
        "analyzed": "no ca musā bhaṇāmi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_17:21.4",
        "original": "Sakena dārena ca homi tuṭṭho”ti.",
        "analyzed": "sakena dārena ca homi tuṭṭhaḥ iti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pv_17:22.1",
        "original": "Maṭṭhakuṇḍalīpetavatthu pañcamaṁ.",
        "analyzed": "maṭṭha kuṇḍalī peta vatthu pañcama ",
        "folio": "22"
    }
]