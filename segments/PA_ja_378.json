        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_378:0.4",
        "original": "3. Darīmukhajātaka",
        "analyzed": "darīmukhajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_378:1.1",
        "original": "“Paṅko ca kāmā palipo ca kāmā,",
        "analyzed": "paṅko ca kāmā palipo ca kāmā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_378:1.2",
        "original": "Bhayañca metaṁ timūlaṁ pavuttaṁ;",
        "analyzed": "bhayam ca mā itam ti mūlam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_378:1.3",
        "original": "Rajo ca dhūmo ca mayā pakāsitā,",
        "analyzed": "rajaḥ ca dhūmaḥ ca mayā pakāsitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_378:1.4",
        "original": "Hitvā tuvaṁ pabbaja brahmadatta”.",
        "analyzed": "hitvā tuvam pabbaja brahma datta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_378:2.1",
        "original": "“Gadhito ca ratto ca adhimucchito ca,",
        "analyzed": "gadhitaḥ ca rattaḥ ca adhimucchitaḥ ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_378:2.2",
        "original": "Kāmesvahaṁ brāhmaṇa bhiṁsarūpaṁ;",
        "analyzed": "kāmesu aham brāhmaṇa bhisa rūpa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_378:2.3",
        "original": "Taṁ nussahe jīvikattho pahātuṁ,",
        "analyzed": "tam nussahe jīvika tthaḥ pahātu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_378:2.4",
        "original": "Kāhāmi puññāni anappakāni”.",
        "analyzed": "kāhāmi puññāni anappakāni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_378:3.1",
        "original": "“Yo atthakāmassa hitānukampino,",
        "analyzed": "yaḥ attha kāmassa hita anukampinaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_378:3.2",
        "original": "Ovajjamāno na karoti sāsanaṁ;",
        "analyzed": "ovajjamānaḥ na karoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_378:3.3",
        "original": "Idameva seyyo iti maññamāno,",
        "analyzed": "idam eva seyyo iti maññamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_378:3.4",
        "original": "Punappunaṁ gabbhamupeti mando.",
        "analyzed": "punappunam gabbham upeti mandaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_378:4.1",
        "original": "So ghorarūpaṁ nirayaṁ upeti,",
        "analyzed": "saḥ ghora rūpam nirayam upeti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_378:4.2",
        "original": "Subhāsubhaṁ muttakarīsapūraṁ;",
        "analyzed": "subha asubham mutta karīsa pūra ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_378:4.3",
        "original": "Sattā sakāye na jahanti giddhā,",
        "analyzed": "sattā sa kāye na jahanti giddhā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_378:4.4",
        "original": "Ye honti kāmesu avītarāgā”.",
        "analyzed": "ye honti kāmesu a vīta rāgā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_378:5.1",
        "original": "“Mīḷhena littā ruhirena makkhitā,",
        "analyzed": "mīḷhena littā ruhirena makkhitā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_378:5.2",
        "original": "Semhena littā upanikkhamanti;",
        "analyzed": "semhena littāḥ upanikkhamanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_378:5.3",
        "original": "Yaṁ yañhi kāyena phusanti tāvade,",
        "analyzed": "yam yat hi kāyena phusanti tāvade ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_378:5.4",
        "original": "Sabbaṁ asātaṁ dukhameva kevalaṁ.",
        "analyzed": "sabbam asātam dukham eva kevala ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_378:6.1",
        "original": "Disvā vadāmi na hi aññato savaṁ,",
        "analyzed": "disvā vadāmi na hi aññato sava ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_378:6.2",
        "original": "Pubbenivāsaṁ bahukaṁ sarāmi;",
        "analyzed": "pubbenivāsam bahukam sarāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_378:6.3",
        "original": "Citrāhi gāthāhi subhāsitāhi,",
        "analyzed": "citrāhi gāthāhi su bhāsitāhi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_378:6.4",
        "original": "Darīmukho nijjhāpayi sumedhan”ti.",
        "analyzed": "darīmukhaḥ sumedhanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_378:7.1",
        "original": "Darīmukhajātakaṁ tatiyaṁ.",
        "analyzed": "darīmukha jātakam tatiya ",
        "folio": "7"
    }
]