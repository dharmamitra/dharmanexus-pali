[
    {
        "segmentnr": "PA_pv_32:0.1",
        "original": "Petavatthu",
        "analyzed": "petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_32:0.2",
        "original": "Cūḷavagga",
        "analyzed": "cūḷa vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_32:0.3",
        "original": "7. Migaluddakapetavatthu",
        "analyzed": "miga luddaka peta vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_32:1.1",
        "original": "“Naranāripurakkhato yuvā,",
        "analyzed": "nara nārī purakkhataḥ yuvā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_32:1.2",
        "original": "Rajanīyehi kāmaguṇehi sobhasi;",
        "analyzed": "rajanīyehi kāma guṇehi sobhasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_32:1.3",
        "original": "Divasaṁ anubhosi kāraṇaṁ,",
        "analyzed": "divasam anubhosi kāraṇa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_32:1.4",
        "original": "Kimakāsi purimāya jātiyā”ti.",
        "analyzed": "kim akāsi purimāya jāti yāti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_32:2.1",
        "original": "“Ahaṁ rājagahe ramme,",
        "analyzed": "aham rājagahe ramme ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_32:2.2",
        "original": "ramaṇīye giribbaje;",
        "analyzed": "ramaṇīye giribbaje ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_32:2.3",
        "original": "Migaluddo pure āsiṁ,",
        "analyzed": "migaluddaḥ pure āsi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_32:2.4",
        "original": "lohitapāṇi dāruṇo.",
        "analyzed": "lohita pāṇi dāruṇaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_32:3.1",
        "original": "Avirodhakaresu pāṇisu,",
        "analyzed": "avirodha karesu pāṇisu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_32:3.2",
        "original": "Puthusattesu paduṭṭhamānaso;",
        "analyzed": "puthu sattesu paduṭṭha mānasaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_32:3.3",
        "original": "Vicariṁ atidāruṇo sadā,",
        "analyzed": "vicarim ati dāruṇaḥ sadā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_32:3.4",
        "original": "Parahiṁsāya rato asaññato.",
        "analyzed": "para hisāya rataḥ asaññataḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_32:4.1",
        "original": "Tassa me sahāyo suhadayo,",
        "analyzed": "tassa me sahāyaḥ su hadayaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_32:4.2",
        "original": "Saddho āsi upāsako;",
        "analyzed": "saddhaḥ āsi upāsakaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_32:4.3",
        "original": "Sopi maṁ anukampanto,",
        "analyzed": "saḥ upi mam anukampantaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_32:4.4",
        "original": "Nivāresi punappunaṁ.",
        "analyzed": "nivāresi punappuna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_32:5.1",
        "original": "‘Mākāsi pāpakaṁ kammaṁ,",
        "analyzed": "mā akāsi pāpakam kamma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_32:5.2",
        "original": "Mā tāta duggatiṁ agā;",
        "analyzed": "mā tāta duggatim agāḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_32:5.3",
        "original": "Sace icchasi pecca sukhaṁ,",
        "analyzed": "sace icchasi pecca sukha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_32:5.4",
        "original": "Virama pāṇavadhā asaṁyamā’.",
        "analyzed": "virama pāṇa vadhā a sa yamā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_32:6.1",
        "original": "Tassāhaṁ vacanaṁ sutvā,",
        "analyzed": "tassa aham vacanam sutvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_32:6.2",
        "original": "Sukhakāmassa hitānukampino;",
        "analyzed": "sukha kāmassa hita anukampino ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_32:6.3",
        "original": "Nākāsiṁ sakalānusāsaniṁ,",
        "analyzed": "nāka asim sakala anusāsani ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_32:6.4",
        "original": "Cirapāpābhirato abuddhimā.",
        "analyzed": "cira pāpa abhirataḥ abuddhimā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_32:7.1",
        "original": "So maṁ puna bhūrisumedhaso,",
        "analyzed": "saḥ mam punar bhūri sumedhasaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_32:7.2",
        "original": "Anukampāya saṁyame nivesayi;",
        "analyzed": "anukampāya sayame ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_32:7.3",
        "original": "‘Sace divā hanasi pāṇino,",
        "analyzed": "sace divā hanasi pāṇinaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_32:7.4",
        "original": "Atha te rattiṁ bhavatu saṁyamo’.",
        "analyzed": "atha te rattim bhavatu sayamaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_32:8.1",
        "original": "Svāhaṁ divā hanitvā pāṇino,",
        "analyzed": "svāham divā hanitvā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_32:8.2",
        "original": "Virato rattimahosi saññato;",
        "analyzed": "virataḥ ratti mahosi saññataḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_32:8.3",
        "original": "Rattāhaṁ paricāremi,",
        "analyzed": "rattā aham paricāremi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_32:8.4",
        "original": "Divā khajjāmi duggato.",
        "analyzed": "divā khajjāmi duggataḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_32:9.1",
        "original": "Tassa kammassa kusalassa,",
        "analyzed": "tassa kammassa kusalassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_32:9.2",
        "original": "Anubhomi rattiṁ amānusiṁ;",
        "analyzed": "anubhomi rattim amānusi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_32:9.3",
        "original": "Divā paṭihatāva kukkurā,",
        "analyzed": "divā paṭihatā ava kukkurā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_32:9.4",
        "original": "Upadhāvanti samantā khādituṁ.",
        "analyzed": "upadhāvanti samantā khāditu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_32:10.1",
        "original": "Ye ca te satatānuyogino,",
        "analyzed": "ye ca te satata anuyoginaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_32:10.2",
        "original": "Dhuvaṁ payuttā sugatassa sāsane;",
        "analyzed": "dhuvam payuttā sugatassa sāsane ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_32:10.3",
        "original": "Maññāmi te amatameva kevalaṁ,",
        "analyzed": "maññāmi te amatam eva kevala ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_32:10.4",
        "original": "Adhigacchanti padaṁ asaṅkhatan”ti.",
        "analyzed": "adhigacchanti padam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_32:11.1",
        "original": "Migaluddakapetavatthu sattamaṁ.",
        "analyzed": "miga luddaka peta vatthu sattama ",
        "folio": "11"
    }
]