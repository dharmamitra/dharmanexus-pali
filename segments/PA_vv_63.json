        "segmentnr": "PA_vv_63:27.1",
        "original": "“Sahassaraṁsīva yathā mahappabho,",
        "analyzed": "sahassa rasī iva yathā mahappabho ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_63:27.2",
        "original": "Disaṁ yathā bhāti nabhe anukkamaṁ;",
        "analyzed": "disam yathā bhāti nabhe anukkama ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_63:27.3",
        "original": "Tathāpakāro tavāyaṁ mahāratho,",
        "analyzed": "tathā apakāraḥ tava ayam mahā rathaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_63:27.4",
        "original": "Samantato yojanasattamāyato.",
        "analyzed": "samantataḥ yojana sattamāyataḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_63:28.1",
        "original": "Suvaṇṇapaṭṭehi samantamotthaṭo,",
        "analyzed": "suvaṇṇa paṭṭehi ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_63:28.2",
        "original": "Urassa muttāhi maṇīhi cittito;",
        "analyzed": "urassa muttāhi maṇīhi cittitaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_63:28.3",
        "original": "Lekhā suvaṇṇassa ca rūpiyassa ca,",
        "analyzed": "lekhā suvaṇṇassa ca rūpiyassa ca ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_63:28.4",
        "original": "Sobhenti veḷuriyamayā sunimmitā.",
        "analyzed": "sobhenti veḷuriya mayā su nimmitā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_63:29.1",
        "original": "Sīsañcidaṁ veḷuriyassa nimmitaṁ,",
        "analyzed": "veḷuriyassa ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_63:29.2",
        "original": "Yugañcidaṁ lohitakāya cittitaṁ;",
        "analyzed": "yugañcid idam lohitakāya ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_63:29.3",
        "original": "Yuttā suvaṇṇassa ca rūpiyassa ca,",
        "analyzed": "yuttā suvaṇṇassa ca rūpiyassa ca ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_63:29.4",
        "original": "Sobhanti assā ca ime manojavā.",
        "analyzed": "sobhanti assā ca ime manojavā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_63:30.1",
        "original": "So tiṭṭhasi hemarathe adhiṭṭhito,",
        "analyzed": "saḥ tiṭṭhasi hema rathe adhiṭṭhito ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_63:30.2",
        "original": "Devānamindova sahassavāhano;",
        "analyzed": "devānam indo uva sahassa vāhanaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_63:30.3",
        "original": "Pucchāmi tāhaṁ yasavanta kovidaṁ,",
        "analyzed": "pucchāmi tā aham yasavanta kovida ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_63:30.4",
        "original": "Kathaṁ tayā laddho ayaṁ uḷāro”ti.",
        "analyzed": "katham tayā laddhaḥ ayam uḷārā iti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_63:31.1",
        "original": "“Sujāto nāmahaṁ bhante,",
        "analyzed": "sujātaḥ nāma aham bhante ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_63:31.2",
        "original": "rājaputto pure ahuṁ;",
        "analyzed": "rāja puttaḥ pure ahu ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_63:31.3",
        "original": "Tvañca maṁ anukampāya,",
        "analyzed": "tvañca mama anukampāya ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_63:31.4",
        "original": "saññamasmiṁ nivesayi.",
        "analyzed": "saññamasmin nivesayi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_63:32.1",
        "original": "Khīṇāyukañca maṁ ñatvā,",
        "analyzed": "khīṇāyukam ca mam ñatvā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_63:32.2",
        "original": "sarīraṁ pādāsi satthuno;",
        "analyzed": "sarīram pādāsi satthunaḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_63:32.3",
        "original": "Imaṁ sujāta pūjehi,",
        "analyzed": "imam sujāta pūjehi ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_63:32.4",
        "original": "taṁ te atthāya hehiti.",
        "analyzed": "tam te atthāya hehi iti ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_63:33.1",
        "original": "Tāhaṁ gandhehi mālehi,",
        "analyzed": "tā aham gandhehi mālehi ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_63:33.2",
        "original": "pūjayitvā samuyyuto;",
        "analyzed": "pūjayitvā samuyyutaḥ ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_63:33.3",
        "original": "Pahāya mānusaṁ dehaṁ,",
        "analyzed": "pahāya mānusam deha ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_63:33.4",
        "original": "upapannomhi nandanaṁ.",
        "analyzed": "upapannaḥ asi nandana ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_63:34.1",
        "original": "Nandane ca vane ramme,",
        "analyzed": "nandane ca vane ramme ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_vv_63:34.2",
        "original": "nānādijagaṇāyute;",
        "analyzed": "nānā ādi ja gaṇa āyute ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_vv_63:34.3",
        "original": "Ramāmi naccagītehi,",
        "analyzed": "ramāmi nacca gītehi ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_vv_63:34.4",
        "original": "accharāhi purakkhato”ti.",
        "analyzed": "accharāhi purakkhato ti ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_vv_63:35.1",
        "original": "Cūḷarathavimānaṁ terasamaṁ.",
        "analyzed": "cūḷa ratha vimānam ",
        "folio": "35"
    }
