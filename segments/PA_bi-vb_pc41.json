        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.2",
        "original": "Tena kho pana samayena rañño pasenadissa kosalassa uyyāne cittāgāre paṭibhānacittaṁ kataṁ hoti.",
        "analyzed": "tena kho pana samayena raññaḥ pasenadissa kosalassa uyyāne cittāgāre paṭibhāna cittam katam hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.3",
        "original": "Bahū manussā cittāgāraṁ dassanāya gacchanti.",
        "analyzed": "bahū manussā cittāgāram dassanāya gacchanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.4",
        "original": "Chabbaggiyāpi bhikkhuniyo cittāgāraṁ dassanāya agamaṁsu.",
        "analyzed": "chabbaggiyāḥ api bhikkhuniyaḥ cittāgāram dassanāya agamasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.5",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.6",
        "original": "“kathañhi nāma bhikkhuniyo cittāgāraṁ dassanāya gacchissanti, seyyathāpi gihiniyo kāmabhoginiyo”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ cittāgāram dassanāya gacchissanti seyyathā api gihiniyaḥ kāmabhoginiyaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.7",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.8",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc41:1.9",
        "original": "“kathañhi nāma chabbaggiyā bhikkhuniyo cittāgāraṁ dassanāya gacchissantī”ti …pe…",
        "analyzed": "katham hi nāma chabbaggiyāḥ bhikkhuniyaḥ cittāgāram dassanāya gacchissanti iti pe ",
        "folio": "1"
    },
    {
