        "analyzed": "makkaṭaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_mil_7.7.10:0.4",
        "original": "10. Tacchakaṅgapañha",
        "analyzed": "tat śakaṅga pañha ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_mil_7.7.10:1.1",
        "original": "“Bhante nāgasena, ‘tacchakassa dve aṅgāni gahetabbānī’ti yaṁ vadesi, katamāni tāni dve aṅgāni gahetabbānī”ti?",
        "analyzed": "bhante nāgasena tat śakassa dve aṅgāni gahetabbāni iti yam vadesi katamāni tāni dve aṅgāni gahetabbāni iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.7.10:1.2",
        "original": "“Yathā, mahārāja, tacchako kāḷasuttaṁ anulometvā rukkhaṁ tacchati;",
        "analyzed": "yathā mahā rāja tacchakaḥ kāḷasuttam anulometvā rukkham tacchati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.7.10:1.3",
        "original": "evameva kho, mahārāja, yoginā yogāvacarena jinasāsanamanulomayitvā sīlapathaviyaṁ patiṭṭhahitvā saddhāhatthena paññāvāsiṁ gahetvā kilesā tacchetabbā.",
        "analyzed": "evam eva kho mahā rāja yoginā yogāvacarena jina sāsanam anulomayitvā sīla pathaviyam patiṭṭhahitvā saddhā hatthena paññāvāsim gahetvā kilesā tat śetabbā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.7.10:1.4",
        "original": "Idaṁ, mahārāja, tacchakassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
        "analyzed": "idam mahā rāja tat śakassa paṭhamam aṅgam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.1",
        "original": "Puna caparaṁ, mahārāja, tacchako phegguṁ apaharitvā sāramādiyati;",
        "analyzed": "punar capara mahā rāja tat śako pheggum apaharitvā sāram ādiyati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.2_0",
        "original": "evameva kho, mahārāja, yoginā yogāvacarena sassataṁ ucchedaṁ taṁ jīvaṁ taṁ sarīraṁ",
        "analyzed": "evam eva khalu mahā rāja yoginā yogāvacarena ucchedam tam jīvam tam sarīra ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.2_1",
        "original": "aññaṁ jīvaṁ aññaṁ sarīraṁ taduttamaṁ aññaduttamaṁ akatamabhabbaṁ apurisakāraṁ abrahmacariyavāsaṁ",
        "analyzed": "aññam jīvam aññam sarīram tad uttamam aññat uttamam a katama bhabbam a purisakāram a brahmacariya vāsa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.2_2",
        "original": "sattavināsaṁ navasattapātubhāvaṁ saṅkhārasassatabhāvaṁ yo karoti, so paṭisaṁvedeti,",
        "analyzed": "satta vināsam nava satta pātubhāvam saṅkhāra sassata bhāvam yaḥ karoti so paṭisavedeti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.2_3",
        "original": "añño karoti, añño paṭisaṁvedeti, kammaphaladassanā ca kiriyaphaladiṭṭhi ca iti evarūpāni",
        "analyzed": "aññaḥ karoti aññaḥ paṭisavedeti kamma phala dassanā ca kiriya phala diṭṭhi ca iti evarūpāni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.2_4",
        "original": "ceva aññāni ca vivādapathāni apanetvā saṅkhārānaṁ sabhāvaṁ paramasuññataṁ nirīhanijjīvataṁ",
        "analyzed": "ca iva aññāni ca vivāda pathāni apanetvā saṅkhārānam sabhāvam parama suññatam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.2_5",
        "original": "accantaṁ suññataṁ ādiyitabbaṁ.",
        "analyzed": "accantam suññatam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.3",
        "original": "Idaṁ, mahārāja, tacchakassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
        "analyzed": "idam mahā rāja tat śakassa dutiyam aṅgam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:2.4",
        "original": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena suttanipāte—",
        "analyzed": "mahā rāja bhagavatā devātidevena sutta nipāte ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_mil_7.7.10:3.1",
        "original": "‘Kāraṇḍavaṁ niddhamatha,",
        "analyzed": "kāraṇḍavam niddham atha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.7.10:3.2",
        "original": "Kasambuṁ apakassatha;",
        "analyzed": "kasambum apakassatha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.7.10:3.3",
        "original": "Tato palāpe vāhetha,",
        "analyzed": "tatas palāpe vāhetha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.7.10:3.4",
        "original": "Assamaṇe samaṇamānine.",
        "analyzed": "assamaṇe samaṇa mānine ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_mil_7.7.10:4.1",
        "original": "Niddhamitvāna pāpicche,",
        "analyzed": "niddhamitvāna pāpicche ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.7.10:4.2",
        "original": "pāpaācāragocare;",
        "analyzed": "pāpa ācāra gocare ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.7.10:4.3",
        "original": "Suddhā suddhehi saṁvāsaṁ,",
        "analyzed": "suddhā suddhehi savāsa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.7.10:4.4",
        "original": "kappayavho patissatā;",
        "analyzed": "kappayavhaḥ patissatā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.7.10:4.5",
        "original": "Tato samaggā nipakā,",
        "analyzed": "tatas samaggā nipakā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.7.10:4.6",
        "original": "dukkhassantaṁ karissathā’”ti.",
        "analyzed": "dukkhassantam karissatha iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mil_7.7.10:5.1",
        "original": "Tacchakaṅgapañho dasamo.",
        "analyzed": "tacchakaṅgapañhaḥ dasamo ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mil_7.7.10:6.1",
        "original": "Makkaṭakavaggo chaṭṭho.",
        "analyzed": "makkaṭaka vaggaḥ chaṭṭhaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mil_7.7.10:7.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_mil_7.7.10:8.1",
        "original": "Makkaṭo dārako kummo,",
        "analyzed": "makkaṭaḥ dārako kummo ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_mil_7.7.10:8.2",
        "original": "vanaṁ rukkho ca pañcamo;",
        "analyzed": "vanam rukkhaḥ ca pañcamaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_mil_7.7.10:8.3",
        "original": "Megho maṇi māgaviko,",
        "analyzed": "meghaḥ maṇi māgavikaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_mil_7.7.10:8.4",
        "original": "bāḷisī tacchakena cāti.",
        "analyzed": "bāḷisī tad śakena ca ati ",
        "folio": "8"
    }
]