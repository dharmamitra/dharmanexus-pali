        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_112:0.3",
        "original": "Brāhmaṇadhammayāgavagga",
        "analyzed": "brāhmaṇa dhamma yāga vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_112:0.4",
        "original": "Lokasutta",
        "analyzed": "lokasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_112:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_112:2.1",
        "original": "“Loko, bhikkhave, tathāgatena abhisambuddho,",
        "analyzed": "lokaḥ bhikkhave tathāgatena abhisambuddhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:2.2",
        "original": "lokasmā tathāgato visaṁyutto.",
        "analyzed": "lokasma tathāgataḥ visa yuttaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:2.3",
        "original": "Lokasamudayo, bhikkhave, tathāgatena abhisambuddho,",
        "analyzed": "loka samudayaḥ bhikkhave tathāgatena abhisambuddhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:2.4",
        "original": "lokasamudayo tathāgatassa pahīno.",
        "analyzed": "loka samudayaḥ tathāgatassa pahīno ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:2.5",
        "original": "Lokanirodho, bhikkhave, tathāgatena abhisambuddho,",
        "analyzed": "loka nirodhaḥ bhikkhave tathāgatena abhisambuddhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:2.6",
        "original": "lokanirodho tathāgatassa sacchikato.",
        "analyzed": "loka nirodhaḥ tathāgatassa sacchikato ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:2.7",
        "original": "Lokanirodhagāminī paṭipadā, bhikkhave, tathāgatena abhisambuddhā,",
        "analyzed": "loka nirodha gāminī paṭipadā bhikkhave tathāgatena abhisambuddhā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:2.8",
        "original": "lokanirodhagāminī paṭipadā tathāgatassa bhāvitā.",
        "analyzed": "loka nirodha gāminī paṭipadā tathāgatassa bhāvitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_112:3.1_0",
        "original": "Yaṁ, bhikkhave, sadevakassa lokassa samārakassa sabrahmakassa sassamaṇabrāhmaṇiyā",
        "analyzed": "ya bhikkhave sa devakassa lokassa sa mārakassa sa brahmakassa sa ssamaṇa brāhmaṇiyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_112:3.1_1",
        "original": "pajāya sadevamanussāya diṭṭhaṁ sutaṁ mutaṁ viññātaṁ pattaṁ pariyesitaṁ anuvicaritaṁ",
        "analyzed": "pajāya sa deva manussāya diṭṭham sutam mutam viññātam pattam pariyesitam anuvicarita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_112:3.1_2",
        "original": "manasā yasmā taṁ tathāgatena abhisambuddhaṁ,",
        "analyzed": "manasā yasmai tam tathāgatena abhisambuddha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_112:3.2",
        "original": "tasmā ‘tathāgato’ti vuccati.",
        "analyzed": "tasmā tathāgataḥ iti vuccati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_112:4.1_0",
        "original": "Yañca, bhikkhave, rattiṁ tathāgato anuttaraṁ sammāsambodhiṁ abhisambujjhati, yañca",
        "analyzed": "yat ca bhikkhave rattim tathāgataḥ anuttaram sammāsambodhim abhisambujjhati yat ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_112:4.1_1",
        "original": "rattiṁ anupādisesāya nibbānadhātuyā parinibbāyati, yaṁ etasmiṁ antare bhāsati lapati",
        "analyzed": "rattim an upādisesāya nibbānadhātuyā parinibbāyati yam etasmin antare bhāsati lapati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_112:4.1_2",
        "original": "niddisati, sabbaṁ taṁ tatheva hoti no aññathā,",
        "analyzed": "niddisati sabbam tam tathā iva hoti no aññathā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_112:4.2",
        "original": "tasmā ‘tathāgato’ti vuccati.",
        "analyzed": "tasmā tathāgataḥ iti vuccati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_112:5.1",
        "original": "Yathāvādī, bhikkhave, tathāgato tathākārī, yathākārī tathāvādī,",
        "analyzed": "yathāvādī bhikkhave tathāgataḥ tathākārī yathākārī tathāvādī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_112:5.2",
        "original": "iti yathāvādī tathākārī yathākārī tathāvādī,",
        "analyzed": "iti yathāvādī tathākārī yathākārī tathāvādī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_112:5.3",
        "original": "tasmā ‘tathāgato’ti vuccati.",
        "analyzed": "tasmā tathāgataḥ iti vuccati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_112:6.1",
        "original": "Sadevake, bhikkhave, loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya tathāgato abhibhū anabhibhūto aññadatthudaso vasavattī,",
        "analyzed": "sa devake bhikkhave loke sa mārake sa brahmake sa ssamaṇa brāhmaṇiyā pajāya sa deva manussāya tathāgataḥ abhibhū an abhibhūtaḥ añña datthudasaḥ vasa vattī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_112:6.2",
        "original": "tasmā tathāgatoti vuccatī”ti.",
        "analyzed": "tasmā tathāgataḥ iti vuccati iti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_112:6.3",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_112:6.4",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_112:7.1",
        "original": "“Sabbalokaṁ abhiññāya,",
        "analyzed": "sabba lokam abhiññāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_112:7.2",
        "original": "sabbaloke yathātathaṁ;",
        "analyzed": "sabba loke yathā tatha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_112:7.3",
        "original": "Sabbalokavisaṁyutto,",
        "analyzed": "sabba loka visayuttaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_112:7.4",
        "original": "sabbaloke anūpayo.",
        "analyzed": "sabba loke ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_112:8.1",
        "original": "Sa ve sabbābhibhū dhīro,",
        "analyzed": "sa ve sabba abhibhū dhīraḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_112:8.2",
        "original": "sabbaganthappamocano;",
        "analyzed": "sabba gantha ppamocanaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_112:8.3",
        "original": "Phuṭṭhāssa paramā santi,",
        "analyzed": "phuṭṭhā assa paramā santi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_112:8.4",
        "original": "nibbānaṁ akutobhayaṁ.",
        "analyzed": "nibbānam akutobhaya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_112:9.1",
        "original": "Esa khīṇāsavo buddho,",
        "analyzed": "esa khīṇāsavaḥ buddhaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_112:9.2",
        "original": "anīgho chinnasaṁsayo;",
        "analyzed": "anīghaḥ chinna sa sayaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_112:9.3",
        "original": "Sabbakammakkhayaṁ patto,",
        "analyzed": "sabba kamma kkhayam patto ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_112:9.4",
        "original": "vimutto upadhisaṅkhaye.",
        "analyzed": "vimuttaḥ upadhi saṅkhaye ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_112:10.1",
        "original": "Esa so bhagavā buddho,",
        "analyzed": "esa saḥ bhagavā buddhaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_112:10.2",
        "original": "esa sīho anuttaro;",
        "analyzed": "esa sīhaḥ anuttaraḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_112:10.3",
        "original": "Sadevakassa lokassa,",
        "analyzed": "sa devakassa lokassa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_112:10.4",
        "original": "brahmacakkaṁ pavattayi.",
        "analyzed": "brahma cakkam pavattayi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_112:11.1",
        "original": "Iti devā manussā ca,",
        "analyzed": "iti devā manuḥ sā ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_iti_112:11.2",
        "original": "ye buddhaṁ saraṇaṁ gatā;",
        "analyzed": "ye buddham saraṇam gatā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_iti_112:11.3",
        "original": "Saṅgamma taṁ namassanti,",
        "analyzed": "saṅgamma tam namassanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_iti_112:11.4",
        "original": "mahantaṁ vītasāradaṁ.",
        "analyzed": "mahantam vīta sārada ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_iti_112:12.1",
        "original": "Danto damayataṁ seṭṭho,",
        "analyzed": "dantaḥ damayatam seṭṭhaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_iti_112:12.2",
        "original": "santo samayataṁ isi;",
        "analyzed": "santaḥ isi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_iti_112:12.3",
        "original": "Mutto mocayataṁ aggo,",
        "analyzed": "muttaḥ mocayatam aggaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_iti_112:12.4",
        "original": "tiṇṇo tārayataṁ varo.",
        "analyzed": "tiṇṇaḥ tārayatam varaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_iti_112:13.1",
        "original": "Iti hetaṁ namassanti,",
        "analyzed": "iti ha itam namassanti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_iti_112:13.2",
        "original": "mahantaṁ vītasāradaṁ;",
        "analyzed": "mahantam vīta sārada ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_iti_112:13.3",
        "original": "Sadevakasmiṁ lokasmiṁ,",
        "analyzed": "sa devakasmin ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_iti_112:13.4",
        "original": "natthi te paṭipuggalo”ti.",
        "analyzed": "na atthi te paṭipuggalo ti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_iti_112:14.1",
        "original": "Ayampi attho vutto bhagavatā, iti me sutanti.",
        "analyzed": "ayam pi atthaḥ vuttaḥ bhagavatā iti me sutam ti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_iti_112:14.2",
        "original": "Terasamaṁ.",
        "analyzed": "terasama ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_iti_112:14.3",
        "original": "Catukkanipāto niṭṭhito.",
        "analyzed": "catukka nipātaḥ niṭṭhito ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_iti_112:15.0",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_iti_112:15.1",
        "original": "Brāhmaṇasulabhā jānaṁ,",
        "analyzed": "brāhmaṇa sulabhā jāna ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_iti_112:15.2",
        "original": "Samaṇasīlā taṇhā brahmā;",
        "analyzed": "samaṇa sīlā taṇhā brahmā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_iti_112:15.3",
        "original": "Bahukārā kuhapurisā,",
        "analyzed": "bahu kārā kuha purisā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_iti_112:15.4",
        "original": "Cara sampannalokena terasāti.",
        "analyzed": "cara sampanna lokena terasā ati ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_iti_112:16.0",
        "original": "Suttasaṅgaho",
        "analyzed": "sutta saṅgaho ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_iti_112:16.1",
        "original": "Sattavisekanipātaṁ,",
        "analyzed": "satta viseka nipāta ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_iti_112:16.2",
        "original": "Dukkaṁ bāvīsasuttasaṅgahitaṁ;",
        "analyzed": "dukkam ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_iti_112:16.3",
        "original": "Samapaññāsamathatikaṁ,",
        "analyzed": "sama paññā sama thatika ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_iti_112:16.4",
        "original": "Terasa catukkañca iti yamidaṁ.",
        "analyzed": "terasa catukkañcaḥ iti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_iti_112:17.1",
        "original": "Dvidasuttarasuttasate,",
        "analyzed": "dvi das uttaras utta sa te ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_iti_112:17.2",
        "original": "Saṅgāyitvā samādahiṁsu purā;",
        "analyzed": "saṅgāyitvā samādahisu purā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_iti_112:17.3",
        "original": "Arahanto ciraṭṭhitiyā,",
        "analyzed": "arahanto ciraṭṭhitiyā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_iti_112:17.4",
        "original": "Tamāhu nāmena itivuttanti.",
        "analyzed": "tam āhu nāmena iti vuttam ti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_iti_112:17.5",
        "original": "Itivuttakapāḷi niṭṭhitā.",
        "analyzed": "      ",
        "folio": "17"
    }
]