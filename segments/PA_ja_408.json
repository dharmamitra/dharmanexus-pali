        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_408:7.1",
        "original": "“Ayameva kālo na hi añño atthi,",
        "analyzed": "ayam eva kālaḥ na hi aññaḥ atthi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_408:7.2",
