    },
    {
        "segmentnr": "PA_ja_341:1.1",
        "original": "Narānamārāmakarāsu nārisu,",
        "analyzed": "nara anama ārāma karāsu nārisu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_341:1.2",
        "original": "Anekacittāsu aniggahāsu ca;",
        "analyzed": "anekacittāsu aniggahāsu ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_341:1.3",
        "original": "Sabbattha nāpītikarāpi ce siyā,",
        "analyzed": "sabbattha na api iti karā api ce siyā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_341:1.4",
        "original": "Na vissase titthasamā hi nāriyo.",
        "analyzed": "na vissase tittha samāḥ hi nāriyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_341:2.1",
        "original": "“Yaṁ ve disvā kaṇḍarīkinnarānaṁ,",
        "analyzed": "yam veḥ disvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_341:2.2",
        "original": "Sabbitthiyo na ramanti agāre;",
        "analyzed": "sabbitthiyaḥ na ramanti agāre ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_341:2.3",
        "original": "Taṁ tādisaṁ maccaṁ cajitvā bhariyā,",
        "analyzed": "tam tādisam maccam cajitvā bhariyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_341:2.4",
        "original": "Aññaṁ disvā purisaṁ pīṭhasappiṁ”.",
