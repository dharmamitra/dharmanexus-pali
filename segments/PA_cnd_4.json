        "folio": "16"
    },
    {
        "segmentnr": "PA_cnd_4:17.1",
        "original": "“Esa bhiyyo pasīdāmi,",
        "analyzed": "esa bhiyyas pasīdāmi ",
        "folio": "17"
    },
