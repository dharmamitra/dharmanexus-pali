        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.9:29.3",
        "original": "Sabbe taṁ saraṇaṁ yanti,",
        "analyzed": "sabbe tam saraṇam yanti ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.9:29.4",
        "original": "Tvaṁ no satthā anuttaro.",
