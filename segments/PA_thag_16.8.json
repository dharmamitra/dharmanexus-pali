    },
    {
        "segmentnr": "PA_thag_16.8:0.2",
        "original": "Vīsatinipāta",
        "analyzed": "vīsati nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_16.8:0.3",
        "original": "Paṭhamavagga",
        "analyzed": "paṭhama vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_16.8:0.4",
        "original": "Aṅgulimālattheragāthā",
        "analyzed": "aṅguli mālatthera gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_16.8:1.1",
        "original": "“Gacchaṁ vadesi samaṇaṭṭhitomhi,",
        "analyzed": "gaccham vadesi samaṇa ṭṭhitaḥ amhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.8:1.2",
        "original": "Mamañca brūsi ṭhitamaṭṭhitoti;",
        "analyzed": "brūsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.8:1.3",
        "original": "Pucchāmi taṁ samaṇa etamatthaṁ,",
        "analyzed": "pucchāmi tam samaṇaḥ etam attha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.8:1.4",
        "original": "‘Kathaṁ ṭhito tvaṁ ahamaṭṭhitomhi’”.",
        "analyzed": "katham ṭhitaḥ tvam aham aṭṭhitaḥ amhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.8:2.1",
        "original": "“Ṭhito ahaṁ aṅgulimāla sabbadā,",
        "analyzed": "ṭhitaḥ aham aṅgulimāla sabbadā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.8:2.2",
        "original": "Sabbesu bhūtesu nidhāya daṇḍaṁ;",
        "analyzed": "sabbesu bhūtesu nidhāya daṇḍa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.8:2.3",
        "original": "Tuvañca pāṇesu asaññatosi,",
        "analyzed": "tuvam ca pāṇesu asaññataḥ asi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.8:2.4",
        "original": "Tasmā ṭhitohaṁ tuvamaṭṭhitosi”.",
        "analyzed": "tasmai ṭhitā uham tuvam aṭṭhitā asi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.8:3.1",
        "original": "“Cirassaṁ vata me mahito mahesī,",
        "analyzed": "cirassam vata me mahitaḥ mahesī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.8:3.2",
        "original": "Mahāvanaṁ samaṇo paccapādi;",
        "analyzed": "mahā vanam samaṇaḥ paccapādi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.8:3.3",
        "original": "Sohaṁ cajissāmi sahassapāpaṁ,",
        "analyzed": "saḥ ham cajissāmi sahassa pāpa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.8:3.4",
        "original": "Sutvāna gāthaṁ tava dhammayuttaṁ”.",
        "analyzed": "sutvāna gātham tava dhamma yutta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.8:4.1",
        "original": "Icceva coro asimāvudhañca,",
        "analyzed": "id ca iva ca uraḥ asim āvudhan ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.8:4.2",
        "original": "Sobbhe papāte narake anvakāsi;",
        "analyzed": "sā ubbhe papāte narake anvakāsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.8:4.3",
        "original": "Avandi coro sugatassa pāde,",
        "analyzed": "avandi coraḥ sugatassa pāde ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.8:4.4",
        "original": "Tattheva pabbajjamayāci buddhaṁ.",
        "analyzed": "tattha iva pabbajjam ayāci buddha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.8:5.1",
        "original": "Buddho ca kho kāruṇiko mahesi,",
        "analyzed": "buddhaḥ ca kho kāruṇikaḥ mahesi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.8:5.2",
        "original": "Yo satthā lokassa sadevakassa;",
        "analyzed": "yaḥ satthā lokassa sa devakassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.8:5.3",
        "original": "“Tamehi bhikkhū”ti tadā avoca,",
        "analyzed": "tam ehi bhikkhu iti tadā avoca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.8:5.4",
        "original": "Eseva tassa ahu bhikkhubhāvo.",
        "analyzed": "esa iva tassa ahu bhikkhu bhāvaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.8:6.1",
        "original": "“Yo ca pubbe pamajjitvā,",
        "analyzed": "yaḥ ca pubbe pamajjitvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.8:6.2",
        "original": "pacchā so nappamajjati;",
        "analyzed": "pacchā so na appamajjati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.8:6.3",
        "original": "Somaṁ lokaṁ pabhāseti,",
        "analyzed": "somam lokam pabhāsā iti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.8:6.4",
        "original": "abbhā muttova candimā.",
        "analyzed": "abbhā muttā uva candimā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.8:7.1",
        "original": "Yassa pāpaṁ kataṁ kammaṁ,",
        "analyzed": "yassa pāpam katam kamma ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.8:7.2",
        "original": "kusalena pidhīyati;",
        "analyzed": "kusalena pidhīyati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.8:7.3",
        "original": "Somaṁ lokaṁ pabhāseti,",
        "analyzed": "somam lokam pabhāsā iti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.8:7.4",
        "original": "abbhā muttova candimā.",
        "analyzed": "abbhā muttā uva candimā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.8:8.1",
        "original": "Yo have daharo bhikkhu,",
        "analyzed": "yaḥ have daharaḥ bhikkhu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.8:8.2",
        "original": "yuñjati buddhasāsane;",
        "analyzed": "yuñjati buddha sāsane ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.8:8.3",
        "original": "Somaṁ lokaṁ pabhāseti,",
        "analyzed": "somam lokam pabhāsā iti ",
        "folio": "8"
    },
    {
