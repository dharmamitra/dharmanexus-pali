        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_pc62:2.1.7",
        "original": "“Sappāṇakan”ti jānanto, “paribhogena marissantī”ti jānanto paribhuñjati, āpatti pācittiyassa.",
        "analyzed": "sappāṇakanti jānantaḥ paribhogena marissanti iti jānantaḥ paribhuñjati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_pc62:2.2.1",
        "original": "Sappāṇake sappāṇakasaññī paribhuñjati, āpatti pācittiyassa.",
        "analyzed": "sappāṇake sappāṇaka saññī paribhuñjati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_pc62:2.2.2",
        "original": "Sappāṇake vematiko paribhuñjati, āpatti dukkaṭassa.",
