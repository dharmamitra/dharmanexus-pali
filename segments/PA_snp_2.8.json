[
    {
        "segmentnr": "PA_snp_2.8:0.1",
        "original": "Sutta Nipāta 2.8",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.8:0.2",
        "original": "Dhamma (nāvā) sutta",
        "analyzed": "dhamma nāvā sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.8:1.1",
        "original": "Yasmā hi dhammaṁ puriso vijaññā,",
        "analyzed": "yasmai hi dhammam puriso ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.8:1.2",
        "original": "Indaṁva naṁ devatā pūjayeyya;",
        "analyzed": "indava nam devatāḥ pūjayeyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.8:1.3",
        "original": "So pūjito tasmi pasannacitto,",
        "analyzed": "saḥ pūjitaḥ tasmi pasanna cittaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.8:1.4",
        "original": "Bahussuto pātukaroti dhammaṁ.",
        "analyzed": "bahu sutaḥ pātukaroti dhamma ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.8:2.1",
        "original": "Tadaṭṭhikatvāna nisamma dhīro,",
        "analyzed": "nisamma dhīraḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.8:2.2",
        "original": "Dhammānudhammaṁ paṭipajjamāno;",
        "analyzed": "dhamma anudhammam paṭipajjamānaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.8:2.3",
        "original": "Viññū vibhāvī nipuṇo ca hoti,",
        "analyzed": "viññū vibhāvī nipuṇaḥ ca hoti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.8:2.4",
        "original": "Yo tādisaṁ bhajati appamatto.",
        "analyzed": "yaḥ tādisam bhajati appamattaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.8:3.1",
        "original": "Khuddañca bālaṁ upasevamāno,",
        "analyzed": "khuddam ca bālam upasevamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.8:3.2",
        "original": "Anāgatatthañca usūyakañca;",
        "analyzed": "anāgatatthañca usūyakañca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.8:3.3",
        "original": "Idheva dhammaṁ avibhāvayitvā,",
        "analyzed": "idha iva dhammam a vibhāvayitvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.8:3.4",
        "original": "Avitiṇṇakaṅkho maraṇaṁ upeti.",
        "analyzed": "a vitiṇṇa kaṅkhaḥ maraṇam upeti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.8:4.1",
        "original": "Yathā naro āpagamotaritvā,",
        "analyzed": "yathā naraḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.8:4.2",
        "original": "Mahodakaṁ salilaṁ sīghasotaṁ;",
        "analyzed": "mahā udakam salilam sīghasota ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.8:4.3",
        "original": "So vuyhamāno anusotagāmī,",
        "analyzed": "saḥ vuyhamānaḥ anusota gāmī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.8:4.4",
        "original": "Kiṁ so pare sakkhati tārayetuṁ.",
        "analyzed": "kim saḥ pare sakkhati tārayetu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.8:5.1",
        "original": "Tatheva dhammaṁ avibhāvayitvā,",
        "analyzed": "tathā iva dhammaṃ a vibhāvayitvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.8:5.2",
        "original": "Bahussutānaṁ anisāmayatthaṁ;",
        "analyzed": "bahussutānam anisāmayattha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.8:5.3",
        "original": "Sayaṁ ajānaṁ avitiṇṇakaṅkho,",
        "analyzed": "sayam a jānam a vitiṇṇa kaṅkhaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.8:5.4",
        "original": "Kiṁ so pare sakkhati nijjhapetuṁ.",
        "analyzed": "kim saḥ pare sakkhati nijjhapetu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.8:6.1",
        "original": "Yathāpi nāvaṁ daḷhamāruhitvā,",
        "analyzed": "yathā api nāvam daḷham āruhitvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.8:6.2",
        "original": "Phiyena rittena samaṅgibhūto;",
        "analyzed": "phiyena rittena samaṅgi bhūtaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.8:6.3",
        "original": "So tāraye tattha bahūpi aññe,",
        "analyzed": "so tāraye tattha bahū api aññe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.8:6.4",
        "original": "Tatrūpayaññū kusalo mutīmā.",
        "analyzed": "tatru upayaññū kusalaḥ mutīmā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.8:7.1",
        "original": "Evampi yo vedagu bhāvitatto,",
        "analyzed": "evampi yaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.8:7.2",
        "original": "Bahussuto hoti avedhadhammo;",
        "analyzed": "bahu sutaḥ hoti a vedha dhammaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.8:7.3",
        "original": "So kho pare nijjhapaye pajānaṁ,",
        "analyzed": "saḥ kho pare nijjhapaye pajāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.8:7.4",
        "original": "Sotāvadhānūpanisūpapanne.",
        "analyzed": "sotā vadha anūpa nisū papanne ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.8:8.1",
        "original": "Tasmā have sappurisaṁ bhajetha,",
        "analyzed": "tasmai have sappurisam bhajetha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.8:8.2",
        "original": "Medhāvinañceva bahussutañca;",
        "analyzed": "medhāvinañca iva bahussutañca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.8:8.3",
        "original": "Aññāya atthaṁ paṭipajjamāno,",
        "analyzed": "aññāya attham paṭipajjamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.8:8.4",
        "original": "Viññātadhammo sa sukhaṁ labhethāti.",
        "analyzed": "viññāta dhammaḥ sa sukham labhetha ati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.8:8.5",
        "original": "Nāvāsuttaṁ aṭṭhamaṁ.",
        "analyzed": "nāvāsuttam aṭṭhama ",
        "folio": "8"
    }
]