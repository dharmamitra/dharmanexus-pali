[
    {
        "segmentnr": "PA_ja_491:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_491:0.2",
        "original": "Pakiṇṇakanipāta",
        "analyzed": "pakiṇṇaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_491:0.3",
        "original": "Sālikedāravagga",
        "analyzed": "sāli kedāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_491:0.4",
        "original": "8. Mahāmorajātaka",
        "analyzed": "mahāmorajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_491:1.1",
        "original": "“Sace hi tyāhaṁ dhanahetu gāhito,",
        "analyzed": "sace hi tyā aham dhana hetu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_491:1.2",
        "original": "Mā maṁ vadhī jīvagāhaṁ gahetvā;",
        "analyzed": "mā mam vadhī jīva gāham ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_491:1.3",
        "original": "Rañño ca maṁ samma upantikaṁ nehi,",
        "analyzed": "raññaḥ ca mama samma upantikam nehi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_491:1.4",
        "original": "Maññe dhanaṁ lacchasinapparūpaṁ”.",
        "analyzed": "maññe dhanam lacchasinapparūpa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_491:2.1",
        "original": "“Na me ayaṁ tuyha vadhāya ajja,",
        "analyzed": "na me ayam tuyha vadhāya ajja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_491:2.2",
        "original": "Samāhito cāpadhure khurappo;",
        "analyzed": "samāhitaḥ cāpa dhure khura ppaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_491:2.3",
        "original": "Pāsañca tyāhaṁ adhipātayissaṁ,",
        "analyzed": "pāsam ca tyāham adhipātayissa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_491:2.4",
        "original": "Yathāsukhaṁ gacchatu morarājā”.",
        "analyzed": "yathāsukham gacchatu mora rājā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_491:3.1",
        "original": "“Yaṁ satta vassāni mamānubandhi,",
        "analyzed": "yam satta vassāni mama anubandhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_491:3.2",
        "original": "Rattindivaṁ khuppipāsaṁ sahanto;",
        "analyzed": "rattindivam khuppipāsam sahantaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_491:3.3",
        "original": "Atha kissa maṁ pāsavasūpanītaṁ,",
        "analyzed": "atha kissa mama pāsava su upanīta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_491:3.4",
        "original": "Pamuttave icchasi bandhanasmā.",
        "analyzed": "pamuttave icchasi bandhanasmā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_491:4.1",
        "original": "Pāṇātipātā virato nusajja,",
        "analyzed": "pāṇa atipātā virataḥ nusajja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_491:4.2",
        "original": "Abhayaṁ nu te sabbabhūtesu dinnaṁ;",
        "analyzed": "abhayam nu te sabba bhūtesu dinna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_491:4.3",
        "original": "Yaṁ maṁ tuvaṁ pāsavasūpanītaṁ,",
        "analyzed": "yam mama tuvam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_491:4.4",
        "original": "Pamuttave icchasi bandhanasmā”.",
        "analyzed": "pamuttave icchasi bandhanasmā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_491:5.1",
        "original": "“Pāṇātipātā viratassa brūhi,",
        "analyzed": "pāṇa atipātā viratassa brūhi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_491:5.2",
        "original": "Abhayañca yo sabbabhūtesu deti;",
        "analyzed": "abhayam ca yaḥ sabba bhūtesu deti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_491:5.3",
        "original": "Pucchāmi taṁ morarājetamatthaṁ,",
        "analyzed": "pucchāmi tam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_491:5.4",
        "original": "Ito cuto kiṁ labhate sukhaṁ so”.",
        "analyzed": "itas cutas kim labhate sukham saḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_491:6.1",
        "original": "“Pāṇātipātā viratassa brūmi,",
        "analyzed": "pāṇa atipātā viratassa brūmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_491:6.2",
        "original": "Abhayañca yo sabbabhūtesu deti;",
        "analyzed": "abhayam ca yaḥ sabba bhūtesu deti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_491:6.3",
        "original": "Diṭṭheva dhamme labhate pasaṁsaṁ,",
        "analyzed": "diṭṭhā iva dhamme labhate pasasa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_491:6.4",
        "original": "Saggañca so yāti sarīrabhedā”.",
        "analyzed": "saggam ca saḥ yāti sarīra bhedā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_491:7.1",
        "original": "“Na santi devā iti āhu eke,",
        "analyzed": "na santi devā iti āhu eke ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_491:7.2",
        "original": "Idheva jīvo vibhavaṁ upeti;",
        "analyzed": "idhā iva jīvaḥ vibhavam upeti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_491:7.3",
        "original": "Tathā phalaṁ sukatadukkaṭānaṁ,",
        "analyzed": "tathā phalam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_491:7.4",
        "original": "Dattupaññattañca vadanti dānaṁ;",
        "analyzed": "vadanti dāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_491:7.5",
        "original": "Tesaṁ vaco arahataṁ saddahāno,",
        "analyzed": "tesam vacaḥ arahatam saddahānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_491:7.6",
        "original": "Tasmā ahaṁ sakuṇe bādhayāmi”.",
        "analyzed": "tasmai aham sakuṇe bādhayāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_491:8.1",
        "original": "“Cando ca suriyo ca ubho sudassanā,",
        "analyzed": "cando ca suriyaḥ ca ubho su dassanā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_491:8.2",
        "original": "Gacchanti obhāsayamantalikkhe;",
        "analyzed": "gacchanti obhāsa yamantalikkhe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_491:8.3",
        "original": "Imassa lokassa parassa vā te,",
        "analyzed": "imassa lokassa parassa vā te ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_491:8.4",
        "original": "Kathaṁ nu te āhu manussaloke”.",
        "analyzed": "katham nu te āhu manussa loke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_491:9.1",
        "original": "“Cando ca suriyo ca ubho sudassanā,",
        "analyzed": "cando ca suriyaḥ ca ubho su dassanā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_491:9.2",
        "original": "Gacchanti obhāsayamantalikkhe;",
        "analyzed": "gacchanti obhāsa yamantalikkhe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_491:9.3",
        "original": "Parassa lokassa na te imassa,",
        "analyzed": "parassa lokassa na te imassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_491:9.4",
        "original": "Devāti te āhu manussaloke”.",
        "analyzed": "devā iti te āhu manussa loke ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_491:10.1",
        "original": "“Ettheva te nīhatā hīnavādā,",
        "analyzed": "etthā iva te nīhatāḥ hīna vādā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_491:10.2",
        "original": "Ahetukā ye na vadanti kammaṁ;",
        "analyzed": "ahetukāḥ ye na vadanti kamma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_491:10.3",
        "original": "Tathā phalaṁ sukatadukkaṭānaṁ,",
        "analyzed": "tathā phalam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_491:10.4",
        "original": "Dattupaññattaṁ ye ca vadanti dānaṁ”.",
        "analyzed": "dattu paññattam ye ca vadanti dāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_491:11.1",
        "original": "“Addhā hi saccaṁ vacanaṁ tavedaṁ,",
        "analyzed": "addhā hi saccam vacanam tava īda ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_491:11.2",
        "original": "Kathañhi dānaṁ aphalaṁ bhaveyya;",
        "analyzed": "katham hi dānam aphalam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_491:11.3",
        "original": "Tathā phalaṁ sukatadukkaṭānaṁ,",
        "analyzed": "tathā phalam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_491:11.4",
        "original": "Dattupaññattañca kathaṁ bhaveyya.",
        "analyzed": "dattu paññattam ca katham ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_491:12.1",
        "original": "Kathaṅkaro kintikaro kimācaraṁ,",
        "analyzed": "kathaṅkaro kintikaro kimācara ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_491:12.2",
        "original": "Kiṁ sevamāno kena tapoguṇena;",
        "analyzed": "kim sevamānaḥ kena tapaḥ guṇena ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_491:12.3",
        "original": "Akkhāhi me morarājetamatthaṁ,",
        "analyzed": "akkhāhi me ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_491:12.4",
        "original": "Yathā ahaṁ no nirayaṁ pateyyaṁ”.",
        "analyzed": "yathā aham naḥ nirayam pateyya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_491:13.1",
        "original": "“Ye keci atthi samaṇā pathabyā,",
        "analyzed": "ye keci atthi samaṇā pathabyā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_491:13.2",
        "original": "Kāsāyavatthā anagāriyā te;",
        "analyzed": "kāsāyavatthāḥ anagāriyā te ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_491:13.3",
        "original": "Pātova piṇḍāya caranti kāle,",
        "analyzed": "pātaḥ uva piṇḍāya caranti kāle ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_491:13.4",
        "original": "Vikālacariyā viratā hi santo.",
        "analyzed": "vikāla cariyāḥ viratāḥ hi santaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_491:14.1",
        "original": "Te tattha kālenupasaṅkamitvā,",
        "analyzed": "te tattha kālena upasaṅkamitvā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_491:14.2",
        "original": "Pucchāhi yaṁ te manaso piyaṁ siyā;",
        "analyzed": "pucchāhi yam te manasaḥ piyam siyā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_491:14.3",
        "original": "Te te pavakkhanti yathāpajānaṁ,",
        "analyzed": "te te pavakkhanti yathā apajāna ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_491:14.4",
        "original": "Imassa lokassa parassa catthaṁ”.",
        "analyzed": "imassa lokassa parassa cattha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_491:15.1",
        "original": "“Tacaṁva jiṇṇaṁ urago purāṇaṁ,",
        "analyzed": "jiṇṇam uragaḥ purāṇa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_491:15.2",
        "original": "Paṇḍūpalāsaṁ harito dumova;",
        "analyzed": "paṇḍū palāsam haritaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_491:15.3",
        "original": "Esappahīno mama luddabhāvo,",
        "analyzed": "mama ludda bhāvaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_491:15.4",
        "original": "Jahāmahaṁ luddakabhāvamajja.",
        "analyzed": "jahām aham luddaka bhāvam ajja ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_491:16.1",
        "original": "Ye cāpi me sakuṇā atthi baddhā,",
        "analyzed": "ye ca api me sakuṇāḥ atthi baddhā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_491:16.2",
        "original": "Satāninekāni nivesanasmiṁ;",
        "analyzed": "satāninekāni ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_491:16.3",
        "original": "Tesampahaṁ jīvitamajja dammi,",
        "analyzed": "jīvitam ajja dammi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_491:16.4",
        "original": "Mokkhañca te pattā sakaṁ niketaṁ”.",
        "analyzed": "mokkham ca te pattā sakam niketa ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_491:17.1",
        "original": "“Luddo carī pāsahattho araññe,",
        "analyzed": "luddaḥ carī pāsa hatthaḥ araññe ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_491:17.2",
        "original": "Bādhetu morādhipatiṁ yasassiṁ;",
        "analyzed": "bādhetu mā urā adhipatim yasassi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_491:17.3",
        "original": "Bandhitvā morādhipatiṁ yasassiṁ,",
        "analyzed": "bandhitvā mora adhipatim ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_491:17.4",
        "original": "Dukkhā sa pamucci yathāhaṁ pamutto”ti.",
        "analyzed": "dukkhā sa pamucci yathā aham pamuttaḥ ti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_491:18.1",
        "original": "Mahāmorajātakaṁ aṭṭhamaṁ.",
        "analyzed": "mahāmorajātakam aṭṭhama ",
        "folio": "18"
    }
]