[
    {
        "segmentnr": "PA_snp_4.4:0.1",
        "original": "Sutta Nipāta 4.4",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.4:0.2",
        "original": "Suddhaṭṭhakasutta",
        "analyzed": "suddhaṭṭhaka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.4:1.1",
        "original": "Passāmi suddhaṁ paramaṁ arogaṁ,",
        "analyzed": "passāmi suddham paramam aroga ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.4:1.2",
        "original": "Diṭṭhena saṁsuddhi narassa hoti;",
        "analyzed": "diṭṭhena sa suddhi narassa hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.4:1.3",
        "original": "Evābhijānaṁ paramanti ñatvā,",
        "analyzed": "eva abhijānam paramanti ñatvā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.4:1.4",
        "original": "Suddhānupassīti pacceti ñāṇaṁ.",
        "analyzed": "suddha anupassī iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.4:2.1",
        "original": "Diṭṭhena ce suddhi narassa hoti,",
        "analyzed": "diṭṭhena ce suddhi narassa hoti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.4:2.2",
        "original": "Ñāṇena vā so pajahāti dukkhaṁ;",
        "analyzed": "ñāṇena vā so pajahāti dukkha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.4:2.3",
        "original": "Aññena so sujjhati sopadhīko,",
        "analyzed": "aññena so sujjhati sa upadhīkaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.4:2.4",
        "original": "Diṭṭhī hi naṁ pāva tathā vadānaṁ.",
        "analyzed": "diṭṭhī hi nam pāva tathā vadāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.4:3.1",
        "original": "Na brāhmaṇo aññato suddhimāha,",
        "analyzed": "na brāhmaṇaḥ aññatas suddhim āha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.4:3.2",
        "original": "Diṭṭhe sute sīlavate mute vā;",
        "analyzed": "diṭṭhe sute sīlavate mute vā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.4:3.3",
        "original": "Puññe ca pāpe ca anūpalitto,",
        "analyzed": "puññe ca pāpe ca an upalittaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.4:3.4",
        "original": "Attañjaho nayidha pakubbamāno.",
        "analyzed": "attañjahaḥ nayidha pakubbamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.4:4.1",
        "original": "Purimaṁ pahāya aparaṁ sitāse,",
        "analyzed": "purimam pahāya aparam sitāse ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.4:4.2",
        "original": "Ejānugā te na taranti saṅgaṁ;",
        "analyzed": "ejānugā te na taranti saṅga ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.4:4.3",
        "original": "Te uggahāyanti nirassajanti,",
        "analyzed": "te uggahāyanti nirassajanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.4:4.4",
        "original": "Kapīva sākhaṁ pamuñcaṁ gahāyaṁ.",
        "analyzed": "kapī iva sākham pamuñcam gahāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.4:5.1",
        "original": "Sayaṁ samādāya vatāni jantu,",
        "analyzed": "sayam samādāya vatāni jantu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.4:5.2",
        "original": "Uccāvacaṁ gacchati saññasatto;",
        "analyzed": "uccāvacam gacchati sañña satto ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.4:5.3",
        "original": "Vidvā ca vedehi samecca dhammaṁ,",
        "analyzed": "vidvā ca vedehi samet ca dhamma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.4:5.4",
        "original": "Na uccāvacaṁ gacchati bhūripañño.",
        "analyzed": "na uccāvacam gacchati bhūri paññaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.4:6.1",
        "original": "Sa sabbadhammesu visenibhūto,",
        "analyzed": "sa sabba dhammesu visenibhūtaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.4:6.2",
        "original": "Yaṁ kiñci diṭṭhaṁ va sutaṁ mutaṁ vā;",
        "analyzed": "yam kiñci diṭṭham vā sutam mutam vā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.4:6.3",
        "original": "Tameva dassiṁ vivaṭaṁ carantaṁ,",
        "analyzed": "tam eva dassim vivaṭam caranta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.4:6.4",
        "original": "Kenīdha lokasmi vikappayeyya.",
        "analyzed": "kena idha lokasmi vikappayeyya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.4:7.1",
        "original": "Na kappayanti na purekkharonti,",
        "analyzed": "na kappayanti na purekkharonti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.4:7.2",
        "original": "Accantasuddhīti na te vadanti;",
        "analyzed": "na te vadanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.4:7.3",
        "original": "Ādānaganthaṁ gathitaṁ visajja,",
        "analyzed": "ādāna gantham gathitam visajja ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.4:7.4",
        "original": "Āsaṁ na kubbanti kuhiñci loke.",
        "analyzed": "āsam na kubbanti kuhiñci loke ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.4:8.1",
        "original": "Sīmātigo brāhmaṇo tassa natthi,",
        "analyzed": "sīmātigaḥ brāhmaṇaḥ tassa na atthi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.4:8.2",
        "original": "Ñatvā va disvā va samuggahītaṁ;",
        "analyzed": "ñatvā va disvā va samuggahīta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.4:8.3",
        "original": "Na rāgarāgī na virāgaratto,",
        "analyzed": "na rāga rāgī na virāga rattaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.4:8.4",
        "original": "Tassīdha natthi paramuggahītanti.",
        "analyzed": "tassa idha na atthi param uggahītanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.4:8.5",
        "original": "Suddhaṭṭhakasuttaṁ catutthaṁ.",
        "analyzed": "suddhaṭṭhaka suttam catuttha ",
        "folio": "8"
    }
]