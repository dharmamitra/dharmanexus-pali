    },
    {
        "segmentnr": "PA_ja_421:8.3",
        "original": "Yo sabbajanassa vanditohu,",
        "analyzed": "yaḥ sabba janassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_421:8.4",
        "original": "Taṁ vandāma sarājikā samaccā”.",
        "analyzed": "tam vandāma sa rājikā sa maccā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_421:9.1",
        "original": "“Mā kiñci avacuttha gaṅgamālaṁ,",
        "analyzed": "mā kiñci avacuttha gaṅgamāla ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_421:9.2",
        "original": "Muninaṁ monapathesu sikkhamānaṁ;",
        "analyzed": "muninam mona pathesu sikkhamāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_421:9.3",
        "original": "Eso hi atari aṇṇavaṁ,",
        "analyzed": "esaḥ hi atari aṇṇava ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_421:9.4",
        "original": "Yaṁ taritvā caranti vītasokā”ti.",
        "analyzed": "yam taritvā caranti vīta sokā ati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_421:10.1",
        "original": "Gaṅgamālajātakaṁ pañcamaṁ.",
