        "analyzed": "cujjamānaḥ palāyati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.7:8.3",
        "original": "Na hi te iṇamatthīti,",
        "analyzed": "na hi te iṇam atthi iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.7:8.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.7:9.1",
        "original": "Yo ve kiñcikkhakamyatā,",
        "analyzed": "yaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.7:9.2",
        "original": "panthasmiṁ vajantaṁ janaṁ;",
        "analyzed": "panthasmin vajantam jana ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.7:9.3",
        "original": "Hantvā kiñcikkhamādeti,",
        "analyzed": "hantvā kiñcid kham ādeti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.7:9.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.7:10.1",
        "original": "Attahetu parahetu,",
        "analyzed": "atta hetu para hetu ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.7:10.2",
        "original": "dhanahetu ca yo naro;",
        "analyzed": "dhana hetu ca yaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.7:10.3",
        "original": "Sakkhipuṭṭho musā brūti,",
        "analyzed": "sakkhipuṭṭhaḥ musā brūti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.7:10.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.7:11.1",
        "original": "Yo ñātīnaṁ sakhīnaṁ vā,",
        "analyzed": "yaḥ ñātīnam sakhīnam vā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.7:11.2",
        "original": "dāresu paṭidissati;",
        "analyzed": "dāresu paṭidissati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.7:11.3",
        "original": "Sāhasā sampiyena vā,",
        "analyzed": "sāhasā sampiyena vā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.7:11.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.7:12.1",
        "original": "Yo mātaraṁ pitaraṁ vā,",
        "analyzed": "yaḥ mātaram pitaram vā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.7:12.2",
        "original": "jiṇṇakaṁ gatayobbanaṁ;",
        "analyzed": "jiṇṇakam gata yobbana ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.7:12.3",
        "original": "Pahu santo na bharati,",
        "analyzed": "pahu santaḥ na bharati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.7:12.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.7:13.1",
        "original": "Yo mātaraṁ pitaraṁ vā,",
        "analyzed": "yaḥ mātaram pitaram vā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.7:13.2",
        "original": "Bhātaraṁ bhaginiṁ sasuṁ;",
        "analyzed": "bhātaram bhaginim sasu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.7:13.3",
        "original": "Hanti roseti vācāya,",
        "analyzed": "hanti rosā iti vācāya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.7:13.4",
        "original": "Taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.7:14.1",
        "original": "Yo atthaṁ pucchito santo,",
        "analyzed": "yaḥ attham pucchito santo ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.7:14.2",
