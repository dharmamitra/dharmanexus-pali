        "analyzed": "sasissassa mune aha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_tha-ap_538:3.3",
        "original": "Abhivādiya sambuddhaṁ,",
        "analyzed": "sambuddha ",
        "folio": "3"
    },
