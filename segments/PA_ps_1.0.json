    },
    {
        "segmentnr": "PA_ps_1.0:0.4",
        "original": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
        "analyzed": "namaḥ tassa bhagavataḥ arahataḥ sammāsambuddhassa ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ps_1.0:1.1",
        "original": "Sotāvadhāne paññā sutamaye ñāṇaṁ.",
        "analyzed": "sota avadhāne paññā suta maye ñāṇa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ps_1.0:2.1",
        "original": "Sutvāna saṁvare paññā sīlamaye ñāṇaṁ.",
        "analyzed": "sutvāna savare paññā sīla maye ñāṇa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ps_1.0:3.1",
        "original": "Saṁvaritvā samādahane paññā samādhibhāvanāmaye ñāṇaṁ.",
        "analyzed": "samādahane paññā samādhi bhāvanā āmaye ñāṇa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ps_1.0:4.1",
        "original": "Paccayapariggahe paññā dhammaṭṭhitiñāṇaṁ.",
        "analyzed": "paccaya pariggahe paññā dhamma ṭṭhiti ñāṇa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ps_1.0:5.1",
        "original": "Atītānāgatapaccuppannānaṁ dhammānaṁ saṅkhipitvā vavatthāne paññā sammasane ñāṇaṁ.",
        "analyzed": "atīta anāgata paccuppannānam dhammānam saṅkhipitvā vavatthāne paññā sammasane ñāṇa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ps_1.0:6.1",
        "original": "Paccuppannānaṁ dhammānaṁ vipariṇāmānupassane paññā udayabbayānupassane ñāṇaṁ.",
        "analyzed": "paccuppannānam dhammānam vipariṇāma anupassane paññā udaya bbaya anupassane ñāṇa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ps_1.0:7.1",
        "original": "Ārammaṇaṁ paṭisaṅkhā bhaṅgānupassane paññā vipassane ñāṇaṁ.",
        "analyzed": "ārammaṇam paṭisaṅkhā bhaṅga anupassane paññā vipassane ñāṇa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ps_1.0:8.1",
        "original": "Bhayatupaṭṭhāne paññā ādīnave ñāṇaṁ.",
        "analyzed": "paññā ādīnave ñāṇa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ps_1.0:9.1",
        "original": "Muñcitukamyatāpaṭisaṅkhāsantiṭṭhanā paññā saṅkhārupekkhāsu ñāṇaṁ.",
        "analyzed": "muñcitu kamya tā paṭisaṅkhā santiṭṭhanā paññā saṅkhāra upekkhāsu ñāṇa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ps_1.0:10.1",
        "original": "Bahiddhā vuṭṭhānavivaṭṭane paññā gotrabhuñāṇaṁ.",
        "analyzed": "bahiddhā vuṭṭhāna vivaṭṭane paññā gotra bhuñāṇa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ps_1.0:11.1",
        "original": "Dubhato vuṭṭhānavivaṭṭane paññā magge ñāṇaṁ.",
        "analyzed": "dubhataḥ vuṭṭhāna vivaṭṭane paññā magge ñāṇa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ps_1.0:12.1",
        "original": "Payogappaṭippassaddhi paññā phale ñāṇaṁ.",
        "analyzed": "payoga paṭippassaddhi paññā phale ñāṇa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ps_1.0:13.1",
        "original": "Chinnavaṭumānupassane paññā vimuttiñāṇaṁ.",
        "analyzed": "chinna vaṭuma anupassane paññā vimutti ñāṇa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ps_1.0:14.1",
        "original": "Tadā samudāgate dhamme passane paññā paccavekkhaṇe ñāṇaṁ.",
        "analyzed": "tadā samudāgate dhamme passane paññā paccavekkhaṇe ñāṇa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ps_1.0:15.1",
        "original": "Ajjhattavavatthāne paññā vatthunānatte ñāṇaṁ.",
        "analyzed": "ajjhatta vavatthāne paññā vatthu nānatte ñāṇa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ps_1.0:16.1",
        "original": "Bahiddhāvavatthāne paññā gocaranānatte ñāṇaṁ.",
        "analyzed": "bahiddhā avatthāne paññā gocara nānatte ñāṇa ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ps_1.0:17.1",
        "original": "Cariyāvavatthāne paññā cariyānānatte ñāṇaṁ.",
        "analyzed": "cariyā vavatthāne paññā cariyā anānatte ñāṇa ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ps_1.0:18.1",
        "original": "Catudhammavavatthāne paññā bhūminānatte ñāṇaṁ.",
        "analyzed": "catu dhamma vavatthāne paññā bhūmi nānatte ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ps_1.0:19.1",
        "original": "Navadhammavavatthāne paññā dhammanānatte ñāṇaṁ.",
        "analyzed": "nava dhamma vavatthāne paññā dhamma nānatte ñāṇa ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ps_1.0:20.1",
        "original": "Abhiññāpaññā ñātaṭṭhe ñāṇaṁ.",
        "analyzed": "abhiññā paññā ñāta ṭṭhe ñāṇa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ps_1.0:21.1",
        "original": "Pariññāpaññā tīraṇaṭṭhe ñāṇaṁ.",
        "analyzed": "pariññā paññā tīraṇa ṭṭhe ñāṇa ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ps_1.0:22.1",
        "original": "Pahāne paññā pariccāgaṭṭhe ñāṇaṁ.",
        "analyzed": "pahāne paññā pariccāgaṭṭhe ñāṇa ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ps_1.0:23.1",
        "original": "Bhāvanāpaññā ekarasaṭṭhe ñāṇaṁ.",
        "analyzed": "bhāvanā paññā ekarasaṭṭhe ñāṇa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ps_1.0:24.1",
        "original": "Sacchikiriyāpaññā phassanaṭṭhe ñāṇaṁ.",
        "analyzed": "sacchikiriyā paññā phassana ṭṭhe ñāṇa ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ps_1.0:25.1",
        "original": "Atthanānatte paññā atthapaṭisambhide ñāṇaṁ.",
        "analyzed": "attha nānatte paññā attha paṭisambhide ñāṇa ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ps_1.0:26.1",
        "original": "Dhammanānatte paññā dhammapaṭisambhide ñāṇaṁ.",
        "analyzed": "dhamma nānatte paññā dhamma paṭisambhide ñāṇa ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ps_1.0:27.1",
        "original": "Niruttinānatte paññā niruttipaṭisambhide ñāṇaṁ.",
        "analyzed": "nirutti nānatte paññā nirutti paṭisambhide ñāṇa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ps_1.0:28.1",
        "original": "Paṭibhānanānatte paññā paṭibhānapaṭisambhide ñāṇaṁ.",
        "analyzed": "paṭibhāna nānatte paññā paṭibhāna paṭisambhide ñāṇa ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ps_1.0:29.1",
        "original": "Vihāranānatte paññā vihāraṭṭhe ñāṇaṁ.",
        "analyzed": "vihāra nānatte paññā vihāra aṭṭhe ñāṇa ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ps_1.0:30.1",
        "original": "Samāpattinānatte paññā samāpattaṭṭhe ñāṇaṁ.",
        "analyzed": "samāpatti nānatte paññā samāpatta ṭṭhe ñāṇa ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ps_1.0:31.1",
        "original": "Vihārasamāpattinānatte paññā vihārasamāpattaṭṭhe ñāṇaṁ.",
        "analyzed": "vihāra samāpatti nānatte paññā vihāra samāpatta ṭṭhe ñāṇa ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ps_1.0:32.1",
        "original": "Avikkhepaparisuddhattā āsavasamucchede paññā ānantarikasamādhimhi ñāṇaṁ.",
        "analyzed": "avikkhepa parisuddha ttā āsava samucchede paññā ānantarika samādhimhi ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ps_1.0:33.1",
        "original": "Dassanādhipateyyaṁ santo ca vihārādhigamo paṇītādhimuttatā paññā araṇavihāre ñāṇaṁ.",
        "analyzed": "dassana ādhipateyyam santaḥ ca vihāra adhigamaḥ paṇīta adhimutta tā paññā araṇa vihāre ñāṇa ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ps_1.0:34.1",
        "original": "Dvīhi balehi samannāgatattā tayo ca saṅkhārānaṁ paṭippassaddhiyā soḷasahi ñāṇacariyāhi navahi samādhicariyāhi vasibhāvatā paññā nirodhasamāpattiyā ñāṇaṁ.",
        "analyzed": "dvīhi balehi samannāgata ttā tayo ca saṅkhārānaṃ paṭippassaddhiyā soḷasahi ñāṇa cariyāhi navahi samādhi cariyāhi vasi bhāva tā paññā nirodha samāpattiyā ñāṇa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ps_1.0:35.1",
        "original": "Sampajānassa pavattapariyādāne paññā parinibbāne ñāṇaṁ.",
        "analyzed": "sampajānassa pavatta pariyādāne paññā parinibbāne ñāṇa ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ps_1.0:36.1",
        "original": "Sabbadhammānaṁ sammā samucchede nirodhe ca anupaṭṭhānatā paññā samasīsaṭṭhe ñāṇaṁ.",
        "analyzed": "sabba dhammānam sammā samucchede nirodhe ca an upaṭṭhāna tā paññā sama sīsa ṭṭhe ñāṇa ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ps_1.0:37.1",
        "original": "Puthunānattatejapariyādāne paññā sallekhaṭṭhe ñāṇaṁ.",
        "analyzed": "puthu na ānatta teja pariyādāne paññā sallekha ṭṭhe ñāṇa ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ps_1.0:38.1",
        "original": "Asallīnattapahitattapaggahaṭṭhe paññā vīriyārambhe ñāṇaṁ.",
        "analyzed": "paññā vīriya ārambhe ñāṇa ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ps_1.0:39.1",
        "original": "Nānādhammappakāsanatā paññā atthasandassane ñāṇaṁ.",
        "analyzed": "nānā dhamma ppakāsana tā paññā attha sandassane ñāṇa ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ps_1.0:40.1",
        "original": "Sabbadhammānaṁ ekasaṅgahatānānattekattapaṭivedhe paññā dassanavisuddhiñāṇaṁ.",
        "analyzed": "sabba dhammānam eka saṅgaha tā anānatta ekatta paṭivedhe paññā dassana visuddhi ñāṇa ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ps_1.0:41.1",
        "original": "Viditattā paññā khantiñāṇaṁ.",
        "analyzed": "vidita ttā paññā khanti ñāṇa ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ps_1.0:42.1",
        "original": "Phuṭṭhattā paññā pariyogāhaṇe ñāṇaṁ.",
        "analyzed": "phuṭṭha tā paññā pariyogāhaṇe ñāṇa ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ps_1.0:43.1",
        "original": "Samodahane paññā padesavihāre ñāṇaṁ.",
        "analyzed": "samodahane paññā padesa vihāre ñāṇa ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ps_1.0:44.1",
        "original": "Adhipatattā paññā saññāvivaṭṭe ñāṇaṁ.",
        "analyzed": "adhipatattā paññā saññā vivaṭṭe ñāṇa ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ps_1.0:45.1",
        "original": "Nānatte paññā cetovivaṭṭe ñāṇaṁ.",
        "analyzed": "nānatte paññā cetaḥ vivaṭṭe ñāṇa ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ps_1.0:46.1",
        "original": "Adhiṭṭhāne paññā cittavivaṭṭe ñāṇaṁ.",
        "analyzed": "adhiṭṭhāne paññā citta vivaṭṭe ñāṇa ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ps_1.0:47.1",
        "original": "Suññate paññā ñāṇavivaṭṭe ñāṇaṁ.",
        "analyzed": "paññā ñāṇa vivaṭṭe ñāṇa ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ps_1.0:48.1",
        "original": "Vosagge paññā vimokkhavivaṭṭe ñāṇaṁ.",
        "analyzed": "vosagge paññā vimokkha vivaṭṭe ñāṇa ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_ps_1.0:49.1",
        "original": "Tathaṭṭhe paññā saccavivaṭṭe ñāṇaṁ.",
        "analyzed": "tathaṭṭhe paññā sacca vivaṭṭe ñāṇa ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_ps_1.0:50.1",
        "original": "Kāyampi cittampi ekavavatthānatā sukhasaññañca lahusaññañca adhiṭṭhānavasena ijjhanaṭṭhe paññā iddhividhe ñāṇaṁ.",
        "analyzed": "kāyampi cittampi eka vavatthāna tā sukha saññañca lahu saññañca adhiṭṭhāna vasena ijjhanaṭṭhe paññā iddhi vidhe ñāṇa ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_ps_1.0:51.1",
        "original": "Vitakkavipphāravasena nānattekattasaddanimittānaṁ pariyogāhaṇe paññā sotadhātuvisuddhiñāṇaṁ.",
        "analyzed": "vitakka vipphāra vasena nānatta ekatta sadda nimittānam pariyogāhaṇe paññā sota dhātu visuddhi ñāṇa ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_ps_1.0:52.1",
        "original": "Tiṇṇannaṁ cittānaṁ vipphārattā indriyānaṁ pasādavasena nānattekattaviññāṇacariyā pariyogāhaṇe paññā cetopariyañāṇaṁ.",
        "analyzed": "tiṇṇannam cittānam vipphāra ttā indriyānam pasāda vasena nānatta ekatta viññāṇa cariyā pariyogāhaṇe paññā cetas pariya ñāṇa ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_ps_1.0:53.1",
        "original": "Paccayappavattānaṁ dhammānaṁ nānattekattakammavipphāravasena pariyogāhaṇe paññā pubbenivāsānussatiñāṇaṁ.",
        "analyzed": "paccaya ppavattānam dhammānam nānatta ekatta kamma vipphāra vasena pariyogāhaṇe paññā pubbenivāsa anussati ñāṇa ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_ps_1.0:54.1",
        "original": "Obhāsavasena nānattekattarūpanimittānaṁ dassanaṭṭhe paññā dibbacakkhuñāṇaṁ.",
        "analyzed": "obhāsa vasena nānatta ekatta rūpa nimittānam dassana aṭṭhe paññā dibba cakkhu ñāṇa ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_ps_1.0:55.1",
        "original": "Catusaṭṭhiyā ākārehi tiṇṇannaṁ indriyānaṁ vasibhāvatā paññā āsavānaṁ khaye ñāṇaṁ.",
        "analyzed": "ākārehi tiṇṇannam indriyānam vasibhāvatā paññā āsavānam khaye ñāṇa ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_ps_1.0:56.1",
        "original": "Pariññaṭṭhe paññā dukkhe ñāṇaṁ.",
        "analyzed": "pariññaṭṭhe paññā dukkhe ñāṇa ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_ps_1.0:57.1",
        "original": "Pahānaṭṭhe paññā samudaye ñāṇaṁ.",
        "analyzed": "pahāna aṭṭhe paññā samudaye ñāṇa ",
        "folio": "57"
    },
    {
        "segmentnr": "PA_ps_1.0:58.1",
        "original": "Sacchikiriyaṭṭhe paññā nirodhe ñāṇaṁ.",
        "analyzed": "sacchikiriya ṭṭhe paññā nirodhe ñāṇa ",
        "folio": "58"
    },
    {
        "segmentnr": "PA_ps_1.0:59.1",
        "original": "Bhāvanaṭṭhe paññā magge ñāṇaṁ.",
        "analyzed": "bhāvanā aṭṭhe paññā magge ñāṇa ",
        "folio": "59"
    },
    {
        "segmentnr": "PA_ps_1.0:60.1",
        "original": "Dukkhe ñāṇaṁ.",
        "analyzed": "dukkhe ñāṇa ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_ps_1.0:61.1",
        "original": "Dukkhasamudaye ñāṇaṁ.",
        "analyzed": "dukkha samudaye ñāṇa ",
        "folio": "61"
    },
    {
        "segmentnr": "PA_ps_1.0:62.1",
        "original": "Dukkhanirodhe ñāṇaṁ.",
        "analyzed": "dukkha nirodhe ",
        "folio": "62"
    },
    {
        "segmentnr": "PA_ps_1.0:63.1",
        "original": "Dukkhanirodhagāminiyā paṭipadāya ñāṇaṁ.",
        "analyzed": "dukkha nirodha gāminiyā paṭipadāya ñāṇa ",
        "folio": "63"
    },
    {
        "segmentnr": "PA_ps_1.0:64.1",
        "original": "Atthapaṭisambhide ñāṇaṁ.",
        "analyzed": "attha paṭisambhide ñāṇa ",
        "folio": "64"
    },
    {
        "segmentnr": "PA_ps_1.0:65.1",
        "original": "Dhammapaṭisambhide ñāṇaṁ.",
        "analyzed": "dhamma paṭisambhide ñāṇa ",
        "folio": "65"
    },
    {
        "segmentnr": "PA_ps_1.0:66.1",
        "original": "Niruttipaṭisambhide ñāṇaṁ.",
        "analyzed": "nirutti paṭisambhide ñāṇa ",
        "folio": "66"
    },
    {
        "segmentnr": "PA_ps_1.0:67.1",
        "original": "Paṭibhānapaṭisambhide ñāṇaṁ.",
        "analyzed": "paṭibhāna paṭisambhide ñāṇa ",
        "folio": "67"
    },
    {
        "segmentnr": "PA_ps_1.0:68.1",
        "original": "Indriyaparopariyattañāṇaṁ.",
        "analyzed": "indriya para upari yat ta ñāṇa ",
        "folio": "68"
    },
