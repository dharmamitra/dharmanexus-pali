[
    {
        "segmentnr": "PA_thag_10.1:0.1",
        "original": "Theragāthā 10.1",
        "analyzed": "theragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.1:0.2",
        "original": "Dasakanipāta",
        "analyzed": "dasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.1:0.3",
        "original": "Paṭhamavagga",
        "analyzed": "paṭhama vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.1:0.4",
        "original": "Kāḷudāyittheragāthā",
        "analyzed": "kāḷudāyittheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.1:1.1",
        "original": "“Aṅgārino dāni dumā bhadante,",
        "analyzed": "aṅgārinaḥ dāni dumāḥ bhadante ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.1:1.2",
        "original": "Phalesino chadanaṁ vippahāya;",
        "analyzed": "phala esinaḥ chadanam vippahāya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.1:1.3",
        "original": "Te accimantova pabhāsayanti,",
        "analyzed": "te accimantaḥ iva pabhāsayanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.1:1.4",
        "original": "Samayo mahāvīra bhāgī rasānaṁ.",
        "analyzed": "samayaḥ mahāvīra bhāgī rasāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.1:2.1",
        "original": "Dumāni phullāni manoramāni,",
        "analyzed": "dumāni phullāni manoramāni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.1:2.2",
        "original": "Samantato sabbadisā pavanti;",
        "analyzed": "samantataḥ sabbadisā pavanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.1:2.3",
        "original": "Pattaṁ pahāya phalamāsasānā,",
        "analyzed": "pattam pahāya phalam āsasānā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.1:2.4",
        "original": "Kālo ito pakkamanāya vīra.",
        "analyzed": "kālaḥ itas pakkamanāya vīra ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.1:3.1",
        "original": "Nevātisītaṁ na panātiuṇhaṁ,",
        "analyzed": "na iva atisītam na panāti uṇha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.1:3.2",
        "original": "Sukhā utu addhaniyā bhadante;",
        "analyzed": "sukhā utu addhaniyā bhadante ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.1:3.3",
        "original": "Passantu taṁ sākiyā koḷiyā ca,",
        "analyzed": "passantu tam sākiyā koḷiyā ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.1:3.4",
        "original": "Pacchāmukhaṁ rohiniyaṁ tarantaṁ.",
        "analyzed": "pacchā mukham rohiniyam taranta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.1:4.1",
        "original": "Āsāya kasate khettaṁ,",
        "analyzed": "āsāya kasate khetta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.1:4.2",
        "original": "bījaṁ āsāya vappati;",
        "analyzed": "bījam āsāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.1:4.3",
        "original": "Āsāya vāṇijā yanti,",
        "analyzed": "āsāya vāṇijāḥ yanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.1:4.4",
        "original": "samuddaṁ dhanahārakā;",
        "analyzed": "samuddam dhana hārakā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.1:4.5",
        "original": "Yāya āsāya tiṭṭhāmi,",
        "analyzed": "yāya āsāya tiṭṭhāmi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.1:4.6",
        "original": "sā me āsā samijjhatu.",
        "analyzed": "sā me āsā samijjhatu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.1:5.1",
        "original": "Punappunaṁ ceva vapanti bījaṁ,",
        "analyzed": "punappunam ca iva vapanti bīja ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.1:5.2",
        "original": "Punappunaṁ vassati devarājā;",
        "analyzed": "punappunam vassati devarājā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.1:5.3",
        "original": "Punappunaṁ khettaṁ kasanti kassakā,",
        "analyzed": "punappunam khettam kasanti kassakāḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.1:5.4",
        "original": "Punappunaṁ dhaññamupeti raṭṭhaṁ.",
        "analyzed": "punappunam dhaññam upeti raṭṭha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.1:6.1",
        "original": "Punappunaṁ yācanakā caranti,",
        "analyzed": "punappunam yācanakā caranti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.1:6.2",
        "original": "Punappunaṁ dānapatī dadanti;",
        "analyzed": "punar punar dānapatī dadanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.1:6.3",
        "original": "Punappunaṁ dānapatī daditvā,",
        "analyzed": "punar punar dānapatī daditvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.1:6.4",
        "original": "Punappunaṁ saggamupenti ṭhānaṁ.",
        "analyzed": "punappunam saggam upenti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.1:7.1",
        "original": "Vīro have sattayugaṁ puneti,",
        "analyzed": "vīraḥ have satta yugam puneti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.1:7.2",
        "original": "Yasmiṁ kule jāyati bhūripañño;",
        "analyzed": "yasmin kule jāyati bhūri paññaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.1:7.3",
        "original": "Maññāmahaṁ sakkati devadevo,",
        "analyzed": "maññāmaham sakkati devadevaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.1:7.4",
        "original": "Tayā hi jāto muni saccanāmo.",
        "analyzed": "tayā hi jātaḥ muni sacca nāmaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.1:8.1",
        "original": "Suddhodano nāma pitā mahesino,",
        "analyzed": "suddhodanaḥ nāma pitā mahesinaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.1:8.2",
        "original": "Buddhassa mātā pana māyanāmā;",
        "analyzed": "buddhassa mātā pana māya nāmā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.1:8.3",
        "original": "Yā bodhisattaṁ parihariya kucchinā,",
        "analyzed": "yā bodhisattam kucchinā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.1:8.4",
        "original": "Kāyassa bhedā tidivamhi modati.",
        "analyzed": "kāyassa bhedā tidivamhi modati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.1:9.1",
        "original": "Sā gotamī kālakatā ito cutā,",
        "analyzed": "sā gotamī kālakatā itas cutā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.1:9.2",
        "original": "Dibbehi kāmehi samaṅgibhūtā;",
        "analyzed": "dibbehi kāmehi samaṅgi bhūtā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.1:9.3",
        "original": "Sā modati kāmaguṇehi pañcahi,",
        "analyzed": "sā modati kāma guṇehi pañcahi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.1:9.4",
        "original": "Parivāritā devagaṇehi tehi”.",
        "analyzed": "parivāritāḥ deva gaṇehi tehi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.1:10.1",
        "original": "“Buddhassa puttomhi asayhasāhino,",
        "analyzed": "buddhassa putto amhi asayhasāhinaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.1:10.2",
        "original": "Aṅgīrasassappaṭimassa tādino;",
        "analyzed": "aṅgīrasassa appaṭimassa tādinaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.1:10.3",
        "original": "Pitupitā mayhaṁ tuvaṁsi sakka,",
        "analyzed": "pitu pitā mayham tuvasi sakka ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.1:10.4",
        "original": "Dhammena me gotama ayyakosī”ti.",
        "analyzed": "dhammena me gotama ayyakosi iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.1:10.5",
        "original": "… Kāḷudāyī thero ….",
        "analyzed": "kāḷudāyī theraḥ ",
        "folio": "10"
    }
]