        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_498:7.4",
        "original": "Yattheva dakkhissamisiṁ nisinnaṁ”.",
        "analyzed": "yatthā iva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_498:8.1",
