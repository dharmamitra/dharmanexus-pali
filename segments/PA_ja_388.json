        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_388:1.3",
        "original": "Bahuke jane pāsapāṇike,",
        "analyzed": "bahuke jane pāsa pāṇike ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_388:1.4",
