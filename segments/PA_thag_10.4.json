        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.4:1.1",
        "original": "“Dandhā mayhaṁ gatī āsi,",
        "analyzed": "dandhā mayham gatī āsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.4:1.2",
        "original": "paribhūto pure ahaṁ;",
        "analyzed": "paribhūtaḥ pure aha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.4:1.3",
        "original": "Bhātā ca maṁ paṇāmesi,",
        "analyzed": "bhātā ca mama paṇāmesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.4:1.4",
        "original": "‘gaccha dāni tuvaṁ gharaṁ’.",
        "analyzed": "gaccha dāni tuvam ghara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.4:2.1",
        "original": "Sohaṁ paṇāmito santo,",
        "analyzed": "saḥ aham paṇāmitaḥ santaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.4:2.2",
        "original": "saṅghārāmassa koṭṭhake;",
