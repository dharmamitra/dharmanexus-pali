[
    {
        "segmentnr": "PA_ja_420:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_420:0.2",
        "original": "Aṭṭhakanipāta",
        "analyzed": "aṭṭhaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_420:0.3",
        "original": "Kaccānivagga",
        "analyzed": "kaccā nivagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_420:0.4",
        "original": "4. Sumaṅgalajātaka",
        "analyzed": "sumaṅgalajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_420:1.1",
        "original": "“Bhusamhi kuddhoti avekkhiyāna,",
        "analyzed": "bhusamhi kuddhaḥ iti avekkhiyāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_420:1.2",
        "original": "Na tāva daṇḍaṁ paṇayeyya issaro;",
        "analyzed": "na tāva daṇḍam paṇayeyya issaraḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_420:1.3",
        "original": "Aṭṭhānaso appatirūpamattano,",
        "analyzed": "aṭṭhānasaḥ appatirūpa mattano ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_420:1.4",
        "original": "Parassa dukkhāni bhusaṁ udīraye.",
        "analyzed": "parassa dukkhāni bhusam udīraye ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_420:2.1",
        "original": "Yato ca jāneyya pasādamattano,",
        "analyzed": "yatas ca jāneyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_420:2.2",
        "original": "Atthaṁ niyuñjeyya parassa dukkaṭaṁ;",
        "analyzed": "attham niyuñjeyya parassa dukkaṭa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_420:2.3",
        "original": "Tadāyamatthoti sayaṁ avekkhiya,",
        "analyzed": "tadā ayam atthā uti sayam avekkhiya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_420:2.4",
        "original": "Athassa daṇḍaṁ sadisaṁ nivesaye.",
        "analyzed": "athassa daṇḍam sadisam nivesaye ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_420:3.1",
        "original": "Na cāpi jhāpeti paraṁ na attanaṁ,",
        "analyzed": "na ca api param na ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_420:3.2",
        "original": "Amucchito yo nayate nayānayaṁ;",
        "analyzed": "amucchitaḥ yaḥ nayate naya anaya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_420:3.3",
        "original": "Yo daṇḍadhāro bhavatīdha issaro,",
        "analyzed": "yaḥ daṇḍadhāraḥ bhavati idha issaraḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_420:3.4",
        "original": "Sa vaṇṇagutto siriyā na dhaṁsati.",
        "analyzed": "sa vaṇṇa guttaḥ siriyā na dhasati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_420:4.1",
        "original": "Ye khattiyā se anisammakārino,",
        "analyzed": "ye khattiyā se a nisamma kārinaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_420:4.2",
        "original": "Paṇenti daṇḍaṁ sahasā pamucchitā;",
        "analyzed": "paṇenti daṇḍam sahasā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_420:4.3",
        "original": "Avaṇṇasaṁyutā jahanti jīvitaṁ,",
        "analyzed": "avaṇṇa sayutāḥ jahanti jīvita ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_420:4.4",
        "original": "Ito vimuttāpi ca yanti duggatiṁ.",
        "analyzed": "itas vimuttāḥ api ca yanti duggati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_420:5.1",
        "original": "Dhamme ca ye ariyappavedite ratā,",
        "analyzed": "dhamme ca ye ariya ppavedite ratā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_420:5.2",
        "original": "Anuttarā te vacasā manasā kammunā ca;",
        "analyzed": "anuttarā te vacasā manasā kammunā ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_420:5.3",
        "original": "Te santisoraccasamādhisaṇṭhitā,",
        "analyzed": "te ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_420:5.4",
        "original": "Vajanti lokaṁ dubhayaṁ tathāvidhā.",
        "analyzed": "vajanti lokam dubhayam tathāvidhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_420:6.1",
        "original": "Rājāhamasmi narapamadānamissaro,",
        "analyzed": "rājā aham asmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_420:6.2",
        "original": "Sacepi kujjhāmi ṭhapemi attanaṁ;",
        "analyzed": "sace api kujjhāmi ṭhapemi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_420:6.3",
        "original": "Nisedhayanto janataṁ tathāvidhaṁ,",
        "analyzed": "nisedhayantaḥ janatam tathāvidha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_420:6.4",
        "original": "Paṇemi daṇḍaṁ anukampa yoniso”.",
        "analyzed": "paṇemi daṇḍam anukampa yonisaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_420:7.1",
        "original": "“Sirī ca lakkhī ca taveva khattiya,",
        "analyzed": "sirī ca lakkhī ca tava iva khattiya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_420:7.2",
        "original": "Janādhipa mā vijahi kudācanaṁ;",
        "analyzed": "janādhipa mā vijahi kudācana ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_420:7.3",
        "original": "Akkodhano niccapasannacitto,",
        "analyzed": "          ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_420:7.4",
        "original": "Anīgho tuvaṁ vassasatāni pālaya.",
        "analyzed": "anīghaḥ tuvam vassa satāni pālaya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_420:8.1",
        "original": "Guṇehi etehi upeta khattiya,",
        "analyzed": "guṇehi etehi upeta khattiya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_420:8.2",
        "original": "Ṭhitamariyavattī suvaco akodhano;",
        "analyzed": "ṭhita mariya vattī su vacaḥ a kodhanaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_420:8.3",
        "original": "Sukhī anuppīḷa pasāsamediniṁ,",
        "analyzed": "sukhī anuppīḍa pasāsa medini ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_420:8.4",
        "original": "Ito vimuttopi ca yāhi suggatiṁ.",
        "analyzed": "itas vimuttaḥ api ca yāhi suggati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_420:9.1",
        "original": "Evaṁ sunītena subhāsitena,",
        "analyzed": "evam su nītena su bhāsitena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_420:9.2",
        "original": "Dhammena ñāyena upāyaso nayaṁ;",
        "analyzed": "dhammena ñāyena upāyasaḥ naya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_420:9.3",
        "original": "Nibbāpaye saṅkhubhitaṁ mahājanaṁ,",
        "analyzed": "nibbāpaye saṅkhubhitam mahā jana ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_420:9.4",
        "original": "Mahāva megho salilena medinin”ti.",
        "analyzed": "mahā ava meghaḥ salilena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_420:10.1",
        "original": "Sumaṅgalajātakaṁ catutthaṁ.",
        "analyzed": "sumaṅgalajātakam ",
        "folio": "10"
    }
]