        "original": "Ko me bandhaṁ muñcaṁ loke,",
        "analyzed": "kaḥ me bandham muñcam loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.3:4.4",
        "original": "sambodhiṁ vedayissati.",
        "analyzed": "sambodhim vedayissati ",
        "folio": "4"
    },
