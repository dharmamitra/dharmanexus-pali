    {
        "segmentnr": "PA_snp_1.7:23.2",
        "original": "Arahaṁ paṭijānāti;",
        "analyzed": "araham paṭijānāti ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.7:23.3",
        "original": "Coro sabrahmake loke,",
        "analyzed": "coro sabrahmake loke ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.7:23.4",
        "original": "Eso kho vasalādhamo;",
        "analyzed": "esaḥ kho vasalādhamaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.7:23.5",
        "original": "Ete kho vasalā vuttā,",
        "analyzed": "ete kho vasalā vuttā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.7:23.6",
        "original": "Mayā yete pakāsitā.",
        "analyzed": "mayā pakāsitā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.7:24.1",
        "original": "Na jaccā vasalo hoti,",
        "analyzed": "na jaccā vasalaḥ hoti ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.7:24.2",
        "original": "Na jaccā hoti brāhmaṇo;",
        "analyzed": "na brāhmaṇaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.7:24.3",
        "original": "Kammunā vasalo hoti,",
        "analyzed": "kammunā vasalaḥ hoti ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.7:24.4",
        "original": "Kammunā hoti brāhmaṇo.",
        "analyzed": "kammunā hoti brāhmaṇaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.7:25.1",
        "original": "Tadamināpi jānātha,",
        "analyzed": "tat aminā api jānātha ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.7:25.2",
        "original": "Yathāmedaṁ nidassanaṁ;",
        "analyzed": "yathāmā idam nidassana ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.7:25.3",
        "original": "Caṇḍālaputto sopāko,",
        "analyzed": "caṇḍāla putto sopākaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.7:25.4",
        "original": "Mātaṅgo iti vissuto.",
        "analyzed": "mātaṅgo iti vissutaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.7:26.1",
        "original": "So yasaṁ paramaṁ patto,",
        "analyzed": "saḥ yasam paramam ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.7:26.2",
        "original": "Mātaṅgo yaṁ sudullabhaṁ;",
        "analyzed": "mātaṅgaḥ yam su dullabha ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.7:26.3",
        "original": "Āgacchuṁ tassupaṭṭhānaṁ,",
        "analyzed": "āgacchum ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.7:26.4",
        "original": "Khattiyā brāhmaṇā bahū.",
        "analyzed": "khattiyāḥ brāhmaṇāḥ bahū ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.7:27.1",
        "original": "Devayānaṁ abhiruyha,",
        "analyzed": "deva yānam abhiruyha ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.7:27.2",
        "original": "Virajaṁ so mahāpathaṁ;",
        "analyzed": "virajam saḥ mahā patha ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.7:27.3",
        "original": "Kāmarāgaṁ virājetvā,",
        "analyzed": "kāma rāgam virājetvā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.7:27.4",
        "original": "Brahmalokūpago ahu;",
        "analyzed": "brahma loka upagaḥ ahu ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.7:27.5",
        "original": "Na naṁ jāti nivāresi,",
        "analyzed": "na nam jāti nivāresi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.7:27.6",
        "original": "Brahmalokūpapattiyā.",
        "analyzed": "brahma loka upapattiyā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.7:28.1",
        "original": "Ajjhāyakakule jātā,",
        "analyzed": "ajjhāyaka kule jātā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.7:28.2",
        "original": "Brāhmaṇā mantabandhavā;",
        "analyzed": "brāhmaṇā manta bandhavā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.7:28.3",
        "original": "Te ca pāpesu kammesu,",
        "analyzed": "te ca pāpesu kammesu ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.7:28.4",
        "original": "Abhiṇhamupadissare.",
        "analyzed": "abhiṇham upadissare ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.7:29.1",
        "original": "Diṭṭheva dhamme gārayhā,",
        "analyzed": "diṭṭhā iva dhamme gārayhā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.7:29.2",
        "original": "Samparāye ca duggati;",
        "analyzed": "samparāye ca duggati ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.7:29.3",
        "original": "Na ne jāti nivāreti,",
        "analyzed": "na ne jāti nivāreti ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.7:29.4",
        "original": "Duggatyā garahāya vā.",
        "analyzed": "duggatyai garahāya vā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.7:30.1",
        "original": "Na jaccā vasalo hoti,",
        "analyzed": "na jaccā vasalaḥ hoti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_1.7:30.2",
        "original": "Na jaccā hoti brāhmaṇo;",
        "analyzed": "na brāhmaṇaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_1.7:30.3",
        "original": "Kammunā vasalo hoti,",
        "analyzed": "kammunā vasalaḥ hoti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_1.7:30.4",
        "original": "Kammunā hoti brāhmaṇo”ti.",
        "analyzed": "kammunā hoti brāhmaṇa iti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_1.7:31.1",
        "original": "Evaṁ vutte, aggikabhāradvājo brāhmaṇo bhagavantaṁ etadavoca:",
        "analyzed": "evam vutte aggikabhāradvājaḥ brāhmaṇaḥ bhagavantam etat avoca ",
        "folio": "31"
    },
