    },
    {
        "segmentnr": "PA_cp_26:7.3",
        "original": "Sabbo taṁ jano ocināyatu,",
        "analyzed": "sabbaḥ tam janaḥ ocināyatu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_cp_26:7.4",
        "original": "Evaṁ tava attho bhavissati’.",
