[
    {
        "segmentnr": "PA_snp_5.5:0.1",
        "original": "Sutta Nipāta 5.5",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.5:0.2",
        "original": "Mettagūmāṇavapucchā",
        "analyzed": "mettagūmāṇavapucchā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.5:1.1",
        "original": "“Pucchāmi taṁ bhagavā brūhi me taṁ,",
        "analyzed": "pucchāmi tam bhagavā brūhi me ta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.5:1.2",
        "original": "(iccāyasmā mettagū)",
        "analyzed": "it ca ayasmā met tagū ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.5:1.3",
        "original": "Maññāmi taṁ vedaguṁ bhāvitattaṁ;",
        "analyzed": "maññāmi tam vedagum bhāvitatta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.5:1.4",
        "original": "Kuto nu dukkhā samudāgatā ime,",
        "analyzed": "kutas nu dukkhā samudāgatāḥ ime ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.5:1.5",
        "original": "Ye keci lokasmimanekarūpā”.",
        "analyzed": "ye lokasmin aneka rūpā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.5:2.1",
        "original": "“Dukkhassa ve maṁ pabhavaṁ apucchasi,",
        "analyzed": "dukkhassa ve mama pabhavam apucchasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.5:2.2",
        "original": "(mettagūti bhagavā)",
        "analyzed": "bhagavā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.5:2.3",
        "original": "Taṁ te pavakkhāmi yathā pajānaṁ;",
        "analyzed": "tam te pavakkhāmi yathā pajāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.5:2.4",
        "original": "Upadhinidānā pabhavanti dukkhā,",
        "analyzed": "upadhi nidānā pabhavanti dukkhā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.5:2.5",
        "original": "Ye keci lokasmimanekarūpā.",
        "analyzed": "ye lokasmin aneka rūpā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.5:3.1",
        "original": "Yo ve avidvā upadhiṁ karoti,",
        "analyzed": "yaḥ ve a vidvāḥ upadhim karoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.5:3.2",
        "original": "Punappunaṁ dukkhamupeti mando;",
        "analyzed": "punappunam dukkham upeti mandaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.5:3.3",
        "original": "Tasmā pajānaṁ upadhiṁ na kayirā,",
        "analyzed": "tasmai pajānam upadhim na kayirā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.5:3.4",
        "original": "Dukkhassa jātippabhavānupassī”.",
        "analyzed": "dukkhassa jāti ppabhava anupassī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.5:4.1",
        "original": "“Yaṁ taṁ apucchimha akittayī no,",
        "analyzed": "yam tam apucchimha a kittayī naḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.5:4.2",
        "original": "Aññaṁ taṁ pucchāma tadiṅgha brūhi;",
        "analyzed": "aññam tam pucchāma tat iṅgha brūhi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.5:4.3",
        "original": "Kathaṁ nu dhīrā vitaranti oghaṁ,",
        "analyzed": "katham nu dhīrāḥ vitaranti ogha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.5:4.4",
        "original": "Jātiṁ jaraṁ sokapariddavañca;",
        "analyzed": "jātim jaram soka pariddavañca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.5:4.5",
        "original": "Taṁ me muni sādhu viyākarohi,",
        "analyzed": "tam me muni sādhu viyākarohi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.5:4.6",
        "original": "Tathā hi te vidito esa dhammo”.",
        "analyzed": "tathā hi te viditaḥ esa dhammo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.5:5.1",
        "original": "“Kittayissāmi te dhammaṁ,",
        "analyzed": "kittayissāmi te dhamma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.5:5.2",
        "original": "(mettagūti bhagavā)",
        "analyzed": "bhagavā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.5:5.3",
        "original": "Diṭṭhe dhamme anītihaṁ;",
        "analyzed": "diṭṭhe dhamme anīti ha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.5:5.4",
        "original": "Yaṁ viditvā sato caraṁ,",
        "analyzed": "yam viditvā satas cara ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.5:5.5",
        "original": "Tare loke visattikaṁ”.",
        "analyzed": "tare loke visattika ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.5:6.1",
        "original": "“Tañcāhaṁ abhinandāmi,",
        "analyzed": "tat ca aham abhinandāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.5:6.2",
        "original": "mahesi dhammamuttamaṁ;",
        "analyzed": "mahesi dhammam uttama ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.5:6.3",
        "original": "Yaṁ viditvā sato caraṁ,",
        "analyzed": "yam viditvā satas cara ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.5:6.4",
        "original": "tare loke visattikaṁ”.",
        "analyzed": "tare loke visattika ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.5:7.1",
        "original": "“Yaṁ kiñci sampajānāsi,",
        "analyzed": "yam kiñci sampajānāsi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.5:7.2",
        "original": "(mettagūti bhagavā)",
        "analyzed": "bhagavā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.5:7.3",
        "original": "Uddhaṁ adho tiriyañcāpi majjhe;",
        "analyzed": "uddham adhas majjhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.5:7.4",
        "original": "Etesu nandiñca nivesanañca,",
        "analyzed": "etesu nandiñca nivesanañca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.5:7.5",
        "original": "Panujja viññāṇaṁ bhave na tiṭṭhe.",
        "analyzed": "panujja viññāṇam bhave na tiṭṭhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.5:8.1",
        "original": "Evaṁvihārī sato appamatto,",
        "analyzed": "eva vihārī sataḥ appamatto ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.5:8.2",
        "original": "Bhikkhu caraṁ hitvā mamāyitāni;",
        "analyzed": "bhikkhu caram hitvā mamāyitāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.5:8.3",
        "original": "Jātiṁ jaraṁ sokapariddavañca,",
        "analyzed": "jātim jaram soka pariddavañca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.5:8.4",
        "original": "Idheva vidvā pajaheyya dukkhaṁ”.",
        "analyzed": "idhā iva vidvā pajaheyya dukkha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.5:9.1",
        "original": "“Etābhinandāmi vaco mahesino,",
        "analyzed": "vacaḥ mahā īsinaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.5:9.2",
        "original": "Sukittitaṁ gotamanūpadhīkaṁ;",
        "analyzed": "su kittitam gotama anūpa dhīka ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.5:9.3",
        "original": "Addhā hi bhagavā pahāsi dukkhaṁ,",
        "analyzed": "addhā hi bhagavā pahāsi dukkha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.5:9.4",
        "original": "Tathā hi te vidito esa dhammo.",
        "analyzed": "tathā hi te viditaḥ esa dhammo ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_5.5:10.1",
        "original": "Te cāpi nūnappajaheyyu dukkhaṁ,",
        "analyzed": "te ca api nūnappajaheyyu dukkha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.5:10.2",
        "original": "Ye tvaṁ muni aṭṭhitaṁ ovadeyya;",
        "analyzed": "ye tvam muni aṭṭhitam ovadeyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.5:10.3",
        "original": "Taṁ taṁ namassāmi samecca nāga,",
        "analyzed": "tam tam namaḥ sāmi samet ca nāga ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.5:10.4",
        "original": "Appeva maṁ bhagavā aṭṭhitaṁ ovadeyya”.",
        "analyzed": "appā iva mama bhagavā aṭṭhitam ovadeyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_5.5:11.1",
        "original": "“Yaṁ brāhmaṇaṁ vedagumābhijaññā,",
        "analyzed": "yam brāhmaṇam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.5:11.2",
        "original": "Akiñcanaṁ kāmabhave asattaṁ;",
        "analyzed": "akiñcanam kāma bhave asatta ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.5:11.3",
        "original": "Addhā hi so oghamimaṁ atāri,",
        "analyzed": "addhā hi saḥ ogham imam atāri ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.5:11.4",
        "original": "Tiṇṇo ca pāraṁ akhilo akaṅkho.",
        "analyzed": "ca pāram akhilaḥ akaṅkhaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_5.5:12.1",
        "original": "Vidvā ca yo vedagū naro idha,",
        "analyzed": "vidvā ca yaḥ vedagū naraḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.5:12.2",
        "original": "Bhavābhave saṅgamimaṁ visajja;",
        "analyzed": "bhava abhave saṅgam imam visajja ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.5:12.3",
        "original": "So vītataṇho anīgho nirāso,",
        "analyzed": "saḥ vīta taṇhaḥ anīghaḥ nirāsaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.5:12.4",
        "original": "Atāri so jātijaranti brūmī”ti.",
        "analyzed": "atāri saḥ jāti jaranti brūmi iti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_5.5:12.5",
        "original": "Mettagūmāṇavapucchā catutthī.",
        "analyzed": "mettagūmāṇava pucchā catutthī ",
        "folio": "12"
    }
]