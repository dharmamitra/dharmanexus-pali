[
    {
        "segmentnr": "PA_snp_3.4:0.1",
        "original": "Sutta Nipāta 3.4",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.4:0.2",
        "original": "Pūraḷāsa (sundarikabhāradvāja) sutta",
        "analyzed": "pūraḷāsa sundarikabhāradvāja sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.4:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.4:1.2",
        "original": "ekaṁ samayaṁ bhagavā kosalesu viharati sundarikāya nadiyā tīre.",
        "analyzed": "ekam samayam bhagavā kosalesu viharati sundarikāya nadiyā tīre ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.4:1.3",
        "original": "Tena kho pana samayena sundarikabhāradvājo brāhmaṇo sundarikāya nadiyā tīre aggiṁ juhati, aggihuttaṁ paricarati.",
        "analyzed": "tena khalu pana samayena sundarikabhāradvājaḥ brāhmaṇaḥ sundarikāya nadiyā tīre aggim juhati aggi huttam paricarati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.4:1.4",
        "original": "Atha kho sundarikabhāradvājo brāhmaṇo aggiṁ juhitvā aggihuttaṁ paricaritvā uṭṭhāyāsanā samantā catuddisā anuvilokesi:",
        "analyzed": "atha kho sundarikabhāradvājaḥ brāhmaṇaḥ aggim juhitvā aggihuttam paricaritvā uṭṭhāya āsanā samantā catuddisā anuvilokesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.4:1.5",
        "original": "“ko nu kho imaṁ habyasesaṁ bhuñjeyyā”ti?",
        "analyzed": "kaḥ nu kho imam habyasesam bhuñjeyya ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.4:1.6",
        "original": "Addasā kho sundarikabhāradvājo brāhmaṇo bhagavantaṁ avidūre aññatarasmiṁ rukkhamūle sasīsaṁ pārutaṁ nisinnaṁ;",
        "analyzed": "addasā kho sundarikabhāradvājaḥ brāhmaṇaḥ bhagavantam avidūre aññatarasmim rukkha mūle sa sīsam pārutam nisinna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.4:1.7",
        "original": "disvāna vāmena hatthena habyasesaṁ gahetvā dakkhiṇena hatthena kamaṇḍaluṁ gahetvā yena bhagavā tenupasaṅkami.",
        "analyzed": "disvāna vāmena hatthena habya sesam gahetvā dakkhiṇena hatthena kamaṇḍalum gahetvā yena bhagavā tena upasaṅkami ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.4:2.1",
        "original": "Atha kho bhagavā sundarikabhāradvājassa brāhmaṇassa padasaddena sīsaṁ vivari.",
        "analyzed": "atha kho bhagavā sundarikabhāradvājassa brāhmaṇassa pada saddena sīsam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.4:2.2",
        "original": "Atha kho sundarikabhāradvājo brāhmaṇo: “muṇḍo ayaṁ bhavaṁ, muṇḍako ayaṁ bhavan”ti tatova puna nivattitukāmo ahosi.",
        "analyzed": "atha kho sundarikabhāradvājaḥ brāhmaṇaḥ muṇḍaḥ ayam bhava muṇḍakaḥ ayam bhavanti tatas uva puna nivattitu kāmaḥ ahosi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.4:2.3",
        "original": "Atha kho sundarikabhāradvājassa brāhmaṇassa etadahosi:",
        "analyzed": "atha kho sundarikabhāradvājassa brāhmaṇassa etat ahosi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.4:2.4",
        "original": "“muṇḍāpi hi idhekacce brāhmaṇā bhavanti, yannūnāhaṁ upasaṅkamitvā jātiṁ puccheyyan”ti.",
        "analyzed": "muṇḍāḥ api hi idha ekacce brāhmaṇāḥ bhavanti yat nūna aham upasaṅkamitvā jātim puccheyyanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.4:2.5",
        "original": "Atha kho sundarikabhāradvājo brāhmaṇo yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṁ etadavoca:",
        "analyzed": "atha khalu sundarikabhāradvājaḥ brāhmaṇaḥ yena bhagavā tena upasaṅkami upasaṅkamitvā bhagavantam etat avoca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.4:2.6",
        "original": "“kiṁjacco bhavan”ti?",
        "analyzed": "kija ccaḥ bhavanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.4:3.1",
        "original": "Atha kho bhagavā sundarikabhāradvājaṁ brāhmaṇaṁ gāthāhi ajjhabhāsi:",
        "analyzed": "atha khalu bhagavā sundarikabhāradvājam brāhmaṇam gāthāhi ajjhabhāsi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.4:4.1",
        "original": "“Na brāhmaṇo nomhi na rājaputto,",
        "analyzed": "na brāhmaṇaḥ na umhi na rāja putto ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.4:4.2",
        "original": "Na vessāyano uda koci nomhi;",
        "analyzed": "na vessāyanaḥ koci na umhi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.4:4.3",
        "original": "Gottaṁ pariññāya puthujjanānaṁ,",
        "analyzed": "gottam pariññāya puthujjanāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.4:4.4",
        "original": "Akiñcano manta carāmi loke.",
        "analyzed": "akiñcanaḥ manta carāmi loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.4:5.1",
        "original": "Saṅghāṭivāsī agaho carāmi,",
        "analyzed": "saṅghāṭi vāsī agaho carāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.4:5.2",
        "original": "Nivuttakeso abhinibbutatto;",
        "analyzed": "nivutta kesaḥ abhinibbuta ttaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.4:5.3",
        "original": "Alippamāno idha māṇavehi,",
        "analyzed": "a lippamānaḥ idha māṇavehi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.4:5.4",
        "original": "Akallaṁ maṁ brāhmaṇa pucchasi gottapañhaṁ”.",
        "analyzed": "akallam mama brāhmaṇa pucchasi gotta pañha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.4:6.1",
        "original": "“Pucchanti ve bho brāhmaṇā brāhmaṇebhi,",
        "analyzed": "pucchanti ve bho brāhmaṇāḥ brāhmaṇebhiḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.4:6.2",
        "original": "Saha brāhmaṇo no bhavan”ti.",
        "analyzed": "saha brāhmaṇaḥ naḥ bhavanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.4:7.1",
        "original": "“Brāhmaṇo hi ce tvaṁ brūsi,",
        "analyzed": "brāhmaṇaḥ hi ced tvam brūsi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.4:7.2",
        "original": "Mañca brūsi abrāhmaṇaṁ;",
        "analyzed": "mañca brūsi abrāhmaṇa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.4:7.3",
        "original": "Taṁ taṁ sāvittiṁ pucchāmi,",
        "analyzed": "tam tam sāvittim pucchāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.4:7.4",
        "original": "Tipadaṁ catuvīsatakkharaṁ”.",
        "analyzed": "ti padam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.4:8.1",
        "original": "“Kiṁ nissitā isayo manujā,",
        "analyzed": "kim nissitāḥ isayaḥ manujā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.4:8.2",
        "original": "Khattiyā brāhmaṇā devatānaṁ;",
        "analyzed": "khattiyāḥ brāhmaṇāḥ devatāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.4:8.3",
        "original": "Yaññamakappayiṁsu puthū idha loke”.",
        "analyzed": "puthū idha loke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.4:9.1",
        "original": "“Yadantagū vedagū yaññakāle,",
        "analyzed": "yad anta gū veda gū yañ a kāle ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.4:9.2",
        "original": "Yassāhutiṁ labhe tassijjheti brūmi”.",
        "analyzed": "yassa āhutim labhe brūmi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.4:10.1",
        "original": "“Addhā hi tassa hutamijjhe,",
        "analyzed": "addhā hi tassa hutamijjhe ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.4:10.2",
        "original": "(iti brāhmaṇo)",
        "analyzed": "iti brāhmaṇaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.4:10.3",
        "original": "Yaṁ tādisaṁ vedagumaddasāma;",
        "analyzed": "yam tādisam vedagumat dasāma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.4:10.4",
        "original": "Tumhādisānañhi adassanena,",
        "analyzed": "tumha ādisānam hi a dassanena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.4:10.5",
        "original": "Añño jano bhuñjati pūraḷāsaṁ”.",
        "analyzed": "aññaḥ janaḥ bhuñjati pūraḷāsa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.4:11.1",
        "original": "“Tasmātiha tvaṁ brāhmaṇa atthena,",
        "analyzed": "tasmāt atiha tvam brāhmaṇa atthena ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.4:11.2",
        "original": "Atthiko upasaṅkamma puccha;",
        "analyzed": "atthikaḥ upasaṅkamma puccha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.4:11.3",
        "original": "Santaṁ vidhūmaṁ anīghaṁ nirāsaṁ,",
        "analyzed": "santam vidhūmam anīgham nirāsa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.4:11.4",
        "original": "Appevidha abhivinde sumedhaṁ”.",
        "analyzed": "appevidha abhivinde sumedha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.4:12.1",
        "original": "“Yaññe ratohaṁ bho gotama,",
        "analyzed": "yaññe rataḥ ham bho gotama ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.4:12.2",
        "original": "Yaññaṁ yiṭṭhukāmo nāhaṁ pajānāmi;",
        "analyzed": "yaññam yiṭṭhu kāmaḥ na aham pajānāmi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.4:12.3",
        "original": "Anusāsatu maṁ bhavaṁ,",
        "analyzed": "anusāsatu mama bhava ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.4:12.4",
        "original": "Yattha hutaṁ ijjhate brūhi me taṁ”.",
        "analyzed": "yattha hutam ijjhate brūhi me ta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.4:13.1",
        "original": "“Tena hi tvaṁ, brāhmaṇa, odahassu sotaṁ; dhammaṁ te desessāmi—",
        "analyzed": "tena hi tva brāhmaṇaḥ odahassu sa uta dhammam te desessāmi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.4:14.1",
        "original": "Mā jātiṁ pucchī caraṇañca puccha,",
        "analyzed": "mā jātim pucchī caraṇam ca puccha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.4:14.2",
        "original": "Kaṭṭhā have jāyati jātavedo;",
        "analyzed": "kaṭṭhā have jāyati jātavedaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.4:14.3",
        "original": "Nīcākulīnopi munī dhitīmā,",
        "analyzed": "nīca akulīnaḥ upi munī ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.4:14.4",
        "original": "Ājāniyo hoti hirīnisedho.",
        "analyzed": "ājāniyaḥ hoti hirīnisedhaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.4:15.1",
        "original": "Saccena danto damasā upeto,",
        "analyzed": "saccena dantaḥ damasāḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.4:15.2",
        "original": "Vedantagū vūsitabrahmacariyo;",
        "analyzed": "vedantagū vūsita brahmacariyaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.4:15.3",
        "original": "Kālena tamhi habyaṁ pavecche,",
        "analyzed": "kālena habyam ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.4:15.4",
        "original": "Yo brāhmaṇo puññapekkho yajetha.",
        "analyzed": "yaḥ brāhmaṇaḥ puññapekkhaḥ yajetha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.4:16.1",
        "original": "Ye kāme hitvā agahā caranti,",
        "analyzed": "ye kāme hitvā agahā caranti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.4:16.2",
        "original": "Susaññatattā tasaraṁva ujjuṁ;",
        "analyzed": "su sañña tat tā tasaravaḥ ujju ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.4:16.3",
        "original": "Kālena tesu habyaṁ pavecche,",
        "analyzed": "kālena tesu habyam ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.4:16.4",
        "original": "Yo brāhmaṇo puññapekkho yajetha.",
        "analyzed": "yaḥ brāhmaṇaḥ puññapekkhaḥ yajetha ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.4:17.1",
        "original": "Ye vītarāgā susamāhitindriyā,",
        "analyzed": "ye vīta rāgā su samāhita indriyā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.4:17.2",
        "original": "Candova rāhuggahaṇā pamuttā;",
        "analyzed": "candā uva rāhu ggahaṇā pamuttā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.4:17.3",
        "original": "Kālena tesu habyaṁ pavecche,",
        "analyzed": "kālena tesu habyam ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.4:17.4",
        "original": "Yo brāhmaṇo puññapekkho yajetha.",
        "analyzed": "yaḥ brāhmaṇaḥ puññapekkhaḥ yajetha ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.4:18.1",
        "original": "Asajjamānā vicaranti loke,",
        "analyzed": "a sajjamānāḥ vicaranti loke ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.4:18.2",
        "original": "Sadā satā hitvā mamāyitāni;",
        "analyzed": "sadā satā hitvā mamāyitāni ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.4:18.3",
        "original": "Kālena tesu habyaṁ pavecche,",
        "analyzed": "kālena tesu habyam ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.4:18.4",
        "original": "Yo brāhmaṇo puññapekkho yajetha.",
        "analyzed": "yaḥ brāhmaṇaḥ puññapekkhaḥ yajetha ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.4:19.1",
        "original": "Yo kāme hitvā abhibhuyyacārī,",
        "analyzed": "yaḥ kāme hitvā abhibhuyya cārī ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.4:19.2",
        "original": "Yo vedi jātīmaraṇassa antaṁ;",
        "analyzed": "yaḥ vedi jātīmaraṇassa anta ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.4:19.3",
        "original": "Parinibbuto udakarahadova sīto,",
        "analyzed": "parinibbutaḥ udaka rahadaḥ uva sīto ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.4:19.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.4:20.1",
        "original": "Samo samehi visamehi dūre,",
        "analyzed": "samaḥ samehi visamehi dūre ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.4:20.2",
        "original": "Tathāgato hoti anantapañño;",
        "analyzed": "tathāgataḥ hoti ananta paññaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.4:20.3",
        "original": "Anūpalitto idha vā huraṁ vā,",
        "analyzed": "vā huram vā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.4:20.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.4:21.1",
        "original": "Yamhi na māyā vasati na māno,",
        "analyzed": "yamhi na māyā vasati na mānaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.4:21.2",
        "original": "Yo vītalobho amamo nirāso;",
        "analyzed": "yaḥ vīta lobhaḥ amamaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.4:21.3",
        "original": "Paṇunnakodho abhinibbutatto,",
        "analyzed": "paṇunna kodhaḥ abhinibbuta ttaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.4:21.4",
        "original": "Yo brāhmaṇo sokamalaṁ ahāsi;",
        "analyzed": "yaḥ brāhmaṇaḥ soka malam ahāsi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.4:21.5",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.4:22.1",
        "original": "Nivesanaṁ yo manaso ahāsi,",
        "analyzed": "nivesanam yaḥ manasaḥ ahāsi ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.4:22.2",
        "original": "Pariggahā yassa na santi keci;",
        "analyzed": "pariggahāḥ yassa na santi keci ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.4:22.3",
        "original": "Anupādiyāno idha vā huraṁ vā,",
        "analyzed": "an upādiyānaḥ vā huram vā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.4:22.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.4:23.1",
        "original": "Samāhito yo udatāri oghaṁ,",
        "analyzed": "samāhitaḥ yaḥ udatāri ogha ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.4:23.2",
        "original": "Dhammaṁ caññāsi paramāya diṭṭhiyā;",
        "analyzed": "dhammam caññāsi paramāya diṭṭhiyā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.4:23.3",
        "original": "Khīṇāsavo antimadehadhārī,",
        "analyzed": "khīṇa āsavaḥ antima dehadhārī ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.4:23.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.4:24.1",
        "original": "Bhavāsavā yassa vacī kharā ca,",
        "analyzed": "bhava asavāḥ yassa vacī kharā ca ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.4:24.2",
        "original": "Vidhūpitā atthagatā na santi;",
        "analyzed": "vidhūpitāḥ attha gatāḥ na santi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.4:24.3",
        "original": "Sa vedagū sabbadhi vippamutto,",
        "analyzed": "sa vedagū sabbadhi vippamuttaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.4:24.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.4:25.1",
        "original": "Saṅgātigo yassa na santi saṅgā,",
        "analyzed": "saṅga atigaḥ yassa na santi saṅgā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.4:25.2",
        "original": "Yo mānasattesu amānasatto;",
        "analyzed": "yaḥ māna sattesu a māna sattaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.4:25.3",
        "original": "Dukkhaṁ pariññāya sakhettavatthuṁ,",
        "analyzed": "dukkham pariññāya sa khetta vatthu ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.4:25.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.4:26.1",
        "original": "Āsaṁ anissāya vivekadassī,",
        "analyzed": "āsam a nissāya viveka dassī ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.4:26.2",
        "original": "Paravediyaṁ diṭṭhimupātivatto;",
        "analyzed": "para vediyam diṭṭhim upātivatto ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.4:26.3",
        "original": "Ārammaṇā yassa na santi keci,",
        "analyzed": "ārammaṇāḥ yassa na santi keci ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.4:26.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.4:27.1",
        "original": "Paroparā yassa samecca dhammā,",
        "analyzed": "para uparā yassa samet ca dhammā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.4:27.2",
        "original": "Vidhūpitā atthagatā na santi;",
        "analyzed": "vidhūpitāḥ attha gatāḥ na santi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.4:27.3",
        "original": "Santo upādānakhaye vimutto,",
        "analyzed": "santaḥ upādāna khaye vimuttaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.4:27.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.4:28.1",
        "original": "Saṁyojanaṁ jātikhayantadassī,",
        "analyzed": "sa yojanam jāti khayanta dassī ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.4:28.2",
        "original": "Yopānudi rāgapathaṁ asesaṁ;",
        "analyzed": "rāga patham asesa ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.4:28.3",
        "original": "Suddho nidoso vimalo akāco,",
        "analyzed": "suddhaḥ nidosaḥ vimalaḥ akācaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.4:28.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.4:29.1",
        "original": "Yo attano attānaṁ nānupassati,",
        "analyzed": "yaḥ attanaḥ attānam na anupassati ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.4:29.2",
        "original": "Samāhito ujjugato ṭhitatto;",
        "analyzed": "samāhitaḥ ujjugataḥ ṭhitattaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.4:29.3",
        "original": "Sa ve anejo akhilo akaṅkho,",
        "analyzed": "sa veḥ anejaḥ akhilaḥ akaṅkhaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.4:29.4",
        "original": "Tathāgato arahati pūraḷāsaṁ.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.4:30.1",
        "original": "Mohantarā yassa na santi keci,",
        "analyzed": "mohantarāḥ yassa na santi keci ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.4:30.2",
        "original": "Sabbesu dhammesu ca ñāṇadassī;",
        "analyzed": "sabbesu dhammesu ca ñāṇa dassī ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.4:30.3",
        "original": "Sarīrañca antimaṁ dhāreti,",
        "analyzed": "sarīram ca antimam dhārā iti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.4:30.4",
        "original": "Patto ca sambodhimanuttaraṁ sivaṁ;",
        "analyzed": "patto ca sambodhim anuttaram ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.4:30.5",
        "original": "Ettāvatā yakkhassa suddhi,",
        "analyzed": "ettāvatā yakkhassa suddhi ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.4:30.6",
        "original": "Tathāgato arahati pūraḷāsaṁ”.",
        "analyzed": "tathāgataḥ arahati pūraḍā āsa ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.4:31.1",
        "original": "“Hutañca mayhaṁ hutamatthu saccaṁ,",
        "analyzed": "hutam ca mayham hutam atthu sat ca ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.4:31.2",
        "original": "Yaṁ tādisaṁ vedagunaṁ alatthaṁ;",
        "analyzed": "yam tādisam vedagunam alattha ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.4:31.3",
        "original": "Brahmā hi sakkhi paṭigaṇhātu me bhagavā,",
        "analyzed": "brahmā hi sakkhi paṭigaṇhātu me bhagavā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.4:31.4",
        "original": "Bhuñjatu me bhagavā pūraḷāsaṁ”.",
        "analyzed": "bhuñjatu me bhagavā pūraḍāsa ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.4:32.1",
        "original": "“Gāthābhigītaṁ me abhojaneyyaṁ,",
        "analyzed": "gāthā abhigītam me abhojaneyya ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.4:32.2",
        "original": "Sampassataṁ brāhmaṇa nesa dhammo;",
        "analyzed": "sampassatam brāhmaṇa na isa dhammaḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.4:32.3",
        "original": "Gāthābhigītaṁ panudanti buddhā,",
        "analyzed": "gāthā abhigītam panudanti buddhāḥ ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.4:32.4",
        "original": "Dhamme satī brāhmaṇa vuttiresā.",
        "analyzed": "dhamme satī brāhmaṇa vuttiḥ esā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.4:33.1",
        "original": "Aññena ca kevalinaṁ mahesiṁ,",
        "analyzed": "aññena ca kevalinam mahesi ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.4:33.2",
        "original": "Khīṇāsavaṁ kukkuccavūpasantaṁ;",
        "analyzed": "khīṇa āsavam ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.4:33.3",
        "original": "Annena pānena upaṭṭhahassu,",
        "analyzed": "annena pānena upaṭṭhahassu ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.4:33.4",
        "original": "Khettañhi taṁ puññapekkhassa hoti”.",
        "analyzed": "khettam hi tam puññapekkhassa hoti ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.4:34.1",
        "original": "“Sādhāhaṁ bhagavā tathā vijaññaṁ,",
        "analyzed": "sādha aham bhagavā tathā vijañña ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.4:34.2",
        "original": "Yo dakkhiṇaṁ bhuñjeyya mādisassa;",
        "analyzed": "yaḥ dakkhiṇam bhuñjeyya mādisassa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.4:34.3",
        "original": "Yaṁ yaññakāle pariyesamāno,",
        "analyzed": "yam yañña kāle pariyesamānaḥ ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.4:34.4",
        "original": "Pappuyya tava sāsanaṁ”.",
        "analyzed": "pappuyya tava sāsana ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.4:35.1",
        "original": "“Sārambhā yassa vigatā,",
        "analyzed": "sā ārambhā vigatā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.4:35.2",
        "original": "Cittaṁ yassa anāvilaṁ;",
        "analyzed": "cittam yassa anāvila ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.4:35.3",
        "original": "Vippamutto ca kāmehi,",
        "analyzed": "vippamuttaḥ ca kāmehi ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.4:35.4",
        "original": "Thinaṁ yassa panūditaṁ.",
        "analyzed": "thinam yassa ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.4:36.1",
        "original": "Sīmantānaṁ vinetāraṁ,",
        "analyzed": "sīmantānam vinetāra ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.4:36.2",
        "original": "Jātimaraṇakovidaṁ;",
        "analyzed": "jāti maraṇa kovida ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.4:36.3",
        "original": "Muniṁ moneyyasampannaṁ,",
        "analyzed": "munim ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.4:36.4",
        "original": "Tādisaṁ yaññamāgataṁ.",
        "analyzed": "tādisam yaññam āgata ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.4:37.1",
        "original": "Bhakuṭiṁ vinayitvāna,",
        "analyzed": "bhakuṭim vinayitvāna ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.4:37.2",
        "original": "Pañjalikā namassatha;",
        "analyzed": "pañjalikāḥ namassatha ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.4:37.3",
        "original": "Pūjetha annapānena,",
        "analyzed": "pūjetha anna pānena ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.4:37.4",
        "original": "Evaṁ ijjhanti dakkhiṇā”.",
        "analyzed": "evam ijjhanti dakkhiṇā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.4:38.1",
        "original": "“Buddho bhavaṁ arahati pūraḷāsaṁ,",
        "analyzed": "buddhaḥ bhavam arahati pūraḷāsa ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.4:38.2",
        "original": "Puññakhettamanuttaraṁ;",
        "analyzed": "puñña khettam anuttara ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.4:38.3",
        "original": "Āyāgo sabbalokassa,",
        "analyzed": "āyāgaḥ sabba lokassa ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.4:38.4",
        "original": "Bhoto dinnaṁ mahapphalan”ti.",
        "analyzed": "bhoto dinnam mahapphalanti ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.4:39.1",
        "original": "Atha kho sundarikabhāradvājo brāhmaṇo bhagavantaṁ etadavoca:",
        "analyzed": "atha khalu sundarikabhāradvājaḥ brāhmaṇaḥ bhagavantam etat avoca ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.2",
        "original": "“abhikkantaṁ, bho gotama, abhikkantaṁ, bho gotama.",
        "analyzed": "abhikkanta bho gotama abhikkanta bho gotama ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.3_0",
        "original": "Seyyathāpi, bho gotama, nikkujjitaṁ vā ukkujjeyya, paṭicchannaṁ vā vivareyya, mūḷhassa",
        "analyzed": "seyyathā api bho gotama nikkujjitam vā ukkujjeyya paṭicchannam vā vivareyya mūḷhassa ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.3_1",
        "original": "vā maggaṁ ācikkheyya, andhakāre vā telapajjotaṁ dhāreyya: ‘cakkhumanto rūpāni dakkhantī’ti;",
        "analyzed": "vā maggam ācikkheyya andhakāre vā tela pajjotam dhāreyya cakkhumantaḥ rūpāni dakkhanti iti ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.3_2",
        "original": "evamevaṁ bhotā gotamena anekapariyāyena dhammo pakāsito.",
        "analyzed": "evam evam bhotā gotamena aneka pariyāyena dhammo pakāsito ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.4",
        "original": "Esāhaṁ bhavantaṁ gotamaṁ saraṇaṁ gacchāmi dhammañca bhikkhusaṅghañca.",
        "analyzed": "esā aham bhavantam gotamam saraṇam gacchāmi dhammañca bhikkhu saṅghañca ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.5",
        "original": "Labheyyāhaṁ bhoto gotamassa santike pabbajjaṁ, labheyyaṁ upasampadan”ti.",
        "analyzed": "labheyya aham bhoto gotamassa santike pabbajja labheyyam upasampadanti ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.6",
        "original": "Alattha kho sundarikabhāradvājo brāhmaṇo …pe…",
        "analyzed": "alattha kho sundarikabhāradvājaḥ brāhmaṇaḥ pe ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.7",
        "original": "arahataṁ ahosīti.",
        "analyzed": "arahatam ahosi iti ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.4:39.8",
        "original": "Sundarikabhāradvājasuttaṁ catutthaṁ.",
        "analyzed": "sundarikabhāradvājasuttam ",
        "folio": "39"
    }
]