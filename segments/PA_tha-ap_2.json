        "folio": "57"
    },
    {
        "segmentnr": "PA_tha-ap_2:57.3",
        "original": "Sutvā tathā ye paṭipattiyuttā,",
        "analyzed": "sutvā tathā ye paṭipatti yuttā ",
        "folio": "57"
    },
    {
        "segmentnr": "PA_tha-ap_2:57.4",
        "original": "Bhavanti te saccadasā sapaññā.",
        "analyzed": "bhavanti te saccadasā sa paññā ",
        "folio": "57"
    },
    {
        "segmentnr": "PA_tha-ap_2:58.1",
        "original": "Paccekabuddhehi jinehi bhāsitā,",
        "analyzed": "paccekabuddhehi jinehi bhāsitā ",
        "folio": "58"
    },
    {
        "segmentnr": "PA_tha-ap_2:58.2",
