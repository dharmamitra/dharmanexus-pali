        "analyzed": "yaḥ sugatesu manam pada usaye ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.10:12.1",
        "original": "Sataṁ sahassānaṁ nirabbudānaṁ,",
        "analyzed": "satam sahassānam nirabbudāna ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.10:12.2",
        "original": "Chattiṁsati pañca ca abbudāni;",
        "analyzed": "chattisati pañca ca abbudāni ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.10:12.3",
        "original": "Yamariyagarahī nirayaṁ upeti,",
        "analyzed": "nirayam upeti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.10:12.4",
        "original": "Vācaṁ manañca paṇidhāya pāpakaṁ.",
        "analyzed": "vācam manaṃca paṇidhāya pāpaka ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.10:13.1",
        "original": "Abhūtavādī nirayaṁ upeti,",
        "analyzed": "abhūta vādī nirayam upeti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.10:13.2",
        "original": "Yo vāpi katvā na karomicāha;",
        "analyzed": "yaḥ vā api katvā na karomi ca āha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.10:13.3",
        "original": "Ubhopi te pecca samā bhavanti,",
        "analyzed": "ubhau api te samāḥ bhavanti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.10:13.4",
        "original": "Nihīnakammā manujā parattha.",
        "analyzed": "nihīna kammāḥ manujā parattha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.10:14.1",
        "original": "Yo appaduṭṭhassa narassa dussati,",
        "analyzed": "yaḥ appaduṭṭhassa narassa dussati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.10:14.2",
        "original": "Suddhassa posassa anaṅgaṇassa;",
        "analyzed": "suddhassa posassa anaṅgaṇassa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.10:14.3",
        "original": "Tameva bālaṁ pacceti pāpaṁ,",
        "analyzed": "tam eva bālam pacceti pāpa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.10:14.4",
        "original": "Sukhumo rajo paṭivātaṁva khitto.",
        "analyzed": "su khumaḥ rajaḥ khitto ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.10:15.1",
        "original": "Yo lobhaguṇe anuyutto,",
        "analyzed": "yaḥ lobha guṇe anuyuttaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.10:15.2",
        "original": "So vacasā paribhāsati aññe;",
        "analyzed": "saḥ vacasā paribhāsati aññe ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.10:15.3",
        "original": "Asaddho kadariyo avadaññū,",
        "analyzed": "a saddhaḥ kadariyaḥ a vadaññū ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.10:15.4",
        "original": "Macchari pesuṇiyaṁ anuyutto.",
        "analyzed": "macchari pesuṇiyam anuyuttaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.10:16.1",
        "original": "Mukhadugga vibhūta anariya,",
        "analyzed": "mukha dugga vibhūta anariya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.10:16.2",
        "original": "Bhūnahu pāpaka dukkaṭakāri;",
        "analyzed": "bhūnahu pāpaka dukkaṭa kāri ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.10:16.3",
        "original": "Purisanta kalī avajāta,",
        "analyzed": "purisanta kalī avajāta ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.10:16.4",
        "original": "Mā bahubhāṇidha nerayikosi.",
        "analyzed": "mā bahubhāṇidha na īrayikaḥ asi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.10:17.1",
        "original": "Rajamākirasī ahitāya,",
        "analyzed": "rajamākirasī ahitāya ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.10:17.2",
        "original": "Sante garahasi kibbisakārī;",
        "analyzed": "sante gara hasi kibbisa kārī ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.10:17.3",
        "original": "Bahūni duccaritāni caritvā,",
        "analyzed": "bahūni duccaritāni caritvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.10:17.4",
        "original": "Gacchasi kho papataṁ cirarattaṁ.",
        "analyzed": "gacchasi khalu papatam ciraratta ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.10:18.1",
        "original": "Na hi nassati kassaci kammaṁ,",
        "analyzed": "na hi nassati kassaci kamma ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.10:18.2",
        "original": "Eti hataṁ labhateva suvāmi;",
        "analyzed": "eti hatam labhatā iva suvāmi ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.10:18.3",
        "original": "Dukkhaṁ mando paraloke,",
        "analyzed": "dukkham mando para loke ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.10:18.4",
        "original": "Attani passati kibbisakārī.",
        "analyzed": "attani passati kibbisa kārī ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.10:19.1",
        "original": "Ayosaṅkusamāhataṭṭhānaṁ,",
        "analyzed": "ayosaṅku samāhata ṭṭhāna ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.10:19.2",
        "original": "Tiṇhadhāramayasūlamupeti;",
        "analyzed": "tiṇha dhāra maya sūlam upeti ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.10:19.3",
        "original": "Atha tattaayoguḷasannibhaṁ,",
        "analyzed": "atha ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.10:19.4",
        "original": "Bhojanamatthi tathā patirūpaṁ.",
        "analyzed": "bhojanam atthi tathā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.10:20.1",
        "original": "Na hi vaggu vadanti vadantā,",
        "analyzed": "na hi vaggu vadanti vadantā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.10:20.2",
        "original": "Nābhijavanti na tāṇamupenti;",
        "analyzed": "na abhijavanti na tāṇam upenti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.10:20.3",
        "original": "Aṅgāre santhate sayanti,",
        "analyzed": "aṅgāre santhate sayanti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.10:20.4",
        "original": "Ginisampajjalitaṁ pavisanti.",
        "analyzed": "pavisanti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.10:21.1",
        "original": "Jālena ca onahiyāna,",
        "analyzed": "jālena ca onahiyāna ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.10:21.2",
        "original": "Tattha hananti ayomayakuṭebhi;",
        "analyzed": "tattha hananti ayomayakuṭebhi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.10:21.3",
        "original": "Andhaṁva timisamāyanti,",
        "analyzed": "andhava timisamāyanti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.10:21.4",
        "original": "Taṁ vitatañhi yathā mahikāyo.",
        "analyzed": "tam vitatañhi yathā mahi kāyaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.10:22.1",
        "original": "Atha lohamayaṁ pana kumbhiṁ,",
        "analyzed": "atha loha mayam kumbhi ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.10:22.2",
        "original": "Ginisampajjalitaṁ pavisanti;",
        "analyzed": "pavisanti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.10:22.3",
        "original": "Paccanti hi tāsu cirarattaṁ,",
        "analyzed": "paccanti hi tāsu ciraratta ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.10:22.4",
        "original": "Agginisamāsu samuppilavāte.",
        "analyzed": "aggi nisamāsu samuppila vāte ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.10:23.1",
        "original": "Atha pubbalohitamisse,",
        "analyzed": "atha pubbalohitamisse ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.10:23.2",
        "original": "Tattha kiṁ paccati kibbisakārī;",
        "analyzed": "tattha kim paccati kibbisakārī ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.10:23.3",
        "original": "Yaṁ yaṁ disakaṁ adhiseti,",
        "analyzed": "yam yam disakam adhiseti ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.10:23.4",
        "original": "Tattha kilissati samphusamāno.",
        "analyzed": "tattha kilissati samphusamānaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.10:24.1",
        "original": "Puḷavāvasathe salilasmiṁ,",
        "analyzed": "puḷava āvasathe ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.10:24.2",
        "original": "Tattha kiṁ paccati kibbisakārī;",
        "analyzed": "tattha kim paccati kibbisakārī ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.10:24.3",
        "original": "Gantuṁ na hi tīramapatthi,",
        "analyzed": "gantum na hi tīram apatthi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.10:24.4",
        "original": "Sabbasamā hi samantakapallā.",
        "analyzed": "sabba samā hi samanta kapallā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.10:25.1",
        "original": "Asipattavanaṁ pana tiṇhaṁ,",
        "analyzed": "asipattavanam pana ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.10:25.2",
        "original": "Taṁ pavisanti samucchidagattā;",
        "analyzed": "tam pavisanti samucchida gattā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.10:25.3",
        "original": "Jivhaṁ balisena gahetvā,",
        "analyzed": "jivham balisena gahetvā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.10:25.4",
        "original": "Ārajayārajayā vihananti.",
        "analyzed": "ārajayā ārajayā vihananti ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.10:26.1",
        "original": "Atha vetaraṇiṁ pana duggaṁ,",
        "analyzed": "atha vetaraṇim pana ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.10:26.2",
        "original": "Tiṇhadhārakhuradhāramupenti;",
        "analyzed": "tiṇha dhāra khura dhāram upenti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.10:26.3",
        "original": "Tattha mandā papatanti,",
        "analyzed": "tattha mandāḥ papatanti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.10:26.4",
        "original": "Pāpakarā pāpāni karitvā.",
        "analyzed": "pāpa karā pāpāni karitvā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.10:27.1",
        "original": "Khādanti hi tattha rudante,",
        "analyzed": "khādanti hi tattha rudante ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.10:27.2",
        "original": "Sāmā sabalā kākolagaṇā ca;",
        "analyzed": "sāmā sabalā kākolagaṇā ca ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.10:27.3",
        "original": "Soṇā siṅgālā paṭigiddhā,",
        "analyzed": "soṇā siṅgālā paṭigiddhā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.10:27.4",
        "original": "Kulalā vāyasā ca vitudanti.",
        "analyzed": "kulalā vāyasā ca vitudanti ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.10:28.1",
        "original": "Kicchā vatayaṁ idha vutti,",
        "analyzed": "kicchā vatayam idha ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.10:28.2",
        "original": "Yaṁ jano phusati kibbisakārī;",
        "analyzed": "yam janaḥ phusati kibbisa kārī ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.10:28.3",
        "original": "Tasmā idha jīvitasese,",
        "analyzed": "tasmai idha jīvita sese ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.10:28.4",
        "original": "Kiccakaro siyā naro na cappamajje.",
        "analyzed": "kicca karaḥ siyā naraḥ na ca ppamajje ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.10:29.1",
        "original": "Te gaṇitā vidūhi tilavāhā,",
        "analyzed": "te gaṇitāḥ vidūhi tila vāhā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.10:29.2",
        "original": "Ye padume niraye upanītā;",
        "analyzed": "ye padume niraye upanītā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.10:29.3",
        "original": "Nahutāni hi koṭiyo pañca bhavanti,",
        "analyzed": "nahutāni hi koṭiyo pañca bhavanti ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.10:29.4",
        "original": "Dvādasa koṭisatāni punaññā.",
        "analyzed": "dvādasa koṭi satāni punaññā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.10:30.1",
        "original": "Yāva dukhā nirayā idha vuttā,",
        "analyzed": "yāva dukhā nirayā idha vuttā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.10:30.2",
        "original": "Tatthapi tāva ciraṁ vasitabbaṁ;",
        "analyzed": "tattha api tāva ciram ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.10:30.3",
        "original": "Tasmā sucipesalasādhuguṇesu,",
        "analyzed": "tasmā suci pesala sādhu guṇesu ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.10:30.4",
        "original": "Vācaṁ manaṁ satataṁ parirakkhe”ti.",
        "analyzed": "vācam manam satatam parirakkheti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.10:30.5",
        "original": "Kokālikasuttaṁ dasamaṁ.",
        "analyzed": "kokālikasuttam dasama ",
        "folio": "30"
    }
]