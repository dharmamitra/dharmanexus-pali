    },
    {
        "segmentnr": "PA_pv_29:1.1",
        "original": "“Bhusāni eko sāliṁ punāparo,",
        "analyzed": "bhusāni ekaḥ sālim ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_29:1.2",
