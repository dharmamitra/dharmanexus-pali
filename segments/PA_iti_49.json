    },
    {
        "segmentnr": "PA_iti_49:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_49:2.1",
        "original": "“Dvīhi, bhikkhave, diṭṭhigatehi pariyuṭṭhitā devamanussā olīyanti eke, atidhāvanti eke; cakkhumanto ca passanti.",
        "analyzed": "dvīhi bhikkhave diṭṭhi gatehi pariyuṭṭhitāḥ deva manussāḥ olīyanti eke atidhāvanti eke cakkhumanto ca passanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_49:3.1",
        "original": "Kathañca, bhikkhave, olīyanti eke?",
        "analyzed": "kathañca bhikkhave olīyanti eke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_49:3.2",
        "original": "Bhavārāmā, bhikkhave, devamanussā bhavaratā bhavasammuditā tesaṁ bhavanirodhāya dhamme desiyamāne cittaṁ na pakkhandati na pasīdati na santiṭṭhati nādhimuccati.",
        "analyzed": "bhava ārāmāḥ bhikkhave deva manussāḥ bhava ratāḥ bhava sammuditā tesam bhava nirodhāya dhamme desiyamāne cittam na pakkhandati na pasīdati na santiṭṭhati na adhimuccati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_49:3.3",
        "original": "Evaṁ kho, bhikkhave, olīyanti eke.",
        "analyzed": "evam kho bhikkhave olīyanti eke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_49:4.1",
        "original": "Kathañca, bhikkhave, atidhāvanti eke?",
        "analyzed": "kathañca bhikkhave atidhāvanti eke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_49:4.2",
        "original": "Bhaveneva kho paneke aṭṭīyamānā harāyamānā jigucchamānā vibhavaṁ abhinandanti—",
        "analyzed": "bhavena iva khalu paneke aṭṭīyamānāḥ harāyamānāḥ jigucchamānāḥ vibhavam abhinandanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_49:4.3",
        "original": "yato kira, bho, ayaṁ attā kāyassa bhedā paraṁ maraṇā ucchijjati vinassati na hoti paraṁ maraṇā;",
        "analyzed": "yatas kira bho ayam attā kāyassa bhedā param maraṇā ucchijjati vinassati na hoti param maraṇā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_49:4.4",
        "original": "etaṁ santaṁ etaṁ paṇītaṁ etaṁ yāthāvanti.",
        "analyzed": "etam santam etam paṇītam etam yāthāvanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_49:4.5",
        "original": "Evaṁ kho, bhikkhave, atidhāvanti eke.",
        "analyzed": "evam kho bhikkhave atidhāvanti eke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_49:5.1",
        "original": "Kathañca, bhikkhave, cakkhumanto passanti?",
        "analyzed": "katham ca bhikkhave cakkhumantaḥ passanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_49:5.2",
        "original": "Idha bhikkhu bhūtaṁ bhūtato passati;",
        "analyzed": "idha bhikkhu bhūtam bhūtataḥ passati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_49:5.3",
        "original": "bhūtaṁ bhūtato disvā bhūtassa nibbidāya virāgāya nirodhāya paṭipanno hoti.",
        "analyzed": "bhūtam bhūtataḥ disvā bhūtassa nibbidāya virāgāya nirodhāya paṭipannaḥ hoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_49:5.4",
        "original": "Evaṁ kho, bhikkhave, cakkhumanto passantī”ti.",
        "analyzed": "evam kho bhikkhave cakkhumanto passanti iti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_49:5.5",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_49:5.6",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "5"
    },
