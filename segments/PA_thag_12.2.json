        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_12.2:2.4",
        "original": "vandissaṁ bahukaṁ janaṁ.",
        "analyzed": "vandissam bahukam jana ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_12.2:3.1",
