        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.6:5.2_2",
        "original": "sādhusammatā bahujanassa, seyyathidaṁ—",
        "analyzed": "sādhu sammatā bahu janassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.6:5.3",
        "original": "pūraṇo kassapo …pe… nigaṇṭho nāṭaputto—",
        "analyzed": "pūraṇaḥ kassapaḥ pe nigaṇṭhaḥ nāṭa putto ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.6:5.4",
        "original": "tepi mayā pañhe puṭṭhā na sampāyanti,",
        "analyzed": "te api mayā pañhe puṭṭhāḥ na sampāyanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.6:5.5",
        "original": "asampāyantā kopañca dosañca appaccayañca pātukaronti, api ca maññevettha paṭipucchanti;",
        "analyzed": "a sampāyantā kopañca dosañca appaccayañca pātukaronti api ca maññā iva itthā paṭipucchanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.6:5.6",
        "original": "kiṁ pana me samaṇo gotamo ime pañhe puṭṭho byākarissati.",
        "analyzed": "kim pana me samaṇaḥ gotamaḥ ime pañhe puṭṭhaḥ byākarissati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.6:5.7",
        "original": "Samaṇo hi gotamo daharo ceva jātiyā, navo ca pabbajjāyā”ti.",
        "analyzed": "samaṇaḥ hi gotamaḥ daharaḥ ca iva jātiyā navaḥ ca pabbajjāya ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.6:6.1",
        "original": "Atha kho sabhiyassa paribbājakassa etadahosi:",
        "analyzed": "atha kho sabhiyassa paribbājakassa etat ahosi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.6:6.2",
        "original": "“samaṇo kho daharoti na uññātabbo na paribhotabbo.",
        "analyzed": "samaṇo kho daharo ti na uññātabbaḥ na paribhotabbaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.6:6.3",
        "original": "Daharopi cesa samaṇo gotamo mahiddhiko hoti mahānubhāvo, yannūnāhaṁ samaṇaṁ gotamaṁ upasaṅkamitvā ime pañhe puccheyyan”ti.",
        "analyzed": "daharaḥ api ca isa samaṇaḥ gotamaḥ mahiddhikaḥ hoti mahā anubhāvaḥ yat nūna aham samaṇam gotamam upasaṅkamitvā ime pañhe puccheyyanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.6:7.1",
        "original": "Atha kho sabhiyo paribbājako yena rājagahaṁ tena cārikaṁ pakkāmi.",
        "analyzed": "atha kho sabhiyaḥ paribbājakaḥ yena rājagaham tena cārikam pakkāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.6:7.2",
        "original": "Anupubbena cārikaṁ caramāno yena rājagahaṁ veḷuvanaṁ kalandakanivāpo, yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavatā saddhiṁ sammodi.",
        "analyzed": "anupubbena cārikam caramānaḥ yena rājagaham veḷuvanam kalandakanivāpaḥ yena bhagavā tena upasaṅkami upasaṅkamitvā bhagavatā saddhim sammodi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.6:7.3",
        "original": "Sammodanīyaṁ kathaṁ sāraṇīyaṁ vītisāretvā ekamantaṁ nisīdi.",
        "analyzed": "sammodanīyam katham sāraṇīyam vītisāretvā ekamantam nisīdi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.6:7.4",
        "original": "Ekamantaṁ nisinno kho sabhiyo paribbājako bhagavantaṁ gāthāya ajjhabhāsi—",
        "analyzed": "ekamantam nisinnaḥ kho sabhiyaḥ paribbājakaḥ bhagavantam gāthāya ajjhabhāsi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.6:8.1",
        "original": "“Kaṅkhī vecikicchī āgamaṁ,",
        "analyzed": "kaṅkhī vecikicchī āgama ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.6:8.2",
        "original": "(iti sabhiyo)",
        "analyzed": "iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.6:8.3",
        "original": "Pañhe pucchituṁ abhikaṅkhamāno;",
        "analyzed": "pañhe pucchitum abhikaṅkhamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.6:8.4",
        "original": "Tesantakaro bhavāhi pañhe me puṭṭho,",
        "analyzed": "bhavāhi pañhe me puṭṭhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.6:8.5",
        "original": "Anupubbaṁ anudhammaṁ byākarohi me”.",
        "analyzed": "anupubbam anudhammam byākarohi me ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.6:9.1",
        "original": "“Dūrato āgatosi sabhiya,",
        "analyzed": "dūratas āgataḥ asi sabhiya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.6:9.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.6:9.3",
        "original": "Pañhe pucchituṁ abhikaṅkhamāno;",
        "analyzed": "pañhe pucchitum abhikaṅkhamānaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.6:9.4",
        "original": "Tesantakaro bhavāmi pañhe te puṭṭho,",
        "analyzed": "bhavāmi pañhe te puṭṭhaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.6:9.5",
        "original": "Anupubbaṁ anudhammaṁ byākaromi te.",
        "analyzed": "anupubbam anudhammam byākaromi te ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.6:10.1",
        "original": "Puccha maṁ sabhiya pañhaṁ,",
        "analyzed": "puccha mama sabhiya pañha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.6:10.2",
        "original": "Yaṁ kiñci manasicchasi;",
        "analyzed": "yam kiñci manasi icchasi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.6:10.3",
        "original": "Tassa tasseva pañhassa,",
        "analyzed": "tassa tassa iva pañhassa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.6:10.4",
        "original": "Ahaṁ antaṁ karomi te”ti.",
        "analyzed": "aham antam karomi te iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.6:11.1",
        "original": "Atha kho sabhiyassa paribbājakassa etadahosi:",
        "analyzed": "atha kho sabhiyassa paribbājakassa etat ahosi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.6:11.2",
        "original": "“acchariyaṁ vata bho, abbhutaṁ vata bho.",
        "analyzed": "acchariyam vata bho abbhutam vata bho ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.6:11.3",
        "original": "Yaṁ vatāhaṁ aññesu samaṇabrāhmaṇesu okāsakammamattampi nālatthaṁ taṁ me idaṁ samaṇena gotamena okāsakammaṁ katan”ti.",
        "analyzed": "yam vata aham aññesu samaṇa brāhmaṇesu okāsa kamma mattam pi na alattham tam me idam samaṇena gotamena okāsa kammam katanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.6:11.4",
        "original": "Attamano pamudito udaggo pītisomanassajāto bhagavantaṁ pañhaṁ apucchi—",
        "analyzed": "atta manaḥ pamuditaḥ udaggaḥ pīti somanassa jātaḥ bhagavantam pañham apucchi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.6:12.1",
        "original": "“Kiṁpattinamāhu bhikkhunaṁ,",
        "analyzed": "kipattinam āhu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.6:12.2",
        "original": "(iti sabhiyo)",
        "analyzed": "iti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.6:12.3",
        "original": "Sorataṁ kena kathañca dantamāhu;",
        "analyzed": "soratam kena kathañca dantam āhu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.6:12.4",
        "original": "Buddhoti kathaṁ pavuccati,",
        "analyzed": "buddhaḥ iti katham pavuccati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.6:12.5",
        "original": "Puṭṭho me bhagavā byākarohi”.",
        "analyzed": "puṭṭhaḥ me bhagavā byākarohi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.6:13.1",
        "original": "“Pajjena katena attanā,",
        "analyzed": "pajjena katena attanā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.6:13.2",
        "original": "(sabhiyāti bhagavā)",
        "analyzed": "sabhiyāti bhagavān ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.6:13.3",
        "original": "Parinibbānagato vitiṇṇakaṅkho;",
        "analyzed": "parinibbāna gataḥ vitiṇṇa kaṅkhaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.6:13.4",
        "original": "Vibhavañca bhavañca vippahāya,",
        "analyzed": "vibhavañca bhavañca vippahāya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.6:13.5",
        "original": "Vusitavā khīṇapunabbhavo sa bhikkhu.",
        "analyzed": "vusitavā khīṇa punabbhavaḥ sa bhikkhu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.6:14.1",
        "original": "Sabbattha upekkhako satimā,",
        "analyzed": "sabbattha upekkhakaḥ satimā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.6:14.2",
        "original": "Na so hiṁsati kañci sabbaloke;",
        "analyzed": "na saḥ hisati kañci sabbaloke ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.6:14.3",
        "original": "Tiṇṇo samaṇo anāvilo,",
        "analyzed": "tiṇṇaḥ samaṇaḥ anāvilaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.6:14.4",
        "original": "Ussadā yassa na santi sorato so.",
        "analyzed": "ussadāḥ yassa na santi so rataḥ so ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.6:15.1",
        "original": "Yassindriyāni bhāvitāni,",
        "analyzed": "yaḥ indriyāni bhāvitāni ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.6:15.2",
        "original": "Ajjhattaṁ bahiddhā ca sabbaloke;",
        "analyzed": "ajjhattam bahiddhā ca sabba loke ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.6:15.3",
        "original": "Nibbijjha imaṁ parañca lokaṁ,",
        "analyzed": "nibbijjhe imam parañca loka ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.6:15.4",
        "original": "Kālaṁ kaṅkhati bhāvito sa danto.",
        "analyzed": "kālam kaṅkhati bhāvitaḥ sa dantaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.6:16.1",
        "original": "Kappāni viceyya kevalāni,",
        "analyzed": "kappāni viceyya kevalāni ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.6:16.2",
        "original": "Saṁsāraṁ dubhayaṁ cutūpapātaṁ;",
        "analyzed": "sa sāram dubhayam ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.6:16.3",
        "original": "Vigatarajamanaṅgaṇaṁ visuddhaṁ,",
        "analyzed": "vigata rajam anaṅgaṇam visuddha ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.6:16.4",
        "original": "Pattaṁ jātikhayaṁ tamāhu buddhan”ti.",
        "analyzed": "pattam jāti khayam tam āhu buddha nti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.6:17.1",
        "original": "Atha kho sabhiyo paribbājako bhagavato bhāsitaṁ abhinanditvā anumoditvā attamano pamudito udaggo pītisomanassajāto bhagavantaṁ uttariṁ pañhaṁ apucchi—",
        "analyzed": "atha kho sabhiyaḥ paribbājakaḥ bhagavataḥ bhāsitam abhinanditvā anumoditvā atta manaḥ pamuditaḥ udaggaḥ pīti somanassa jātaḥ bhagavantam uttarim pañham apucchi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.6:18.1",
        "original": "“Kiṁpattinamāhu brāhmaṇaṁ,",
        "analyzed": "kipattinam āhuḥ brāhmaṇa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.6:18.2",
        "original": "(iti sabhiyo)",
        "analyzed": "iti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.6:18.3",
        "original": "Samaṇaṁ kena kathañca nhātakoti;",
        "analyzed": "samaṇam kena kathaṃca ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.6:18.4",
        "original": "Nāgoti kathaṁ pavuccati,",
        "analyzed": "nāgaḥ iti katham pavuccati ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.6:18.5",
        "original": "Puṭṭho me bhagavā byākarohi”.",
        "analyzed": "puṭṭhaḥ me bhagavā byākarohi ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.6:19.1",
        "original": "“Bāhitvā sabbapāpakāni,",
        "analyzed": "bāhitvā sabba pāpakāni ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.6:19.2",
        "original": "(sabhiyāti bhagavā)",
        "analyzed": "sabhiyāti bhagavān ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.6:19.3",
        "original": "Vimalo sādhusamāhito ṭhitatto;",
        "analyzed": "vimalaḥ sādhu samāhitaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.6:19.4",
        "original": "Saṁsāramaticca kevalī so,",
        "analyzed": "kevalī saḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.6:19.5",
        "original": "Asito tādi pavuccate sa brahmā.",
        "analyzed": "asitaḥ ta ādi pavuccate sa brahmā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.6:20.1",
        "original": "Samitāvi pahāya puññapāpaṁ,",
        "analyzed": "samitāvi pahāya puñña pāpa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.6:20.2",
        "original": "Virajo ñatvā imaṁ parañca lokaṁ;",
        "analyzed": "virajaḥ ñatvā imam parañca loka ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.6:20.3",
        "original": "Jātimaraṇaṁ upātivatto,",
        "analyzed": "jāti maraṇam upātivattaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.6:20.4",
        "original": "Samaṇo tādi pavuccate tathattā.",
        "analyzed": "samaṇaḥ ta ādi pavuccate tathattā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.6:21.1",
        "original": "Ninhāya sabbapāpakāni,",
        "analyzed": "ninhāya sabba pāpakāni ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.6:21.2",
        "original": "Ajjhattaṁ bahiddhā ca sabbaloke;",
        "analyzed": "ajjhattam bahiddhā ca sabba loke ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.6:21.3",
        "original": "Devamanussesu kappiyesu,",
        "analyzed": "deva manussesu kappiyesu ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.6:21.4",
        "original": "Kappaṁ neti tamāhu nhātakoti.",
        "analyzed": "kappam neti tam āhu nhātakā iti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_3.6:22.1",
        "original": "Āguṁ na karoti kiñci loke,",
        "analyzed": "āgum na karoti kiñci loke ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.6:22.2",
        "original": "Sabbasaṁyoge visajja bandhanāni;",
        "analyzed": "sabba sayoge visajja bandhanāni ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.6:22.3",
        "original": "Sabbattha na sajjatī vimutto,",
        "analyzed": "sabbattha na sajjati vimuttaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.6:22.4",
        "original": "Nāgo tādi pavuccate tathattā”ti.",
        "analyzed": "pavuccate tathattā iti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_3.6:23.1",
        "original": "Atha kho sabhiyo paribbājako …pe… bhagavantaṁ uttariṁ pañhaṁ apucchi—",
        "analyzed": "atha kho sabhiyaḥ paribbājakaḥ pe bhagavantam uttarim pañham apucchi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_3.6:24.1",
        "original": "“Kaṁ khettajinaṁ vadanti buddhā,",
        "analyzed": "kam khettajinam vadanti buddhāḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.6:24.2",
        "original": "(iti sabhiyo)",
        "analyzed": "iti ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.6:24.3",
        "original": "Kusalaṁ kena kathañca paṇḍitoti;",
        "analyzed": "kusalam kena kathañca paṇḍita uti ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.6:24.4",
        "original": "Muni nāma kathaṁ pavuccati,",
        "analyzed": "muni nāma katham pavuccati ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.6:24.5",
        "original": "Puṭṭho me bhagavā byākarohi”.",
        "analyzed": "puṭṭhaḥ me bhagavā byākarohi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_3.6:25.1",
        "original": "“Khettāni viceyya kevalāni,",
        "analyzed": "khettāni viceyya kevalāni ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.6:25.2",
        "original": "(sabhiyāti bhagavā)",
        "analyzed": "sabhiyāti bhagavān ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.6:25.3",
        "original": "Dibbaṁ mānusakañca brahmakhettaṁ;",
        "analyzed": "dibbam mānusakañca brahmakhetta ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.6:25.4",
        "original": "Sabbakhettamūlabandhanā pamutto,",
        "analyzed": "sabba khetta mūla bandhanā pamuttaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.6:25.5",
        "original": "Khettajino tādi pavuccate tathattā.",
        "analyzed": "tathattā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_3.6:26.1",
        "original": "Kosāni viceyya kevalāni,",
        "analyzed": "kosāni viceyya kevalāni ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.6:26.2",
        "original": "Dibbaṁ mānusakañca brahmakosaṁ;",
        "analyzed": "dibbam mānusakañca brahmakosa ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.6:26.3",
        "original": "Sabbakosamūlabandhanā pamutto,",
        "analyzed": "sabba kosa mūla bandhanā pamutto ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.6:26.4",
        "original": "Kusalo tādi pavuccate tathattā.",
        "analyzed": "kusalaḥ ta ādi pavuccate tathattā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_3.6:27.1",
        "original": "Dubhayāni viceyya paṇḍarāni,",
        "analyzed": "dubhayāni viceyya paṇḍarāni ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.6:27.2",
        "original": "Ajjhattaṁ bahiddhā ca suddhipañño;",
        "analyzed": "ajjhattam bahiddhā ca suddhi pañño ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.6:27.3",
        "original": "Kaṇhaṁ sukkaṁ upātivatto,",
        "analyzed": "kaṇham sukkam upātivattaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.6:27.4",
        "original": "Paṇḍito tādi pavuccate tathattā.",
        "analyzed": "tathattā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_3.6:28.1",
        "original": "Asatañca satañca ñatvā dhammaṁ,",
        "analyzed": "a satañca satañca ñatvā dhamma ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.6:28.2",
        "original": "Ajjhattaṁ bahiddhā ca sabbaloke;",
        "analyzed": "ajjhattam bahiddhā ca sabba loke ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.6:28.3",
        "original": "Devamanussehi pūjanīyo,",
        "analyzed": "deva manussehi pūjanīyaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.6:28.4",
        "original": "Saṅgaṁ jālamaticca so munī”ti.",
        "analyzed": "saṅgam jāla matit ca saḥ muni iti ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_3.6:29.1",
        "original": "Atha kho sabhiyo paribbājako …pe… bhagavantaṁ uttariṁ pañhaṁ apucchi—",
        "analyzed": "atha kho sabhiyaḥ paribbājakaḥ pe bhagavantam uttarim pañham apucchi ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_3.6:30.1",
        "original": "“Kiṁpattinamāhu vedaguṁ,",
        "analyzed": "kipattinam āhuḥ vedagu ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.6:30.2",
        "original": "(iti sabhiyo)",
        "analyzed": "iti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.6:30.3",
        "original": "Anuviditaṁ kena kathañca viriyavāti;",
        "analyzed": "anuviditam kena kathañca ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.6:30.4",
        "original": "Ājāniyo kinti nāma hoti,",
        "analyzed": "ājāniyaḥ kinti nāma hoti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.6:30.5",
        "original": "Puṭṭho me bhagavā byākarohi”.",
        "analyzed": "puṭṭhaḥ me bhagavā byākarohi ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_snp_3.6:31.1",
        "original": "“Vedāni viceyya kevalāni,",
        "analyzed": "vedāni viceyya kevalāni ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.6:31.2",
        "original": "(sabhiyāti bhagavā)",
        "analyzed": "sabhiyāti bhagavān ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.6:31.3",
        "original": "Samaṇānaṁ yānidhatthi brāhmaṇānaṁ;",
        "analyzed": "samaṇānam brāhmaṇāna ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.6:31.4",
        "original": "Sabbavedanāsu vītarāgo,",
        "analyzed": "sabba vedanāsu vīta rāgaḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.6:31.5",
        "original": "Sabbaṁ vedamaticca vedagū so.",
        "analyzed": "sabbam vedam aticca vedagū saḥ ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_snp_3.6:32.1",
        "original": "Anuvicca papañcanāmarūpaṁ,",
        "analyzed": "anuvid ca papañca nāma rūpa ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.6:32.2",
        "original": "Ajjhattaṁ bahiddhā ca rogamūlaṁ;",
        "analyzed": "ajjhattam bahiddhā ca ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.6:32.3",
        "original": "Sabbarogamūlabandhanā pamutto,",
        "analyzed": "sabba roga mūla bandhanā pamutto ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.6:32.4",
        "original": "Anuvidito tādi pavuccate tathattā.",
        "analyzed": "anuviditaḥ tādi pavuccate tathattā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_snp_3.6:33.1",
        "original": "Virato idha sabbapāpakehi,",
        "analyzed": "virataḥ idha sabba pāpakehi ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.6:33.2",
        "original": "Nirayadukkhaṁ aticca viriyavāso;",
        "analyzed": "niraya dukkham aticca viriya vāso ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.6:33.3",
        "original": "So vīriyavā padhānavā,",
        "analyzed": "saḥ vīriyavā padhānavā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.6:33.4",
        "original": "Dhīro tādi pavuccate tathattā.",
        "analyzed": "tathattā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_snp_3.6:34.1",
        "original": "Yassassu lunāni bandhanāni,",
        "analyzed": "yassassu lunāni bandhanāni ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.6:34.2",
        "original": "Ajjhattaṁ bahiddhā ca saṅgamūlaṁ;",
        "analyzed": "ajjhattam bahiddhā ca saṅga mūla ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.6:34.3",
        "original": "Sabbasaṅgamūlabandhanā pamutto,",
        "analyzed": "sabba saṅga mūla bandhanā pamuttaḥ ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.6:34.4",
        "original": "Ājāniyo tādi pavuccate tathattā”ti.",
        "analyzed": "ājāniyaḥ tādi pavuccate tathattā ati ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_3.6:35.1",
        "original": "Atha kho sabhiyo paribbājako …pe… bhagavantaṁ uttariṁ pañhaṁ apucchi—",
        "analyzed": "atha kho sabhiyaḥ paribbājakaḥ pe bhagavantam uttarim pañham apucchi ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_3.6:36.1",
        "original": "“Kiṁpattinamāhu sottiyaṁ,",
        "analyzed": "kipattinam āhuḥ ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.6:36.2",
        "original": "(iti sabhiyo)",
        "analyzed": "iti ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.6:36.3",
        "original": "Ariyaṁ kena kathañca caraṇavāti;",
        "analyzed": "ariyam kena kathañca ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.6:36.4",
        "original": "Paribbājako kinti nāma hoti,",
        "analyzed": "paribbājakaḥ kinti nāma hoti ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.6:36.5",
        "original": "Puṭṭho me bhagavā byākarohi”.",
        "analyzed": "puṭṭhaḥ me bhagavā byākarohi ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_snp_3.6:37.1",
        "original": "“Sutvā sabbadhammaṁ abhiññāya loke,",
        "analyzed": "sutvā sabba dhammam abhiññāya loke ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.6:37.2",
        "original": "(sabhiyāti bhagavā)",
        "analyzed": "sabhiyāti bhagavān ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.6:37.3",
        "original": "Sāvajjānavajjaṁ yadatthi kiñci;",
        "analyzed": "sāvat ja anavajjam yat atthi kiñci ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.6:37.4",
        "original": "Abhibhuṁ akathaṅkathiṁ vimuttaṁ,",
        "analyzed": "abhibhum akathaṅkathim ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.6:37.5",
        "original": "Anighaṁ sabbadhimāhu sottiyoti.",
        "analyzed": "anigham sabbadhim āhu sottiyā iti ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_3.6:38.1",
        "original": "Chetvā āsavāni ālayāni,",
        "analyzed": "chetvā āsavāni ālayāni ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.6:38.2",
        "original": "Vidvā so na upeti gabbhaseyyaṁ;",
        "analyzed": "vidvā saḥ naḥ upeti gabbhaseyya ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.6:38.3",
        "original": "Saññaṁ tividhaṁ panujja paṅkaṁ,",
        "analyzed": "saññam tividham panujja paṅka ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.6:38.4",
        "original": "Kappaṁ neti tamāhu ariyoti.",
        "analyzed": "kappam na iti tam āhu ariyaḥ iti ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_3.6:39.1",
        "original": "Yo idha caraṇesu pattipatto,",
        "analyzed": "yaḥ idha caraṇesu patti pattaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.6:39.2",
        "original": "Kusalo sabbadā ājānāti dhammaṁ;",
        "analyzed": "kusalaḥ sabbadā ājānāti dhamma ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.6:39.3",
        "original": "Sabbattha na sajjati vimuttacitto,",
        "analyzed": "sabbattha na sajjati vimutta cittaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.6:39.4",
        "original": "Paṭighā yassa na santi caraṇavā so.",
        "analyzed": "paṭighāḥ yassa na santi caraṇavā so ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_snp_3.6:40.1",
        "original": "Dukkhavepakkaṁ yadatthi kammaṁ,",
        "analyzed": "dukkha vepakkam yat atthi kamma ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.6:40.2",
        "original": "Uddhamadho tiriyaṁ vāpi majjhe;",
        "analyzed": "uddham adhas tiriyam vā api majjhe ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.6:40.3",
        "original": "Paribbājayitvā pariññacārī,",
        "analyzed": "paribbājayitvā pariñña cārī ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.6:40.4",
        "original": "Māyaṁ mānamathopi lobhakodhaṁ;",
        "analyzed": "mā ayam lobha kodha ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.6:40.5",
        "original": "Pariyantamakāsi nāmarūpaṁ,",
        "analyzed": "pariyantam akāsi nāma rūpa ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.6:40.6",
        "original": "Taṁ paribbājakamāhu pattipattan”ti.",
        "analyzed": "tam paribbājakam āhu patti pattanti ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_snp_3.6:41.1_0",
        "original": "Atha kho sabhiyo paribbājako bhagavato bhāsitaṁ abhinanditvā anumoditvā attamano",
        "analyzed": "atha kho sabhiyaḥ paribbājakaḥ bhagavataḥ bhāsitam abhinanditvā anumoditvā atta manaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_3.6:41.1_1",
        "original": "pamudito udaggo pītisomanassajāto uṭṭhāyāsanā ekaṁsaṁ uttarāsaṅgaṁ karitvā yena bhagavā",
        "analyzed": "pamuditaḥ udaggaḥ pīti somanassa jāto uṭṭhāya āsanā ekasam uttarāsaṅgam karitvā yena bhagavā ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_3.6:41.1_2",
        "original": "tenañjaliṁ paṇāmetvā bhagavantaṁ sammukhā sāruppāhi gāthāhi abhitthavi:",
        "analyzed": "tena añjalim paṇāmetvā bhagavantam sammukhā sāruppāhi gāthāhi ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_snp_3.6:42.1",
        "original": "“Yāni ca tīṇi yāni ca saṭṭhi,",
        "analyzed": "yāni ca tīṇi yāni ca saṭṭhi ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.6:42.2",
        "original": "Samaṇappavādasitāni bhūripañña;",
        "analyzed": "samaṇa ppavāda sitāni bhūri pañña ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.6:42.3",
        "original": "Saññakkharasaññanissitāni,",
        "analyzed": "sañña kkhara sañña nissitāni ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.6:42.4",
        "original": "Osaraṇāni vineyya oghatamagā.",
        "analyzed": "osaraṇāni vineyyaḥ oghatama gā ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_snp_3.6:43.1",
        "original": "Antagūsi pāragū dukkhassa,",
        "analyzed": "antagū asi pāragū dukkhassa ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.6:43.2",
        "original": "Arahāsi sammāsambuddho khīṇāsavaṁ taṁ maññe;",
        "analyzed": "arahā asi sammāsambuddhaḥ khīṇa āsavam tam maññe ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.6:43.3",
        "original": "Jutimā mutimā pahūtapañño,",
        "analyzed": "jutimā mutimā pahūta pañño ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.6:43.4",
        "original": "Dukkhassantakara atāresi maṁ.",
        "analyzed": "atāresi ma ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_snp_3.6:44.1",
        "original": "Yaṁ me kaṅkhitamaññāsi,",
        "analyzed": "yam me kaṅkhitam aññāsi ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.6:44.2",
        "original": "Vicikicchā maṁ tārayi namo te;",
        "analyzed": "vicikiccha mama tārayi namaḥ te ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.6:44.3",
        "original": "Muni monapathesu pattipatta,",
        "analyzed": "muni mona pathesu patti patta ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.6:44.4",
        "original": "Akhila ādiccabandhu soratosi.",
        "analyzed": "akhilaḥ soratosi ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_snp_3.6:45.1",
        "original": "Yā me kaṅkhā pure āsi,",
        "analyzed": "yā me kaṅkhā pure āsi ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.6:45.2",
        "original": "Taṁ me byākāsi cakkhumā;",
        "analyzed": "tam me byākāsi cakkhumā ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.6:45.3",
        "original": "Addhā munīsi sambuddho,",
        "analyzed": "addhā munī isi sambuddhaḥ ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.6:45.4",
        "original": "Natthi nīvaraṇā tava.",
        "analyzed": "natthi nīvaraṇā tava ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_snp_3.6:46.1",
        "original": "Upāyāsā ca te sabbe,",
        "analyzed": "upāyāsā ca te sabbe ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_3.6:46.2",
        "original": "Viddhastā vinaḷīkatā;",
        "analyzed": "viddhastā vinaḍīkatā ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_3.6:46.3",
        "original": "Sītibhūto damappatto,",
        "analyzed": "sīti bhūtaḥ dama ppatto ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_3.6:46.4",
        "original": "Dhitimā saccanikkamo.",
        "analyzed": "dh timā sacca nikkamaḥ ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_snp_3.6:47.1",
        "original": "Tassa te nāganāgassa,",
        "analyzed": "tassa te nāga nāgassa ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_3.6:47.2",
        "original": "Mahāvīrassa bhāsato;",
        "analyzed": "mahāvīrassa bhāsato ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_3.6:47.3",
        "original": "Sabbe devānumodanti,",
        "analyzed": "sabbe devā anumodanti ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_3.6:47.4",
        "original": "Ubho nāradapabbatā.",
        "analyzed": "ubho nāradapabbatā ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_snp_3.6:48.1",
        "original": "Namo te purisājañña,",
        "analyzed": "namaḥ te purisa ajañña ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_3.6:48.2",
        "original": "Namo te purisuttama;",
        "analyzed": "namaḥ te purisuttama ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_3.6:48.3",
        "original": "Sadevakasmiṁ lokasmiṁ,",
        "analyzed": "sa devakasmin ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_3.6:48.4",
        "original": "Natthi te paṭipuggalo.",
        "analyzed": "na atthi te paṭipuggalo ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_snp_3.6:49.1",
        "original": "Tuvaṁ buddho tuvaṁ satthā,",
        "analyzed": "tuvam buddhaḥ tuvam satthā ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_3.6:49.2",
        "original": "Tuvaṁ mārābhibhū muni;",
        "analyzed": "tuvam māra abhibhū muni ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_3.6:49.3",
        "original": "Tuvaṁ anusaye chetvā,",
        "analyzed": "tuvam anusaye chetvā ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_3.6:49.4",
        "original": "Tiṇṇo tāresimaṁ pajaṁ.",
        "analyzed": "tiṇṇaḥ tāresimam paja ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_snp_3.6:50.1",
        "original": "Upadhī te samatikkantā,",
        "analyzed": "upadhī te samatikkantā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_3.6:50.2",
        "original": "Āsavā te padālitā;",
        "analyzed": "āsavā te padālitā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_3.6:50.3",
        "original": "Sīhosi anupādāno,",
        "analyzed": "sīhaḥ asi anupādānaḥ ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_3.6:50.4",
        "original": "Pahīnabhayabheravo.",
        "analyzed": "pahīna bhaya bheravaḥ ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_snp_3.6:51.1",
        "original": "Puṇḍarīkaṁ yathā vaggu,",
        "analyzed": "puṇḍarīkam yathā vaggu ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_snp_3.6:51.2",
        "original": "Toye na upalimpati;",
        "analyzed": "toye na upalimpati ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_snp_3.6:51.3",
        "original": "Evaṁ puññe ca pāpe ca,",
        "analyzed": "evam puññe ca pāpe ca ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_snp_3.6:51.4",
        "original": "Ubhaye tvaṁ na limpasi;",
        "analyzed": "ubhaye tvam na limpasi ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_snp_3.6:51.5",
        "original": "Pāde vīra pasārehi,",
        "analyzed": "pāde vīra pasārehi ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_snp_3.6:51.6",
        "original": "Sabhiyo vandati satthuno”ti.",
        "analyzed": "sabhiyaḥ vandati satthunoti ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_snp_3.6:52.1",
        "original": "Atha kho sabhiyo paribbājako bhagavato pādesu sirasā nipatitvā bhagavantaṁ etadavoca:",
        "analyzed": "atha kho sabhiyaḥ paribbājakaḥ bhagavataḥ pādesu sirasā nipatitvā bhagavantam etat avoca ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_snp_3.6:52.2",
        "original": "“abhikkantaṁ, bhante …pe…",
        "analyzed": "abhikkanta bhante pe ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_snp_3.6:52.3",
        "original": "esāhaṁ bhagavantaṁ saraṇaṁ gacchāmi dhammañca bhikkhusaṅghañca;",
        "analyzed": "esā aham bhagavantam saraṇam gacchāmi dhammañca bhikkhu saṅghañca ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_snp_3.6:52.4",
        "original": "labheyyāhaṁ, bhante, bhagavato santike pabbajjaṁ, labheyyaṁ upasampadan”ti.",
        "analyzed": "labheyya āha bhante bhagavato santike pabbajja labheyyam upasampadanti ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_snp_3.6:53.1_0",
        "original": "“Yo kho, sabhiya, aññatitthiyapubbo imasmiṁ dhammavinaye ākaṅkhati pabbajjaṁ, ākaṅkhati",
        "analyzed": "yaḥ kho sabhiya añña titthiya pubbaḥ imasmin dhamma vinaye ākaṅkhati pabbajje ākaṅkhati ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_snp_3.6:53.1_1",
        "original": "upasampadaṁ, so cattāro māse parivasati; catunnaṁ māsānaṁ accayena āraddhacittā bhikkhū",
        "analyzed": "upasampadā saḥ cattāraḥ māse parivasati catunnam māsānam accayena āraddha cittāḥ bhikkhū ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_snp_3.6:53.1_2",
        "original": "pabbājenti, upasampādenti bhikkhubhāvāya.",
        "analyzed": "pabbājenti upasampādenti bhikkhu bhāvāya ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_snp_3.6:53.2",
        "original": "Api ca mettha puggalavemattatā viditā”ti.",
        "analyzed": "api ca mā ittha puggala vematta tā viditā ati ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_snp_3.6:54.1_0",
        "original": "“Sace, bhante, aññatitthiyapubbā imasmiṁ dhammavinaye ākaṅkhantā pabbajjaṁ, ākaṅkhantā",
        "analyzed": "sace bhante añña titthiya pubbā imasmin dhamma vinaye ākaṅkhantā pabbajje ākaṅkhantā ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_snp_3.6:54.1_1",
        "original": "upasampadaṁ cattāro māse parivasanti, catunnaṁ māsānaṁ accayena āraddhacittā bhikkhū",
        "analyzed": "upasampadam cattāraḥ māse parivasanti catunnam māsānam accayena āraddha cittāḥ bhikkhū ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_snp_3.6:54.1_2",
        "original": "pabbājenti, upasampādenti bhikkhubhāvāya, ahaṁ cattāri vassāni parivasissāmi; catunnaṁ",
        "analyzed": "pabbājenti upasampādenti bhikkhu bhāvāya aham cattāri vassāni parivasissāmi ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_snp_3.6:54.1_3",
        "original": "vassānaṁ accayena āraddhacittā bhikkhū pabbājentu upasampādentu bhikkhubhāvāyā”ti.",
        "analyzed": "vassānaṃ accayena āraddha cittāḥ bhikkhū pabbājentu upasampādentu bhikkhu bhāvāya ati ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_snp_3.6:54.2",
        "original": "Alattha kho sabhiyo paribbājako bhagavato santike pabbajjaṁ alattha upasampadaṁ …pe…",
        "analyzed": "alattha kho sabhiyaḥ paribbājakaḥ bhagavato santike pabbajjam alattha upasampadam pe ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_snp_3.6:54.3",
        "original": "aññataro kho panāyasmā sabhiyo arahataṁ ahosīti.",
        "analyzed": "aññataraḥ kho pana ayasman sabhiyaḥ arahatam ahosi iti ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_snp_3.6:54.4",
        "original": "Sabhiyasuttaṁ chaṭṭhaṁ.",
        "analyzed": "sabhiya suttam chaṭṭha ",
        "folio": "54"
    }
]