        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_408:5.1",
        "original": "“Karaṇḍako kaliṅgānaṁ,",
        "analyzed": "karaṇḍako kaliṅgāna ",
        "folio": "5"
    },
