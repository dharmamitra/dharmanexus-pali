        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_476:1.1",
        "original": "“Idheva haṁsa nipata,",
        "analyzed": "idhā iva hasa nipata ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_476:1.2",
        "original": "piyaṁ me tava dassanaṁ;",
        "analyzed": "piyam me tava dassana ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_476:1.3",
        "original": "Issarosi anuppatto,",
        "analyzed": "issarā asi anuppatto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_476:1.4",
        "original": "yamidhatthi pavedaya”.",
        "analyzed": "yamidhatthi pavedaya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_476:2.1",
        "original": "“Savanena ekassa piyā bhavanti,",
        "analyzed": "savanena ekassa piyā bhavanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_476:2.2",
        "original": "Disvā panekassa viyeti chando;",
        "analyzed": "disvā pana ekassa viya iti chandaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_476:2.3",
        "original": "Disvā ca sutvā ca piyā bhavanti,",
        "analyzed": "disvā ca sutvā ca piyāḥ bhavanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_476:2.4",
        "original": "Kaccinnu me pīyasi dassanena.",
        "analyzed": "kaccit nu me pīyasi dassanena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_476:3.1",
        "original": "Savanena piyo mesi,",
        "analyzed": "savanena piyaḥ mā isi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_476:3.2",
        "original": "bhiyyo cāgamma dassanaṁ;",
        "analyzed": "bhiyyas ca āgamma dassana ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_476:3.3",
        "original": "Evaṁ piyadassano me,",
        "analyzed": "evam piya dassanaḥ me ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_476:3.4",
        "original": "vasa haṁsa mamantike”.",
        "analyzed": "vasa hasa mama antike ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_476:4.1",
        "original": "“Vaseyyāma tavāgāre,",
        "analyzed": "vaseyyāma tava āgāre ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_476:4.2",
        "original": "niccaṁ sakkatapūjitā;",
        "analyzed": "niccam sakkata pūjitā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_476:4.3",
        "original": "Matto ca ekadā vajje,",
        "analyzed": "matto ca ekadā vajje ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_476:4.4",
        "original": "‘haṁsarājaṁ pacantu me’”.",
        "analyzed": "hasarājam pacantu me ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_476:5.1",
        "original": "“Dhiratthu taṁ majjapānaṁ,",
        "analyzed": "dhiratthu tam majja pāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_476:5.2",
        "original": "yaṁ me piyataraṁ tayā;",
        "analyzed": "yam me piyataram tayā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_476:5.3",
        "original": "Na cāpi majjaṁ pissāmi,",
        "analyzed": "na ca api majjam pissāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_476:5.4",
        "original": "yāva me vacchasī ghare”.",
        "analyzed": "yāva me vacchasī ghare ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_476:6.1",
        "original": "“Suvijānaṁ siṅgālānaṁ,",
        "analyzed": "su vijānam siṅgālāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_476:6.2",
        "original": "sakuṇānañca vassitaṁ;",
        "analyzed": "sakuṇānam ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_476:6.3",
        "original": "Manussavassitaṁ rāja,",
        "analyzed": "rāja ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_476:6.4",
        "original": "dubbijānataraṁ tato.",
        "analyzed": "tatas ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_476:7.1",
        "original": "Api ce maññatī poso,",
        "analyzed": "api ce maññati poso ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_476:7.2",
        "original": "ñāti mitto sakhāti vā;",
        "analyzed": "vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_476:7.3",
        "original": "Yo pubbe sumano hutvā,",
        "analyzed": "yaḥ pubbe sumanaḥ hutvā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_476:7.4",
        "original": "pacchā sampajjate diso.",
        "analyzed": "pacchā sampajjate disaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_476:8.1",
        "original": "Yasmiṁ mano nivisati,",
        "analyzed": "yasmin manaḥ nivisati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_476:8.2",
        "original": "avidūre sahāpi so;",
        "analyzed": "avidūre saha api saḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_476:8.3",
        "original": "Santikepi hi so dūre,",
        "analyzed": "santike api hi saḥ dūre ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_476:8.4",
        "original": "yasmiṁ nāvisate mano.",
        "analyzed": "yasmin na āvisate manaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_476:9.1",
        "original": "Antopi so hoti pasannacitto,",
        "analyzed": "antar pi saḥ hoti pasanna cittaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_476:9.2",
        "original": "Pāraṁ samuddassa pasannacitto;",
        "analyzed": "pāram samuddassa pasanna cittaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_476:9.3",
        "original": "Antopi so hoti paduṭṭhacitto,",
        "analyzed": "antar api saḥ hoti paduṭṭha cittaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_476:9.4",
        "original": "Pāraṁ samuddassa paduṭṭhacitto.",
        "analyzed": "pāram samuddassa paduṭṭha cittaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_476:10.1",
        "original": "Saṁvasantā vivasanti,",
        "analyzed": "sa vasantā vivasanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_476:10.2",
        "original": "ye disā te rathesabha;",
        "analyzed": "ye disā te rathesabha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_476:10.3",
        "original": "Ārā santo saṁvasanti,",
        "analyzed": "ārā santaḥ savasanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_476:10.4",
        "original": "manasā raṭṭhavaḍḍhana.",
        "analyzed": "manasā raṭṭhavaḍḍhana ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_476:11.1",
        "original": "Aticiraṁ nivāsena,",
        "analyzed": "aticiram nivāsena ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_476:11.2",
        "original": "piyo bhavati appiyo;",
        "analyzed": "piyaḥ bhavati appiyaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_476:11.3",
        "original": "Āmanta kho taṁ gacchāma,",
        "analyzed": "āmanta khalu tam gacchāma ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_476:11.4",
        "original": "purā te homa appiyā”.",
        "analyzed": "purā te homa appiyā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_476:12.1",
        "original": "“Evañce yācamānānaṁ,",
        "analyzed": "evañce yācamānāna ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_476:12.2",
        "original": "Añjaliṁ nāvabujjhasi;",
        "analyzed": "añjalim na avabujjhasi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_476:12.3",
        "original": "Paricārakānaṁ sataṁ,",
        "analyzed": "paricārakānam sata ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_476:12.4",
        "original": "Vacanaṁ na karosi no;",
        "analyzed": "vacanam na karosi no ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_476:12.5",
        "original": "Evaṁ taṁ abhiyācāma,",
        "analyzed": "evam tam abhiyācāma ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_476:12.6",
        "original": "Puna kayirāsi pariyāyaṁ”.",
        "analyzed": "puna kayirā asi pariyāya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_476:13.1",
        "original": "“Evañce no viharataṁ,",
        "analyzed": "evañce naḥ viharata ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_476:13.2",
        "original": "antarāyo na hessati;",
        "analyzed": "antarāyaḥ na hessati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_476:13.3",
        "original": "Tuyhañcāpi mahārāja,",
        "analyzed": "tuyhan ca api mahā rāja ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_476:13.4",
        "original": "mayhañca raṭṭhavaḍḍhana;",
        "analyzed": "mayhan ca raṭṭhavaḍḍhana ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_476:13.5",
        "original": "Appeva nāma passemu,",
        "analyzed": "appā iva nāma passemu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_476:13.6",
        "original": "ahorattānamaccaye”ti.",
        "analyzed": "ahar rattā namaccayā iti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_476:14.1",
        "original": "Javanahaṁsajātakaṁ tatiyaṁ.",
        "analyzed": "javana hasa jātakam tatiya ",
        "folio": "14"
    }
