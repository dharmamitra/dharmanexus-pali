    {
        "segmentnr": "PA_iti_98:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_98:2.1",
        "original": "“Dvemāni, bhikkhave, dānāni—",
        "analyzed": "dve imāni bhikkhave dānāni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_98:2.2",
        "original": "āmisadānañca dhammadānañca.",
        "analyzed": "āmisa dānañca dhamma dānañca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_98:2.3",
        "original": "Etadaggaṁ, bhikkhave, imesaṁ dvinnaṁ dānānaṁ yadidaṁ dhammadānaṁ.",
        "analyzed": "etat agga bhikkhave imesam dvinnam dānānam yat idam dhamma dāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_98:3.1",
        "original": "Dveme, bhikkhave, saṁvibhāgā—",
        "analyzed": "dve me bhikkhave savibhāgā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_98:3.2",
        "original": "āmisasaṁvibhāgo ca dhammasaṁvibhāgo ca.",
        "analyzed": "āmisa savibhāgo ca dhamma savibhāgo ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_98:3.3",
        "original": "Etadaggaṁ, bhikkhave, imesaṁ dvinnaṁ saṁvibhāgānaṁ yadidaṁ dhammasaṁvibhāgo.",
        "analyzed": "etat agga bhikkhave imesam dvinnam savibhāgānam yat idam dhamma savibhāgaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_98:4.1",
        "original": "Dveme, bhikkhave, anuggahā—",
        "analyzed": "dve me bhikkhave anuggahā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_98:4.2",
        "original": "āmisānuggaho ca dhammānuggaho ca.",
        "analyzed": "āmisa anuggahaḥ ca dhamma anuggahaḥ ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_98:4.3",
        "original": "Etadaggaṁ, bhikkhave, imesaṁ dvinnaṁ anuggahānaṁ yadidaṁ dhammānuggaho”ti.",
        "analyzed": "etat agga bhikkhave imesam dvinnam anuggahānam yat idam dhamma anuggaho ti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_98:4.4",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_98:4.5",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_98:5.1",
        "original": "“Yamāhu dānaṁ paramaṁ anuttaraṁ,",
        "analyzed": "yam āhu dānam paramam anuttara ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_98:5.2",
        "original": "Yaṁ saṁvibhāgaṁ bhagavā avaṇṇayi;",
        "analyzed": "yam savibhāgam bhagavān avaṇṇayi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_98:5.3",
        "original": "Aggamhi khettamhi pasannacitto,",
        "analyzed": "aggamhi khettamhi pasanna cittaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_98:5.4",
        "original": "Viññū pajānaṁ ko na yajetha kāle.",
        "analyzed": "viññū pajānam kaḥ na yajetha kāle ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_98:6.1",
        "original": "Ye ceva bhāsanti suṇanti cūbhayaṁ,",
        "analyzed": "ye ca iva bhāsanti suṇanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_98:6.2",
        "original": "Pasannacittā sugatassa sāsane;",
        "analyzed": "pasanna cittā sugatassa sāsane ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_98:6.3",
        "original": "Tesaṁ so attho paramo visujjhati,",
        "analyzed": "tesam so atthaḥ paramaḥ visujjhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_98:6.4",
        "original": "Ye appamattā sugatassa sāsane”ti.",
        "analyzed": "ye appamattā sugatassa sāsane iti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_98:7.1",
        "original": "Ayampi attho vutto bhagavatā, iti me sutanti.",
        "analyzed": "ayam pi atthaḥ vuttaḥ bhagavatā iti me sutam ti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_98:7.2",
        "original": "Navamaṁ.",
        "analyzed": "navama ",
        "folio": "7"
    }
]