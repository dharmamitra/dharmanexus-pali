        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.37",
        "original": "Thullanandā bhikkhunī mahāmattānaṁ etamatthaṁ ārocesi.",
        "analyzed": "thullanandā bhikkhunī mahāmattānam etam attham ārocesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.38",
        "original": "Mahāmattā taṁ purisaṁ daṇḍāpesuṁ.",
        "analyzed": "mahāmattāḥ tam purisam daṇḍāpesu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.39",
        "original": "Atha kho so puriso daṇḍito bhikkhunupassayassa avidūre ājīvakaseyyaṁ kārāpetvā ājīvake uyyojesi—",
        "analyzed": "atha kho saḥ purisaḥ daṇḍitaḥ bhikkhunu passayassa avidūre ājīvaka seyyam kārāpetvā ājīvake uyyojesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.40",
        "original": "“etā bhikkhuniyo accāvadathā”ti.",
        "analyzed": "etāḥ bhikkhuniyaḥ accāvadatha ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_ss1:1.41",
        "original": "Thullanandā bhikkhunī mahāmattānaṁ etamatthaṁ ārocesi.",
