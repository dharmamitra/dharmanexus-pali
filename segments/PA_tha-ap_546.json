        "original": "Pabhāhi anurañjanto,",
        "analyzed": "pabhāhi anurañjantaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_tha-ap_546:4.2",
        "original": "loke lokantagū jino;",
        "analyzed": "loke lokantagū jinau ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_tha-ap_546:4.3",
        "original": "Vacanena vibodhento,",
        "analyzed": "vacanena ",
