    },
    {
        "segmentnr": "PA_thag_16.1:0.4",
        "original": "Adhimuttattheragāthā",
        "analyzed": "adhimuttattheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_16.1:1.1",
        "original": "“Yaññatthaṁ vā dhanatthaṁ vā,",
        "analyzed": "yañña ttham vā dhana ttham vā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.1:1.2",
        "original": "ye hanāma mayaṁ pure;",
        "analyzed": "ye hanāma mayam pure ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.1:1.3",
        "original": "Avasesaṁ bhayaṁ hoti,",
        "analyzed": "avasesam bhayam hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.1:1.4",
        "original": "vedhanti vilapanti ca.",
        "analyzed": "vedhanti vilapanti ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.1:2.1",
        "original": "Tassa te natthi bhītattaṁ,",
        "analyzed": "tassa te natthi bhītatta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.1:2.2",
        "original": "bhiyyo vaṇṇo pasīdati;",
        "analyzed": "bhiyyas vaṇṇaḥ pasīdati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.1:2.3",
        "original": "Kasmā na paridevesi,",
        "analyzed": "kasmā na paridevesi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.1:2.4",
        "original": "evarūpe mahabbhaye”.",
        "analyzed": "evarūpe mahat bhaye ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_16.1:3.1",
        "original": "“Natthi cetasikaṁ dukkhaṁ,",
        "analyzed": "natthi cetasikam dukkha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.1:3.2",
        "original": "anapekkhassa gāmaṇi;",
        "analyzed": "anapekkhassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.1:3.3",
        "original": "Atikkantā bhayā sabbe,",
        "analyzed": "atikkantā bhayā sabbe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.1:3.4",
        "original": "khīṇasaṁyojanassa ve.",
        "analyzed": "khīṇa sayojanassa ve ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_16.1:4.1",
        "original": "Khīṇāya bhavanettiyā,",
        "analyzed": "khīṇāya bhavanettiyā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.1:4.2",
        "original": "diṭṭhe dhamme yathātathe;",
        "analyzed": "diṭṭhe dhamme yathātathe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.1:4.3",
        "original": "Na bhayaṁ maraṇe hoti,",
        "analyzed": "na bhayam maraṇe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.1:4.4",
        "original": "bhāranikkhepane yathā.",
        "analyzed": "bhāra nikkhepane yathā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_16.1:5.1",
        "original": "Suciṇṇaṁ brahmacariyaṁ me,",
        "analyzed": "su ciṇṇam brahmacariyam me ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.1:5.2",
        "original": "maggo cāpi subhāvito;",
        "analyzed": "maggo ca api su bhāvito ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.1:5.3",
        "original": "Maraṇe me bhayaṁ natthi,",
        "analyzed": "maraṇe me bhayam na atthi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.1:5.4",
        "original": "rogānamiva saṅkhaye.",
        "analyzed": "rogānam iva saṅkhaye ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_16.1:6.1",
        "original": "Suciṇṇaṁ brahmacariyaṁ me,",
        "analyzed": "su ciṇṇam brahmacariyam me ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.1:6.2",
        "original": "maggo cāpi subhāvito;",
        "analyzed": "maggo ca api su bhāvito ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.1:6.3",
        "original": "Nirassādā bhavā diṭṭhā,",
        "analyzed": "nirassādā bhavā diṭṭhā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.1:6.4",
        "original": "visaṁ pitvāva chaḍḍitaṁ.",
        "analyzed": "visam pitvā ava chaḍḍita ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_16.1:7.1",
        "original": "Pāragū anupādāno,",
        "analyzed": "pāragū an upādānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.1:7.2",
        "original": "katakicco anāsavo;",
        "analyzed": "kata kiccaḥ an āsavaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.1:7.3",
        "original": "Tuṭṭho āyukkhayā hoti,",
        "analyzed": "tuṭṭhaḥ āyu khayā hoti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.1:7.4",
        "original": "mutto āghātanā yathā.",
        "analyzed": "āghātanā yathā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_16.1:8.1",
        "original": "Uttamaṁ dhammataṁ patto,",
        "analyzed": "uttamam dhammatam patto ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.1:8.2",
        "original": "sabbaloke anatthiko;",
        "analyzed": "sabba loke anatthiko ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.1:8.3",
        "original": "Ādittāva gharā mutto,",
        "analyzed": "ādittā ava gharā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.1:8.4",
        "original": "maraṇasmiṁ na socati.",
        "analyzed": "na socati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.1:9.1",
        "original": "Yadatthi saṅgataṁ kiñci,",
        "analyzed": "yat atthi saṅgatam kiñci ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.1:9.2",
        "original": "bhavo vā yattha labbhati;",
        "analyzed": "bhavaḥ vā yattha labbhati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.1:9.3",
        "original": "Sabbaṁ anissaraṁ etaṁ,",
        "analyzed": "sabbam a nissaram eta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.1:9.4",
        "original": "iti vuttaṁ mahesinā.",
        "analyzed": "iti vuttam mahesinā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.1:10.1",
        "original": "Yo taṁ tathā pajānāti,",
        "analyzed": "yaḥ tam tathā pajānāti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.1:10.2",
        "original": "yathā buddhena desitaṁ;",
        "analyzed": "yathā buddhena desita ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.1:10.3",
        "original": "Na gaṇhāti bhavaṁ kiñci,",
        "analyzed": "na gaṇhāti bhavam kiñci ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.1:10.4",
        "original": "sutattaṁva ayoguḷaṁ.",
        "analyzed": "sutattava ayoguḷa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.1:11.1",
        "original": "Na me hoti ‘ahosin’ti,",
        "analyzed": "na me ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.1:11.2",
        "original": "‘bhavissan’ti na hoti me;",
        "analyzed": "bhavissanti na hoti me ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.1:11.3",
        "original": "Saṅkhārā vigamissanti,",
        "analyzed": "saṅkhārāḥ vigamissanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.1:11.4",
        "original": "tattha kā paridevanā.",
        "analyzed": "kā paridevanā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.1:12.1",
        "original": "Suddhaṁ dhammasamuppādaṁ,",
        "analyzed": "suddham dhammasamuppāda ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.1:12.2",
        "original": "Suddhaṁ saṅkhārasantatiṁ;",
        "analyzed": "suddham saṅkhāra santati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.1:12.3",
        "original": "Passantassa yathābhūtaṁ,",
        "analyzed": "passantassa yathābhūta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.1:12.4",
        "original": "Na bhayaṁ hoti gāmaṇi.",
        "analyzed": "na bhayam gāmaṇi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.1:13.1",
        "original": "Tiṇakaṭṭhasamaṁ lokaṁ,",
        "analyzed": "tiṇa kaṭṭha samam loka ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.1:13.2",
        "original": "yadā paññāya passati;",
        "analyzed": "yadā paññāya passati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.1:13.3",
        "original": "Mamattaṁ so asaṁvindaṁ,",
        "analyzed": "mamattam saḥ asavinda ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.1:13.4",
        "original": "‘natthi me’ti na socati.",
        "analyzed": "natthi me iti na socati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.1:14.1",
        "original": "Ukkaṇṭhāmi sarīrena,",
        "analyzed": "ukkaṇṭhāmi sarīrena ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.1:14.2",
        "original": "bhavenamhi anatthiko;",
        "analyzed": "bhavenamhi anatthikaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.1:14.3",
        "original": "Soyaṁ bhijjissati kāyo,",
        "analyzed": "saḥ ayam bhijjissati kāyaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.1:14.4",
        "original": "añño ca na bhavissati.",
        "analyzed": "aññaḥ ca na bhavissati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thag_16.1:15.1",
        "original": "Yaṁ vo kiccaṁ sarīrena,",
        "analyzed": "yam vaḥ kiccam sarīrena ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.1:15.2",
        "original": "taṁ karotha yadicchatha;",
        "analyzed": "tam karotha yat icchatha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.1:15.3",
        "original": "Na me tappaccayā tattha,",
        "analyzed": "na me tad paccayā tattha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.1:15.4",
        "original": "doso pemañca hehiti”.",
        "analyzed": "hehiti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thag_16.1:16.1",
        "original": "Tassa taṁ vacanaṁ sutvā,",
        "analyzed": "tassa tam vacanam sutvā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.1:16.2",
        "original": "abbhutaṁ lomahaṁsanaṁ;",
        "analyzed": "abbhutam loma hasana ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.1:16.3",
        "original": "Satthāni nikkhipitvāna,",
        "analyzed": "satthāni nikkhipitvāna ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.1:16.4",
        "original": "māṇavā etadabravuṁ.",
        "analyzed": "māṇavāḥ etat abravu ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thag_16.1:17.1",
        "original": "“Kiṁ bhadante karitvāna,",
        "analyzed": "kim bhadante karitvāna ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.1:17.2",
        "original": "ko vā ācariyo tava;",
        "analyzed": "kaḥ vai tava ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.1:17.3",
        "original": "Kassa sāsanamāgamma,",
        "analyzed": "kassa sāsanam āgamma ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.1:17.4",
        "original": "labbhate taṁ asokatā”.",
        "analyzed": "labbhate tam asoka tā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thag_16.1:18.1",
        "original": "“Sabbaññū sabbadassāvī,",
        "analyzed": "sabbaññū sabba dassāvī ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_16.1:18.2",
        "original": "jino ācariyo mama;",
        "analyzed": "jinaḥ ācariyaḥ mama ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thag_16.1:18.3",
        "original": "Mahākāruṇiko satthā,",
        "analyzed": "mahā kāruṇikaḥ satthā ",
        "folio": "18"
    },
