        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.2",
        "original": "Tena kho pana samayena bhikkhuniyo anuvassaṁ vuṭṭhāpenti, upassayo na sammati.",
        "analyzed": "tena kho pana samayena bhikkhuniyaḥ anuvassam vuṭṭhāpenti upassayaḥ na sammati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.3",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.4",
        "original": "“kathañhi nāma bhikkhuniyo anuvassaṁ vuṭṭhāpessanti, upassayo na sammatī”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ anuvassam vuṭṭhāpessanti upassayaḥ na ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.5",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.6",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.7",
        "original": "“kathañhi nāma bhikkhuniyo anuvassaṁ vuṭṭhāpessantī”ti …pe…",
        "analyzed": "katham hi nāma bhikkhuniyaḥ anuvassam vuṭṭhāpessanti iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.8",
        "original": "“saccaṁ kira, bhikkhave, bhikkhuniyo anuvassaṁ vuṭṭhāpentī”ti?",
        "analyzed": "saccam kira bhikkhave bhikkhuniyaḥ anuvassam vuṭṭhāpenti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.9",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.10",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.11",
        "original": "kathañhi nāma, bhikkhave, bhikkhuniyo anuvassaṁ vuṭṭhāpessanti.",
        "analyzed": "katham hi nāma bhikkhave bhikkhuniyaḥ anuvassam vuṭṭhāpessanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.12",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
        "analyzed": "na ita bhikkhave appasannānam vā pasādāya pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc82:1.13",
        "original": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
