        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_11.1:7.3",
        "original": "Saṅkappaṁ nābhijānāmi,",
        "analyzed": "saṅkappam na abhijānāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_11.1:7.4",
        "original": "anariyaṁ dosasaṁhitaṁ.",
        "analyzed": "anariyam dosa sahita ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_11.1:8.1",
        "original": "Pariciṇṇo mayā satthā,",
        "analyzed": "pariciṇṇaḥ mayā satthā ",
        "folio": "8"
    },
