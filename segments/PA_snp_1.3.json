        "original": "Daḷhanikkamo thāmabalūpapanno,",
        "analyzed": "daḷha nikkamaḥ thāma bala upapannaḥ ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_1.3:34.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_snp_1.3:35.1",
        "original": "“Paṭisallānaṁ jhānamariñcamāno,",
        "analyzed": "paṭisallānam jhānam ariñcamāno ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_1.3:35.2",
        "original": "Dhammesu niccaṁ anudhammacārī;",
        "analyzed": "dhammesu niccam anudhamma cārī ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_snp_1.3:35.3",
