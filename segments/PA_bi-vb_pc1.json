        "analyzed": "māgadhakam vuccati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc1:2.1.7",
        "original": "“Khādissāmīti paṭiggaṇhā”ti, āpatti dukkaṭassa.",
        "analyzed": "khādissāmi iti paṭiggaṇhāti āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc1:2.1.8",
        "original": "Ajjhohāre ajjhohāre āpatti pācittiyassa.",
        "analyzed": "ajjhohāre ajjhohāre āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc1:2.2.1",
        "original": "Lasuṇe lasuṇasaññā khādati, āpatti pācittiyassa.",
        "analyzed": "lasuṇe lasuṇa saññā khādati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc1:2.2.2",
        "original": "Lasuṇe vematikā khādati, āpatti pācittiyassa.",
        "analyzed": "lasuṇe vematikā khādati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc1:2.2.3",
