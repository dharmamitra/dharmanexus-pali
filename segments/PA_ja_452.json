    },
    {
        "segmentnr": "PA_ja_452:0.4",
        "original": "14. Bhūripaññajātaka",
        "analyzed": "bhūri pañña jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_452:1.1",
        "original": "“Saccaṁ kira tvaṁ api bhūripañña,",
        "analyzed": "saccam kira tvam api bhūri pañña ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_452:1.2",
        "original": "Yā tādisī sīri dhitī matī ca;",
        "analyzed": "yā tādisī sīri dhitī matī ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_452:1.3",
        "original": "Na tāyatebhāvavasūpanitaṁ,",
        "analyzed": "na ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_452:1.4",
        "original": "Yo yavakaṁ bhuñjasi appasūpaṁ”.",
        "analyzed": "yaḥ yavakam bhuñjasi appasūpa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_452:2.1",
        "original": "“Sukhaṁ dukkhena paripācayanto,",
        "analyzed": "sukham dukkhena paripācayantaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_452:2.2",
        "original": "Kālā kālaṁ vicinaṁ chandachanno;",
        "analyzed": "kālā kālam vicinam chanda channo ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_452:2.3",
        "original": "Atthassa dvārāni avāpuranto,",
        "analyzed": "atthassa dvārāni avāpurantaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_452:2.4",
        "original": "Tenāhaṁ tussāmi yavodanena.",
        "analyzed": "tena aham tussāmi yavodanena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_452:3.1",
        "original": "Kālañca ñatvā abhijīhanāya,",
        "analyzed": "kālañca ñatvā abhijīhanāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_452:3.2",
        "original": "Mantehi atthaṁ paripācayitvā;",
        "analyzed": "mantehi attham paripācayitvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_452:3.3",
        "original": "Vijambhissaṁ sīhavijambhitāni,",
        "analyzed": "vijambhissam sīha vijambhitāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_452:3.4",
        "original": "Tāyiddhiyā dakkhasi maṁ punāpi”.",
        "analyzed": "dakkhasi mam puna api ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_452:4.1",
        "original": "“Sukhīpi heke na karonti pāpaṁ,",
        "analyzed": "sukhīpi ha ike na karonti pāpa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_452:4.2",
        "original": "Avaṇṇasaṁsaggabhayā puneke;",
        "analyzed": "avaṇṇa sa sagga bhayā punar eke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_452:4.3",
        "original": "Pahū samāno vipulatthacintī,",
        "analyzed": "samānaḥ vipulattha cintī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_452:4.4",
        "original": "Kiṅkāraṇā me na karosi dukkhaṁ”.",
        "analyzed": "kiṅkāraṇā me na karosi dukkha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_452:5.1",
        "original": "“Na paṇḍitā attasukhassa hetu,",
        "analyzed": "na paṇḍitāḥ atta sukhassa hetu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_452:5.2",
        "original": "Pāpāni kammāni samācaranti;",
        "analyzed": "pāpāni kammāni samācaranti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_452:5.3",
        "original": "Dukkhena phuṭṭhā khalitāpi santā,",
        "analyzed": "dukkhena phuṭṭhā khalitā api santā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_452:5.4",
        "original": "Chandā ca dosā na jahanti dhammaṁ”.",
        "analyzed": "chandā ca dosā na jahanti dhamma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_452:6.1",
        "original": "“Yena kenaci vaṇṇena,",
        "analyzed": "yena kenacid vaṇṇena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_452:6.2",
        "original": "mudunā dāruṇena vā;",
        "analyzed": "mudunā dāruṇena vā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_452:6.3",
        "original": "Uddhare dīnamattānaṁ,",
        "analyzed": "uddhare ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_452:6.4",
        "original": "pacchā dhammaṁ samācare”.",
        "analyzed": "pacchā dhammam samācare ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_452:7.1",
        "original": "“Yassa rukkhassa chāyāya,",
        "analyzed": "yassa rukkhassa chāyāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_452:7.2",
        "original": "nisīdeyya sayeyya vā;",
        "analyzed": "nisīdeyya sayeyya vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_452:7.3",
        "original": "Na tassa sākhaṁ bhañjeyya,",
        "analyzed": "na tassa sākham bhañjeyya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_452:7.4",
        "original": "mittadubbho hi pāpako.",
        "analyzed": "mitta dubbhaḥ hi pāpakaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_452:8.1",
        "original": "Yassāpi dhammaṁ puriso vijaññā,",
        "analyzed": "yassa api dhammam puriso ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_452:8.2",
        "original": "Ye cassa kaṅkhaṁ vinayanti santo;",
        "analyzed": "ye ca assa kaṅkham vinayanti santaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_452:8.3",
        "original": "Taṁ hissa dīpañca parāyanañca,",
        "analyzed": "tam hissa dīpam ca parāyanam ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_452:8.4",
        "original": "Na tena mettiṁ jarayetha pañño”.",
        "analyzed": "na tena mettim jarayetha pañño ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_452:9.1",
        "original": "“Alaso gihī kāmabhogī na sādhu,",
        "analyzed": "alasaḥ gihī kāma bhogī na sādhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_452:9.2",
        "original": "Asaññato pabbajito na sādhu;",
        "analyzed": "asaññataḥ pabbajitaḥ na sādhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_452:9.3",
        "original": "Rājā na sādhu anisammakārī,",
        "analyzed": "rājā na sādhu anisammakārī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_452:9.4",
        "original": "Yo paṇḍito kodhano taṁ na sādhu.",
        "analyzed": "yaḥ paṇḍitaḥ kodhanaḥ tam na sādhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_452:10.1",
        "original": "Nisamma khattiyo kayirā,",
        "analyzed": "nisamma khattiyaḥ kayirā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_452:10.2",
        "original": "nānisamma disampati;",
        "analyzed": "disampati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_452:10.3",
        "original": "Nisammakārino rāja,",
        "analyzed": "nisamma kārinaḥ rāja ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_452:10.4",
        "original": "yaso kitti ca vaḍḍhatī”ti.",
        "analyzed": "yasaḥ kitti ca vaḍḍhati iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_452:11.1",
        "original": "Bhūripaññajātakaṁ cuddasamaṁ.",
        "analyzed": "bhūri pañña jātakam ",
        "folio": "11"
    }
]