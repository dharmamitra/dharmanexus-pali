[
    {
        "segmentnr": "PA_thig_14.1:0.1",
        "original": "Therīgāthā 14.1",
        "analyzed": "therīgāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_14.1:0.2",
        "original": "Tiṁsanipāta",
        "analyzed": "ti sa nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_14.1:0.3",
        "original": "Subhājīvakambavanikātherīgāthā",
        "analyzed": "subhā jīva kamba vanikā therī gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_14.1:1.1",
        "original": "Jīvakambavanaṁ rammaṁ,",
        "analyzed": "jīvakambavanam ramma ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_14.1:1.2",
        "original": "Gacchantiṁ bhikkhuniṁ subhaṁ;",
        "analyzed": "gacchantim bhikkhunim subha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_14.1:1.3",
        "original": "Dhuttako sannivāresi,",
        "analyzed": "dhuttakaḥ sannivāresi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_14.1:1.4",
        "original": "Tamenaṁ abravī subhā.",
        "analyzed": "tam enam abravīt subhā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_14.1:2.1",
        "original": "“Kiṁ te aparādhitaṁ mayā,",
        "analyzed": "kim te aparādhitam mayā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_14.1:2.2",
        "original": "Yaṁ maṁ ovariyāna tiṭṭhasi;",
        "analyzed": "yam mama ovariyāna tiṭṭhasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_14.1:2.3",
        "original": "Na hi pabbajitāya āvuso,",
        "analyzed": "na hi pabbajitāya āvuso ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_14.1:2.4",
        "original": "Puriso samphusanāya kappati.",
        "analyzed": "purisaḥ samphusanāya kappati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_14.1:3.1",
        "original": "Garuke mama satthusāsane,",
        "analyzed": "garuke mama satthu sāsane ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_14.1:3.2",
        "original": "Yā sikkhā sugatena desitā;",
        "analyzed": "yā sikkhā sugatena desitā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_14.1:3.3",
        "original": "Parisuddhapadaṁ anaṅgaṇaṁ,",
        "analyzed": "parisuddha padam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_14.1:3.4",
        "original": "Kiṁ maṁ ovariyāna tiṭṭhasi.",
        "analyzed": "kim mama ovariyāna tiṭṭhasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_14.1:4.1",
        "original": "Āvilacitto anāvilaṁ,",
        "analyzed": "āvila cittaḥ anāvila ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_14.1:4.2",
        "original": "Sarajo vītarajaṁ anaṅgaṇaṁ;",
        "analyzed": "sa rajaḥ vīta rajam anaṅgaṇa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_14.1:4.3",
        "original": "Sabbattha vimuttamānasaṁ,",
        "analyzed": "sabbattha vimutta mānasa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_14.1:4.4",
        "original": "Kiṁ maṁ ovariyāna tiṭṭhasi”.",
        "analyzed": "kim mama ovariyāna tiṭṭhasi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_14.1:5.1",
        "original": "“Daharā ca apāpikā casi,",
        "analyzed": "daharā ca a pāpikā ca si ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_14.1:5.2",
        "original": "Kiṁ te pabbajjā karissati;",
        "analyzed": "kim te pabbajjā karissati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_14.1:5.3",
        "original": "Nikkhipa kāsāyacīvaraṁ,",
        "analyzed": "nikkhipa kāsāya cīvara ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_14.1:5.4",
        "original": "Ehi ramāma supupphite vane.",
        "analyzed": "ehi ramāma su pupphite vane ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_14.1:6.1",
        "original": "Madhurañca pavanti sabbaso,",
        "analyzed": "madhuram ca pavanti sabbaso ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_14.1:6.2",
        "original": "Kusumarajena samuṭṭhitā dumā;",
        "analyzed": "kusuma rajena samuṭṭhitā dumā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_14.1:6.3",
        "original": "Paṭhamavasanto sukho utu,",
        "analyzed": "sukhaḥ utu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_14.1:6.4",
        "original": "Ehi ramāma supupphite vane.",
        "analyzed": "ehi ramāma su pupphite vane ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_14.1:7.1",
        "original": "Kusumitasikharā ca pādapā,",
        "analyzed": "kusumita sikharā ca pādapā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_14.1:7.2",
        "original": "Abhigajjantiva māluteritā;",
        "analyzed": "abhigajjantī iva mālutā īritā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_14.1:7.3",
        "original": "Kā tuyhaṁ rati bhavissati,",
        "analyzed": "kā tuyham rati bhavissati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_14.1:7.4",
        "original": "Yadi ekā vanamogahissasi.",
        "analyzed": "yadi ekā vanamogahissasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_14.1:8.1",
        "original": "Vāḷamigasaṅghasevitaṁ,",
        "analyzed": "vāḷa miga saṅgha sevita ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_14.1:8.2",
        "original": "Kuñjaramattakareṇuloḷitaṁ;",
        "analyzed": "kuñjara matta kareṇu loḷita ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_14.1:8.3",
        "original": "Asahāyikā gantumicchasi,",
        "analyzed": "asahāyikā gantum icchasi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_14.1:8.4",
        "original": "Rahitaṁ bhiṁsanakaṁ mahāvanaṁ.",
        "analyzed": "rahitam bhisanakam mahā vana ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_14.1:9.1",
        "original": "Tapanīyakatāva dhītikā,",
        "analyzed": "dhītikā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_14.1:9.2",
        "original": "Vicarasi cittalateva accharā;",
        "analyzed": "vicarasi cittalatā iva accharā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_14.1:9.3",
        "original": "Kāsikasukhumehi vaggubhi,",
        "analyzed": "kāsika su khumehi vaggubhi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_14.1:9.4",
        "original": "Sobhasī suvasanehi nūpame.",
        "analyzed": "sobhasī su vasanā ihi nūpame ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_14.1:10.1",
        "original": "Ahaṁ tava vasānugo siyaṁ,",
        "analyzed": "aham tava vasā anugaḥ siya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_14.1:10.2",
        "original": "Yadi viharemase kānanantare;",
        "analyzed": "yadi viharemase kānanantare ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_14.1:10.3",
        "original": "Na hi matthi tayā piyattaro,",
        "analyzed": "na hi matthi tayā piyattaraḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_14.1:10.4",
        "original": "Pāṇo kinnarimandalocane.",
        "analyzed": "pāṇo kinnari manda locane ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_14.1:11.1",
        "original": "Yadi me vacanaṁ karissasi,",
        "analyzed": "yadi me vacanam karissasi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_14.1:11.2",
        "original": "Sukhitā ehi agāramāvasa;",
        "analyzed": "sukhitā ehi agāram āvasa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_14.1:11.3",
        "original": "Pāsādanivātavāsinī,",
        "analyzed": "       ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_14.1:11.4",
        "original": "Parikammaṁ te karontu nāriyo.",
        "analyzed": "parikammam te karontu nāriyaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_14.1:12.1",
        "original": "Kāsikasukhumāni dhāraya,",
        "analyzed": "dhāraya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_14.1:12.2",
        "original": "Abhiropehi ca mālavaṇṇakaṁ;",
        "analyzed": "abhiropehi ca mālavaṇṇaka ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_14.1:12.3",
        "original": "Kañcanamaṇimuttakaṁ bahuṁ,",
        "analyzed": "kañcana maṇi muttakam bahu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_14.1:12.4",
        "original": "Vividhaṁ ābharaṇaṁ karomi te.",
        "analyzed": "vividham ābharaṇam karomi te ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_14.1:13.1",
        "original": "Sudhotarajapacchadaṁ subhaṁ,",
        "analyzed": "subha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_14.1:13.2",
        "original": "Gonakatūlikasanthataṁ navaṁ;",
        "analyzed": "gonaka tūlika santhatam nava ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_14.1:13.3",
        "original": "Abhiruha sayanaṁ mahārahaṁ,",
        "analyzed": "abhiruha sayanam mahāraha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_14.1:13.4",
        "original": "Candanamaṇḍitasāragandhikaṁ.",
        "analyzed": "          ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_14.1:14.1",
        "original": "Uppalaṁ cudakā samuggataṁ,",
        "analyzed": "uppalam ca udakā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_14.1:14.2",
        "original": "Yathā taṁ amanussasevitaṁ;",
        "analyzed": "yathā tam amanuḥ sasevita ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_14.1:14.3",
        "original": "Evaṁ tvaṁ brahmacārinī,",
        "analyzed": "evam tvam brahmacārinī ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_14.1:14.4",
        "original": "Sakesaṅgesu jaraṁ gamissasi”.",
        "analyzed": "sa kesaṅgesu jaram gamissasi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_thig_14.1:15.1",
        "original": "“Kiṁ te idha sārasammataṁ,",
        "analyzed": "kim te sārasammata ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_14.1:15.2",
        "original": "Kuṇapapūramhi susānavaḍḍhane;",
        "analyzed": "kuṇapa pūramhi su sāna vaḍḍhane ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_14.1:15.3",
        "original": "Bhedanadhamme kaḷevare,",
        "analyzed": "bhedana dhamme kaḷevare ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_14.1:15.4",
        "original": "Yaṁ disvā vimano udikkhasi”.",
        "analyzed": "yam disvā vimanaḥ udikkhasi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_thig_14.1:16.1",
        "original": "“Akkhīni ca tūriyāriva,",
        "analyzed": "akkhīni ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_14.1:16.2",
        "original": "Kinnariyāriva pabbatantare;",
        "analyzed": "kinnariyāri iva pabbatantare ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_14.1:16.3",
        "original": "Tava me nayanāni dakkhiya,",
        "analyzed": "tava me nayanāni dakkhiya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_14.1:16.4",
        "original": "Bhiyyo kāmaratī pavaḍḍhati.",
        "analyzed": "bhiyyas kāma ratī pavaḍḍhati ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_thig_14.1:17.1",
        "original": "Uppalasikharopamāni te,",
        "analyzed": "uppala sikhara upamāni te ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_14.1:17.2",
        "original": "Vimale hāṭakasannibhe mukhe;",
        "analyzed": "vimale hāṭaka sannibhe mukhe ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_14.1:17.3",
        "original": "Tava me nayanāni dakkhiya,",
        "analyzed": "tava me nayanāni dakkhiya ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_14.1:17.4",
        "original": "Bhiyyo kāmaguṇo pavaḍḍhati.",
        "analyzed": "bhiyyas kāma guṇaḥ pavaḍḍhati ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_thig_14.1:18.1",
        "original": "Api dūragatā saramhase,",
        "analyzed": "api dūra gatā saramhase ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_14.1:18.2",
        "original": "Āyatapamhe visuddhadassane;",
        "analyzed": "āyata pamhe visuddha dassane ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_14.1:18.3",
        "original": "Na hi matthi tayā piyattaro,",
        "analyzed": "na hi matthi tayā piyattaraḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_14.1:18.4",
        "original": "Nayanā kinnarimandalocane”.",
        "analyzed": "        ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_thig_14.1:19.1",
        "original": "“Apathena payātumicchasi,",
        "analyzed": "apathena payātum icchasi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_14.1:19.2",
        "original": "Candaṁ kīḷanakaṁ gavesasi;",
        "analyzed": "candam kīḷanakam gavesasi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_14.1:19.3",
        "original": "Meruṁ laṅghetumicchasi,",
        "analyzed": "merum laṅghetum icchasi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_14.1:19.4",
        "original": "Yo tvaṁ buddhasutaṁ maggayasi.",
        "analyzed": "yaḥ tvam buddha sutam maggayasi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thig_14.1:20.1",
        "original": "Natthi hi loke sadevake,",
        "analyzed": "na atthi hi loke sa devake ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_14.1:20.2",
        "original": "Rāgo yatthapi dāni me siyā;",
        "analyzed": "rāgaḥ dāni me siyā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_14.1:20.3",
        "original": "Napi naṁ jānāmi kīriso,",
        "analyzed": "na api nam jānāmi kīriso ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_14.1:20.4",
        "original": "Atha maggena hato samūlako.",
        "analyzed": "atha maggena hataḥ sa mūlakaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thig_14.1:21.1",
        "original": "Iṅgālakuyāva ujjhito,",
        "analyzed": "ujjhitaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_14.1:21.2",
        "original": "Visapattoriva aggito kato;",
        "analyzed": "visapattoḥ iva kataḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_14.1:21.3",
        "original": "Napi naṁ passāmi kīriso,",
        "analyzed": "na pi nam passāmi kīriso ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_14.1:21.4",
        "original": "Atha maggena hato samūlako.",
        "analyzed": "atha maggena hataḥ sa mūlakaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thig_14.1:22.1",
        "original": "Yassā siyā apaccavekkhitaṁ,",
        "analyzed": "yassa siyā apaccavekkhita ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_14.1:22.2",
        "original": "Satthā vā anupāsito siyā;",
        "analyzed": "satthā vā an upāsito siyā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_14.1:22.3",
        "original": "Tvaṁ tādisikaṁ palobhaya,",
        "analyzed": "tvam tādisikam palobhaya ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_14.1:22.4",
        "original": "Jānantiṁ so imaṁ vihaññasi.",
        "analyzed": "jānantim saḥ imam vihaññasi ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_thig_14.1:23.1",
        "original": "Mayhañhi akkuṭṭhavandite,",
        "analyzed": "mayhan hi akkuṭṭhavandite ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_14.1:23.2",
        "original": "Sukhadukkhe ca satī upaṭṭhitā;",
        "analyzed": "sukha dukkhe ca satī upaṭṭhitā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_14.1:23.3",
        "original": "Saṅkhatamasubhanti jāniya,",
        "analyzed": "jāniya ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_14.1:23.4",
        "original": "Sabbattheva mano na limpati.",
        "analyzed": "sabbattha iva manaḥ na limpati ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_thig_14.1:24.1",
        "original": "Sāhaṁ sugatassa sāvikā,",
        "analyzed": "sā aham sugatassa sāvikā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_14.1:24.2",
        "original": "Maggaṭṭhaṅgikayānayāyinī;",
        "analyzed": "magga ṭṭhaṅgika yāna yāyinī ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_14.1:24.3",
        "original": "Uddhaṭasallā anāsavā,",
        "analyzed": "uddhaṭa sallā anāsavā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_14.1:24.4",
        "original": "Suññāgāragatā ramāmahaṁ.",
        "analyzed": "suñña āgāra gatāḥ ramāmaha ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_thig_14.1:25.1",
        "original": "Diṭṭhā hi mayā sucittitā,",
        "analyzed": "diṭṭhā hi mayā su citti tā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_14.1:25.2",
        "original": "Sombhā dārukapillakāni vā;",
        "analyzed": "sombhā dāru kapillakāni vā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_14.1:25.3",
        "original": "Tantīhi ca khīlakehi ca,",
        "analyzed": "tantīhi ca khīlakehi ca ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_14.1:25.4",
        "original": "Vinibaddhā vividhaṁ panaccakā.",
        "analyzed": "vinibaddhā vividham ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_thig_14.1:26.1",
        "original": "Tamhuddhaṭe tantikhīlake,",
        "analyzed": "tamhuddhaṭe tantikhīlake ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_14.1:26.2",
        "original": "Vissaṭṭhe vikale parikrite;",
        "analyzed": "vissaṭṭhe vikale parikrite ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_14.1:26.3",
        "original": "Na vindeyya khaṇḍaso kate,",
        "analyzed": "na vindeyya khaṇḍasaḥ kate ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_14.1:26.4",
        "original": "Kimhi tattha manaṁ nivesaye.",
        "analyzed": "kimhi tattha manam nivesaye ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_thig_14.1:27.1",
        "original": "Tathūpamā dehakāni maṁ,",
        "analyzed": "tathā upamā dehakāni ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_14.1:27.2",
        "original": "Tehi dhammehi vinā na vattanti;",
        "analyzed": "tehi dhammehi vinā na vattanti ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_14.1:27.3",
        "original": "Dhammehi vinā na vattati,",
        "analyzed": "dhammehi vinā na vattati ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_14.1:27.4",
        "original": "Kimhi tattha manaṁ nivesaye.",
        "analyzed": "kimhi tattha manam nivesaye ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_thig_14.1:28.1",
        "original": "Yathā haritālena makkhitaṁ,",
        "analyzed": "yathā haritālena makkhita ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_14.1:28.2",
        "original": "Addasa cittikaṁ bhittiyā kataṁ;",
        "analyzed": "addasa cittikam bhittiyā kata ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_14.1:28.3",
        "original": "Tamhi te viparītadassanaṁ,",
        "analyzed": "tamhi te viparīta dassana ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_14.1:28.4",
        "original": "Saññā mānusikā niratthikā.",
        "analyzed": "saññā mānusikā niratthikā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_thig_14.1:29.1",
        "original": "Māyaṁ viya aggato kataṁ,",
        "analyzed": "mā ayam viya aggataḥ kata ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_14.1:29.2",
        "original": "Supinanteva suvaṇṇapādapaṁ;",
        "analyzed": "su pinantā iva suvaṇṇa pādapa ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_14.1:29.3",
        "original": "Upagacchasi andha rittakaṁ,",
        "analyzed": "upagacchasi andha rittaka ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_14.1:29.4",
        "original": "Janamajjheriva rupparūpakaṁ.",
        "analyzed": "jana majjheḥ iva ruppa rūpaka ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_thig_14.1:30.1",
        "original": "Vaṭṭaniriva koṭarohitā,",
        "analyzed": "vaṭṭaniḥ iva koṭa rohitā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_14.1:30.2",
        "original": "Majjhe pubbuḷakā saassukā;",
        "analyzed": "majjhe pubbuḷakā sa assukā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_14.1:30.3",
        "original": "Pīḷakoḷikā cettha jāyati,",
        "analyzed": "pīḷakoḷikā ca itthā jāyati ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_14.1:30.4",
        "original": "Vividhā cakkhuvidhā ca piṇḍitā”.",
        "analyzed": "vividhā cakkhuvidhā ca piṇḍitā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_thig_14.1:31.1",
        "original": "Uppāṭiya cārudassanā,",
        "analyzed": "uppāṭiya cāru dassanā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_14.1:31.2",
        "original": "Na ca pajjittha asaṅgamānasā;",
        "analyzed": "na ca pajjittha asaṅga mānasā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_14.1:31.3",
        "original": "“Handa te cakkhuṁ harassu taṁ”,",
        "analyzed": "handa te cakkhum harassu ta ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_14.1:31.4",
        "original": "Tassa narassa adāsi tāvade.",
        "analyzed": "tassa narassa adāsi tāvade ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_thig_14.1:32.1",
        "original": "Tassa ca viramāsi tāvade,",
        "analyzed": "tassa ca viramāsi tāvade ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_14.1:32.2",
        "original": "Rāgo tattha khamāpayī ca naṁ;",
        "analyzed": "rāgaḥ ca na ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_14.1:32.3",
        "original": "“Sotthi siyā brahmacārinī,",
        "analyzed": "sā utthi siyā brahmacārinī ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_14.1:32.4",
        "original": "Na puno edisakaṁ bhavissati”.",
        "analyzed": "na punar edisakam bhavissati ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_thig_14.1:33.1",
        "original": "“Āsādiya edisaṁ janaṁ,",
        "analyzed": "āsādiya edisam jana ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_14.1:33.2",
        "original": "Aggiṁ pajjalitaṁva liṅgiya;",
        "analyzed": "aggim pajjalitava liṅgiya ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_14.1:33.3",
        "original": "Gaṇhiya āsīvisaṁ viya,",
        "analyzed": "āsīvisam viya ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_14.1:33.4",
        "original": "Api nu sotthi siyā khamehi no”.",
        "analyzed": "api nu sā utthi siyā khamehi naḥ ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_thig_14.1:34.1",
        "original": "Muttā ca tato sā bhikkhunī,",
        "analyzed": "muttā ca tatas sā bhikkhunī ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_14.1:34.2",
        "original": "Agamī buddhavarassa santikaṁ;",
        "analyzed": "agamī buddha varassa santika ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_14.1:34.3",
        "original": "Passiya varapuññalakkhaṇaṁ,",
        "analyzed": "passiya vara puñña lakkhaṇa ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_14.1:34.4",
        "original": "Cakkhu āsi yathā purāṇakanti.",
        "analyzed": "cakkhu āsi yathā purāṇakanti ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_thig_14.1:35.1",
        "original": "… Subhā jīvakambavanikā therī ….",
        "analyzed": "      ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_thig_14.1:36.1",
        "original": "Tiṁsanipāto niṭṭhito.",
        "analyzed": "tisa nipātaḥ niṭṭhito ",
        "folio": "36"
    }
]