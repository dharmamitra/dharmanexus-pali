        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.7:1.1",
        "original": "“Eko ahaṁ sakka mahantamoghaṁ,",
        "analyzed": "ekaḥ aham sakka mahantam ogha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.7:1.2",
        "original": "(iccāyasmā upasīvo)",
        "analyzed": "id ca ayasmai upasīvo ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.7:1.3",
        "original": "Anissito no visahāmi tārituṁ;",
        "analyzed": "a nissitaḥ naḥ visahāmi tāritu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.7:1.4",
        "original": "Ārammaṇaṁ brūhi samantacakkhu,",
        "analyzed": "ārammaṇam brūhi samanta cakkhu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.7:1.5",
        "original": "Yaṁ nissito oghamimaṁ tareyyaṁ”.",
        "analyzed": "yam nissitaḥ ogham imam tareyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.7:2.1",
        "original": "“Ākiñcaññaṁ pekkhamāno satimā,",
        "analyzed": "ākiñcaññam pekkhamānaḥ satimā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.7:2.2",
        "original": "(upasīvāti bhagavā)",
        "analyzed": "upasīvāti bhagavā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.7:2.3",
        "original": "Natthīti nissāya tarassu oghaṁ;",
        "analyzed": "natthi iti nissāya tarassu ogha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.7:2.4",
        "original": "Kāme pahāya virato kathāhi,",
        "analyzed": "kāme pahāya virataḥ kathāhi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.7:2.5",
        "original": "Taṇhakkhayaṁ nattamahābhipassa”.",
        "analyzed": "taṇha khayam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.7:3.1",
        "original": "“Sabbesu kāmesu yo vītarāgo,",
        "analyzed": "sabbesu kāmesu yaḥ vīta rāgaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.7:3.2",
        "original": "(iccāyasmā upasīvo)",
        "analyzed": "id ca ayasmai upasīvo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.7:3.3",
        "original": "Ākiñcaññaṁ nissito hitvā maññaṁ;",
        "analyzed": "ākiñcaññam nissitaḥ hitvā mañña ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.7:3.4",
        "original": "Saññāvimokkhe parame vimutto,",
        "analyzed": "saññā vimokkhe parame vimutto ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.7:3.5",
        "original": "Tiṭṭhe nu so tattha anānuyāyī”.",
        "analyzed": "tiṭṭhe nu saḥ tattha anānuyāyī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.7:4.1",
        "original": "“Sabbesu kāmesu yo vītarāgo,",
        "analyzed": "sabbesu kāmesu yaḥ vīta rāgaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.7:4.2",
        "original": "(upasīvāti bhagavā)",
        "analyzed": "upasīvāti bhagavā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.7:4.3",
        "original": "Ākiñcaññaṁ nissito hitvā maññaṁ;",
        "analyzed": "ākiñcaññam nissitaḥ hitvā mañña ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.7:4.4",
        "original": "Saññāvimokkhe parame vimutto,",
        "analyzed": "saññā vimokkhe parame vimutto ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.7:4.5",
        "original": "Tiṭṭheyya so tattha anānuyāyī”.",
        "analyzed": "tiṭṭheyya saḥ tattha an anuyāyī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.7:5.1",
        "original": "“Tiṭṭhe ce so tattha anānuyāyī,",
        "analyzed": "tiṭṭhe ce so tattha anānuyāyī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.7:5.2",
        "original": "Pūgampi vassānaṁ samantacakkhu;",
        "analyzed": "pūgam pi vassānam samanta cakkhu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.7:5.3",
        "original": "Tattheva so sītisiyā vimutto,",
        "analyzed": "tattha iva saḥ sīti siyā vimutto ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.7:5.4",
        "original": "Cavetha viññāṇaṁ tathāvidhassa”.",
        "analyzed": "cavetha viññāṇam tathāvidhassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.7:6.1",
        "original": "“Accī yathā vātavegena khittā,",
        "analyzed": "accī yathā vāta vegena khittā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.7:6.2",
        "original": "(upasīvāti bhagavā)",
        "analyzed": "upasīvāti bhagavā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.7:6.3",
        "original": "Atthaṁ paleti na upeti saṅkhaṁ;",
        "analyzed": "attham paleti naḥ upeti saṅkha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.7:6.4",
        "original": "Evaṁ munī nāmakāyā vimutto,",
        "analyzed": "evam muniḥ nāma kāyāḥ vimuttaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.7:6.5",
        "original": "Atthaṁ paleti na upeti saṅkhaṁ”.",
        "analyzed": "attham paleti naḥ upeti saṅkha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.7:7.1",
        "original": "“Atthaṅgato so uda vā so natthi,",
        "analyzed": "atthaṅgataḥ saḥ uda vā so natthi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.7:7.2",
        "original": "Udāhu ve sassatiyā arogo;",
        "analyzed": "udāhu ve sassatiyā arogaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.7:7.3",
        "original": "Taṁ me munī sādhu viyākarohi,",
        "analyzed": "tam me munī sādhu viyākarohi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.7:7.4",
        "original": "Tathā hi te vidito esa dhammo”.",
        "analyzed": "tathā hi te viditaḥ esa dhammo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_5.7:8.1",
        "original": "“Atthaṅgatassa na pamāṇamatthi,",
        "analyzed": "atthaṅgatassa na pamāṇam atthi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.7:8.2",
        "original": "(upasīvāti bhagavā)",
        "analyzed": "upasīvāti bhagavā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.7:8.3",
        "original": "Yena naṁ vajjuṁ taṁ tassa natthi;",
        "analyzed": "yena nam vajjum tam tassa natthi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.7:8.4",
        "original": "Sabbesu dhammesu samohatesu,",
        "analyzed": "sabbesu dhammesu samohatesu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.7:8.5",
        "original": "Samūhatā vādapathāpi sabbe”ti.",
        "analyzed": "samūha tā vāda pathā api ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_5.7:8.6",
        "original": "Upasīvamāṇavapucchā chaṭṭhī.",
        "analyzed": "upasīvamāṇa vapucchā chaṭṭhī ",
        "folio": "8"
    }
