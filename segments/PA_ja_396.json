    {
        "segmentnr": "PA_ja_396:0.4",
        "original": "1. Kukkujātaka",
        "analyzed": "kukkujātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_396:1.1",
        "original": "“Diyaḍḍhakukkū udayena kaṇṇikā,",
        "analyzed": "diyaḍḍha kukkū udayena kaṇṇikā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_396:1.2",
        "original": "Vidatthiyo aṭṭha parikkhipanti naṁ;",
        "analyzed": "vidatthiyaḥ aṭṭha parikkhipanti na ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_396:1.3",
        "original": "Sā siṁsapā sāramayā apheggukā,",
        "analyzed": "sā sisapā sāra mayā apheggukā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_396:1.4",
        "original": "Kuhiṁ ṭhitā upparito na dhaṁsati”.",
        "analyzed": "kuhim ṭhitāḥ upparitas na dhasati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_396:2.1",
        "original": "“Yā tiṁsati sāramayā anujjukā,",
        "analyzed": "yā tisati sāramayā anujjukā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_396:2.2",
        "original": "Parikiriya gopānasiyo samaṁ ṭhitā;",
        "analyzed": "samam ṭhitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_396:2.3",
        "original": "Tāhi susaṅgahitā balasā pīḷitā,",
        "analyzed": "tāhi su saṅga hitā balasā pīḍitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_396:2.4",
        "original": "Samaṁ ṭhitā upparito na dhaṁsati.",
        "analyzed": "samam ṭhitāḥ upparitas na dhasati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_396:3.1",
        "original": "Evampi mittehi daḷhehi paṇḍito,",
        "analyzed": "evam pi mittehi daḷhehi paṇḍitaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_396:3.2",
        "original": "Abhejjarūpehi sucīhi mantibhi;",
        "analyzed": "abhejja rūpehi sucīhi mantibhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_396:3.3",
        "original": "Susaṅgahīto siriyā na dhaṁsati,",
        "analyzed": "su saṅgahītaḥ siriyā na dhasati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_396:3.4",
        "original": "Gopānasī bhāravahāva kaṇṇikā.",
        "analyzed": "gopānasī bhāravahā ava kaṇṇikā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_396:4.1",
        "original": "Kharattacaṁ bellaṁ yathāpi satthavā,",
        "analyzed": "kharattacam bellam yathā api satthavā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_396:4.2",
        "original": "Anāmasantopi karoti tittakaṁ;",
        "analyzed": "an āmasantaḥ api karoti tittaka ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_396:4.3",
        "original": "Samāharaṁ sāduṁ karoti patthiva,",
        "analyzed": "samāharam sādum karoti patthiva ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_396:4.4",
        "original": "Asāduṁ kayirā tanubandhamuddharaṁ.",
        "analyzed": "asādum kayirā tanu bandham uddhara ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_396:5.1",
        "original": "Evampi gāmanigamesu paṇḍito,",
        "analyzed": "evam pi gāma nigamesu paṇḍitaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_396:5.2",
        "original": "Asāhasaṁ rājadhanāni saṅgharaṁ;",
        "analyzed": "asāhasam rāja dhanāni saṅghara ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_396:5.3",
        "original": "Dhammānuvattī paṭipajjamāno,",
        "analyzed": "dhamma anuvattī paṭipajjamānaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_396:5.4",
        "original": "Sa phāti kayirā aviheṭhayaṁ paraṁ.",
        "analyzed": "sa phāti kayirāḥ a viheṭhayam para ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_396:6.1",
        "original": "Odātamūlaṁ sucivārisambhavaṁ,",
        "analyzed": "odāta mūlam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_396:6.2",
        "original": "Jātaṁ yathā pokkharaṇīsu ambujaṁ;",
        "analyzed": "jātam yathā pokkharaṇīsu ambuja ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_396:6.3",
        "original": "Padumaṁ yathā agginikāsiphālimaṁ,",
        "analyzed": "padumam yathā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_396:6.4",
        "original": "Na kaddamo na rajo na vāri limpati.",
        "analyzed": "na kaddamaḥ na rajaḥ na vāri limpati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_396:7.1",
        "original": "Evampi vohārasuciṁ asāhasaṁ,",
        "analyzed": "vohāra sucim asāhasa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_396:7.2",
        "original": "Visuddhakammantamapetapāpakaṁ;",
        "analyzed": "visuddha kammantam apeta pāpaka ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_396:7.3",
        "original": "Na limpati kammakilesa tādiso,",
        "analyzed": "na limpati tādiso ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_396:7.4",
        "original": "Jātaṁ yathā pokkharaṇīsu ambujan”ti.",
        "analyzed": "jātam yathā pokkharaṇīsu ambujanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_396:8.1",
        "original": "Kukkujātakaṁ paṭhamaṁ.",
        "analyzed": "kukkujātakam ",
        "folio": "8"
    }
]