    },
    {
        "segmentnr": "PA_ja_506:1.1",
        "original": "“Kā nu vijjurivābhāsi,",
        "analyzed": "kā nu vijjuḥ iva ābhāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_506:1.2",
        "original": "Osadhī viya tārakā;",
        "analyzed": "osadhī viya tārakā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_506:1.3",
        "original": "Devatā nusi gandhabbī,",
        "analyzed": "devatā nusi gandhabbī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_506:1.4",
        "original": "Na taṁ maññāmi mānusiṁ”.",
        "analyzed": "na tam maññāmi mānusi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_506:2.1",
        "original": "“Namhi devī na gandhabbī,",
        "analyzed": "namhi devī na gandhabbī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_506:2.2",
        "original": "na mahārāja mānusī;",
        "analyzed": "na mahā rāja mānusī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_506:2.3",
        "original": "Nāgakaññāsmi bhaddante,",
        "analyzed": "nāga kaññā asmi bhaddante ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_506:2.4",
        "original": "atthenamhi idhāgatā”.",
        "analyzed": "idha āgatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_506:3.1",
        "original": "“Vibbhantacittā kupitindriyāsi,",
        "analyzed": "kupiti indriyā asi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_506:3.2",
        "original": "Nettehi te vārigaṇā savanti;",
        "analyzed": "te vāri gaṇāḥ savanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_506:3.3",
        "original": "Kiṁ te naṭṭhaṁ kiṁ pana patthayānā,",
        "analyzed": "kim te naṭṭham kim pana patthayānā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_506:3.4",
        "original": "Idhāgatā nāri tadiṅgha brūhi”.",
        "analyzed": "idha āgatā nāri tat iṅgha brūhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_506:4.1",
        "original": "“Yamuggatejo uragoti cāhu,",
        "analyzed": "uragā iti ca āhu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_506:4.2",
        "original": "Nāgoti naṁ āhu janā janinda;",
        "analyzed": "nāgā iti nam āhu janāḥ janinda ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_506:4.3",
        "original": "Tamaggahī puriso jīvikattho,",
        "analyzed": "tam aggahī purisaḥ jīvi katthaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_506:4.4",
        "original": "Taṁ bandhanā muñca patī mameso”.",
        "analyzed": "tam bandhanā muñca patī mama iso ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_506:5.1",
        "original": "“Kathaṁ nvayaṁ balaviriyūpapanno,",
        "analyzed": "katham nu ayam bala viriya upapannaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_506:5.2",
        "original": "Hatthattamāgacchi vanibbakassa;",
        "analyzed": "hatthattam āgacchi vanibbakassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_506:5.3",
        "original": "Akkhāhi me nāgakaññe tamatthaṁ,",
        "analyzed": "akkhāhi me nāga kaññe tam attha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_506:5.4",
        "original": "Kathaṁ vijānemu gahītanāgaṁ”.",
        "analyzed": "katham vijānemu gahīta nāga ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_506:6.1",
        "original": "“Nagarampi nāgo bhasmaṁ kareyya,",
        "analyzed": "nagaram pi nāgaḥ bhasmam kareyya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_506:6.2",
        "original": "Tathā hi so balaviriyūpapanno;",
        "analyzed": "tathā hi saḥ bala viriya upapannaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_506:6.3",
        "original": "Dhammañca nāgo apacāyamāno,",
        "analyzed": "dhammañca nāgaḥ apacāyamāno ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_506:6.4",
        "original": "Tasmā parakkamma tapo karoti.",
        "analyzed": "tasmāt parakkamma tapaḥ karoti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_506:7.1",
        "original": "Cātuddasiṁ pañcadasiñca rāja,",
        "analyzed": "cātuddasim pañcadasim ca rāja ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_506:7.2",
        "original": "Catuppathe sammati nāgarājā;",
        "analyzed": "catuppathe sammati nāga rājā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_506:7.3",
        "original": "Tamaggahī puriso jīvikattho,",
        "analyzed": "tam aggahī purisaḥ jīvi katthaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_506:7.4",
        "original": "Taṁ bandhanā muñca patī mameso”.",
        "analyzed": "tam bandhanā muñca patī mama iso ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_506:8.1",
        "original": "“Soḷasitthisahassāni,",
        "analyzed": "soḷasitthi sahassāni ",
        "folio": "8"
    },
