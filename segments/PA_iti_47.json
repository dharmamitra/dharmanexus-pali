    },
    {
        "segmentnr": "PA_iti_47:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_47:2.1",
        "original": "“Jāgaro cassa, bhikkhave, bhikkhu vihareyya sato sampajāno samāhito pamudito vippasanno ca tattha kālavipassī ca kusalesu dhammesu.",
        "analyzed": "jāgaraḥ cassa bhikkhave bhikkhu vihareyya sataḥ sampajānaḥ samāhitaḥ pamuditaḥ vippasannaḥ ca tattha kāla vipassī ca kusalesu dhammesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_47:2.2_0",
        "original": "Jāgarassa, bhikkhave, bhikkhuno viharato satassa sampajānassa samāhitassa pamuditassa",
        "analyzed": "jāgarassa bhikkhave bhikkhunaḥ viharataḥ satassa sampajānassa samāhitassa pamuditassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_47:2.2_1",
        "original": "vippasannassa tattha kālavipassino kusalesu dhammesu dvinnaṁ phalānaṁ aññataraṁ phalaṁ pāṭikaṅkhaṁ—",
        "analyzed": "vippasannassa tattha kāla vipassino kusalesu dhammesu dvinnam phalānam aññataram phalam pāṭikaṅkha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_47:2.3",
        "original": "diṭṭheva dhamme aññā, sati vā upādisese anāgāmitā”ti.",
        "analyzed": "diṭṭhā iva dhamme aññā sati vā upādisese anāgāmi tā ti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_47:2.4",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_47:2.5",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_47:3.1",
        "original": "“Jāgarantā suṇāthetaṁ,",
        "analyzed": "jāgarantā suṇātha ita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_47:3.2",
        "original": "ye suttā te pabujjhatha;",
        "analyzed": "ye suttā te pabujjhatha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_47:3.3",
        "original": "Suttā jāgaritaṁ seyyo,",
        "analyzed": "suttā jāgaritam seyyaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_47:3.4",
        "original": "natthi jāgarato bhayaṁ.",
        "analyzed": "na atthi jāgarataḥ bhaya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_47:4.1",
        "original": "Yo jāgaro ca satimā sampajāno,",
        "analyzed": "yaḥ jāgaraḥ ca satimā sampajānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_47:4.2",
        "original": "Samāhito mudito vippasanno ca;",
        "analyzed": "samāhitaḥ muditaḥ vippasannaḥ ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_47:4.3",
        "original": "Kālena so sammā dhammaṁ parivīmaṁsamāno,",
        "analyzed": "kālena so sammā dhammam parivīmasamānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_47:4.4",
        "original": "Ekodibhūto vihane tamaṁ so.",
        "analyzed": "eka udi bhūtaḥ vihane tamam saḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_47:5.1",
        "original": "Tasmā have jāgariyaṁ bhajetha,",
        "analyzed": "tasmai have jāgariyam bhajetha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_47:5.2",
        "original": "Ātāpī bhikkhu nipako jhānalābhī;",
        "analyzed": "ātāpī bhikkhu nipakaḥ jhāna lābhī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_47:5.3",
        "original": "Saṁyojanaṁ jātijarāya chetvā,",
        "analyzed": "sa yojanam jāti jarāya chetvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_47:5.4",
        "original": "Idheva sambodhimanuttaraṁ phuse”ti.",
        "analyzed": "idhā iva sambodhim anuttaram phusa iti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_47:6.1",
        "original": "Ayampi attho vutto bhagavatā, iti me sutanti.",
        "analyzed": "ayam pi atthaḥ vuttaḥ bhagavatā iti me sutam ti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_47:6.2",
        "original": "Dasamaṁ.",
        "analyzed": "dasama ",
        "folio": "6"
    }
