    },
    {
        "segmentnr": "PA_bi-vb_pc85:1.1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc85:1.1.2",
        "original": "Tena kho pana samayena chabbaggiyā bhikkhuniyo yānena yāyanti.",
        "analyzed": "tena kho pana samayena chabbaggiyāḥ bhikkhuniyaḥ yānena yāyanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc85:1.1.3",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
