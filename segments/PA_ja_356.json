        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_356:1.1",
        "original": "“Eko araññe girikandarāyaṁ,",
        "analyzed": "ekaḥ araññe giri kandarāya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_356:1.2",
        "original": "Paggayha paggayha silaṁ pavecchasi;",
        "analyzed": "paggayha paggayha silam pavecchasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_356:1.3",
        "original": "Punappunaṁ santaramānarūpo,",
        "analyzed": "punar punar santaramāna rūpaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_356:1.4",
        "original": "Koraṇḍiya ko nu tava yidhattho”.",
        "analyzed": "koraṇḍiya kaḥ nu tava yidhatthaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_356:2.1",
        "original": "“Ahañhimaṁ sāgara sevitantaṁ,",
        "analyzed": "ahan himam sāgara ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_356:2.2",
        "original": "Samaṁ karissāmi yathāpi pāṇi;",
        "analyzed": "samam karissāmi yathā api pāṇi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_356:2.3",
        "original": "Vikiriya sānūni ca pabbatāni ca,",
        "analyzed": "vikiriya sānūni ca pabbatāni ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_356:2.4",
        "original": "Tasmā silaṁ dariyā pakkhipāmi”.",
        "analyzed": "tasmai silam dariyā pakkhipāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_356:3.1",
        "original": "“Nayimaṁ mahiṁ arahati pāṇikappaṁ,",
        "analyzed": "nayi imam mahim arahati pāṇikappa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_356:3.2",
        "original": "Samaṁ manusso karaṇāya meko;",
        "analyzed": "samam manussaḥ karaṇāya mekaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_356:3.3",
        "original": "Maññāmimaññeva dariṁ jigīsaṁ,",
        "analyzed": "darim jigīsa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_356:3.4",
        "original": "Koraṇḍiya hāhasi jīvalokaṁ”.",
        "analyzed": "koraṇḍiya hāhasi jīvaloka ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_356:4.1",
        "original": "“Sace ahaṁ bhūtadharaṁ na sakkā,",
        "analyzed": "sace aham bhūta dharam na sakkā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_356:4.2",
        "original": "Samaṁ manusso karaṇāya meko;",
        "analyzed": "samam manussaḥ karaṇāya mekaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_356:4.3",
        "original": "Evameva tvaṁ brahme ime manusse,",
        "analyzed": "evam eva tvam brahme ime manusse ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_356:4.4",
        "original": "Nānādiṭṭhike nānayissasi te”.",
        "analyzed": "nānādiṭṭhike nānayissasi te ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_356:5.1",
        "original": "“Saṅkhittarūpena bhavaṁ mamatthaṁ,",
        "analyzed": "saṅkhitta rūpena bhavam mamattha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_356:5.2",
        "original": "Akkhāsi koraṇḍiya evametaṁ;",
        "analyzed": "akkhāsi koraṇḍiya evam eta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_356:5.3",
        "original": "Yathā na sakkā pathavī samāyaṁ,",
        "analyzed": "yathā na sakkā pathavī samāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_356:5.4",
        "original": "Kattuṁ manussena tathā manussā”ti.",
        "analyzed": "kattum manussena tathā manussa ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_356:6.1",
        "original": "Koraṇḍiyajātakaṁ chaṭṭhaṁ.",
        "analyzed": "koraṇḍiya jātakam chaṭṭha ",
        "folio": "6"
    }
