[
    {
        "segmentnr": "PA_ja_464:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_464:0.2",
        "original": "Dvādasakanipāta",
        "analyzed": "dvādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_464:0.3",
        "original": "Cūḷakuṇālavagga",
        "analyzed": "cūḷa kuṇāla vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_464:0.4",
        "original": "1. Cūḷakuṇālajātaka",
        "analyzed": "cūḍa kuṇāla jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_464:1.1",
        "original": "“Luddhānaṁ lahucittānaṁ,",
        "analyzed": "luddhānam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_464:1.2",
        "original": "akataññūna dubbhinaṁ;",
        "analyzed": "a kataññūna dubbhina ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_464:1.3",
        "original": "Nādevasatto puriso,",
        "analyzed": "na a deva sattaḥ purisaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_464:1.4",
        "original": "thīnaṁ saddhātumarahati.",
        "analyzed": "thīnam sat dhātum arahati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_464:2.1",
        "original": "Na tā pajānanti kataṁ na kiccaṁ,",
        "analyzed": "na tā pajānanti katam na kit ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_464:2.2",
        "original": "Na mātaraṁ pitaraṁ bhātaraṁ vā;",
        "analyzed": "na mātaram pitaram bhātaram vā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_464:2.3",
        "original": "Anariyā samatikkantadhammā,",
        "analyzed": "an ariyāḥ samatikkanta dhammāḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_464:2.4",
        "original": "Sasseva cittassa vasaṁ vajanti.",
        "analyzed": "sassa iva cittassa vasam vajanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_464:3.1",
        "original": "Cirānuvutthampi piyaṁ manāpaṁ,",
        "analyzed": "cira anuvuttham piyam manāpa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_464:3.2",
        "original": "Anukampakaṁ pāṇasamampi bhattuṁ;",
        "analyzed": "anukampakam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_464:3.3",
        "original": "Āvāsu kiccesu ca naṁ jahanti,",
        "analyzed": "āvāsu kiccesu ca nam jahanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_464:3.4",
        "original": "Tasmāhamitthīnaṁ na vissasāmi.",
        "analyzed": "tasmā aham itthīnam na vissasāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_464:4.1",
        "original": "Thīnañhi cittaṁ yathā vānarassa,",
        "analyzed": "thīnam hi cittam yathā vānarassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_464:4.2",
        "original": "Kannappakannaṁ yathā rukkhachāyā;",
        "analyzed": "kannappakannam yathā rukkha chāyā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_464:4.3",
        "original": "Calācalaṁ hadayamitthiyānaṁ,",
        "analyzed": "calācalam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_464:4.4",
        "original": "Cakkassa nemi viya parivattati.",
        "analyzed": "cakkassa nemi viya parivattati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_464:5.1",
        "original": "Yadā tā passanti samekkhamānā,",
        "analyzed": "yadā tā passanti samekkhamānā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_464:5.2",
        "original": "Ādeyyarūpaṁ purisassa vittaṁ;",
        "analyzed": "ādeyya rūpam purisassa vitta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_464:5.3",
        "original": "Saṇhāhi vācāhi nayanti menaṁ,",
        "analyzed": "saṇhāhi vācāhi nayanti menā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_464:5.4",
        "original": "Kambojakā jalajeneva assaṁ.",
        "analyzed": "kambojakā jalajena iva assa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_464:6.1",
        "original": "Yadā na passanti samekkhamānā,",
        "analyzed": "yadā na passanti samekkhamānā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_464:6.2",
        "original": "Ādeyyarūpaṁ purisassa vittaṁ;",
        "analyzed": "ādeyya rūpam purisassa vitta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_464:6.3",
        "original": "Samantato naṁ parivajjayanti,",
        "analyzed": "samantataḥ nam parivajjayanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_464:6.4",
        "original": "Tiṇṇo nadīpāragatova kullaṁ.",
        "analyzed": "tiṇṇaḥ nadī pāra gataḥ uva ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_464:7.1",
        "original": "Silesūpamā sikhiriva sabbabhakkhā,",
        "analyzed": "silesu upamā sikhiḥ iva sabba bhakkhā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_464:7.2",
        "original": "Tikkhamāyā nadīriva sīghasotā;",
        "analyzed": "tikkhamāyāḥ nadīḥ iva sīgha sotā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_464:7.3",
        "original": "Sevanti hetā piyamappiyañca,",
        "analyzed": "sevanti hetā piyam appiyañca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_464:7.4",
        "original": "Nāvā yathā orakūlaṁ parañca.",
        "analyzed": "nāvā yathā orakūlam parañca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_464:8.1",
        "original": "Na tā ekassa na dvinnaṁ,",
        "analyzed": "na tāḥ ekassa na dvinna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_464:8.2",
        "original": "āpaṇova pasārito;",
        "analyzed": "āpaṇaḥ uva pasāritaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_464:8.3",
        "original": "Yo tā mayhanti maññeyya,",
        "analyzed": "yaḥ tā mayhanti maññeyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_464:8.4",
        "original": "vātaṁ jālena bādhaye.",
        "analyzed": "vātam jālena bādhaye ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_464:9.1",
        "original": "Yathā nadī ca pantho ca,",
        "analyzed": "yathā nadī ca pantho ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_464:9.2",
        "original": "pānāgāraṁ sabhā papā;",
        "analyzed": "pānāgāram sabhā papā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_464:9.3",
        "original": "Evaṁ lokitthiyo nāma,",
        "analyzed": "evam lokitthiyaḥ nāma ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_464:9.4",
        "original": "velā tāsaṁ na vijjati.",
        "analyzed": "velā tāsam na vijjati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_464:10.1",
        "original": "Ghatāsanasamā etā,",
        "analyzed": "ghata asana samāḥ etā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_464:10.2",
        "original": "kaṇhasappasirūpamā;",
        "analyzed": "kaṇha sappa sirūpamā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_464:10.3",
        "original": "Gāvo bahitiṇasseva,",
        "analyzed": "gāvaḥ bahitiṇassa iva ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_464:10.4",
        "original": "omasanti varaṁ varaṁ.",
        "analyzed": "omasanti varam vara ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_464:11.1",
        "original": "Ghatāsanaṁ kuñjaraṁ kaṇhasappaṁ,",
        "analyzed": "ghata asanam kuñjaram kaṇhasappa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_464:11.2",
        "original": "Muddhābhisittaṁ pamadā ca sabbā;",
        "analyzed": "muddhābhisittam pamadā ca sabbā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_464:11.3",
        "original": "Ete naro niccayato bhajetha,",
        "analyzed": "ete naraḥ niccayataḥ bhajetha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_464:11.4",
        "original": "Tesaṁ have dubbidu sabbabhāvo.",
        "analyzed": "tesam have sabba bhāvaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_464:12.1",
        "original": "Naccantavaṇṇā na bahūnaṁ kantā,",
        "analyzed": "na bahūnam kantā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_464:12.2",
        "original": "Na dakkhiṇā pamadā sevitabbā;",
        "analyzed": "na dakkhiṇā pamadā sevitabbā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_464:12.3",
        "original": "Na parassa bhariyā na dhanassa hetu,",
        "analyzed": "na parassa bhariyā na dhanassa hetu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_464:12.4",
        "original": "Etitthiyo pañca na sevitabbā”ti.",
        "analyzed": "etitthiyaḥ pañca na sevitabbā ati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_464:13.1",
        "original": "Cūḷakuṇālajātakaṁ paṭhamaṁ.",
        "analyzed": "cūḷa kuṇāla jātakam ",
        "folio": "13"
    }
]