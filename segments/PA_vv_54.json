    },
    {
        "segmentnr": "PA_vv_54:0.4",
        "original": "4. Kakkaṭakarasadāyakavimānavatthu",
        "analyzed": "kakkaṭaka rasa dāyaka vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_54:1.1",
        "original": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
        "analyzed": "uccam idam maṇi thūṇam vimāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_54:1.2",
        "original": "Samantato dvādasa yojanāni;",
        "analyzed": "samantataḥ dvādasa yojanāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_54:1.3",
        "original": "Kūṭāgārā sattasatā uḷārā,",
        "analyzed": "kūṭāgārā satta satā uḷārā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_54:1.4",
        "original": "Veḷuriyathambhā rucakatthatā subhā.",
        "analyzed": "veḷuriya thambhā rucakatthatā subhā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_54:2.1",
        "original": "Tatthacchasi pivasi khādasi ca,",
        "analyzed": "tatthacchasi pivasi khādasi ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_54:2.2",
        "original": "Dibbā ca vīṇā pavadanti vagguṁ;",
        "analyzed": "dibbā ca vīṇā pavadanti vaggu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_54:2.3",
        "original": "Dibbā rasā kāmaguṇettha pañca,",
        "analyzed": "dibbāḥ rasāḥ kāma guṇā ittha pañca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_54:2.4",
        "original": "Nāriyo ca naccanti suvaṇṇachannā.",
        "analyzed": "nāriyaḥ ca naccanti suvaṇṇa channā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_54:3.1",
        "original": "Kena tetādiso vaṇṇo,",
        "analyzed": "kena tetādisaḥ vaṇṇaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_54:3.2",
        "original": "kena te idha mijjhati;",
        "analyzed": "kena te idha mijjhati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_54:3.3",
        "original": "Uppajjanti ca te bhogā,",
        "analyzed": "uppajjanti ca te bhogāḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_54:3.4",
        "original": "ye keci manaso piyā.",
        "analyzed": "ye keci manasaḥ piyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_54:4.1",
        "original": "Pucchāmi taṁ deva mahānubhāva,",
        "analyzed": "pucchāmi tam deva mahā anubhāva ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_54:4.2",
        "original": "Manussabhūto kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtaḥ kim akāsi puñña ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_54:4.3",
        "original": "Kenāsi evaṁ jalitānubhāvo,",
        "analyzed": "kena asi evam jalita anubhāvaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_54:4.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_54:5.1",
        "original": "So devaputto attamano,",
        "analyzed": "saḥ deva puttaḥ atta mano ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_54:5.2",
        "original": "moggallānena pucchito;",
        "analyzed": "moggallānena pucchito ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_54:5.3",
        "original": "Pañhaṁ puṭṭho viyākāsi,",
        "analyzed": "pañham puṭṭhaḥ viyākāsi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_54:5.4",
        "original": "yassa kammassidaṁ phalaṁ.",
        "analyzed": "yassa kammassa idam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_54:6.1",
        "original": "“Satisamuppādakaro,",
        "analyzed": "sati samuppāda karaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_54:6.2",
        "original": "dvāre kakkaṭako ṭhito;",
        "analyzed": "dvāre kakkaṭakaḥ ṭhitaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_54:6.3",
        "original": "Niṭṭhito jātarūpassa,",
        "analyzed": "niṭṭhitaḥ jātarūpassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_54:6.4",
        "original": "sobhati dasapādako.",
        "analyzed": "sobhati dasa pādako ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_54:7.1",
        "original": "Tena metādiso vaṇṇo,",
        "analyzed": "tena mā itādisaḥ vaṇṇaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_54:7.2",
        "original": "tena me idha mijjhati;",
        "analyzed": "tena me idha mijjhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_54:7.3",
        "original": "Uppajjanti ca me bhogā,",
        "analyzed": "uppajjanti ca me bhogāḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_54:7.4",
        "original": "ye keci manaso piyā.",
        "analyzed": "ye keci manasaḥ piyā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_54:8.1",
        "original": "Akkhāmi te bhikkhu mahānubhāva,",
        "analyzed": "akkhāmi te bhikkhu mahā anubhāva ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_54:8.2",
        "original": "Manussabhūto yamakāsi puññaṁ;",
        "analyzed": "manussa bhūtaḥ yamakā asi puñña ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_54:8.3",
        "original": "Tenamhi evaṁ jalitānubhāvo,",
        "analyzed": "evam jalita anubhāvaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_54:8.4",
        "original": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca me sabbadisā pabhāsati iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_54:9.1",
        "original": "Kakkaṭakarasadāyakavimānaṁ catutthaṁ.",
        "analyzed": "kakkaṭaka rasa dāyaka vimānam ",
        "folio": "9"
    },
