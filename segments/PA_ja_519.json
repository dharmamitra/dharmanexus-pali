        "segmentnr": "PA_ja_519:25.4",
        "original": "yaṁ maṁ khādeyya rakkhaso;",
        "analyzed": "yam mama khādeyya rakkhasaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_519:25.5",
        "original": "Yañca me ayyaputtassa,",
        "analyzed": "yat ca me ayya puttassa ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_519:25.6",
        "original": "mano hessati aññathā”.",
        "analyzed": "manaḥ hessati aññathā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_519:26.1",
        "original": "“Corīnaṁ bahubuddhīnaṁ,",
        "analyzed": "corīnam bahubuddhīna ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_519:26.2",
        "original": "yāsu saccaṁ sudullabhaṁ;",
        "analyzed": "yāsu saccam su dullabha ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_519:26.3",
        "original": "Thīnaṁ bhāvo durājāno,",
        "analyzed": "bhāvaḥ ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_519:26.4",
        "original": "macchassevodake gataṁ”.",
        "analyzed": "macchassa iva udake gata ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_519:27.1",
        "original": "“Tathā maṁ saccaṁ pāletu,",
        "analyzed": "tathā mam saccam pāletu ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_519:27.2",
        "original": "pālayissati ce mamaṁ;",
        "analyzed": "pālayissati ce mama ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_519:27.3",
        "original": "Yathāhaṁ nābhijānāmi,",
        "analyzed": "yathā aham na abhijānāmi ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_519:27.4",
        "original": "aññaṁ piyataraṁ tayā;",
        "analyzed": "aññam piyataram tayā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_519:27.5",
        "original": "Etena saccavajjena,",
        "analyzed": "etena saccavajjena ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_519:27.6",
        "original": "byādhi te vūpasammatu”.",
        "analyzed": "byādhi te vūpasammatu ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_519:28.1",
        "original": "“Ye kuñjarā sattasatā uḷārā,",
        "analyzed": "ye kuñjarā satta satā uḷārā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_519:28.2",
        "original": "Rakkhanti rattindivamuyyutāvudhā;",
        "analyzed": "rakkhanti ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_519:28.3",
        "original": "Dhanuggahānañca satāni soḷasa,",
        "analyzed": "satāni soḷasa ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_519:28.4",
        "original": "Kathaṁvidhe passasi bhadde sattavo”.",
        "analyzed": "kathavidhe passasi bhadde sattavaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_519:29.1",
        "original": "“Alaṅkatāyo padumuttarattacā,",
        "analyzed": "padum uttarattacā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_519:29.2",
        "original": "Virāgitā passati haṁsagaggarā;",
        "analyzed": "virāgitā passati hasa gaggarā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_519:29.3",
        "original": "Tāsaṁ suṇitvā mitagītavāditaṁ,",
        "analyzed": "mita gīta vādita ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_519:29.4",
        "original": "Na dāni me tāta tathā yathā pure.",
        "analyzed": "na dāni me tāta tathā yathā pure ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_519:30.1",
        "original": "Suvaṇṇasaṅkaccadharā suviggahā,",
        "analyzed": "suvaṇṇa saṅkacca dharā su viggahā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_519:30.2",
        "original": "Alaṅkatā mānusiyaccharūpamā;",
        "analyzed": "alaṅkatā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_519:30.3",
        "original": "Senopiyā tāta aninditaṅgiyo,",
        "analyzed": "tāta aninditaṅgiyo ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_519:30.4",
        "original": "Khattiyakaññā paṭilobhayanti naṁ.",
        "analyzed": "khattiya kaññā paṭilobhayanti na ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_519:31.1",
        "original": "Sace ahaṁ tāta tathā yathā pure,",
        "analyzed": "sace aham tāta tathā yathā pure ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_519:31.2",
        "original": "Patiṁ tamuñchāya punā vane bhare;",
        "analyzed": "patim tam uñchāya punā vane bhare ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_519:31.3",
        "original": "Sammānaye maṁ na ca maṁ vimānaye,",
        "analyzed": "sammānaye mam na ca mam vimānaye ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_519:31.4",
        "original": "Itopi me tāta tato varaṁ siyā.",
        "analyzed": "itas upi me tāta tatas varam siyā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_519:32.1",
        "original": "Yamannapāne vipulasmi ohite,",
        "analyzed": "yam anna pāne vipulasmi ohite ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_519:32.2",
        "original": "Nārī vimaṭṭhābharaṇā alaṅkatā;",
        "analyzed": "nārī vimaṭṭha ābharaṇā alaṅkatā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_519:32.3",
        "original": "Sabbaṅgupetā patino ca appiyā,",
        "analyzed": "sabbaṅgupetā patino ca appiyā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_519:32.4",
        "original": "Abajjha tassā maraṇaṁ tato varaṁ.",
        "analyzed": "abajjha tassāḥ maraṇam tatas vara ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_519:33.1",
        "original": "Api ce daliddā kapaṇā anāḷhiyā,",
        "analyzed": "api ce daliddā kapaṇā an āḷhiyā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_519:33.2",
        "original": "Kaṭādutīyā patino ca sā piyā;",
        "analyzed": "ca sā piyā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_519:33.3",
        "original": "Sabbaṅgupetāyapi appiyāya,",
        "analyzed": "sabbaṅgupetāya api appiyāya ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_519:33.4",
        "original": "Ayameva seyyā kapaṇāpi yā piyā”.",
        "analyzed": "ayam eva seyyā kapaṇā api yā piyā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_519:34.1",
        "original": "“Sudullabhitthī purisassa yā hitā,",
        "analyzed": "su dulla bhitthī purisassa yā hitā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_519:34.2",
        "original": "Bhattitthiyā dullabho yo hito ca;",
        "analyzed": "dullabhaḥ yaḥ hito ca ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_519:34.3",
        "original": "Hitā ca te sīlavatī ca bhariyā,",
        "analyzed": "hitā ca te sīlavatī ca bhariyā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_519:34.4",
        "original": "Janinda dhammaṁ cara sambulāya”.",
        "analyzed": "janinda dhammam cara sambulāya ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_519:35.1",
        "original": "“Sace tuvaṁ vipule laddhabhoge,",
        "analyzed": "sace tuvam vipule laddha bhoge ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_519:35.2",
        "original": "Issāvatiṇṇā maraṇaṁ upesi;",
        "analyzed": "issā avatiṇṇā maraṇam upesi ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_519:35.3",
        "original": "Ahañca te bhadde imā rājakaññā,",
        "analyzed": "aham ca te bhadde imā rāja kaññā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_519:35.4",
        "original": "Sabbe te vacanakarā bhavāmā”ti.",
        "analyzed": "sabbe te vacana karāḥ bhavāma ati ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_519:36.1",
        "original": "Sambulājātakaṁ navamaṁ.",
        "analyzed": "sambulājātakam navama ",
        "folio": "36"
    }
]