        "original": "abhijātaṁ jutindharaṁ;",
        "analyzed": "abhijātam jutindhara ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cp_26:3.3",
        "original": "Setacchattaṁ dhārayitvāna,",
        "analyzed": "sa itacchattam dhārayitvāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cp_26:3.4",
        "original": "sayane poseti maṁ pitā.",
        "analyzed": "sayane posa iti mama pitā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cp_26:4.1",
        "original": "Niddāyamāno sayanavare,",
        "analyzed": "niddāyamānaḥ sayanavare ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_26:4.2",
        "original": "Pabujjhitvānahaṁ tadā;",
        "analyzed": "pabujjhitvān aham tadā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_26:4.3",
        "original": "Addasaṁ paṇḍaraṁ chattaṁ,",
        "analyzed": "addasam paṇḍaram chatta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_26:4.4",
        "original": "Yenāhaṁ nirayaṁ gato.",
        "analyzed": "yena aham nirayam gataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_26:5.1",
        "original": "Saha diṭṭhassa me chattaṁ,",
        "analyzed": "saha diṭṭhassa me chatta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_cp_26:5.2",
        "original": "tāso uppajji bheravo;",
        "analyzed": "tāsaḥ uppajji bheravaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_cp_26:5.3",
        "original": "Vinicchayaṁ samāpanno,",
        "analyzed": "vinicchayam samāpannaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_cp_26:5.4",
        "original": "‘kathāhaṁ imaṁ muñcissaṁ’.",
