    {
        "segmentnr": "PA_tha-ap_2:0.3",
        "original": "2. Paccekabuddhaapadāna",
        "analyzed": "paccekabuddha apadāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_tha-ap_2:1.1",
        "original": "Atha paccekabuddhāpadānaṁ suṇātha—",
        "analyzed": "atha paccekabuddha apadānam suṇātha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_tha-ap_2:2.1",
        "original": "“Tathāgataṁ jetavane vasantaṁ,",
        "analyzed": "tathāgatam jetavane vasanta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_tha-ap_2:2.2",
        "original": "Apucchi vedehamunī nataṅgo;",
        "analyzed": "apucchi veda iha munī nataṅgo ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_tha-ap_2:2.3",
        "original": "‘Paccekabuddhā kira nāma honti,",
        "analyzed": "paccekabuddhāḥ kira nāma honti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_tha-ap_2:2.4",
        "original": "Bhavanti te hetubhi kehi vīra’.",
        "analyzed": "bhavanti te hetubhi kehi vīra ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_tha-ap_2:3.1",
        "original": "Tadāha sabbaññuvaro mahesi,",
        "analyzed": "tat āha sabbaññu varaḥ mahesi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_tha-ap_2:3.2",
        "original": "Ānandabhaddaṁ madhurassarena;",
        "analyzed": "ānanda bhaddam madhurassarena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_tha-ap_2:3.3",
        "original": "‘Ye pubbabuddhesu katādhikārā,",
        "analyzed": "ye pubba buddhesu kata adhikārā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_tha-ap_2:3.4",
        "original": "Aladdhamokkhā jinasāsanesu.",
        "analyzed": "a laddha mokkhāḥ jina sāsanesu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_tha-ap_2:4.1",
        "original": "Teneva saṁvegamukhena dhīrā,",
        "analyzed": "tena iva sa vega mukhena dhīrā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_tha-ap_2:4.2",
        "original": "Vināpi buddhehi sutikkhapaññā;",
        "analyzed": "vinā api buddhehi su tikkha paññā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_tha-ap_2:4.3",
        "original": "Ārammaṇenāpi parittakena,",
        "analyzed": "ārammaṇena api parittakena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_tha-ap_2:4.4",
        "original": "Paccekabodhiṁ anupāpuṇanti.",
        "analyzed": "pacceka bodhim anupāpuṇanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_tha-ap_2:5.1",
        "original": "Sabbamhi lokamhi mamaṁ ṭhapetvā,",
        "analyzed": "sabbamhi lokamhi ṭhapetvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_tha-ap_2:5.2",
        "original": "Paccekabuddhehi samova natthi;",
        "analyzed": "paccekabuddhehi samaḥ uva natthi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_tha-ap_2:5.3",
        "original": "Tesaṁ imaṁ vaṇṇapadesamattaṁ,",
        "analyzed": "tesam imam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_tha-ap_2:5.4",
        "original": "Vakkhāmahaṁ sādhu mahāmunīnaṁ.",
        "analyzed": "sādhu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_tha-ap_2:6.1",
        "original": "Sayameva buddhānaṁ mahāisīnaṁ,",
        "analyzed": "sayam eva buddhānam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_tha-ap_2:6.2",
        "original": "Sādhūni vākyāni madhūva khuddaṁ;",
        "analyzed": "sādhūni vākyāni khudda ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_tha-ap_2:6.3",
        "original": "Anuttaraṁ bhesajaṁ patthayantā,",
        "analyzed": "anuttaram bhesajam patthayantā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_tha-ap_2:6.4",
        "original": "Suṇātha sabbesu pasannacittā’.",
        "analyzed": "suṇātha sabbesu pasanna cittā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_tha-ap_2:7.1",
        "original": "Paccekabuddhānaṁ samāgatānaṁ,",
        "analyzed": "paccekabuddhānam samāgatāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_tha-ap_2:7.2",
        "original": "Paramparaṁ byākaraṇāni yāni;",
        "analyzed": "paramparam byākaraṇāni yāni ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_tha-ap_2:7.3",
        "original": "Ādīnavo yañca virāgavatthuṁ,",
        "analyzed": "ādīnavaḥ yat ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_tha-ap_2:7.4",
        "original": "Yathā ca bodhiṁ anupāpuṇiṁsu.",
        "analyzed": "yathā ca bodhim anupāpuṇisu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_tha-ap_2:8.1",
        "original": "Sarāgavatthūsu virāgasaññī,",
        "analyzed": "sa rāga vatthūsu virāga saññī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_tha-ap_2:8.2",
        "original": "Rattamhi lokamhi virattacittā;",
        "analyzed": "rattamhi lokamhi viratta cittā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_tha-ap_2:8.3",
        "original": "Hitvā papañce jiya phanditāni,",
        "analyzed": "hitvā papañce jiya phanditāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_tha-ap_2:8.4",
        "original": "Tatheva bodhiṁ anupāpuṇiṁsu.",
        "analyzed": "tathā iva bodhim anupāpuṇisu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_tha-ap_2:9.1",
        "original": "Sabbesu bhūtesu nidhāya daṇḍaṁ,",
        "analyzed": "sabbesu bhūtesu nidhāya daṇḍa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_2:9.2",
        "original": "Aviheṭhayaṁ aññatarampi tesaṁ;",
        "analyzed": "aviheṭhayam aññataram pi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_2:9.3",
        "original": "Mettena cittena hitānukampī,",
        "analyzed": "mettena cittena hita anukampī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_2:9.4",
        "original": "Eko care khaggavisāṇakappo.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_tha-ap_2:10.1",
        "original": "Sabbesu bhūtesu nidhāya daṇḍaṁ,",
        "analyzed": "sabbesu bhūtesu nidhāya daṇḍa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_2:10.2",
        "original": "Aviheṭhayaṁ aññatarampi tesaṁ;",
        "analyzed": "aviheṭhayam aññataram pi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_2:10.3",
        "original": "Na puttamiccheyya kuto sahāyaṁ,",
        "analyzed": "na puttam iccheyya kutas sahāya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_2:10.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_tha-ap_2:11.1",
        "original": "‘Saṁsaggajātassa bhavanti snehā,",
        "analyzed": "sa sagga jātassa bhavanti snehāḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_tha-ap_2:11.2",
        "original": "Snehanvayaṁ dukkhamidaṁ pahoti;",
        "analyzed": "dukkham idam pahoti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_tha-ap_2:11.3",
        "original": "Ādīnavaṁ snehajaṁ pekkhamāno,",
        "analyzed": "ādīnavam sneha jam pekkhamāno ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_tha-ap_2:11.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_tha-ap_2:12.1",
        "original": "‘Mitte suhajje anukampamāno,",
        "analyzed": "mitte su hajje anukampamānaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_tha-ap_2:12.2",
        "original": "Hāpeti atthaṁ paṭibaddhacitto;",
        "analyzed": "hāpeti attham paṭibaddha cittaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_tha-ap_2:12.3",
        "original": "Etaṁ bhayaṁ santhave pekkhamāno,",
        "analyzed": "etam bhayam santhave pekkhamāno ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_tha-ap_2:12.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_tha-ap_2:13.1",
        "original": "‘Vaṁso visālova yathā visatto,",
        "analyzed": "vaso visālaḥ uva yathā visattaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_tha-ap_2:13.2",
        "original": "Puttesu dāresu ca yā apekkhā;",
        "analyzed": "puttesu dāresu ca yā apekkhā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_tha-ap_2:13.3",
        "original": "Vaṁse kaḷīrova asajjamāno,",
        "analyzed": "vase kaḷīraḥ uva a sajjamānaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_tha-ap_2:13.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_tha-ap_2:14.1",
        "original": "‘Migo araññamhi yathā abaddho,",
        "analyzed": "migaḥ araññamhi yathā a baddhaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_tha-ap_2:14.2",
        "original": "Yenicchakaṁ gacchati gocarāya;",
        "analyzed": "yenicchakam gacchati gocarāya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_tha-ap_2:14.3",
        "original": "Viññū naro seritaṁ pekkhamāno,",
        "analyzed": "viññū naraḥ sa īritam pekkhamāno ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_tha-ap_2:14.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_tha-ap_2:15.1",
        "original": "‘Āmantanā hoti sahāyamajjhe,",
        "analyzed": "āmantanā hoti sahāya majjhe ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_tha-ap_2:15.2",
        "original": "Vāse ca ṭhāne gamane cārikāya;",
        "analyzed": "vāse ca ṭhāne gamane cārikāya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_tha-ap_2:15.3",
        "original": "Anabhijjhitaṁ seritaṁ pekkhamāno,",
        "analyzed": "an abhijjhitam sa īritam pekkhamānaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_tha-ap_2:15.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_tha-ap_2:16.1",
        "original": "‘Khiḍḍā ratī hoti sahāyamajjhe,",
        "analyzed": "khiḍḍā ratī hoti sahāya majjhe ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_tha-ap_2:16.2",
        "original": "Puttesu pemaṁ vipulañca hoti;",
        "analyzed": "puttesu pemam vipulam ca hoti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_tha-ap_2:16.3",
        "original": "Piyavippayogaṁ vijigucchamāno,",
        "analyzed": "piya vippayogam vijigucchamānaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_tha-ap_2:16.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_tha-ap_2:17.1",
        "original": "‘Cātuddiso appaṭigho ca hoti,",
        "analyzed": "cātuddisaḥ appaṭighaḥ ca hoti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_tha-ap_2:17.2",
        "original": "Santussamāno itarītarena;",
        "analyzed": "itarītarena ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_tha-ap_2:17.3",
        "original": "Parissayānaṁ sahitā achambhī,",
        "analyzed": "parissayānam sahitāḥ a chambhī ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_tha-ap_2:17.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_tha-ap_2:18.1",
        "original": "‘Dussaṅgahā pabbajitāpi eke,",
        "analyzed": "dussaṅgahā pabbajitā api eke ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_2:18.2",
        "original": "Atho gahaṭṭhā gharamāvasantā;",
        "analyzed": "atho gahaṭṭhā gharam āvasantā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_2:18.3",
        "original": "Appossukko paraputtesu hutvā,",
        "analyzed": "appossukkaḥ paraputtesu hutvā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_2:18.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_2:19.1",
        "original": "‘Oropayitvā gihibyañjanāni,",
        "analyzed": "āropayitvā gihi byañjanāni ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_tha-ap_2:19.2",
        "original": "Sañchinnapatto yathā koviḷāro;",
        "analyzed": "sañchinna pattaḥ yathā koviḍāraḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_tha-ap_2:19.3",
        "original": "Chetvāna vīro gihibandhanāni,",
        "analyzed": "chetvāna vīraḥ gihibandhanāni ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_tha-ap_2:19.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_tha-ap_2:20.1",
        "original": "‘Sace labhetha nipakaṁ sahāyaṁ,",
        "analyzed": "sace labhetha nipakam sahāya ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_2:20.2",
        "original": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
        "analyzed": "saddhim caram sādhu vihāri dhīra ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_2:20.3",
        "original": "Abhibhuyya sabbāni parissayāni,",
        "analyzed": "abhibhuyya sabbāni parissayāni ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_2:20.4",
        "original": "Careyya tenattamano satīmā’.",
        "analyzed": "careyya satī imā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_tha-ap_2:21.1",
        "original": "‘No ce labhetha nipakaṁ sahāyaṁ,",
        "analyzed": "na u ce labhetha nipakam sahāya ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_2:21.2",
        "original": "Saddhiṁ caraṁ sādhuvihāri dhīraṁ;",
        "analyzed": "saddhim caram sādhu vihāri dhīra ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_2:21.3",
        "original": "Rājāva raṭṭhaṁ vijitaṁ pahāya,",
        "analyzed": "rājā ava raṭṭham vijitam pahāya ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_2:21.4",
        "original": "Eko care mātaṅgaraññeva nāgo’.",
        "analyzed": "ekaḥ care mātaṅga raññā iva nāgaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_tha-ap_2:22.1",
        "original": "‘Addhā pasaṁsāma sahāyasampadaṁ,",
        "analyzed": "addhā pasasāma sahāya sampada ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_2:22.2",
        "original": "Seṭṭhā samā sevitabbā sahāyā;",
        "analyzed": "seṭṭhā samā sevitabbā sahāyā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_2:22.3",
        "original": "Ete aladdhā anavajjabhojī,",
        "analyzed": "ete a laddhā an avajja bhojī ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_2:22.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_tha-ap_2:23.1",
        "original": "‘Disvā suvaṇṇassa pabhassarāni,",
        "analyzed": "disvā suvaṇṇassa pabhassarāni ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_2:23.2",
        "original": "Kammāraputtena suniṭṭhitāni;",
        "analyzed": "kammāraputtena su niṭṭhitāni ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_2:23.3",
        "original": "Saṅghaṭṭamānāni duve bhujasmiṁ,",
        "analyzed": "saṅghaṭṭamānāni duve bhujasmi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_2:23.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_tha-ap_2:24.1",
        "original": "‘Evaṁ dutīyena sahā mamassa,",
        "analyzed": "evam dutīyena sahā mamassa ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_2:24.2",
        "original": "Vācābhilāpo abhisajjanā vā;",
        "analyzed": "vācā abhilāpaḥ abhisajjanā vā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_2:24.3",
        "original": "Etaṁ bhayaṁ āyatiṁ pekkhamāno,",
        "analyzed": "etam bhayam āyatim pekkhamāno ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_2:24.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_tha-ap_2:25.1",
        "original": "‘Kāmā hi citrā madhurā manoramā,",
        "analyzed": "kāmā hi citrā madhurā manoramā ",
        "folio": "25"
    },
    {
