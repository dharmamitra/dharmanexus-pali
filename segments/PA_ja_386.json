        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_386:3.4",
        "original": "yaṁ tvaṁ vahasi senakaṁ”.",
        "analyzed": "yam tvam vahasi senaka ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_386:4.1",
        "original": "“Yannu samma ahaṁ bālo,",
        "analyzed": "yat nu aham bālaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_386:4.2",
