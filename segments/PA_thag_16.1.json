        "analyzed": "khaya gāmī anuttaraḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_16.1:19.3",
        "original": "Tassa sāsanamāgamma,",
        "analyzed": "tassa sāsanam āgamma ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_16.1:19.4",
        "original": "labbhate taṁ asokatā”.",
        "analyzed": "labbhate tam asoka tā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_thag_16.1:20.1",
        "original": "Sutvāna corā isino subhāsitaṁ,",
        "analyzed": "sutvāna corāḥ isinaḥ subhāsita ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.1:20.2",
        "original": "Nikkhippa satthāni ca āvudhāni ca;",
        "analyzed": "nikkhippa satthāni ca āvudhāni ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.1:20.3",
        "original": "Tamhā ca kammā viramiṁsu eke,",
        "analyzed": "tamhā ca kammā viramisu eke ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.1:20.4",
        "original": "Eke ca pabbajjamarocayiṁsu.",
        "analyzed": "eke ca pabbajjam arocayisu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_thag_16.1:21.1",
        "original": "Te pabbajitvā sugatassa sāsane,",
        "analyzed": "te pabbajitvā sugatassa sāsane ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_thag_16.1:21.2",
        "original": "Bhāvetva bojjhaṅgabalāni paṇḍitā;",
        "analyzed": "paṇḍitā ",
        "folio": "21"
    },
