    {
        "segmentnr": "PA_vv_64:0.4",
        "original": "14. Mahārathavimānavatthu",
        "analyzed": "mahā ratha vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_64:1.1",
        "original": "“Sahassayuttaṁ hayavāhanaṁ subhaṁ,",
        "analyzed": "sahassa yuttam haya vāhanam subha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_64:1.2",
        "original": "Āruyhimaṁ sandanaṁ nekacittaṁ;",
        "analyzed": "āruyhimam sandanam nekacitta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_64:1.3",
        "original": "Uyyānabhūmiṁ abhito anukkamaṁ,",
        "analyzed": "uyyāna bhūmim abhitas anukkama ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_64:1.4",
        "original": "Purindado bhūtapatīva vāsavo.",
        "analyzed": "purindadaḥ bhūtapatī iva vāsavaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_64:2.1",
        "original": "Sovaṇṇamayā te rathakubbarā ubho,",
        "analyzed": "sovaṇṇa mayāḥ te ratha kubbarāḥ ubho ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_64:2.2",
        "original": "Phalehi aṁsehi atīva saṅgatā;",
        "analyzed": "phalehi asehi atīva saṅga tā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_64:2.3",
        "original": "Sujātagumbā naravīraniṭṭhitā,",
        "analyzed": "        ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_64:2.4",
        "original": "Virocatī pannaraseva cando.",
        "analyzed": "virocatī pannarasā iva cando ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_64:3.1",
        "original": "Suvaṇṇajālāvatato ratho ayaṁ,",
        "analyzed": "suvaṇṇa jāla avatataḥ rathaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_64:3.2",
        "original": "Bahūhi nānāratanehi cittito;",
        "analyzed": "bahūhi nānā ratanehi cittitaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_64:3.3",
        "original": "Sunandighoso ca subhassaro ca,",
        "analyzed": "sunandighosaḥ ca subhassaro ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_64:3.4",
        "original": "Virocatī cāmarahatthabāhubhi.",
        "analyzed": "virocatī cāmara hattha bāhubhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_64:4.1",
        "original": "Imā ca nābhyo manasābhinimmitā,",
        "analyzed": "imā ca nābhyaḥ manasā abhinimmitā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_64:4.2",
        "original": "Rathassa pādantaramajjhabhūsitā;",
        "analyzed": "rathassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_64:4.3",
        "original": "Imā ca nābhyo satarājicittitā,",
        "analyzed": "imā ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_64:4.4",
        "original": "Sateratā vijjurivappabhāsare.",
        "analyzed": "vijjuḥ iva ppabhāsare ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_64:5.1",
        "original": "Anekacittāvatato ratho ayaṁ,",
        "analyzed": "anekacitta avatataḥ rathaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_64:5.2",
        "original": "Puthū ca nemī ca sahassaraṁsiko;",
        "analyzed": "puthū ca nemī ca sahassa rasiko ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_64:5.3",
        "original": "Tesaṁ saro suyyati vaggurūpo,",
        "analyzed": "tesam saraḥ suyyati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_64:5.4",
        "original": "Pañcaṅgikaṁ tūriyamivappavāditaṁ.",
        "analyzed": "pañcaṅgikam tūriyam iva ppavādita ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_64:6.1",
        "original": "Sirasmiṁ cittaṁ maṇicandakappitaṁ,",
        "analyzed": "sirasmim cittam maṇi canda kappita ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_64:6.2",
        "original": "Sadā visuddhaṁ ruciraṁ pabhassaraṁ;",
        "analyzed": "sadā visuddham ruciram pabhassara ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_64:6.3",
        "original": "Suvaṇṇarājīhi atīva saṅgataṁ,",
        "analyzed": "suvaṇṇa rājīhi atīva ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_64:6.4",
        "original": "Veḷuriyarājīva atīva sobhati.",
        "analyzed": "atīva sobhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_64:7.1",
        "original": "Ime ca vāḷī maṇicandakappitā,",
        "analyzed": "ime ca vāḷī maṇi canda kappitā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_64:7.2",
        "original": "Ārohakambū sujavā brahūpamā;",
        "analyzed": "āroha kambū su javā brahu upamā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_64:7.3",
        "original": "Brahā mahantā balino mahājavā,",
        "analyzed": "brahā mahantā balinaḥ mahā javā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_64:7.4",
        "original": "Mano tavaññāya tatheva siṁsare.",
        "analyzed": "mano tavaññāya tathā iva sisare ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_64:8.1",
        "original": "Ime ca sabbe sahitā catukkamā,",
        "analyzed": "ime ca sabbe sahitā catukkamā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_64:8.2",
        "original": "Mano tavaññāya tatheva siṁsare;",
        "analyzed": "mano tavaññāya tathā iva sisare ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_64:8.3",
        "original": "Samaṁ vahantī mudukā anuddhatā,",
        "analyzed": "samam vahantī mudukā anuddhatā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_64:8.4",
        "original": "Āmodamānā turagānamuttama.",
        "analyzed": "āmodamānā turagānam uttama ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_64:9.1",
        "original": "Dhunanti vagganti patanti cambare,",
        "analyzed": "dhunanti vagganti patanti cambare ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_64:9.2",
        "original": "Abbhuddhunantā sukate piḷandhane;",
        "analyzed": "su kate piḷandhane ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_64:9.3",
        "original": "Tesaṁ saro suyyati vaggurūpo,",
        "analyzed": "tesam saraḥ suyyati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_64:9.4",
        "original": "Pañcaṅgikaṁ tūriyamivappavāditaṁ.",
        "analyzed": "pañcaṅgikam tūriyam iva ppavādita ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_64:10.1",
        "original": "Rathassa ghoso apiḷandhanāna ca,",
        "analyzed": "rathassa ghosaḥ a piḷandhanāna ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_64:10.2",
        "original": "Khurassa nādo abhihiṁsanāya ca;",
        "analyzed": "khurassa nādaḥ abhihisanāya ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_64:10.3",
        "original": "Ghoso suvaggū samitassa suyyati,",
        "analyzed": "ghosaḥ suvaggū samitassa suyyati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_64:10.4",
        "original": "Gandhabbatūriyāni vicitrasaṁvane.",
        "analyzed": "gandhabbatūriyāni vicitra savane ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_64:11.1",
        "original": "Rathe ṭhitā tā migamandalocanā,",
        "analyzed": "rathe ṭhitā tā miga manda locanā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_64:11.2",
        "original": "Āḷārapamhā hasitā piyaṁvadā;",
        "analyzed": "āḷārapamhā hasitā piyavadā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_64:11.3",
        "original": "Veḷuriyajālāvatatā tanucchavā,",
        "analyzed": "veḷuriya jāla avatatā tanu chavā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_64:11.4",
        "original": "Sadeva gandhabbasūraggapūjitā.",
        "analyzed": "sa deva gandhabba sūragga pūjitā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_64:12.1",
        "original": "Tā rattarattambarapītavāsasā,",
        "analyzed": "tāḥ ratta ratta ambara pīta vāsasā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_64:12.2",
        "original": "Visālanettā abhirattalocanā;",
        "analyzed": "visāla nettā abhiratta locanā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_64:12.3",
        "original": "Kule sujātā sutanū sucimhitā,",
        "analyzed": "kule su jātā su tanū su cimhitā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_64:12.4",
        "original": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
        "analyzed": "rathe ṭhitā pañjalikā upaṭṭhitā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_64:13.1",
        "original": "Tā kambukeyūradharā suvāsasā,",
        "analyzed": "tā kambu keyūra dharā su vāsasā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_64:13.2",
        "original": "Sumajjhimā ūruthanūpapannā;",
        "analyzed": "su majjhimā ūru thanū upapannā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_64:13.3",
        "original": "Vaṭṭaṅguliyo sumukhā sudassanā,",
        "analyzed": "vaṭṭaṅguliyaḥ su mukhā su dassanā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_64:13.4",
        "original": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
        "analyzed": "rathe ṭhitā pañjalikā upaṭṭhitā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_vv_64:14.1",
        "original": "Aññā suveṇī susu missakesiyo,",
        "analyzed": "aññā suveṇī susu missakesiyaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_64:14.2",
        "original": "Samaṁ vibhattāhi pabhassarāhi ca;",
        "analyzed": "samam ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_64:14.3",
        "original": "Anubbatā tā tava mānase ratā,",
        "analyzed": "anubbatā tā tava mānase ratā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_64:14.4",
        "original": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
        "analyzed": "rathe ṭhitā pañjalikā upaṭṭhitā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_vv_64:15.1",
        "original": "Āveḷiniyo padumuppalacchadā,",
        "analyzed": "padumuppalacchadā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_64:15.2",
        "original": "Alaṅkatā candanasāravāsitā;",
        "analyzed": "candana sāra vāsi tā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_64:15.3",
        "original": "Anubbatā tā tava mānase ratā,",
        "analyzed": "anubbatā tā tava mānase ratā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_64:15.4",
        "original": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
        "analyzed": "rathe ṭhitā pañjalikā upaṭṭhitā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_vv_64:16.1",
        "original": "Tā māliniyo padumuppalacchadā,",
        "analyzed": "tāḥ māliniyaḥ paduma uppala chadā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_64:16.2",
        "original": "Alaṅkatā candanasāravāsitā;",
        "analyzed": "candana sāra vāsi tā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_64:16.3",
        "original": "Anubbatā tā tava mānase ratā,",
        "analyzed": "anubbatā tā tava mānase ratā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_64:16.4",
        "original": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
        "analyzed": "rathe ṭhitā pañjalikā upaṭṭhitā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_vv_64:17.1",
        "original": "Kaṇṭhesu te yāni piḷandhanāni,",
        "analyzed": "kaṇṭhesu te yāni piḷandhanāni ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_64:17.2",
        "original": "Hatthesu pādesu tatheva sīse;",
        "analyzed": "hatthesu pādesu tathā iva sīse ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_64:17.3",
        "original": "Obhāsayantī dasa sabbaso disā,",
        "analyzed": "obhāsayantī dasa sabbasas disā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_64:17.4",
        "original": "Abbhuddayaṁ sāradikova bhāṇumā.",
        "analyzed": "sāradikā uva ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_vv_64:18.1",
        "original": "Vātassa vegena ca sampakampitā,",
        "analyzed": "vātassa vegena ca sampakampitā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_64:18.2",
        "original": "Bhujesu mālā apiḷandhanāni ca;",
        "analyzed": "bhujesu mālā apiḷandhanāni ca ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_64:18.3",
        "original": "Muñcanti ghosaṁ ruciraṁ suciṁ subhaṁ,",
        "analyzed": "muñcanti ghosam ruciram sucim subha ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_64:18.4",
        "original": "Sabbehi viññūhi sutabbarūpaṁ.",
        "analyzed": "sabbehi viññūhi sutabba rūpa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_vv_64:19.1",
        "original": "Uyyānabhūmyā ca duvaddhato ṭhitā,",
        "analyzed": "uyyāna bhūmyā ca ṭhitā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_64:19.2",
        "original": "Rathā ca nāgā tūriyāni ca saro;",
        "analyzed": "rathā ca nāgā tūriyāni ca saro ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_64:19.3",
        "original": "Tameva devinda pamodayanti,",
        "analyzed": "tam eva devinda pamodayanti ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_64:19.4",
        "original": "Vīṇā yathā pokkharapattabāhubhi.",
        "analyzed": "vīṇā yathā pokkharapatta bāhubhi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_vv_64:20.1",
        "original": "Imāsu vīṇāsu bahūsu vaggūsu,",
        "analyzed": "imāsu vīṇāsu bahūsu vaggūsu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_64:20.2",
        "original": "Manuññarūpāsu hadayeritaṁ pītiṁ;",
        "analyzed": "manuñña rūpāsu hadaya īritam pīti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_64:20.3",
        "original": "Pavajjamānāsu atīva accharā,",
        "analyzed": "pavajjamānāsu atīva accharā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_64:20.4",
        "original": "Bhamanti kaññā padumesu sikkhitā.",
        "analyzed": "bhamanti kaññā padumesu sikkhitā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_vv_64:21.1",
        "original": "Yadā ca gītāni ca vāditāni ca,",
        "analyzed": "yadā ca gītāni ca vāditāni ca ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_64:21.2",
        "original": "Naccāni cimāni samenti ekato;",
        "analyzed": "naccāni cimāni samenti ekatas ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_64:21.3",
        "original": "Athettha naccanti athettha accharā,",
        "analyzed": "atha itthā naccanti atha itthā accharā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_64:21.4",
        "original": "Obhāsayantī ubhato varitthiyo.",
        "analyzed": "obhāsayantī ubhatas ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_vv_64:22.1",
        "original": "So modasi tūriyagaṇappabodhano,",
        "analyzed": "saḥ modasi tūriya gaṇa ppabodhano ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_64:22.2",
        "original": "Mahīyamāno vajirāvudhoriva;",
        "analyzed": "mahīyamānaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_64:22.3",
        "original": "Imāsu vīṇāsu bahūsu vaggūsu,",
        "analyzed": "imāsu vīṇāsu bahūsu vaggūsu ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_64:22.4",
        "original": "Manuññarūpāsu hadayeritaṁ pītiṁ.",
        "analyzed": "manuñña rūpāsu hadaya īritam pīti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_vv_64:23.1",
        "original": "Kiṁ tvaṁ pure kammamakāsi attanā,",
        "analyzed": "kim tvam pure attanā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_64:23.2",
        "original": "Manussabhūto purimāya jātiyā;",
        "analyzed": "manussa bhūtaḥ purimāya jātiyā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_64:23.3",
        "original": "Uposathaṁ kaṁ vā tuvaṁ upāvasi,",
        "analyzed": "uposatham kam vā tuvam upāvasi ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_64:23.4",
        "original": "Kaṁ dhammacariyaṁ vatamābhirocayi.",
        "analyzed": "kam dhamma cariyam ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_vv_64:24.1",
        "original": "Nayīdamappassa katassa kammuno,",
        "analyzed": "nayi idam appassa katassa kammunaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_vv_64:24.2",
        "original": "Pubbe suciṇṇassa uposathassa vā;",
        "analyzed": "pubbe su ciṇṇassa uposathassa vā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_vv_64:24.3",
        "original": "Iddhānubhāvo vipulo ayaṁ tava,",
        "analyzed": "iddha anubhāvaḥ vipulaḥ ayam tava ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_vv_64:24.4",
        "original": "Yaṁ devasaṅghaṁ abhirocase bhusaṁ.",
        "analyzed": "yam deva saṅgham abhirocase bhusa ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_vv_64:25.1",
        "original": "Dānassa te idaṁ phalaṁ,",
        "analyzed": "dānassa te idam phala ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_vv_64:25.2",
        "original": "atho sīlassa vā pana;",
        "analyzed": "atho sīlassa vā pana ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_vv_64:25.3",
        "original": "Atho añjalikammassa,",
        "analyzed": "atha u añjali kammassa ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_vv_64:25.4",
        "original": "taṁ me akkhāhi pucchito”ti.",
        "analyzed": "tam me akkhāhi pucchito ti ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_vv_64:26.1",
        "original": "So devaputto attamano,",
        "analyzed": "saḥ deva puttaḥ atta mano ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_vv_64:26.2",
        "original": "moggallānena pucchito;",
        "analyzed": "moggallānena pucchito ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_vv_64:26.3",
        "original": "Pañhaṁ puṭṭho viyākāsi,",
        "analyzed": "pañham puṭṭhaḥ viyākāsi ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_vv_64:26.4",
        "original": "yassa kammassidaṁ phalanti.",
        "analyzed": "yassa kammassa idam phalanti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_vv_64:27.1",
        "original": "“Jitindriyaṁ buddhamanomanikkamaṁ,",
        "analyzed": "jitindriyam ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_64:27.2",
        "original": "Naruttamaṁ kassapamaggapuggalaṁ;",
        "analyzed": "naruttamam kassapa magga puggala ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_64:27.3",
        "original": "Avāpurantaṁ amatassa dvāraṁ,",
        "analyzed": "avāpuḥ antam amatassa dvāra ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_64:27.4",
        "original": "Devātidevaṁ satapuññalakkhaṇaṁ.",
        "analyzed": "devātidevam sata puñña lakkhaṇa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_vv_64:28.1",
        "original": "Tamaddasaṁ kuñjaramoghatiṇṇaṁ,",
        "analyzed": "tam addasam kuñjara mogha tiṇṇa ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_64:28.2",
        "original": "Suvaṇṇasiṅgīnadabimbasādisaṁ;",
        "analyzed": "          ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_64:28.3",
        "original": "Disvāna taṁ khippamahuṁ sucīmano,",
        "analyzed": "disvāna tam khippa mahum sucīmano ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_64:28.4",
        "original": "Tameva disvāna subhāsitaddhajaṁ.",
        "analyzed": "tam eva disvāna ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_vv_64:29.1",
        "original": "Tamannapānaṁ athavāpi cīvaraṁ,",
        "analyzed": "tam anna pānam athavā api cīvara ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_64:29.2",
        "original": "Suciṁ paṇītaṁ rasasā upetaṁ;",
        "analyzed": "sucim paṇītam rasasaḥ upeta ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_64:29.3",
        "original": "Pupphābhikkiṇamhi sake nivesane,",
        "analyzed": "puppha abhikkiṇamhi sake nivesane ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_64:29.4",
        "original": "Patiṭṭhapesiṁ sa asaṅgamānaso.",
        "analyzed": "sa asaṅga mānasaḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_vv_64:30.1",
        "original": "Tamannapānena ca cīvarena ca,",
        "analyzed": "tam anna pānena ca cīvarena ca ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_64:30.2",
        "original": "Khajjena bhojjena ca sāyanena ca;",
        "analyzed": "khajjena bhojjena ca sa ayanena ca ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_64:30.3",
        "original": "Santappayitvā dvipadānamuttamaṁ,",
        "analyzed": "santappayitvā ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_64:30.4",
        "original": "So saggaso devapure ramāmahaṁ.",
        "analyzed": "saḥ saggasaḥ deva pure ramāmaha ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_vv_64:31.1",
        "original": "Etenupāyena imaṁ niraggaḷaṁ,",
        "analyzed": "etena upāyena imam niraggaḷa ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_64:31.2",
        "original": "Yaññaṁ yajitvā tividhaṁ visuddhaṁ;",
        "analyzed": "yaññam yajitvā tividham ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_64:31.3",
        "original": "Pahāyahaṁ mānusakaṁ samussayaṁ,",
        "analyzed": "pahāya aham mānusakam ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_64:31.4",
        "original": "Indūpamo devapure ramāmahaṁ.",
        "analyzed": "indu upamaḥ deva pure ramāmaha ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_vv_64:32.1",
        "original": "Āyuñca vaṇṇañca sukhaṁ balañca,",
        "analyzed": "āyuṃ ca vaṇṇañca sukham balañca ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_64:32.2",
        "original": "Paṇītarūpaṁ abhikaṅkhatā muni;",
        "analyzed": "paṇīta rūpam abhikaṅkhatā muni ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_64:32.3",
        "original": "Annañca pānañca bahuṁ susaṅkhataṁ,",
        "analyzed": "annañca pānañca bahum su saṅkhata ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_64:32.4",
        "original": "Patiṭṭhapetabbamasaṅgamānase.",
        "analyzed": "patiṭṭhapetabbam asaṅga mānase ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_vv_64:33.1",
        "original": "Nayimasmiṁ loke parasmiṁ vā pana,",
        "analyzed": "nayimasmin loke parasmin vā pana ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_64:33.2",
        "original": "Buddhena seṭṭho va samo va vijjati;",
        "analyzed": "buddhena seṭṭhaḥ va samaḥ va vijjati ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_64:33.3",
        "original": "Āhuneyyānaṁ paramāhutiṁ gato,",
        "analyzed": "āhuneyyānam paramāhutim gataḥ ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_64:33.4",
        "original": "Puññatthikānaṁ vipulapphalesinan”ti.",
        "analyzed": "puññatthikānam vipula phala esinam ti ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_vv_64:34.1",
        "original": "Mahārathavimānaṁ cuddasamaṁ.",
        "analyzed": "mahā ratha vimānam ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_vv_64:34.2",
        "original": "Mahārathavaggo pañcamo.",
        "analyzed": "mahā ratha vaggaḥ pañcamaḥ ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_vv_64:35.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_vv_64:36.1",
        "original": "Maṇḍūko revatī chatto,",
        "analyzed": "maṇḍūkaḥ revatī chattaḥ ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_vv_64:36.2",
        "original": "Kakkaṭo dvārapālako;",
        "analyzed": "kakkaṭaḥ dvārapālakaḥ ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_vv_64:36.3",
        "original": "Dve karaṇīyā dve sūci,",
        "analyzed": "dve karaṇīyā dve sūci ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_vv_64:36.4",
        "original": "Tayo nāgā ca dve rathā;",
        "analyzed": "tayaḥ nāgā ca dve rathā ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_vv_64:36.5",
        "original": "Purisānaṁ paṭhamo vaggo pavuccatīti.",
        "analyzed": "purisānam paṭhamaḥ vaggo pavuccati iti ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_vv_64:37.1",
        "original": "Bhāṇavāraṁ tatiyaṁ.",
        "analyzed": "bhāṇa vāram ",
        "folio": "37"
    }
]