        "analyzed": "        ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_506:12.1",
        "original": "Dve ca sādisiyo bhariyā,",
        "analyzed": "dve ca bhariyā ",
        "folio": "12"
    },
    {
