        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.32",
        "original": "“Evañca pana, bhikkhave, samanubhāsitabbā.",
        "analyzed": "evam ca pana bhikkhave samanubhāsitabbā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.33",
        "original": "Byattāya bhikkhuniyā paṭibalāya saṅgho ñāpetabbo—",
        "analyzed": "byattāya bhikkhuniyā paṭibalāya saṅghaḥ ñāpetabbaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.34",
        "original": "‘Suṇātu me, ayye, saṅgho.",
        "analyzed": "suṇātu me ayye saṅghaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.35",
        "original": "Ayaṁ itthannāmā bhikkhunī saṁsaṭṭhā viharati gahapatināpi gahapatiputtenapi.",
        "analyzed": "ayam itthannāmā bhikkhunī sa saṭṭhā viharati gahapatinā api gahapati puttena api ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.36",
        "original": "Sā taṁ vatthuṁ na paṭinissajjati.",
        "analyzed": "sā tam vatthum na paṭinissajjati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.37",
        "original": "Yadi saṅghassa pattakallaṁ, saṅgho itthannāmaṁ bhikkhuniṁ samanubhāseyya tassa vatthussa paṭinissaggāya.",
        "analyzed": "yadi saṅghassa pattakalla saṅghaḥ itthannāmam bhikkhunim samanubhāseyya tassa vatthussa paṭinissaggāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.38",
        "original": "Esā ñatti.",
        "analyzed": "esā ñatti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.39",
        "original": "Suṇātu me, ayye, saṅgho.",
        "analyzed": "suṇātu me ayye saṅghaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.40",
        "original": "Ayaṁ itthannāmā bhikkhunī saṁsaṭṭhā viharati gahapatināpi gahapatiputtenapi.",
        "analyzed": "ayam itthannāmā bhikkhunī sa saṭṭhā viharati gahapatinā api gahapati puttena api ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.41",
        "original": "Sā taṁ vatthuṁ na paṭinissajjati.",
        "analyzed": "sā tam vatthum na paṭinissajjati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.42",
        "original": "Saṅgho itthannāmaṁ bhikkhuniṁ samanubhāsati tassa vatthussa paṭinissaggāya.",
        "analyzed": "saṅghaḥ itthannāmam bhikkhunim samanubhāsati tassa vatthussa paṭinissaggāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.43",
        "original": "Yassā ayyāya khamati itthannāmāya bhikkhuniyā samanubhāsanā tassa vatthussa paṭinissaggāya, sā tuṇhassa;",
        "analyzed": "yassa ayyāya khamati itthannāmāya bhikkhuniyā samanubhāsanā tassa vatthussa paṭinissaggāya sā tuṇhassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.44",
        "original": "yassā nakkhamati, sā bhāseyya.",
        "analyzed": "yassā nakkhamati sā bhāseyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.45",
        "original": "Dutiyampi etamatthaṁ vadāmi …pe…",
        "analyzed": "dutiyam pi etam attham vadāmi pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.46",
        "original": "tatiyampi etamatthaṁ vadāmi …pe….",
        "analyzed": "tatiyam pi etam attham vadāmi pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.47",
        "original": "Samanubhaṭṭhā saṅghena itthannāmā bhikkhunī tassa vatthussa paṭinissaggāya;",
        "analyzed": "samanubhaṭṭhā saṅghena itthannāmā bhikkhunī tassa vatthussa paṭinissaggāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.48",
        "original": "khamati saṅghassa, tasmā tuṇhī, evametaṁ dhārayāmī’”ti.",
        "analyzed": "khamati saṅghassa tasmai tuṇhī evam etam dhārayāmi iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.49",
        "original": "Ñattiyā dukkaṭaṁ.",
        "analyzed": "ñattiyā dukkaṭa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.50",
        "original": "Dvīhi kammavācāhi dukkaṭā.",
        "analyzed": "dvīhi kamma vācāhi dukkaṭā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.1.51",
        "original": "Kammavācāpariyosāne āpatti pācittiyassa.",
        "analyzed": "kamma vācā pariyosāne āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.2.1",
        "original": "Dhammakamme dhammakammasaññā na paṭinissajjati, āpatti pācittiyassa.",
        "analyzed": "dhamma kamme dhamma kamma saññā na paṭinissajjati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.2.2",
        "original": "Dhammakamme vematikā na paṭinissajjati, āpatti pācittiyassa.",
        "analyzed": "dhamma kamme vematikā na paṭinissajjati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.2.3",
        "original": "Dhammakamme adhammakammasaññā na paṭinissajjati, āpatti pācittiyassa.",
        "analyzed": "dhamma kamme adhamma kamma saññā na paṭinissajjati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.2.4",
        "original": "Adhammakamme dhammakammasaññā, āpatti dukkaṭassa.",
        "analyzed": "adhamma kamme dhamma kamma saññā āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc36:2.2.5",
