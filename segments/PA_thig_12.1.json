[
    {
        "segmentnr": "PA_thig_12.1:0.1",
        "original": "Therīgāthā 12.1",
        "analyzed": "therīgāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_12.1:0.2",
        "original": "Soḷasakanipāta",
        "analyzed": "sa uḍasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_12.1:0.3",
        "original": "Puṇṇātherīgāthā",
        "analyzed": "puṇṇātherīgāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_12.1:1.1",
        "original": "“Udahārī ahaṁ sīte,",
        "analyzed": "uda hārī aham sīte ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_12.1:1.2",
        "original": "sadā udakamotariṁ;",
        "analyzed": "sadā udaka motari ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_12.1:1.3",
        "original": "Ayyānaṁ daṇḍabhayabhītā,",
        "analyzed": "ayyānam daṇḍa bhaya bhītā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_12.1:1.4",
        "original": "vācādosabhayaṭṭitā.",
        "analyzed": "vācā dosa bhaya ṭṭitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_12.1:2.1",
        "original": "Kassa brāhmaṇa tvaṁ bhīto,",
        "analyzed": "kassa brāhmaṇa tvam bhītaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_12.1:2.2",
        "original": "sadā udakamotari;",
        "analyzed": "sadā udaka motari ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_12.1:2.3",
        "original": "Vedhamānehi gattehi,",
        "analyzed": "vedhamānehi gattehi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_12.1:2.4",
        "original": "sītaṁ vedayase bhusaṁ”.",
        "analyzed": "sītam vedayase bhusa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_12.1:3.1",
        "original": "“Jānantī vata maṁ bhoti,",
        "analyzed": "jānantī vata mama ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_12.1:3.2",
        "original": "puṇṇike paripucchasi;",
        "analyzed": "puṇṇike paripucchasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_12.1:3.3",
        "original": "Karontaṁ kusalaṁ kammaṁ,",
        "analyzed": "karontam kusalam kamma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_12.1:3.4",
        "original": "rundhantaṁ katapāpakaṁ.",
        "analyzed": "rundhantam kata pāpaka ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_12.1:4.1",
        "original": "Yo ca vuḍḍho daharo vā,",
        "analyzed": "yaḥ ca vuḍḍhaḥ daharaḥ vā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_12.1:4.2",
        "original": "pāpakammaṁ pakubbati;",
        "analyzed": "pāpa kammam pakubbati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_12.1:4.3",
        "original": "Dakābhisecanā sopi,",
        "analyzed": "daka abhisecanā sā upi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_12.1:4.4",
        "original": "pāpakammā pamuccati”.",
        "analyzed": "pāpa kammā pamuccati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_12.1:5.1",
        "original": "“Ko nu te idamakkhāsi,",
        "analyzed": "kaḥ nu te idam akkhāsi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_12.1:5.2",
        "original": "ajānantassa ajānako;",
        "analyzed": "a jānantassa a jānakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_12.1:5.3",
        "original": "Dakābhisecanā nāma,",
        "analyzed": "dakābhisecanā nāma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_12.1:5.4",
        "original": "pāpakammā pamuccati.",
        "analyzed": "pāpa kammā pamuccati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_12.1:6.1",
        "original": "Saggaṁ nūna gamissanti,",
        "analyzed": "saggam nūna gamissanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_12.1:6.2",
        "original": "sabbe maṇḍūkakacchapā;",
        "analyzed": "sabbe maṇḍūka kacchapā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_12.1:6.3",
        "original": "Nāgā ca susumārā ca,",
        "analyzed": "nāgā ca susumārā ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_12.1:6.4",
        "original": "ye caññe udake carā.",
        "analyzed": "ye caññe udake carā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_12.1:7.1",
        "original": "Orabbhikā sūkarikā,",
        "analyzed": "orabbhikā sūkarikā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_12.1:7.2",
        "original": "macchikā migabandhakā;",
        "analyzed": "macchikā migabandhakā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_12.1:7.3",
        "original": "Corā ca vajjhaghātā ca,",
        "analyzed": "corā ca vajjhaghātā ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_12.1:7.4",
        "original": "ye caññe pāpakammino;",
        "analyzed": "ye ca aññe pāpa kammino ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_12.1:7.5",
        "original": "Dakābhisecanā tepi,",
        "analyzed": "daka abhisecanā te pi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_12.1:7.6",
        "original": "pāpakammā pamuccare.",
        "analyzed": "pāpa kammā pamuccare ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_12.1:8.1",
        "original": "Sace imā nadiyo te,",
        "analyzed": "sace imā nadiyo te ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_12.1:8.2",
        "original": "pāpaṁ pubbe kataṁ vahuṁ;",
        "analyzed": "pāpam pubbe katam vahu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_12.1:8.3",
        "original": "Puññampimā vaheyyuṁ te,",
        "analyzed": "puññampimā vaheyyum te ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_12.1:8.4",
        "original": "tena tvaṁ paribāhiro.",
        "analyzed": "tena tvam paribāhiraḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_12.1:9.1",
        "original": "Yassa brāhmaṇa tvaṁ bhīto,",
        "analyzed": "yassa brāhmaṇa tvam bhītaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_12.1:9.2",
        "original": "sadā udakamotari;",
        "analyzed": "sadā udaka motari ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_12.1:9.3",
        "original": "Tameva brahme mā kāsi,",
        "analyzed": "tam eva brahme mā kāsi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_12.1:9.4",
        "original": "mā te sītaṁ chaviṁ hane”.",
        "analyzed": "mā te sītam chavim hane ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_12.1:10.1",
        "original": "“Kummaggapaṭipannaṁ maṁ,",
        "analyzed": "kummagga paṭipannam ma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_12.1:10.2",
        "original": "ariyamaggaṁ samānayi;",
        "analyzed": "ariya maggam samānayi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_12.1:10.3",
        "original": "Dakābhisecanā bhoti,",
        "analyzed": "daka abhisecanā bhoti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_12.1:10.4",
        "original": "imaṁ sāṭaṁ dadāmi te”.",
        "analyzed": "imam sāṭam dadāmi te ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_12.1:11.1",
        "original": "“Tuyheva sāṭako hotu,",
