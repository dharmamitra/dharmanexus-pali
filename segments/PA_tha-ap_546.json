        "folio": "7"
    },
    {
        "segmentnr": "PA_tha-ap_546:7.4",
        "original": "paṭivattā na vijjati.",
        "analyzed": "paṭivattā na vijjati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_tha-ap_546:8.1",
        "original": "Desento pavaraṁ dhammaṁ,",
        "analyzed": "desento pavaram dhamma ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_tha-ap_546:8.2",
        "original": "santārento sadevakaṁ;",
        "analyzed": "santārento sa devaka ",
        "folio": "8"
    },
