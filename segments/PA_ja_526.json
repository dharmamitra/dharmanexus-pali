    },
    {
        "segmentnr": "PA_ja_526:1.1",
        "original": "“Uddayhate janapado,",
        "analyzed": "uddayhate janapadaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_526:1.2",
        "original": "raṭṭhañcāpi vinassati;",
        "analyzed": "raṭṭhan ca api vinassati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_526:1.3",
        "original": "Ehi niḷinike gaccha,",
        "analyzed": "ehi niḷinike gaccha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_526:1.4",
        "original": "taṁ me brāhmaṇamānaya”.",
        "analyzed": "tam me brāhmaṇam ānaya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_526:2.1",
        "original": "“Nāhaṁ dukkhakkhamā rāja,",
        "analyzed": "na aham dukkha kkhamā rāja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_526:2.2",
        "original": "nāhaṁ addhānakovidā;",
        "analyzed": "na aham addhāna kovidā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_526:2.3",
        "original": "Kathaṁ ahaṁ gamissāmi,",
        "analyzed": "katham aham gamissāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_526:2.4",
        "original": "vanaṁ kuñjarasevitaṁ”.",
        "analyzed": "vanam kuñjara sevita ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_526:3.1",
        "original": "“Phītaṁ janapadaṁ gantvā,",
        "analyzed": "phītam janapadam gantvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_526:3.2",
        "original": "hatthinā ca rathena ca;",
        "analyzed": "hatthinā ca rathena ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_526:3.3",
        "original": "Dārusaṅghāṭayānena,",
        "analyzed": "dāru saṅghāṭa yānena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_526:3.4",
        "original": "evaṁ gaccha niḷinike.",
        "analyzed": "evam gaccha niḷinike ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_526:4.1",
        "original": "Hatthiassarathe pattī,",
        "analyzed": "pattī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_526:4.2",
        "original": "gacchevādāya khattiye;",
        "analyzed": "gaccha iva ādāya khattiye ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_526:4.3",
        "original": "Taveva vaṇṇarūpena,",
        "analyzed": "tava iva vaṇṇa rūpena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_526:4.4",
        "original": "vasaṁ tamānayissasi”.",
        "analyzed": "vasam tam ānayissasi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_526:5.1",
        "original": "“Kadalīdhajapaññāṇo,",
        "analyzed": "kadalī dhaja paññāṇaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_526:5.2",
        "original": "ābhujīparivārito;",
        "analyzed": "ā bhujī parivāritaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_526:5.3",
        "original": "Eso padissati rammo,",
        "analyzed": "eso padissati rammo ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_526:5.4",
        "original": "isisiṅgassa assamo.",
        "analyzed": "isi siṅgassa as samaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_526:6.1",
        "original": "Eso aggissa saṅkhāto,",
        "analyzed": "esaḥ aggissa saṅkhātaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_526:6.2",
        "original": "eso dhūmo padissati;",
        "analyzed": "eso dhūmo padissati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_526:6.3",
        "original": "Maññe no aggiṁ hāpeti,",
        "analyzed": "maññe naḥ aggim hāpa iti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_526:6.4",
        "original": "isisiṅgo mahiddhiko”.",
        "analyzed": "isisiṅgaḥ mahiddhikaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_526:7.1",
        "original": "“Tañca disvāna āyantiṁ,",
        "analyzed": "tat ca disvānaḥ āyanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_526:7.2",
        "original": "āmuttamaṇikuṇḍalaṁ;",
        "analyzed": "āmutta maṇi kuṇḍala ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_526:7.3",
        "original": "Isisiṅgo pāvisi bhīto,",
        "analyzed": "isisiṅgo pāvisi bhītaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_526:7.4",
        "original": "assamaṁ paṇṇachādanaṁ.",
        "analyzed": "assamam paṇṇa chādana ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_526:8.1",
        "original": "Assamassa ca sā dvāre,",
        "analyzed": "assamassa ca sā dvāre ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_526:8.2",
        "original": "geṇḍukenassa kīḷati;",
        "analyzed": "geṇḍukenassa kīḷati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_526:8.3",
        "original": "Vidaṁsayantī aṅgāni,",
        "analyzed": "vidasayantī aṅgāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_526:8.4",
        "original": "guyhaṁ pakāsitāni ca.",
        "analyzed": "guyham pakāsitāni ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_526:9.1",
        "original": "Tañca disvāna kīḷantiṁ,",
        "analyzed": "tat ca disvāna kīḍanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_526:9.2",
        "original": "paṇṇasālagato jaṭī;",
        "analyzed": "paṇṇasāla gataḥ jaṭī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_526:9.3",
        "original": "Assamā nikkhamitvāna,",
        "analyzed": "assamā nikkhamitvāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_526:9.4",
        "original": "idaṁ vacanamabravi.",
        "analyzed": "idam vacanam abravi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_526:10.1",
        "original": "Ambho ko nāma so rukkho,",
        "analyzed": "ambho kaḥ nāma so rukkho ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_526:10.2",
        "original": "yassa tevaṅgataṁ phalaṁ;",
        "analyzed": "tevaṅgatam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_526:10.3",
        "original": "Dūrepi khittaṁ pacceti,",
        "analyzed": "dūre pi khittam pacceti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_526:10.4",
        "original": "na taṁ ohāya gacchati”.",
        "analyzed": "na tam ohāya gacchati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_526:11.1",
        "original": "“Assamassa mama brahme,",
        "analyzed": "assamassa mama brahme ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_526:11.2",
        "original": "samīpe gandhamādane;",
        "analyzed": "samīpe gandhamādane ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_526:11.3",
        "original": "Bahavo tādisā rukkhā,",
        "analyzed": "bahavaḥ tādisāḥ rukkhā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_526:11.4",
        "original": "yassa tevaṅgataṁ phalaṁ;",
        "analyzed": "tevaṅgatam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_526:11.5",
        "original": "Dūrepi khittaṁ pacceti,",
        "analyzed": "dūre pi khittam pacceti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_526:11.6",
        "original": "na maṁ ohāya gacchati”.",
        "analyzed": "na mama ohāya gacchati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_526:12.1",
        "original": "“Etū bhavaṁ assamimaṁ adetu,",
        "analyzed": "etu bhavam assam imam adetu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_526:12.2",
        "original": "Pajjañca bhakkhañca paṭiccha dammi;",
        "analyzed": "pajjañca bhakkhañca paṭiccha dammi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_526:12.3",
        "original": "Idamāsanaṁ atra bhavaṁ nisīdatu,",
        "analyzed": "idam āsanam atra bhavam nisīdatu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_526:12.4",
        "original": "Ito bhavaṁ mūlaphalāni bhuñjatu.",
        "analyzed": "itas bhavam mūla phalāni bhuñjatu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_526:13.1",
        "original": "Kiṁ te idaṁ ūrūnamantarasmiṁ,",
        "analyzed": "kim te idam ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_526:13.2",
        "original": "Supicchitaṁ kaṇharivappakāsati;",
        "analyzed": "su picchitam ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_526:13.3",
        "original": "Akkhāhi me pucchito etamatthaṁ,",
        "analyzed": "akkhāhi me pucchitaḥ etam attha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_526:13.4",
        "original": "Kose nu te uttamaṅgaṁ paviṭṭhaṁ”.",
        "analyzed": "kose nu te uttamaṅgam paviṭṭha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_526:14.1",
        "original": "“Ahaṁ vane mūlaphalesanaṁ caraṁ,",
        "analyzed": "aham vane mūla phala īsanam cara ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_526:14.2",
        "original": "Āsādayiṁ acchaṁ sughorarūpaṁ;",
        "analyzed": "āsādayim accham su ghora rūpa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_526:14.3",
        "original": "So maṁ patitvā sahasājjhapatto,",
        "analyzed": "saḥ mam patitvā sahasā ajjha pattaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_526:14.4",
        "original": "Panujja maṁ abbahi uttamaṅgaṁ.",
        "analyzed": "panujja mama ap bahi uttamaṅga ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_526:15.1",
        "original": "Svāyaṁ vaṇo khajjati kaṇḍuvāyati,",
        "analyzed": "su ayam vaṇaḥ khajjati kaṇḍuvāyati ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_526:15.2",
        "original": "Sabbañca kālaṁ na labhāmi sātaṁ;",
        "analyzed": "sabbañca kālam na labhāmi sāta ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_526:15.3",
        "original": "Paho bhavaṁ kaṇḍumimaṁ vinetuṁ,",
        "analyzed": "pahaḥ bhavam kaṇḍum imam vinetu ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_526:15.4",
        "original": "Kurutaṁ bhavaṁ yācito brāhmaṇatthaṁ”.",
        "analyzed": "kurutam bhavam yācitaḥ brāhmaṇattha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_526:16.1",
        "original": "“Gambhīrarūpo te vaṇo salohito,",
        "analyzed": "gambhīra rūpaḥ te vaṇaḥ sa lohitaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_526:16.2",
        "original": "Apūtiko vaṇagandho mahā ca;",
        "analyzed": "a pūtikaḥ vaṇa gandhaḥ mahā ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_526:16.3",
        "original": "Karomi te kiñci kasāyayogaṁ,",
        "analyzed": "karomi te kiñci kasāya yoga ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_526:16.4",
        "original": "Yathā bhavaṁ paramasukhī bhaveyya”.",
        "analyzed": "yathā bhavam parama sukhī ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_526:17.1",
        "original": "“Na mantayogā na kasāyayogā,",
        "analyzed": "na manta yogā na kasāya yogā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_526:17.2",
        "original": "Na osadhā brahmacāri kamanti;",
        "analyzed": "na osadhā brahmacāri kamanti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_526:17.3",
        "original": "Yaṁ te mudu tena vinehi kaṇḍuṁ,",
        "analyzed": "yam te mudu tena vinehi kaṇḍu ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_526:17.4",
        "original": "Yathā ahaṁ paramasukhī bhaveyyaṁ”.",
        "analyzed": "yathā aham parama sukhī ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_526:18.1",
        "original": "“Ito nu bhoto katamena assamo,",
        "analyzed": "itas nu bhoto katamena assamaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_526:18.2",
        "original": "Kacci bhavaṁ abhiramasi araññe;",
        "analyzed": "kacci bhavam abhiramasi araññe ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_526:18.3",
        "original": "Kacci nu te mūlaphalaṁ pahūtaṁ,",
        "analyzed": "kacci nu te mūla phalam ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_526:18.4",
        "original": "Kacci bhavantaṁ na vihiṁsanti vāḷā”.",
        "analyzed": "kacci bhavantam na vihisanti vāḷā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_526:19.1",
        "original": "“Ito ujuṁ uttarāyaṁ disāyaṁ,",
        "analyzed": "itas ujum uttarāyam disāya ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_526:19.2",
        "original": "Khemānadī himavatā pabhāvī;",
        "analyzed": "khemānadī himavatā pabhāvī ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_526:19.3",
        "original": "Tassā tīre assamo mayha rammo,",
        "analyzed": "tassā tīre assamaḥ mayha rammo ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_526:19.4",
        "original": "Aho bhavaṁ assamaṁ mayhaṁ passe.",
        "analyzed": "aho bhavam assamam mayham passe ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_526:20.1",
        "original": "Ambā ca sālā tilakā ca jambuyo,",
        "analyzed": "ambā ca sālā tilakā ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_526:20.2",
        "original": "Uddālakā pāṭaliyo ca phullā;",
        "analyzed": "uddālakā pāṭaliyaḥ ca phullā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_526:20.3",
        "original": "Samantato kimpurisābhigītaṁ,",
        "analyzed": "kimpurisa abhigīta ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_526:20.4",
        "original": "Aho bhavaṁ assamaṁ mayhaṁ passe.",
        "analyzed": "aho bhavam assamam mayham passe ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_526:21.1",
        "original": "Tālā ca mūlā ca phalā ca mettha,",
        "analyzed": "tālā ca mūlā ca phalā ca mā ittha ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_526:21.2",
        "original": "Vaṇṇena gandhena upetarūpaṁ;",
        "analyzed": "vaṇṇena gandhena upeta rūpa ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_526:21.3",
        "original": "Taṁ bhūmibhāgehi upetarūpaṁ,",
        "analyzed": "tam bhūmi bhāgehi upeta rūpa ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_526:21.4",
        "original": "Aho bhavaṁ assamaṁ mayhaṁ passe.",
        "analyzed": "aho bhavam assamam mayham passe ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_526:22.1",
        "original": "Phalā ca mūlā ca pahūtamettha,",
        "analyzed": "ca mūlā ca pahūtam ettha ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_526:22.2",
        "original": "Vaṇṇena gandhena rasenupetā;",
        "analyzed": "vaṇṇena gandhena rasena upetā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_526:22.3",
        "original": "Āyanti ca luddakā taṁ padesaṁ,",
        "analyzed": "āyanti ca luddakā tam ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_526:22.4",
        "original": "Mā me tato mūlaphalaṁ ahāsuṁ”.",
        "analyzed": "mā me tatas mūla phalam ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_526:23.1",
        "original": "“Pitā mamaṁ mūlaphalesanaṁ gato,",
        "analyzed": "pitā mamam mūla phala isanam gataḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_526:23.2",
        "original": "Idāni āgacchati sāyakāle;",
        "analyzed": "idāni āgacchati sāyakāle ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_526:23.3",
        "original": "Ubhova gacchāmase assamaṁ taṁ,",
        "analyzed": "ubhā uva gacchāmase assamam te ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_526:23.4",
        "original": "Yāva pitā mūlaphalato etu”.",
        "analyzed": "yāva pitā mūla phalataḥ etu ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_526:24.1",
        "original": "“Aññe bahū isayo sādhurūpā,",
        "analyzed": "aññe bahū isayo sādhu rūpā ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_526:24.2",
        "original": "Rājīsayo anumagge vasanti;",
        "analyzed": "rājīsayaḥ anumagge vasanti ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_526:24.3",
        "original": "Teyeva pucchesi mamassamaṁ taṁ,",
        "analyzed": "teyā iva pucchesi mamassamam ta ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_526:24.4",
        "original": "Te taṁ nayissanti mamaṁ sakāse”.",
        "analyzed": "te tam nayissanti mamam sakāse ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_526:25.1",
        "original": "“Na te kaṭṭhāni bhinnāni,",
        "analyzed": "na te kaṭṭhāni bhinnāni ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_526:25.2",
        "original": "na te udakamābhataṁ;",
        "analyzed": "na te udakam ābhata ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_526:25.3",
        "original": "Aggīpi te na hāpito,",
        "analyzed": "aggīpi te na hāpitaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_526:25.4",
        "original": "kiṁ nu mandova jhāyasi.",
        "analyzed": "kim nu mandaḥ uva jhāyasi ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_526:26.1",
        "original": "Bhinnāni kaṭṭhāni huto ca aggi,",
        "analyzed": "bhinnāni kaṭṭhāni hutaḥ ca aggi ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_526:26.2",
        "original": "Tapanīpi te samitā brahmacārī;",
        "analyzed": "tapanīpi te samitā brahmacārī ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_526:26.3",
        "original": "Pīṭhañca mayhaṁ udakañca hoti,",
        "analyzed": "pīṭham ca udakam ca hoti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_526:26.4",
        "original": "Ramasi tuvaṁ brahmabhūto puratthā.",
        "analyzed": "ramasi tuvam brahma bhūtaḥ puratthā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_526:27.1",
        "original": "Abhinnakaṭṭhosi anābhatodako,",
        "analyzed": "a bhinna kaṭṭhaḥ asi an ābhata udakaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_526:27.2",
        "original": "Ahāpitaggīsi asiddhabhojano;",
        "analyzed": "a siddha bhojanaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_526:27.3",
        "original": "Na me tuvaṁ ālapasī mamajja,",
        "analyzed": "na me tuvam ālapasī mamajja ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_526:27.4",
        "original": "Naṭṭhaṁ nu kiṁ cetasikañca dukkhaṁ”.",
        "analyzed": "naṭṭham nu kim cetasikam ca dukkha ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_526:28.1",
        "original": "“Idhāgamā jaṭilo brahmacārī,",
        "analyzed": "idha agamā jaṭilaḥ brahmacārī ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_526:28.2",
        "original": "Sudassaneyyo sutanū vineti;",
        "analyzed": "su dassaneyyaḥ su tanū vineti ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_526:28.3",
        "original": "Nevātidīgho na panātirasso,",
        "analyzed": "na iva ati dīghaḥ na pana ati rassaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_526:28.4",
        "original": "Sukaṇhakaṇhacchadanehi bhoto.",
        "analyzed": "su kaṇha kaṇha chadanehi bhoto ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_526:29.1",
        "original": "Amassujāto apurāṇavaṇṇī,",
        "analyzed": "a massu jātaḥ a purāṇa vaṇṇī ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_526:29.2",
        "original": "Ādhārarūpañca panassa kaṇṭhe;",
        "analyzed": "ādhāra rūpam ca panassa kaṇṭhe ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_526:29.3",
        "original": "Dve yamā gaṇḍā ure sujātā,",
        "analyzed": "dve yamā gaṇḍau ure su jātā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_526:29.4",
        "original": "Suvaṇṇatindukanibhā pabhassarā.",
        "analyzed": "suvaṇṇa tinduka nibhā pabhassarā ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_526:30.1",
        "original": "Mukhañca tassa bhusadassaneyyaṁ,",
        "analyzed": "mukhañca tassa bhusadassaneyya ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_526:30.2",
        "original": "Kaṇṇesu lambanti ca kuñcitaggā;",
        "analyzed": "kaṇṇesu lambanti ca kuñcita aggāḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_526:30.3",
        "original": "Te jotare carato māṇavassa,",
        "analyzed": "te jotare carataḥ māṇavassa ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_526:30.4",
        "original": "Suttañca yaṁ saṁyamanaṁ jaṭānaṁ.",
        "analyzed": "yam sa yamanam jaṭāna ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_526:31.1",
        "original": "Aññā ca tassa saṁyamāni catasso,",
        "analyzed": "aññā ca tassa sayamāni catasso ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_526:31.2",
        "original": "Nīlā pītā lohitikā ca setā;",
        "analyzed": "nīlā pītā lohitikā ca sā itā ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_526:31.3",
        "original": "Tā piṁsare carato māṇavassa,",
        "analyzed": "tā pisare carataḥ māṇavassa ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_526:31.4",
        "original": "Tiriṭisaṅghāriva pāvusamhi.",
        "analyzed": "tiriṭisaṅghāriva pāvusamhi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_526:32.1",
        "original": "Na mikhalaṁ muñjamayaṁ dhāreti,",
        "analyzed": "na mikhalam muñja mayam dhārā iti ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_526:32.2",
        "original": "Na santhare no pana pabbajassa;",
        "analyzed": "na santhare no pana pabbajassa ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_526:32.3",
        "original": "Tā jotare jaghanantare vilaggā,",
        "analyzed": "tā jaghana ntare vilaggā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_526:32.4",
        "original": "Sateratā vijjurivantalikkhe.",
        "analyzed": "sateratā ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_526:33.1",
        "original": "Akhīlakāni ca avaṇṭakāni,",
        "analyzed": "akhīlakāni ca avaṇṭakāni ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_526:33.2",
        "original": "Heṭṭhā nabhyā kaṭisamohitāni;",
        "analyzed": "heṭṭhā ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_526:33.3",
        "original": "Aghaṭṭitā niccakīḷaṁ karonti,",
        "analyzed": "a ghaṭṭitāḥ nicca kīḍam karonti ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_526:33.4",
        "original": "Haṁ tāta kiṁrukkhaphalāni tāni.",
        "analyzed": "ham tāta kirukkha phalāni tāni ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_526:34.1",
        "original": "Jaṭā ca tassa bhusadassaneyyā,",
        "analyzed": "jaṭā ca tassa bhusa dassaneyyā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_526:34.2",
        "original": "Parosataṁ vellitaggā sugandhā;",
        "analyzed": "velli taggā sugandhā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_526:34.3",
        "original": "Dvedhā siro sādhu vibhattarūpo,",
        "analyzed": "dvedhā sādhu ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_526:34.4",
        "original": "Aho nu kho mayha tathā jaṭāssu.",
        "analyzed": "aho nu khalu tathā jaṭāssu ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_526:35.1",
        "original": "Yadā ca so pakirati tā jaṭāyo,",
        "analyzed": "yadā ca saḥ pakirati tā jaṭāyo ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_526:35.2",
        "original": "Vaṇṇena gandhena upetarūpā;",
        "analyzed": "vaṇṇena gandhena upeta rūpā ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_526:35.3",
        "original": "Nīluppalaṁ vātasameritaṁva,",
        "analyzed": "nīluppalam ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_526:35.4",
        "original": "Tatheva saṁvāti panassamo ayaṁ.",
        "analyzed": "tathā iva panassamaḥ ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_526:36.1",
        "original": "Paṅko ca tassa bhusadassaneyyo,",
        "analyzed": "paṅko ca tassa bhusa dassaneyyaḥ ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_526:36.2",
        "original": "Netādiso yādiso mayhaṁ kāye;",
        "analyzed": "netā ādisaḥ yādisaḥ mayham kāye ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_526:36.3",
        "original": "So vāyatī erito mālutena,",
        "analyzed": "saḥ vāyatī eritaḥ mālutena ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_526:36.4",
        "original": "Vanaṁ yathā aggagimhe suphullaṁ.",
        "analyzed": "vanam yathā aggagimhe su phulla ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_526:37.1",
        "original": "Nihanti so rukkhaphalaṁ pathabyā,",
        "analyzed": "nihanti saḥ rukkha phalam pathabyā ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_526:37.2",
        "original": "Sucittarūpaṁ ruciraṁ dassaneyyaṁ;",
        "analyzed": "su citta rūpam ruciram dassaneyya ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_526:37.3",
        "original": "Khittañca tassa punareti hatthaṁ,",
        "analyzed": "khittam ca tassa punar eti hattha ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_526:37.4",
        "original": "Haṁ tāta kiṁrukkhaphalaṁ nu kho taṁ.",
        "analyzed": "han tāta kirukkha phalam nu kho ta ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_526:38.1",
        "original": "Dantā ca tassa bhusadassaneyyā,",
        "analyzed": "dantā ca tassa bhusa dassaneyyā ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_526:38.2",
        "original": "Suddhā samā saṅkhavarūpapannā;",
        "analyzed": "suddhā samā saṅkha varūpa pannā ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_526:38.3",
        "original": "Mano pasādenti vivariyamānā,",
        "analyzed": "manaḥ pasādenti vivariyamānā ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_526:38.4",
        "original": "Na hi nūna so sākamakhādi tehi.",
        "analyzed": "na hi nūna saḥ sākamakhādi ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_526:39.1",
        "original": "Akakkasaṁ aggaḷitaṁ muhuṁ muduṁ,",
        "analyzed": "akakkasam aggaḷitam muhum mudu ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_526:39.2",
        "original": "Ujuṁ anuddhataṁ acapalamassa bhāsitaṁ;",
        "analyzed": "ujum an uddhatam a capalamassa bhāsita ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_526:39.3",
        "original": "Rudaṁ manuññaṁ karavīkasussaraṁ,",
        "analyzed": "rudam manuññam karavīkasussara ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_526:39.4",
        "original": "Hadayaṅgamaṁ rañjayateva me mano.",
        "analyzed": "hadayaṅgamam rañjayatā iva me manaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_526:40.1",
        "original": "Bindussaro nātivisaṭṭhavākyo,",
        "analyzed": "bindussaraḥ na ati visaṭṭha vākyaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_526:40.2",
        "original": "Na nūna sajjhāyamatippayutto;",
        "analyzed": "na nūna sajjhāya mati ppayuttaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_526:40.3",
        "original": "Icchāmi bho taṁ punadeva daṭṭhuṁ,",
        "analyzed": "icchāmi bho tam punar eva daṭṭhu ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_526:40.4",
        "original": "Mitto hi me māṇavohu puratthā.",
        "analyzed": "mittaḥ hi me māṇavaḥ ūhu puratthā ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_526:41.1",
        "original": "Susandhi sabbattha vimaṭṭhimaṁ vaṇaṁ,",
        "analyzed": "su sandhi sabbattha ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_526:41.2",
        "original": "Puthū sujātaṁ kharapattasannitaṁ;",
        "analyzed": "su jātam ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_526:41.3",
        "original": "Teneva maṁ uttariyāna māṇavo,",
        "analyzed": "tena iva mama uttariyāna māṇavaḥ ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_526:41.4",
        "original": "Vivaritaṁ ūruṁ jaghanena piḷayi.",
        "analyzed": "vivaritam ūrum jaghanena piḷayi ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_526:42.1",
        "original": "Tapanti ābhanti virocare ca,",
        "analyzed": "tapanti ābhanti virocare ca ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_526:42.2",
        "original": "Sateratā vijjurivantalikkhe;",
        "analyzed": "sateratā ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_526:42.3",
        "original": "Bāhā mudū añjanalomasādisā,",
        "analyzed": "bāhā mudū añjana loma sādisā ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_526:42.4",
        "original": "Vicitravaṭṭaṅgulikāssa sobhare.",
        "analyzed": "sobhare ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_526:43.1",
        "original": "Akakkasaṅgo na ca dīghalomo,",
        "analyzed": "a kakkasaṅgaḥ na ca dīgha lomaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_526:43.2",
        "original": "Nakhāssa dīghā api lohitaggā;",
        "analyzed": "nakhāssa dīghāḥ api lohitaggā ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_526:43.3",
        "original": "Mudūhi bāhāhi palissajanto,",
        "analyzed": "mudūhi bāhāhi palissajantaḥ ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_526:43.4",
        "original": "Kalyāṇarūpo ramayaṁ upaṭṭhahi.",
        "analyzed": "kalyāṇa rūpaḥ upaṭṭhahi ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_526:44.1",
        "original": "Dumassa tūlūpanibhā pabhassarā,",
        "analyzed": "dumassa tūlūpa nibhā pabhassarā ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_526:44.2",
        "original": "Suvaṇṇakambutalavaṭṭasucchavi;",
        "analyzed": "           ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_526:44.3",
        "original": "Hatthā mudū tehi maṁ samphusitvā,",
        "analyzed": "hatthāḥ mudū tehi mama samphusitvā ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_526:44.4",
        "original": "Ito gato tena maṁ dahanti tāta.",
        "analyzed": "itas tena mama dahanti tāta ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_526:45.1",
        "original": "Na nūna so khārividhaṁ ahāsi,",
        "analyzed": "na nūna saḥ khārividham ahāsi ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_526:45.2",
        "original": "Na nūna so kaṭṭhāni sayaṁ abhañji;",
        "analyzed": "na nūna saḥ kaṭṭhāni sayam abhañji ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_526:45.3",
        "original": "Na nūna so hanti dume kuṭhāriyā,",
        "analyzed": "na nūna saḥ hanti dume kuṭhāriyā ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_526:45.4",
        "original": "Na hissa hatthesu khilāni atthi.",
        "analyzed": "na hissa hatthesu khilāni atthi ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_526:46.1",
        "original": "Accho ca kho tassa vaṇaṁ akāsi,",
        "analyzed": "acchas ca kho tassa vaṇam akāsi ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_526:46.2",
        "original": "So maṁbravi sukhitaṁ maṁ karohi;",
        "analyzed": "saḥ sukhitam mama karohi ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_526:46.3",
        "original": "Tāhaṁ kariṁ tena mamāsi sokhyaṁ,",
        "analyzed": "tā aham karim tena mama asi sokhya ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_526:46.4",
        "original": "So cabravi ‘sukhitosmī’ti brahme.",
        "analyzed": "saḥ cabravi sukhitaḥ smi iti ",
        "folio": "46"
    },
    {
        "segmentnr": "PA_ja_526:47.1",
        "original": "Ayañca te māluvapaṇṇasanthatā,",
        "analyzed": "ayam ca te māluva paṇṇa santhatā ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_526:47.2",
        "original": "Vikiṇṇarūpāva mayā ca tena ca;",
        "analyzed": "vikiṇṇa rūpāva mayā ca tena ca ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_526:47.3",
        "original": "Kilantarūpā udake ramitvā,",
        "analyzed": "kilanta rūpāḥ udake ramitvā ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_526:47.4",
        "original": "Punappunaṁ paṇṇakuṭiṁ vajāma.",
        "analyzed": "punappunam paṇṇa kuṭim vajāma ",
        "folio": "47"
    },
    {
        "segmentnr": "PA_ja_526:48.1",
        "original": "Na majja mantā paṭibhanti tāta,",
        "analyzed": "na majja mantā paṭibhanti tāta ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_ja_526:48.2",
        "original": "Na aggihuttaṁ napi yaññatantaṁ;",
        "analyzed": "na aggi huttam na pi ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_ja_526:48.3",
        "original": "Na cāpi te mūlaphalāni bhuñje,",
        "analyzed": "na ca api te mūla phalāni bhuñje ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_ja_526:48.4",
        "original": "Yāva na passāmi taṁ brahmacāriṁ.",
        "analyzed": "yāva na passāmi tam brahmacāri ",
        "folio": "48"
    },
    {
        "segmentnr": "PA_ja_526:49.1",
        "original": "Addhā pajānāsi tuvampi tāta,",
        "analyzed": "addhā pajānāsi tuvam pi tāta ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_ja_526:49.2",
        "original": "Yassaṁ disaṁ vasate brahmacārī;",
        "analyzed": "yaḥ sam disam vasate brahmacārī ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_ja_526:49.3",
        "original": "Taṁ maṁ disaṁ pāpaya tāta khippaṁ,",
        "analyzed": "tam mama disam pāpaya tāta khippa ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_ja_526:49.4",
        "original": "Mā te ahaṁ amarimassamamhi.",
        "analyzed": "mā te aham ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_ja_526:50.1",
        "original": "Vicitraphullaṁ hi vanaṁ sutaṁ mayā,",
        "analyzed": "vicitra phullam hi vanam sutam mayā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_ja_526:50.2",
        "original": "Dijābhighuṭṭhaṁ dijasaṅghasevitaṁ;",
