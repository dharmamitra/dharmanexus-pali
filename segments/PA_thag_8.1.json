        "analyzed": "mayamā itthā yamāmase ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_8.1:5.3",
        "original": "Ye ca tattha vijānanti,",
        "analyzed": "ye ca tattha vijānanti ",
        "folio": "5"
    },
