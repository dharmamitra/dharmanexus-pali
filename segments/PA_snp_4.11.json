[
    {
        "segmentnr": "PA_snp_4.11:0.1",
        "original": "Sutta Nipāta 4.11",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.11:0.2",
        "original": "Kalahavivādasutta",
        "analyzed": "kalaha vivāda sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.11:1.1",
        "original": "“Kutopahūtā kalahā vivādā,",
        "analyzed": "kutas upahūtā kalahā vivādā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.11:1.2",
        "original": "Paridevasokā sahamaccharā ca;",
        "analyzed": "parideva sokā sahamat śarā ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.11:1.3",
        "original": "Mānātimānā sahapesuṇā ca,",
        "analyzed": "māna ati mānā saha pesuṇā ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.11:1.4",
        "original": "Kutopahūtā te tadiṅgha brūhi”.",
        "analyzed": "kutas upahūtā te tat iṅgha brūhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.11:2.1",
        "original": "“Piyappahūtā kalahā vivādā,",
        "analyzed": "kalahā vivādā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.11:2.2",
        "original": "Paridevasokā sahamaccharā ca;",
        "analyzed": "parideva sokā sahamat śarā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.11:2.3",
        "original": "Mānātimānā sahapesuṇā ca,",
        "analyzed": "māna ati mānā saha pesuṇā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.11:2.4",
        "original": "Maccherayuttā kalahā vivādā;",
        "analyzed": "kalahāḥ vivādā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.11:2.5",
        "original": "Vivādajātesu ca pesuṇāni”.",
        "analyzed": "vivāda jātesu ca pesuṇāni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.11:3.1",
        "original": "“Piyā su lokasmiṁ kutonidānā,",
        "analyzed": "piyā su lokasmin kutonidānā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.11:3.2",
        "original": "Ye cāpi lobhā vicaranti loke;",
        "analyzed": "ye ca api lobhāḥ vicaranti loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.11:3.3",
        "original": "Āsā ca niṭṭhā ca kutonidānā,",
        "analyzed": "āsā ca niṭṭhā ca kutonidānā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.11:3.4",
        "original": "Ye samparāyāya narassa honti”.",
        "analyzed": "ye samparāyāya narassa honti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.11:4.1",
        "original": "“Chandānidānāni piyāni loke,",
        "analyzed": "chandānidānāni piyāni loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.11:4.2",
        "original": "Ye cāpi lobhā vicaranti loke;",
        "analyzed": "ye ca api lobhāḥ vicaranti loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.11:4.3",
        "original": "Āsā ca niṭṭhā ca itonidānā,",
        "analyzed": "āsā ca niṭṭhā ca itas nidānā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.11:4.4",
        "original": "Ye samparāyāya narassa honti”.",
        "analyzed": "ye samparāyāya narassa honti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.11:5.1",
        "original": "“Chando nu lokasmiṁ kutonidāno,",
        "analyzed": "chandaḥ nu lokasmin kutonidānaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.11:5.2",
        "original": "Vinicchayā cāpi kutopahūtā;",
        "analyzed": "vinicchayā ca api kutas upahūtā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.11:5.3",
        "original": "Kodho mosavajjañca kathaṅkathā ca,",
        "analyzed": "kodhaḥ kathaṅkathā ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.11:5.4",
        "original": "Ye vāpi dhammā samaṇena vuttā”.",
        "analyzed": "ye vā api dhammā samaṇena vuttā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.11:6.1",
        "original": "“Sātaṁ asātanti yamāhu loke,",
        "analyzed": "sātam yam āhuḥ loke ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.11:6.2",
        "original": "Tamūpanissāya pahoti chando;",
        "analyzed": "tam ūpanissāya pahoti chandaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.11:6.3",
        "original": "Rūpesu disvā vibhavaṁ bhavañca,",
        "analyzed": "rūpesu disvā vibhavam bhavañca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.11:6.4",
        "original": "Vinicchayaṁ kubbati jantu loke.",
        "analyzed": "vinicchayam kubbati jantu loke ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.11:7.1",
        "original": "Kodho mosavajjañca kathaṅkathā ca,",
        "analyzed": "kodhaḥ kathaṅkathā ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.11:7.2",
        "original": "Etepi dhammā dvayameva sante;",
        "analyzed": "ete pi dhammā dvayam eva sante ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.11:7.3",
        "original": "Kathaṅkathī ñāṇapathāya sikkhe,",
        "analyzed": "kathaṅkathī ñāṇa pathāya sikkhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.11:7.4",
        "original": "Ñatvā pavuttā samaṇena dhammā”.",
        "analyzed": "ñatvā pavuttā samaṇena dhammā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.11:8.1",
        "original": "“Sātaṁ asātañca kutonidānā,",
        "analyzed": "sātam a sātam ca kutonidānā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.11:8.2",
        "original": "Kismiṁ asante na bhavanti hete;",
        "analyzed": "kismin asante na bhavanti hete ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.11:8.3",
        "original": "Vibhavaṁ bhavañcāpi yametamatthaṁ,",
        "analyzed": "vibhavam bhavan ca api yam etam attha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.11:8.4",
        "original": "Etaṁ me pabrūhi yatonidānaṁ”.",
        "analyzed": "etam me pabrūhi yatonidāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.11:9.1",
        "original": "“Phassanidānaṁ sātaṁ asātaṁ,",
        "analyzed": "phassa nidānam sātam asāta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.11:9.2",
        "original": "Phasse asante na bhavanti hete;",
        "analyzed": "phasse asante na bhavanti hete ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.11:9.3",
        "original": "Vibhavaṁ bhavañcāpi yametamatthaṁ,",
        "analyzed": "vibhavam bhavan ca api yam etam attha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.11:9.4",
        "original": "Etaṁ te pabrūmi itonidānaṁ”.",
        "analyzed": "etam te pabrūmi itas nidāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.11:10.1",
        "original": "“Phasso nu lokasmi kutonidāno,",
        "analyzed": "phassaḥ nu lokasmi kutonidānaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.11:10.2",
        "original": "Pariggahā cāpi kutopahūtā;",
        "analyzed": "ca api kutas upahūtā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.11:10.3",
        "original": "Kismiṁ asante na mamattamatthi,",
        "analyzed": "kismin asante na ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.11:10.4",
        "original": "Kismiṁ vibhūte na phusanti phassā”.",
        "analyzed": "kismin vibhūte na phusanti phassā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.11:11.1",
        "original": "“Nāmañca rūpañca paṭicca phasso,",
        "analyzed": "nāmañca rūpañca paṭicca phasso ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.11:11.2",
        "original": "Icchānidānāni pariggahāni;",
        "analyzed": "icchā nidānāni pariggahāni ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.11:11.3",
        "original": "Icchāyasantyā na mamattamatthi,",
        "analyzed": "iccha ayasantyāḥ na ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.11:11.4",
        "original": "Rūpe vibhūte na phusanti phassā”.",
        "analyzed": "rūpe vibhūte na phusanti phassā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.11:12.1",
        "original": "“Kathaṁ sametassa vibhoti rūpaṁ,",
        "analyzed": "katham sametassa vibho ti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.11:12.2",
        "original": "Sukhaṁ dukhañcāpi kathaṁ vibhoti;",
        "analyzed": "sukham dukham ca api katham ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.11:12.3",
        "original": "Etaṁ me pabrūhi yathā vibhoti,",
        "analyzed": "etam me pabrūhi yathā vibhoti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.11:12.4",
        "original": "Taṁ jāniyāmāti me mano ahu”.",
        "analyzed": "tam jāniyāma ati me manaḥ ahu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.11:13.1",
        "original": "“Na saññasaññī na visaññasaññī,",
        "analyzed": "na sañña saññī na visañña saññī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.11:13.2",
        "original": "Nopi asaññī na vibhūtasaññī;",
        "analyzed": "na upi a saññī na vibhūta saññī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.11:13.3",
        "original": "Evaṁ sametassa vibhoti rūpaṁ,",
        "analyzed": "evam sametassa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.11:13.4",
        "original": "Saññānidānā hi papañcasaṅkhā”.",
        "analyzed": "saññā nidānā hi papañca saṅkhā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.11:14.1",
        "original": "“Yaṁ taṁ apucchimha akittayī no,",
        "analyzed": "yam tam apucchimha a kittayī naḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.11:14.2",
        "original": "Aññaṁ taṁ pucchāma tadiṅgha brūhi;",
        "analyzed": "aññam tam pucchāma tat iṅgha brūhi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.11:14.3",
        "original": "Ettāvataggaṁ nu vadanti heke,",
        "analyzed": "nu vadanti ha ike ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.11:14.4",
        "original": "Yakkhassa suddhiṁ idha paṇḍitāse;",
        "analyzed": "yakkhassa suddhim idha paṇḍitāse ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.11:14.5",
        "original": "Udāhu aññampi vadanti etto”.",
        "analyzed": "udāhu aññampi vadanti etto ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.11:15.1",
        "original": "“Ettāvataggampi vadanti heke,",
        "analyzed": "vadanti ha ike ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.11:15.2",
        "original": "Yakkhassa suddhiṁ idha paṇḍitāse;",
        "analyzed": "yakkhassa suddhim idha paṇḍitāse ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.11:15.3",
        "original": "Tesaṁ paneke samayaṁ vadanti,",
        "analyzed": "tesam pana eke samayam vadanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.11:15.4",
        "original": "Anupādisese kusalā vadānā.",
        "analyzed": "an upādisese kusalā vadānā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.11:16.1",
        "original": "Ete ca ñatvā upanissitāti,",
        "analyzed": "ete ca ñatvā upanissitā ati ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.11:16.2",
        "original": "Ñatvā munī nissaye so vimaṁsī;",
        "analyzed": "ñatvā munī nissaye saḥ vimasī ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.11:16.3",
        "original": "Ñatvā vimutto na vivādameti,",
        "analyzed": "ñatvā vimuttaḥ na vivādam eti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.11:16.4",
        "original": "Bhavābhavāya na sameti dhīro”ti.",
        "analyzed": "bhava abhavāya na sameti dhīrā uti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.11:16.5",
        "original": "Kalahavivādasuttaṁ ekādasamaṁ.",
        "analyzed": "kalaha vivāda suttam ekādasama ",
        "folio": "16"
    }
]