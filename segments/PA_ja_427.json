        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_427:1.1",
        "original": "“Parisaṅkupatho nāma,",
        "analyzed": "parisaṅkupathaḥ nāma ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_427:1.2",
        "original": "gijjhapantho sanantano;",
        "analyzed": "gijjhapanthaḥ sanantanaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_427:1.3",
        "original": "Tatrāsi mātāpitaro,",
        "analyzed": "tatra asi mātā pitaraḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_427:1.4",
        "original": "gijjho posesi jiṇṇake;",
        "analyzed": "gijjhaḥ posesi jiṇṇake ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_427:1.5",
        "original": "Tesaṁ ajagaramedaṁ,",
        "analyzed": "tesam ajagaram idam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_427:1.6",
        "original": "accahāsi bahuttaso.",
        "analyzed": "accahāsi bahuttaso ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_427:2.1",
        "original": "Pitā ca puttaṁ avaca,",
        "analyzed": "pitā ca puttam avaca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_427:2.2",
        "original": "Jānaṁ uccaṁ papātinaṁ;",
        "analyzed": "jānam uccam papātina ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_427:2.3",
        "original": "Supattaṁ thāmasampannaṁ,",
        "analyzed": "su pattam thāma sampanna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_427:2.4",
        "original": "Tejassiṁ dūragāminaṁ.",
        "analyzed": "tejassim dūra gāmina ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_427:3.1",
        "original": "Pariplavantaṁ pathaviṁ,",
        "analyzed": "pariplavantam pathavi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_427:3.2",
        "original": "yadā tāta vijānahi;",
        "analyzed": "yadā tāta vijānahi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_427:3.3",
        "original": "Sāgarena parikkhittaṁ,",
        "analyzed": "sāgarena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_427:3.4",
        "original": "cakkaṁva parimaṇḍalaṁ;",
        "analyzed": "cakkava parimaṇḍala ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_427:3.5",
        "original": "Tato tāta nivattassu,",
        "analyzed": "tatas tāta nivattassu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_427:3.6",
        "original": "māssu etto paraṁ gami.",
        "analyzed": "mā assu ettas param gami ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_427:4.1",
        "original": "Udapattosi vegena,",
        "analyzed": "uda pattā asi vegena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_427:4.2",
        "original": "balī pakkhī dijuttamo;",
        "analyzed": "balī pakkhī dijuttamaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_427:4.3",
        "original": "Olokayanto vakkaṅgo,",
        "analyzed": "olokayantaḥ vakkaṅgaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_427:4.4",
        "original": "pabbatāni vanāni ca.",
        "analyzed": "pabbatāni vanāni ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_427:5.1",
        "original": "Addassa pathaviṁ gijjho,",
        "analyzed": "addassa pathavim gijjhaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_427:5.2",
        "original": "yathāsāsi pitussutaṁ;",
        "analyzed": "yathā asa asi pituḥ suta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_427:5.3",
        "original": "Sāgarena parikkhittaṁ,",
        "analyzed": "sāgarena ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_427:5.4",
        "original": "cakkaṁva parimaṇḍalaṁ.",
        "analyzed": "cakkava parimaṇḍala ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_427:6.1",
        "original": "Tañca so samatikkamma,",
        "analyzed": "tat ca saḥ samatikkamma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_427:6.2",
        "original": "paramevaccavattatha;",
        "analyzed": "paramevat cavat tathā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_427:6.3",
        "original": "Tañca vātasikhā tikkhā,",
        "analyzed": "tat ca vāta sikhā tikkhā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_427:6.4",
        "original": "accahāsi baliṁ dijaṁ.",
        "analyzed": "accahāsi balim dija ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_427:7.1",
        "original": "Nāsakkhātigato poso,",
        "analyzed": "na asakkhi atigataḥ poso ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_427:7.2",
        "original": "punadeva nivattituṁ;",
        "analyzed": "punar eva nivattitu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_427:7.3",
        "original": "Dijo byasanamāpādi,",
        "analyzed": "dijaḥ byasanam āpādi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_427:7.4",
        "original": "verambhānaṁ vasaṁ gato.",
        "analyzed": "verambhānam vasam gataḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_427:8.1",
        "original": "Tassa puttā ca dārā ca,",
        "analyzed": "tassa puttā ca dārā ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_427:8.2",
        "original": "ye caññe anujīvino;",
        "analyzed": "ye caññe anujīvino ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_427:8.3",
        "original": "Sabbe byasanamāpāduṁ,",
        "analyzed": "sabbe byasanam āpādu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_427:8.4",
