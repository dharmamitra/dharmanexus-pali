[
    {
        "segmentnr": "PA_vv_80:0.1",
        "original": "Vimānavatthu",
        "analyzed": "vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_80:0.2",
        "original": "Purisavimāna",
        "analyzed": "purisa vimāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_80:0.3",
        "original": "Sunikkhittavagga",
        "analyzed": "su nikkhitta vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_80:0.4",
        "original": "6. Gopālavimānavatthu",
        "analyzed": "gopāla vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_80:1.1",
        "original": "“Disvāna devaṁ paṭipucchi bhikkhu,",
        "analyzed": "disvāna devam paṭipucchi bhikkhu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_80:1.2",
        "original": "Ucce vimānamhi ciraṭṭhitike;",
        "analyzed": "ucce vimānamhi ciraṭṭhitike ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_80:1.3",
        "original": "Āmuttahatthābharaṇaṁ yasassiṁ,",
        "analyzed": "āmutta hattha ābharaṇam yasassi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_80:1.4",
        "original": "Dibbe vimānamhi yathāpi candimā.",
        "analyzed": "dibbe vimānamhi yathā api candimā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_80:2.1",
        "original": "Alaṅkato malyadharo suvattho,",
        "analyzed": "alaṅkataḥ malyadharaḥ suvatthaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_80:2.2",
        "original": "Sukuṇḍalī kappitakesamassu;",
        "analyzed": "su kuṇḍalī kappita kesa massu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_80:2.3",
        "original": "Āmuttahatthābharaṇo yasassī,",
        "analyzed": "āmutta hattha ābharaṇaḥ yasassī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_80:2.4",
        "original": "Dibbe vimānamhi yathāpi candimā.",
        "analyzed": "dibbe vimānamhi yathā api candimā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_80:3.1",
        "original": "Dibbā ca vīṇā pavadanti vagguṁ,",
        "analyzed": "dibbā ca vīṇā pavadanti vaggu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_80:3.2",
        "original": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
        "analyzed": "aṭṭhaṭṭhakā sikkhitā sādhu rūpā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_80:3.3",
        "original": "Dibbā ca kaññā tidasacarā uḷārā,",
        "analyzed": "dibbā ca kaññā tidasa carā uḷārā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_80:3.4",
        "original": "Naccanti gāyanti pamodayanti.",
        "analyzed": "naccanti gāyanti pamodayanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_80:4.1",
        "original": "Deviddhipattosi mahānubhāvo,",
        "analyzed": "deviddhipatto asi mahā anubhāvaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_80:4.2",
        "original": "Manussabhūto kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtaḥ kim akāsi puñña ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_80:4.3",
        "original": "Kenāsi evaṁ jalitānubhāvo,",
        "analyzed": "kena asi evam jalita anubhāvaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_80:4.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_80:5.1",
        "original": "So devaputto attamano,",
        "analyzed": "saḥ deva puttaḥ atta mano ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_80:5.2",
        "original": "…pe…",
        "analyzed": "pe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_80:5.3",
        "original": "yassa kammassidaṁ phalaṁ.",
        "analyzed": "yassa kammassa idam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_80:6.1",
        "original": "“Ahaṁ manussesu manussabhūto,",
        "analyzed": "aham manussesu manussa bhūtaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_80:6.2",
        "original": "Saṅgamma rakkhissaṁ paresaṁ dhenuyo;",
        "analyzed": "saṅgamma rakkhissam paresam dhenuyaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_80:6.3",
        "original": "Tato ca āgā samaṇo mamantike,",
        "analyzed": "tatas ca āgā samaṇaḥ mama antike ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_80:6.4",
        "original": "Gāvo ca māse agamaṁsu khādituṁ.",
        "analyzed": "gāvaḥ ca māse agamasu khāditu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_80:7.1",
        "original": "Dvayajja kiccaṁ ubhayañca kāriyaṁ,",
        "analyzed": "ubhayañca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_80:7.2",
        "original": "Iccevahaṁ bhante tadā vicintayiṁ;",
        "analyzed": "id ca iva aham bhante tadā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_80:7.3",
        "original": "Tato ca saññaṁ paṭiladdha yoniso,",
        "analyzed": "tato ca saññam yoniso ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_80:7.4",
        "original": "Dadāmi bhanteti khipiṁ anantakaṁ.",
        "analyzed": "dadāmi bhanta iti khipim anantaka ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_80:8.1",
        "original": "So māsakhettaṁ turito avāsariṁ,",
        "analyzed": "saḥ māsa khettam turitaḥ avāsari ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_80:8.2",
        "original": "Purā ayaṁ bhañjati yassidaṁ dhanaṁ;",
        "analyzed": "purā ayam bhañjati yaḥ idam ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_80:8.3",
        "original": "Tato ca kaṇho urago mahāviso,",
        "analyzed": "tatas ca kaṇhaḥ uragaḥ mahā visaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_80:8.4",
        "original": "Aḍaṁsi pāde turitassa me sato.",
        "analyzed": "pāde turitassa me sataḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_80:9.1",
        "original": "Svāhaṁ aṭṭomhi dukkhena pīḷito,",
        "analyzed": "svāham dukkhena pīḍitaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_80:9.2",
        "original": "Bhikkhu ca taṁ sāmaṁ muñcitvānantakaṁ;",
        "analyzed": "bhikkhu ca tam sāmam muñcitvā anantaka ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_80:9.3",
        "original": "Ahāsi kummāsaṁ mamānukampayā,",
        "analyzed": "ahāsi kummāsam mama anukampayā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_80:9.4",
        "original": "Tato cuto kālakatomhi devatā.",
        "analyzed": "tatas cutaḥ devatā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_80:10.1",
        "original": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
        "analyzed": "tat eva kammam kusalam katam mayā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_80:10.2",
        "original": "Sukhañca kammaṁ anubhomi attanā;",
        "analyzed": "sukham ca kammam anubhomi attanā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_80:10.3",
        "original": "Tayā hi bhante anukampito bhusaṁ,",
        "analyzed": "tayā hi bhante anukampitaḥ bhusa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_80:10.4",
        "original": "Kataññutāya abhivādayāmi taṁ.",
        "analyzed": "kataññutāya abhivādayāmi ta ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_80:11.1",
        "original": "Sadevake loke samārake ca,",
        "analyzed": "sa devake loke sa mārake ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_80:11.2",
        "original": "Añño muni natthi tayānukampako;",
        "analyzed": "aññaḥ muni na atthi tayā anukampako ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_80:11.3",
        "original": "Tayā hi bhante anukampito bhusaṁ,",
        "analyzed": "tayā hi bhante anukampitaḥ bhusa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_80:11.4",
        "original": "Kataññutāya abhivādayāmi taṁ.",
        "analyzed": "kataññutāya abhivādayāmi ta ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_80:12.1",
        "original": "Imasmiṁ loke parasmiṁ vā pana,",
        "analyzed": "imasmin loke parasmin vā pana ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_80:12.2",
        "original": "Añño munī natthi tayānukampako;",
        "analyzed": "aññaḥ munī na atthi tayā anukampako ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_80:12.3",
        "original": "Tayā hi bhante anukampito bhusaṁ,",
        "analyzed": "tayā hi bhante anukampitaḥ bhusa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_80:12.4",
        "original": "Kataññutāya abhivādayāmi tan”ti.",
        "analyzed": "kataññutāya abhivādayāmi tanti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_vv_80:13.1",
        "original": "Gopālavimānaṁ chaṭṭhaṁ.",
        "analyzed": "gopāla vimānam chaṭṭha ",
        "folio": "13"
    }
]