        "analyzed": "sampattaḥ manda vedhito ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_tha-ap_538:16.1",
        "original": "Dārucīraṁ nivāsetvā,",
        "analyzed": "dāru cīram nivāsetvā ",
        "folio": "16"
    },
