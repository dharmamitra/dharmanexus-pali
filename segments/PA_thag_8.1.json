        "segmentnr": "PA_thag_8.1:7.2",
        "original": "sabbaṁ passati cakkhunā;",
        "analyzed": "sabbam passati cakkhunā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_8.1:7.3",
        "original": "Na ca diṭṭhaṁ sutaṁ dhīro,",
        "analyzed": "na ca diṭṭham sutam dhīro ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_8.1:7.4",
