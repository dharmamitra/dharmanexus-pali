        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_13.1:0.3",
        "original": "Paṭhamavagga",
        "analyzed": "paṭhama vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_13.1:0.4",
        "original": "Soṇakoḷivisattheragāthā",
        "analyzed": "soṇa koḷi visatthera gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_13.1:1.1",
        "original": "“Yāhu raṭṭhe samukkaṭṭho,",
        "analyzed": "yā āhu raṭṭhe samukkaṭṭhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_13.1:1.2",
        "original": "rañño aṅgassa paddhagū;",
        "analyzed": "raññaḥ aṅgassa paddhagū ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_13.1:1.3",
        "original": "Svajja dhammesu ukkaṭṭho,",
        "analyzed": "svajja dhammesu ukkaṭṭhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_13.1:1.4",
        "original": "soṇo dukkhassa pāragū.",
        "analyzed": "soṇaḥ dukkhassa pāragū ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_13.1:2.1",
        "original": "Pañca chinde pañca jahe,",
        "analyzed": "pañca chinde pañca jahe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_13.1:2.2",
        "original": "pañca cuttari bhāvaye;",
        "analyzed": "pañca cuttari bhāvaye ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_13.1:2.3",
        "original": "Pañcasaṅgātigo bhikkhu,",
        "analyzed": "pañca saṅga atigaḥ bhikkhu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_13.1:2.4",
        "original": "oghatiṇṇoti vuccati.",
        "analyzed": "ogha tiṇṇaḥ ti vuccati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_13.1:3.1",
        "original": "Unnaḷassa pamattassa,",
        "analyzed": "unnaḷassa pamattassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_13.1:3.2",
        "original": "bāhirāsassa bhikkhuno;",
        "analyzed": "bāhirāsassa bhikkhuno ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_13.1:3.3",
        "original": "Sīlaṁ samādhi paññā ca,",
        "analyzed": "sīlam samādhi paññā ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_13.1:3.4",
        "original": "pāripūriṁ na gacchati.",
        "analyzed": "pāripūrim na gacchati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_13.1:4.1",
        "original": "Yañhi kiccaṁ apaviddhaṁ,",
        "analyzed": "yat hi kiccam apaviddha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_13.1:4.2",
        "original": "akiccaṁ pana karīyati;",
        "analyzed": "a kiccam pana karīyati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_13.1:4.3",
        "original": "Unnaḷānaṁ pamattānaṁ,",
        "analyzed": "unnaḷānam pamattāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_13.1:4.4",
        "original": "tesaṁ vaḍḍhanti āsavā.",
        "analyzed": "tesam vaḍḍhanti āsavā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_13.1:5.1",
        "original": "Yesañca susamāraddhā,",
        "analyzed": "su samāraddhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_13.1:5.2",
        "original": "niccaṁ kāyagatā sati;",
        "analyzed": "niccam kāya gatā sati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_13.1:5.3",
        "original": "Akiccaṁ te na sevanti,",
        "analyzed": "a kiccam te na sevanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_13.1:5.4",
        "original": "kicce sātaccakārino;",
        "analyzed": "kitce sā tacca kārinaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_13.1:5.5",
        "original": "Satānaṁ sampajānānaṁ,",
        "analyzed": "satānam sampajānāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_13.1:5.6",
        "original": "atthaṁ gacchanti āsavā.",
        "analyzed": "attham gacchanti āsavā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_13.1:6.1",
        "original": "Ujumaggamhi akkhāte,",
        "analyzed": "uju maggamhi akkhāte ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_13.1:6.2",
        "original": "gacchatha mā nivattatha;",
        "analyzed": "gacchatha mā nivattatha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_13.1:6.3",
        "original": "Attanā codayattānaṁ,",
        "analyzed": "attana codayattāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_13.1:6.4",
        "original": "nibbānamabhihāraye.",
        "analyzed": "nibbānam abhihāraye ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_13.1:7.1",
        "original": "Accāraddhamhi vīriyamhi,",
        "analyzed": "acca āraddhamhi vīriyamhi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_13.1:7.2",
        "original": "satthā loke anuttaro;",
        "analyzed": "satthā loke anuttaraḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_13.1:7.3",
        "original": "Vīṇopamaṁ karitvā me,",
        "analyzed": "vīṇā upamam karitvā me ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_13.1:7.4",
        "original": "dhammaṁ desesi cakkhumā;",
        "analyzed": "dhammaṃ desesi cakkhumā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_13.1:7.5",
        "original": "Tassāhaṁ vacanaṁ sutvā,",
        "analyzed": "tassa aham vacanam sutvā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_13.1:7.6",
        "original": "vihāsiṁ sāsane rato.",
        "analyzed": "vihāsim sāsane rato ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_13.1:8.1",
        "original": "Samathaṁ paṭipādesiṁ,",
        "analyzed": "samatham paṭipādesi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_13.1:8.2",
        "original": "uttamatthassa pattiyā;",
        "analyzed": "uttama atthassa pattiyā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_13.1:8.3",
        "original": "Tisso vijjā anuppattā,",
        "analyzed": "tissaḥ vijjāḥ an uppattā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_13.1:8.4",
        "original": "kataṁ buddhassa sāsanaṁ.",
        "analyzed": "katam buddhassa sāsana ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_13.1:9.1",
        "original": "Nekkhamme adhimuttassa,",
        "analyzed": "nekkhamme adhimuttassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_13.1:9.2",
        "original": "pavivekañca cetaso;",
        "analyzed": "pavivekam ca cetasaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_13.1:9.3",
        "original": "Abyāpajjādhimuttassa,",
        "analyzed": "a byāpajja adhimuttassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_13.1:9.4",
        "original": "upādānakkhayassa ca.",
        "analyzed": "upādāna khayassa ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_13.1:10.1",
        "original": "Taṇhakkhayādhimuttassa,",
        "analyzed": "taṇhakkhaya adhimuttassa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_13.1:10.2",
        "original": "asammohañca cetaso;",
        "analyzed": "a sammoham ca cetasaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_13.1:10.3",
        "original": "Disvā āyatanuppādaṁ,",
        "analyzed": "disvā āyatanuppāda ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_13.1:10.4",
        "original": "sammā cittaṁ vimuccati.",
        "analyzed": "sammā cittam vimuccati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_13.1:11.1",
        "original": "Tassa sammā vimuttassa,",
        "analyzed": "tassa sammā vimuttassa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_13.1:11.2",
        "original": "santacittassa bhikkhuno;",
        "analyzed": "santacittassa bhikkhuno ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_13.1:11.3",
        "original": "Katassa paṭicayo natthi,",
        "analyzed": "katassa paṭicayaḥ natthi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_13.1:11.4",
        "original": "karaṇīyaṁ na vijjati.",
        "analyzed": "karaṇīyam na vijjati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_13.1:12.1",
        "original": "Selo yathā ekagghano,",
        "analyzed": "selaḥ yathā ekagghanaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_13.1:12.2",
        "original": "vātena na samīrati;",
        "analyzed": "vātena na samīrati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_13.1:12.3",
        "original": "Evaṁ rūpā rasā saddā,",
        "analyzed": "evam rūpā rasā saddā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_13.1:12.4",
        "original": "gandhā phassā ca kevalā.",
        "analyzed": "gandhā phassā ca kevalā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_13.1:13.1",
        "original": "Iṭṭhā dhammā aniṭṭhā ca,",
        "analyzed": "iṭṭhāḥ dhammāḥ aniṭṭhā ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_13.1:13.2",
        "original": "nappavedhenti tādino;",
        "analyzed": "na ppavedhenti tādinaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_13.1:13.3",
        "original": "Ṭhitaṁ cittaṁ visaññuttaṁ,",
        "analyzed": "ṭhitam cittam visaññutta ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_13.1:13.4",
        "original": "vayañcassānupassatī”ti.",
        "analyzed": "vayam ca assa anupassati iti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_13.1:13.5",
        "original": "… Soṇo koḷiviso thero …",
        "analyzed": "soṇaḥ koḷivisaḥ theraḥ ",
        "folio": "13"
    },
    {
