        "original": "āhu santo patiṭṭhitā.",
        "analyzed": "āhu santaḥ patiṭṭhitā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.3:8.1",
        "original": "Yaṁ buddho bhāsati vācaṁ,",
        "analyzed": "yam buddhaḥ bhāsati vācā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.3:8.2",
        "original": "Khemaṁ nibbānapattiyā;",
        "analyzed": "khemam nibbāna pattiyā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.3:8.3",
        "original": "Dukkhassantakiriyāya,",
        "analyzed": "dukkha ssanta kiriyāya ",
        "folio": "8"
    },
    {
