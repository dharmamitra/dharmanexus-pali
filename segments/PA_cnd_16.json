        "analyzed": "bhadrāvudhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_cnd_16:1.3",
        "original": "Nandiñjahaṁ oghatiṇṇaṁ vimuttaṁ;",
        "analyzed": "nandiñjaham oghatiṇṇam vimutta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_cnd_16:1.4",
        "original": "Kappañjahaṁ abhiyāce sumedhaṁ,",
        "analyzed": "kappañjaham abhiyāce sumedha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_cnd_16:1.5",
        "original": "Sutvāna nāgassa apanamissanti ito.",
        "analyzed": "sutvāna nāgassa apanamissanti itas ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_cnd_16:2.1",
        "original": "Okañjahaṁ taṇhacchidaṁ anejanti.",
        "analyzed": "okañjaham taṇha chidam anejanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.2_0",
        "original": "Okañjahanti rūpadhātuyā yo chando yo rāgo yā nandī yā taṇhā ye upayupādānā cetaso",
        "analyzed": "okañjahanti chandaḥ yaḥ rāgaḥ yā nandī yā taṇhā ye cetaso ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.2_1",
        "original": "adhiṭṭhānābhinivesānusayā, te buddhassa bhagavato pahīnā ucchinnamūlā tālāvatthukatā",
        "analyzed": "adhiṭṭhāna abhinivesa anusayā te buddhassa bhagavataḥ pahīnā ucchinna mūlā tālāvatthukatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.2_2",
        "original": "anabhāvaṅkatā āyatiṁ anuppādadhammā.",
        "analyzed": "an abhāvaṅka tā āyatim an uppāda dhammā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.3",
        "original": "Tasmā buddho okañjaho.",
        "analyzed": "tasmā buddhaḥ okañjahaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.4",
        "original": "Vedanādhātuyā …pe…",
        "analyzed": "vedanā dhātu yā pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.5",
        "original": "saññādhātuyā …",
        "analyzed": "saññā dhātuyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.6",
        "original": "saṅkhāradhātuyā …",
        "analyzed": "saṅkhāra dhātuyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.7_0",
        "original": "viññāṇadhātuyā yo chando yo rāgo yā nandī yā taṇhā ye upayupādānā cetaso adhiṭṭhānābhinivesānusayā,",
        "analyzed": "viññāṇa dhātuyāḥ yo chandaḥ yaḥ rāgaḥ yā nandī yā taṇhā ye upayupādānā cetasaḥ adhiṭṭhāna abhinivesa anusayā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.7_1",
        "original": "te buddhassa bhagavato pahīnā ucchinnamūlā tālāvatthukatā anabhāvaṅkatā āyatiṁ anuppādadhammā.",
        "analyzed": "te buddhassa bhagavataḥ pahīnāḥ ucchinna mūlā tālāvatthukatā anabhāvaṅkatā āyatim an uppāda dhammā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:2.8",
        "original": "Tasmā buddho okañjaho.",
        "analyzed": "tasmā buddhaḥ okañjahaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cnd_16:3.1",
        "original": "Taṇhacchidanti.",
        "analyzed": "taṇha chidanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.2",
        "original": "Taṇhāti rūpataṇhā …pe… dhammataṇhā.",
        "analyzed": "taṇhā ati rūpa taṇhā pe dhamma taṇhā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.3",
        "original": "Sā taṇhā buddhassa bhagavato chinnā ucchinnā samucchinnā vūpasantā paṭippassaddhā abhabbuppattikā ñāṇagginā daḍḍhā.",
        "analyzed": "sā taṇhā buddhassa bhagavataḥ chinnā ucchinnā samucchinnā vūpasantā paṭippassaddhā abhabbuppattikā ñāṇa agginā daḍḍhā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.4",
        "original": "Tasmā buddho taṇhacchido.",
        "analyzed": "tasmā buddhaḥ taṇha chidaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.5",
        "original": "Anejoti ejā vuccati taṇhā.",
        "analyzed": "anejaḥ iti ejā vuccati taṇhā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.6",
        "original": "Yo rāgo sārāgo …pe… abhijjhā lobho akusalamūlaṁ.",
        "analyzed": "yaḥ rāgaḥ sa arāgaḥ pe abhijjhā lobho akusala mūla ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.7",
        "original": "Sā ejā taṇhā buddhassa bhagavato pahīnā ucchinnamūlā tālāvatthukatā anabhāvaṅkatā āyatiṁ anuppādadhammā.",
        "analyzed": "sā ejā taṇhā buddhassa bhagavataḥ pahīnā ucchinna mūlā tālāvatthukatā anabhāvaṅkatā āyatim an uppāda dhammā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.8",
        "original": "Tasmā buddho anejo.",
        "analyzed": "tasmā buddhaḥ anejaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.9_0",
        "original": "Ejāya pahīnattā anejo bhagavā lābhepi na iñjati, alābhepi na iñjati, yasepi na iñjati,",
        "analyzed": "anejaḥ bhagavā lābhe pi naḥ iñjati a lābhe pi naḥ iñjati yase pi naḥ iñjati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.9_1",
        "original": "ayasepi na iñjati, pasaṁsāyapi na iñjati, nindāyapi na iñjati, sukhepi na iñjati,",
        "analyzed": "ayasepi naḥ iñjati nindāyapi naḥ iñjati sukhepi naḥ iñjati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.9_2",
        "original": "dukkhepi na iñjati na calati na vedhati na pavedhati na sampavedhatīti.",
        "analyzed": "dukkhe pi na iñjati na calati na vedhati na pavedhati na sampavedhati iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.10",
        "original": "Tasmā buddho anejoti—",
        "analyzed": "tasmāt buddhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.11",
        "original": "okañjahaṁ taṇhacchidaṁ anejaṁ.",
        "analyzed": "okañja ham taṇha chidam aneja ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.12",
        "original": "Iccāyasmā bhadrāvudhoti.",
        "analyzed": "id ca ayasmai bhadrāvudhā uti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.13",
        "original": "Iccāti padasandhi …pe…",
        "analyzed": "i t ca ati pada sandhi pe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cnd_16:3.14",
        "original": "āyasmāti, piyavacanaṁ …pe…",
