[
    {
        "segmentnr": "PA_snp_4.3:0.1",
        "original": "Sutta Nipāta 4.3",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.3:0.2",
        "original": "Duṭṭhaṭṭhakasutta",
        "analyzed": "duṭṭhaṭṭhaka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.3:1.1",
        "original": "Vadanti ve duṭṭhamanāpi eke,",
        "analyzed": "vadanti eke ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.3:1.2",
        "original": "Athopi ve saccamanā vadanti;",
        "analyzed": "vadanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.3:1.3",
        "original": "Vādañca jātaṁ muni no upeti,",
        "analyzed": "vādam ca jātam muni na u upeti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.3:1.4",
        "original": "Tasmā munī natthi khilo kuhiñci.",
        "analyzed": "tasmā munī na atthi khilo kuhiñci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.3:2.1",
        "original": "Sakañhi diṭṭhiṁ kathamaccayeyya,",
        "analyzed": "sakan hi diṭṭhim katham accayeyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.3:2.2",
        "original": "Chandānunīto ruciyā niviṭṭho;",
        "analyzed": "chanda anunītaḥ ruciyā niviṭṭhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.3:2.3",
        "original": "Sayaṁ samattāni pakubbamāno,",
        "analyzed": "sayam samattāni pakubbamānaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.3:2.4",
        "original": "Yathā hi jāneyya tathā vadeyya.",
        "analyzed": "yathā hi jāneyya tathā vadeyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.3:3.1",
        "original": "Yo attano sīlavatāni jantu,",
        "analyzed": "yaḥ attano sīlavatāni jantu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.3:3.2",
        "original": "Anānupuṭṭhova paresa pāva;",
        "analyzed": "an ānupuṭṭhaḥ uva paresa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.3:3.3",
        "original": "Anariyadhammaṁ kusalā tamāhu,",
        "analyzed": "an ariya dhammam kusalā tam āhu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.3:3.4",
        "original": "Yo ātumānaṁ sayameva pāva.",
        "analyzed": "yaḥ ātumānam sayamā iva pāva ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.3:4.1",
        "original": "Santo ca bhikkhu abhinibbutatto,",
        "analyzed": "santaḥ ca bhikkhu abhinibbutatto ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.3:4.2",
        "original": "Itihanti sīlesu akatthamāno;",
        "analyzed": "itihanti sīlesu akatthamānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.3:4.3",
        "original": "Tamariyadhammaṁ kusalā vadanti,",
        "analyzed": "tam ariya dhammam kusalāḥ vadanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.3:4.4",
        "original": "Yassussadā natthi kuhiñci loke.",
        "analyzed": "na atthi kuhiñci loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.3:5.1",
        "original": "Pakappitā saṅkhatā yassa dhammā,",
        "analyzed": "pakappitā saṅkhatā yassa dhammā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.3:5.2",
        "original": "Purakkhatā santi avīvadātā;",
        "analyzed": "purakkhatā santi a vīvadātā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.3:5.3",
        "original": "Yadattani passati ānisaṁsaṁ,",
        "analyzed": "yadattani passati ānisasa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.3:5.4",
        "original": "Taṁ nissito kuppapaṭiccasantiṁ.",
        "analyzed": "tam nissitaḥ kuppa paṭicca santi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.3:6.1",
        "original": "Diṭṭhīnivesā na hi svātivattā,",
        "analyzed": "diṭṭhīnivesāḥ na hi svātivattā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.3:6.2",
        "original": "Dhammesu niccheyya samuggahītaṁ;",
        "analyzed": "dhammesu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.3:6.3",
        "original": "Tasmā naro tesu nivesanesu,",
        "analyzed": "tasmā naraḥ tesu nivesanesu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.3:6.4",
        "original": "Nirassatī ādiyatī ca dhammaṁ.",
        "analyzed": "nirassatī ādiyatī ca dhamma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.3:7.1",
        "original": "Dhonassa hi natthi kuhiñci loke,",
        "analyzed": "dhonassa hi na atthi kuhiñci loke ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.3:7.2",
        "original": "Pakappitā diṭṭhi bhavābhavesu;",
        "analyzed": "pakappitā bhava abhavesu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.3:7.3",
        "original": "Māyañca mānañca pahāya dhono,",
        "analyzed": "māyañca mānañca pahāya dhono ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.3:7.4",
        "original": "Sa kena gaccheyya anūpayo so.",
        "analyzed": "sa kena gaccheyya saḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.3:8.1",
        "original": "Upayo hi dhammesu upeti vādaṁ,",
        "analyzed": "upayaḥ hi dhammesu upeti vāda ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.3:8.2",
        "original": "Anūpayaṁ kena kathaṁ vadeyya;",
        "analyzed": "anūpayam kena katham vadeyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.3:8.3",
        "original": "Attā nirattā na hi tassa atthi,",
        "analyzed": "attā nirattā na hi tassa atthi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.3:8.4",
        "original": "Adhosi so diṭṭhimidheva sabbanti.",
        "analyzed": "adhosi so diṭṭhi midhā iva sabbanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.3:8.5",
        "original": "Duṭṭhaṭṭhakasuttaṁ tatiyaṁ.",
        "analyzed": "duṭṭhaṭṭhaka suttam tatiya ",
        "folio": "8"
    }
]