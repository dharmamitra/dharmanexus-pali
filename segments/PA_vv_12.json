    },
    {
        "segmentnr": "PA_vv_12:1.1",
        "original": "“Veḷuriyathambhaṁ ruciraṁ pabhassaraṁ,",
        "analyzed": "ruciram pabhassara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_12:1.2",
        "original": "Vimānamāruyha anekacittaṁ;",
        "analyzed": "vimānam āruyha anekacitta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_12:1.3",
        "original": "Tatthacchasi devi mahānubhāve,",
        "analyzed": "devi mahā anubhāve ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_12:1.4",
        "original": "Uccāvacā iddhi vikubbamānā;",
        "analyzed": "uccāvacā id hi vikubbamānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_12:1.5",
        "original": "Imā ca te accharāyo samantato,",
        "analyzed": "imā ca te accharāyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_12:1.6",
        "original": "Naccanti gāyanti pamodayanti ca.",
        "analyzed": "naccanti gāyanti pamodayanti ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_12:2.1",
        "original": "Deviddhipattāsi mahānubhāve,",
        "analyzed": "devi iddhipattā asi mahā anubhāve ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_12:2.2",
        "original": "Manussabhūtā kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtā kim akāsi puñña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_12:2.3",
        "original": "Kenāsi evaṁ jalitānubhāvā,",
        "analyzed": "kena asi evam jalita anubhāvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_12:2.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_12:3.1",
        "original": "Sā devatā attamanā,",
        "analyzed": "sā devatā attamanā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_12:3.2",
        "original": "moggallānena pucchitā;",
        "analyzed": "moggallānena pucchitā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_12:3.3",
        "original": "Pañhaṁ puṭṭhā viyākāsi,",
