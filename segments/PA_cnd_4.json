        "folio": "12"
    },
    {
        "segmentnr": "PA_cnd_4:12.3",
        "original": "Namassamāno vivasemi rattiṁ,",
        "analyzed": "namassamānaḥ vivasemi ratti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_cnd_4:12.4",
        "original": "Teneva maññāmi avippavāsaṁ.",
