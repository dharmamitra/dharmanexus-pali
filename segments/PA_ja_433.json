        "analyzed": "ajara amaraḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_433:1.3",
        "original": "Sace tvaṁ yaññaṁ yājeyya,",
        "analyzed": "sace tvam yaññam yājeyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_433:1.4",
