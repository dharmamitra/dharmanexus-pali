        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_506:17.1",
        "original": "Nabhaṁ phaleyya udadhīpi susse,",
        "analyzed": "nabham phaleyyaḥ udadhīpi susse ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_506:17.2",
        "original": "Saṁvaṭṭaye bhūtadharā vasundharā;",
        "analyzed": "bhūta dharā vasundharā ",
        "folio": "17"
    },
    {
