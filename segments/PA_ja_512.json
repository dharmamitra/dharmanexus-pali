        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_512:12.4",
        "original": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
        "analyzed": "tassā puṇṇam kumbham imam kiṇātha ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_512:13.1",
        "original": "Iddhāni phītāni kulāni assu,",
        "analyzed": "iddhāni phītāni kulāni assu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_512:13.2",
