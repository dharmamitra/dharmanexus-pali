[
    {
        "segmentnr": "PA_snp_1.12:0.1",
        "original": "Sutta Nipāta 1.12",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.12:0.2",
        "original": "Munisutta",
        "analyzed": "muni sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.12:1.1",
        "original": "Santhavāto bhayaṁ jātaṁ,",
        "analyzed": "santhavātaḥ bhayam jāta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.12:1.2",
        "original": "niketā jāyate rajo;",
        "analyzed": "niketā jāyate rajaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.12:1.3",
        "original": "Aniketamasanthavaṁ,",
        "analyzed": "aniketam asanthava ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.12:1.4",
        "original": "etaṁ ve munidassanaṁ.",
        "analyzed": "etam veḥ muni dassana ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.12:2.1",
        "original": "Yo jātamucchijja na ropayeyya,",
        "analyzed": "yaḥ jātam ucchijja na ropayeyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.12:2.2",
        "original": "Jāyantamassa nānuppavecche;",
        "analyzed": "jāyantam assa na anuppavecche ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.12:2.3",
        "original": "Tamāhu ekaṁ muninaṁ carantaṁ,",
        "analyzed": "tam āhuḥ ekam muninam caranta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.12:2.4",
        "original": "Addakkhi so santipadaṁ mahesi.",
        "analyzed": "addakkhi saḥ santipadam mahesi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.12:3.1",
        "original": "Saṅkhāya vatthūni pamāya bījaṁ,",
        "analyzed": "saṅkhāya vatthūni pamāya bīja ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.12:3.2",
        "original": "Sinehamassa nānuppavecche;",
        "analyzed": "sinehamassa na anuppavecche ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.12:3.3",
        "original": "Sa ve munī jātikhayantadassī,",
        "analyzed": "sa ve munī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.12:3.4",
        "original": "Takkaṁ pahāya na upeti saṅkhaṁ.",
        "analyzed": "takkam pahāya naḥ upeti saṅkha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.12:4.1",
        "original": "Aññāya sabbāni nivesanāni,",
        "analyzed": "aññāya sabbāni nivesanāni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.12:4.2",
        "original": "Anikāmayaṁ aññatarampi tesaṁ;",
        "analyzed": "a nikāmayam aññataram pi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.12:4.3",
        "original": "Sa ve munī vītagedho agiddho,",
        "analyzed": "sa ve munī vīta gedhaḥ a giddhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.12:4.4",
        "original": "Nāyūhatī pāragato hi hoti.",
        "analyzed": "na āyūhatī pāra gataḥ hi hoti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.12:5.1",
        "original": "Sabbābhibhuṁ sabbaviduṁ sumedhaṁ,",
        "analyzed": "sabba abhibhum sabba vidum sumedha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.12:5.2",
        "original": "Sabbesu dhammesu anūpalittaṁ;",
        "analyzed": "sabbesu dhammesu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.12:5.3",
        "original": "Sabbañjahaṁ taṇhakkhaye vimuttaṁ,",
        "analyzed": "taṇhakkhaye vimutta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.12:5.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.12:6.1",
        "original": "Paññābalaṁ sīlavatūpapannaṁ,",
        "analyzed": "paññā balam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.12:6.2",
        "original": "Samāhitaṁ jhānarataṁ satīmaṁ;",
        "analyzed": "samāhitam jhāna ratam satīma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.12:6.3",
        "original": "Saṅgā pamuttaṁ akhilaṁ anāsavaṁ,",
        "analyzed": "saṅgā pamuttam akhilam anāsava ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.12:6.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.12:7.1",
        "original": "Ekaṁ carantaṁ munimappamattaṁ,",
        "analyzed": "ekam carantam munim appamatta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.12:7.2",
        "original": "Nindāpasaṁsāsu avedhamānaṁ;",
        "analyzed": "ninda apasasāsu a vedhamāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.12:7.3",
        "original": "Sīhaṁva saddesu asantasantaṁ,",
        "analyzed": "sīhava saddesu asantasanta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.12:7.4",
        "original": "Vātaṁva jālamhi asajjamānaṁ;",
        "analyzed": "vātava jālamhi asajjamāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.12:7.5",
        "original": "Padmaṁva toyena alippamānaṁ,",
        "analyzed": "padmava toyena alippamāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.12:7.6",
        "original": "Netāramaññesamanaññaneyyaṁ;",
        "analyzed": "netāram aññesam an añña neyya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.12:7.7",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.12:8.1",
        "original": "Yo ogahaṇe thambhorivābhijāyati,",
        "analyzed": "thambhoḥ iva abhijāyati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.12:8.2",
        "original": "Yasmiṁ pare vācāpariyantaṁ vadanti;",
        "analyzed": "yasmin pare vācā a pariyantam vadanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.12:8.3",
        "original": "Taṁ vītarāgaṁ susamāhitindriyaṁ,",
        "analyzed": "tam vīta rāgam su samāhita indriya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.12:8.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.12:9.1",
        "original": "Yo ve ṭhitatto tasaraṁva ujju,",
        "analyzed": "yaḥ ve tasaravaḥ ujju ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.12:9.2",
        "original": "Jigucchati kammehi pāpakehi;",
        "analyzed": "jigucchati kammehi pāpakehi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.12:9.3",
        "original": "Vīmaṁsamāno visamaṁ samañca,",
        "analyzed": "vīmasamānaḥ visamam samañca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.12:9.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.12:10.1",
        "original": "Yo saññatatto na karoti pāpaṁ,",
        "analyzed": "yaḥ saññatattaḥ na karoti pāpa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.12:10.2",
        "original": "Daharo majjhimo ca muni yatatto;",
        "analyzed": "daharaḥ majjhimaḥ ca muni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.12:10.3",
        "original": "Arosaneyyo na so roseti kañci,",
        "analyzed": "a rosaneyyaḥ na saḥ roseti kañci ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.12:10.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.12:11.1",
        "original": "Yadaggato majjhato sesato vā,",
        "analyzed": "yat aggataḥ majjhataḥ sesataḥ vā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.12:11.2",
        "original": "Piṇḍaṁ labhetha paradattūpajīvī;",
        "analyzed": "piṇḍam labhetha para dattūpajīvī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.12:11.3",
        "original": "Nālaṁ thutuṁ nopi nipaccavādī,",
        "analyzed": "na alam nipaccavādī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.12:11.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.12:12.1",
        "original": "Muniṁ carantaṁ virataṁ methunasmā,",
        "analyzed": "munim carantam viratam methunasmā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.12:12.2",
        "original": "Yo yobbane nopanibajjhate kvaci;",
        "analyzed": "yaḥ yobbane na upanibajjhate kvaci ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.12:12.3",
        "original": "Madappamādā virataṁ vippamuttaṁ,",
        "analyzed": "mad appamādāḥ viratam vippamutta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.12:12.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.12:13.1",
        "original": "Aññāya lokaṁ paramatthadassiṁ,",
        "analyzed": "aññāya lokam paramattha dassi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.12:13.2",
        "original": "Oghaṁ samuddaṁ atitariya tādiṁ;",
        "analyzed": "ogham samuddam atitariya tādi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.12:13.3",
        "original": "Taṁ chinnaganthaṁ asitaṁ anāsavaṁ,",
        "analyzed": "tam chinna gantham asitam anāsava ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.12:13.4",
        "original": "Taṁ vāpi dhīrā muni vedayanti.",
        "analyzed": "tam vā api dhīrāḥ muni vedayanti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.12:14.1",
        "original": "Asamā ubho dūravihāravuttino,",
        "analyzed": "asamāḥ ubho dūra vihāra vuttino ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.12:14.2",
        "original": "Gihī dāraposī amamo ca subbato;",
        "analyzed": "gihī dāra posī amamo ca subbato ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.12:14.3",
        "original": "Parapāṇarodhāya gihī asaññato,",
        "analyzed": "para pāṇa rodhāya gihī asaññataḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.12:14.4",
        "original": "Niccaṁ munī rakkhati pāṇine yato.",
        "analyzed": "niccam muniḥ rakkhati pāṇine yatas ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.12:15.1",
        "original": "Sikhī yathā nīlagīvo vihaṅgamo,",
        "analyzed": "sikhī yathā nīlagīvaḥ vihaṅgamaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.12:15.2",
        "original": "Haṁsassa nopeti javaṁ kudācanaṁ;",
        "analyzed": "hasassa na upeti javam kudācana ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.12:15.3",
        "original": "Evaṁ gihī nānukaroti bhikkhuno,",
        "analyzed": "evam gihī na anukaroti bhikkhunaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.12:15.4",
        "original": "Munino vivittassa vanamhi jhāyatoti.",
        "analyzed": "munino vivittassa vanamhi jhāyato ti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.12:15.5",
        "original": "Munisuttaṁ dvādasamaṁ.",
        "analyzed": "muni suttam dvādasama ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.12:15.6",
        "original": "Uragavaggo paṭhamo.",
        "analyzed": "uraga vaggo paṭhamaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.12:16.0",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.12:16.1",
        "original": "Urago dhaniyo ceva,",
        "analyzed": "uragaḥ dhaniyaḥ ca iva ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.12:16.2",
        "original": "visāṇañca tathā kasi;",
        "analyzed": "visāṇam ca tathā kasi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.12:16.3",
        "original": "Cundo parābhavo ceva,",
        "analyzed": "cundaḥ parābhavaḥ ca iva ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.12:16.4",
        "original": "vasalo mettabhāvanā.",
        "analyzed": "vasalaḥ metta bhāvanā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.12:17.1",
        "original": "Sātāgiro āḷavako,",
        "analyzed": "āḷavakaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.12:17.2",
        "original": "Vijayo ca tathā muni;",
        "analyzed": "vijayo ca tathā muni ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.12:17.3",
        "original": "Dvādasetāni suttāni,",
        "analyzed": "dvādasa itāni suttāni ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.12:17.4",
        "original": "Uragavaggoti vuccatīti.",
        "analyzed": "uraga vaggo ti vuccati iti ",
        "folio": "17"
    }
]