[
    {
        "segmentnr": "PA_snp_4.2:0.1",
        "original": "Sutta Nipāta 4.2",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.2:0.2",
        "original": "Guhaṭṭhakasutta",
        "analyzed": "guhaṭṭhaka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.2:1.1",
        "original": "Satto guhāyaṁ bahunābhichanno,",
        "analyzed": "sattaḥ guhā ayam bahunā abhichannaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.2:1.2",
        "original": "Tiṭṭhaṁ naro mohanasmiṁ pagāḷho;",
        "analyzed": "tiṭṭham naraḥ mohanasmin pagāḷhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.2:1.3",
        "original": "Dūre vivekā hi tathāvidho so,",
        "analyzed": "dūre hi tathāvidhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.2:1.4",
        "original": "Kāmā hi loke na hi suppahāyā.",
        "analyzed": "kāmāḥ hi loke na hi su ppahāyā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.2:2.1",
        "original": "Icchānidānā bhavasātabaddhā,",
        "analyzed": "icchā nidānā bhava sāta baddhā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.2:2.2",
        "original": "Te duppamuñcā na hi aññamokkhā;",
        "analyzed": "te duppamuñcāḥ na hi aññamokkhā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.2:2.3",
        "original": "Pacchā pure vāpi apekkhamānā,",
        "analyzed": "pacchā pure vā api apekkhamānā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.2:2.4",
        "original": "Ime va kāme purime va jappaṁ.",
        "analyzed": "ime va kāme purime va jappa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.2:3.1",
        "original": "Kāmesu giddhā pasutā pamūḷhā,",
        "analyzed": "kāmesu giddhā pasutā pamūḍhā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.2:3.2",
        "original": "Avadāniyā te visame niviṭṭhā;",
        "analyzed": "avadāniyā te visame niviṭṭhā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.2:3.3",
        "original": "Dukkhūpanītā paridevayanti,",
        "analyzed": "dukkha upanītā paridevayanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.2:3.4",
        "original": "Kiṁsū bhavissāma ito cutāse.",
        "analyzed": "kisū bhavissāma itas cutāse ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.2:4.1",
        "original": "Tasmā hi sikkhetha idheva jantu,",
        "analyzed": "tasmai hi sikkhetha idha iva jantu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.2:4.2",
        "original": "Yaṁ kiñci jaññā visamanti loke;",
        "analyzed": "yam kiñci jaññāḥ visamanti loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.2:4.3",
        "original": "Na tassa hetū visamaṁ careyya,",
        "analyzed": "na tassa hetū visamam careyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.2:4.4",
        "original": "Appañhidaṁ jīvitamāhu dhīrā.",
        "analyzed": "jīvitam āhu dhīrā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.2:5.1",
        "original": "Passāmi loke pariphandamānaṁ,",
        "analyzed": "passāmi loke pariphandamāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.2:5.2",
        "original": "Pajaṁ imaṁ taṇhagataṁ bhavesu;",
        "analyzed": "imam taṇha gatam bhavesu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.2:5.3",
        "original": "Hīnā narā maccumukhe lapanti,",
        "analyzed": "hīnāḥ narāḥ mad cu mukhe lapanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.2:5.4",
        "original": "Avītataṇhāse bhavābhavesu.",
        "analyzed": "a vīta taṇhā se bhava abhavesu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.2:6.1",
        "original": "Mamāyite passatha phandamāne,",
        "analyzed": "mamāyite passatha phandamāne ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.2:6.2",
        "original": "Maccheva appodake khīṇasote;",
        "analyzed": "macchā iva appa udake khīṇa sote ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.2:6.3",
        "original": "Etampi disvā amamo careyya,",
        "analyzed": "etam pi disvā amamaḥ careyya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.2:6.4",
        "original": "Bhavesu āsattimakubbamāno.",
        "analyzed": "bhavesu āsattim akubbamānaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.2:7.1",
        "original": "Ubhosu antesu vineyya chandaṁ,",
        "analyzed": "ubhosu antesu vineyya chanda ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.2:7.2",
        "original": "Phassaṁ pariññāya anānugiddho;",
        "analyzed": "phassam pariññāya an ānugiddhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.2:7.3",
        "original": "Yadattagarahī tadakubbamāno,",
        "analyzed": "yad atta garahī tad a kubbamānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.2:7.4",
        "original": "Na lippatī diṭṭhasutesu dhīro.",
        "analyzed": "na lippati diṭṭha sutesu dhīro ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.2:8.1",
        "original": "Saññaṁ pariññā vitareyya oghaṁ,",
        "analyzed": "saññam pariññā vitareyya ogha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.2:8.2",
        "original": "Pariggahesu muni nopalitto;",
        "analyzed": "pariggahesu muni na upalitto ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.2:8.3",
        "original": "Abbūḷhasallo caramappamatto,",
        "analyzed": "abbūḷha sallaḥ carama ppamatto ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.2:8.4",
        "original": "Nāsīsatī lokamimaṁ parañcāti.",
        "analyzed": "na asi isatī lokam imam paran ca ati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.2:8.5",
        "original": "Guhaṭṭhakasuttaṁ dutiyaṁ.",
        "analyzed": "guhaṭṭhaka suttam dutiya ",
        "folio": "8"
    }
]