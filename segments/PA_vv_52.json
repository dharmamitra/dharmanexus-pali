        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_52:3.1",
        "original": "“Uṭṭhehi revate supāpadhamme,",
        "analyzed": "uṭṭhehi revate su pāpa dhamme ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_52:3.2",
        "original": "Apārutadvāre adānasīle;",
        "analyzed": "a pāruta dvāre a dāna sīle ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_52:3.3",
        "original": "Nessāma taṁ yattha thunanti duggatā,",
        "analyzed": "nessāma tam yattha thunanti duggatā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_52:3.4",
        "original": "Samappitā nerayikā dukkhenā”ti.",
        "analyzed": "samappitā nerayikā dukkhena ati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_52:4.1",
        "original": "Icceva vatvāna yamassa dūtā,",
        "analyzed": "iccā iva vatvāna yamassa dūtā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_52:4.2",
        "original": "Te dve yakkhā lohitakkhā brahantā;",
        "analyzed": "te dve yakkhā lohitakkhā brahantā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_52:4.3",
        "original": "Paccekabāhāsu gahetvā revataṁ,",
        "analyzed": "pacceka bāhāsu gahetvā revata ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_52:4.4",
        "original": "Pakkāmayuṁ devagaṇassa santike.",
        "analyzed": "pakkāmayum deva gaṇassa santike ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_52:5.1",
        "original": "“Ādiccavaṇṇaṁ ruciraṁ pabhassaraṁ,",
