        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_470:14.3",
        "original": "Sakkohamasmi tidasānamindo,",
        "analyzed": "sakkā uham asmi tidasānam indo ",
        "folio": "14"
    },
    {
