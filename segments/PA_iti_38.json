    {
        "segmentnr": "PA_iti_38:0.4",
        "original": "Vitakkasutta",
        "analyzed": "vitakka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_38:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_38:2.1",
        "original": "“Tathāgataṁ, bhikkhave, arahantaṁ sammāsambuddhaṁ dve vitakkā bahulaṁ samudācaranti—",
        "analyzed": "tathāgata bhikkhave arahantam sammāsambuddham dve vitakkāḥ bahulam samudācaranti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_38:2.2",
        "original": "khemo ca vitakko, paviveko ca.",
        "analyzed": "khemaḥ ca vitakko paviveko ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_38:2.3",
        "original": "Abyāpajjhārāmo, bhikkhave, tathāgato abyāpajjharato.",
        "analyzed": "a byāpajjhārāmaḥ bhikkhave tathāgataḥ a byāpajjharataḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_38:2.4",
        "original": "Tamenaṁ, bhikkhave, tathāgataṁ abyāpajjhārāmaṁ abyāpajjharataṁ eseva vitakko bahulaṁ samudācarati:",
        "analyzed": "bhikkhave tathāgatam a byāpajjhārāmam a byāpajjha ratam eṣa iva vitakkaḥ bahulam samudācarati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_38:2.5",
        "original": "‘imāyāhaṁ iriyāya na kañci byābādhemi tasaṁ vā thāvaraṁ vā’ti.",
        "analyzed": "imāya aham iriyāya na kañci byābādhemi tasam vā thāvaram vā ati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_38:3.1",
        "original": "Pavivekārāmo, bhikkhave, tathāgato pavivekarato.",
        "analyzed": "paviveka ārāmaḥ bhikkhave tathāgataḥ paviveka rataḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_38:3.2",
        "original": "Tamenaṁ, bhikkhave, tathāgataṁ pavivekārāmaṁ pavivekarataṁ eseva vitakko bahulaṁ samudācarati:",
        "analyzed": "tam ena bhikkhave tathāgatam paviveka ārāmam paviveka ratam eṣa iva vitakkaḥ bahulam samudācarati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_38:3.3",
        "original": "‘yaṁ akusalaṁ taṁ pahīnan’ti.",
        "analyzed": "yam a kusalam tam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_38:4.1",
        "original": "Tasmātiha, bhikkhave, tumhepi abyāpajjhārāmā viharatha abyāpajjharatā.",
        "analyzed": "tasmāt iha bhikkhave tumhe pi a byāpajjhārāmāḥ viharatha a byāpajjharatā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_38:4.2",
        "original": "Tesaṁ vo, bhikkhave, tumhākaṁ abyāpajjhārāmānaṁ viharataṁ abyāpajjharatānaṁ eseva vitakko bahulaṁ samudācarissati:",
        "analyzed": "tesam vaḥ bhikkhave tumhākam a byāpajjhārāmānam viharatam a byāpajjharatānam esa eva vitakkaḥ bahulam samudācarissati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_38:4.3",
        "original": "‘imāya mayaṁ iriyāya na kañci byābādhema tasaṁ vā thāvaraṁ vā’ti.",
        "analyzed": "imāya mayam iriyāya na kañci tasam vā thāvaram vā ati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_38:5.1",
        "original": "Pavivekārāmā, bhikkhave, viharatha pavivekaratā.",
        "analyzed": "paviveka ārāmāḥ bhikkhave viharatha paviveka ratā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_38:5.2",
        "original": "Tesaṁ vo, bhikkhave, tumhākaṁ pavivekārāmānaṁ viharataṁ pavivekaratānaṁ eseva vitakko bahulaṁ samudācarissati:",
        "analyzed": "tesam vaḥ bhikkhave tumhākam paviveka ārāmānam viharatam paviveka ratānam esa eva vitakkaḥ bahulam samudācarissati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_38:5.3",
        "original": "‘kiṁ akusalaṁ, kiṁ appahīnaṁ, kiṁ pajahāmā’”ti.",
        "analyzed": "kim akusala kim appahīna kim pajahāma ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_38:5.4",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_38:5.5",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_38:6.1",
        "original": "“Tathāgataṁ buddhamasayhasāhinaṁ,",
        "analyzed": "tathāgatam buddham asayha sāhina ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_38:6.2",
        "original": "Duve vitakkā samudācaranti naṁ;",
        "analyzed": "duve vitakkā samudācaranti na ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_38:6.3",
        "original": "Khemo vitakko paṭhamo udīrito,",
        "analyzed": "khemaḥ vitakkaḥ paṭhamaḥ udīritaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_38:6.4",
        "original": "Tato viveko dutiyo pakāsito.",
        "analyzed": "tatas vivekaḥ dutiyaḥ pakāsitaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_38:7.1",
        "original": "Tamonudaṁ pāragataṁ mahesiṁ,",
        "analyzed": "tamonudam pāra gatam mahā isi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_38:7.2",
        "original": "Taṁ pattipattaṁ vasimaṁ anāsavaṁ;",
        "analyzed": "tam patti pattam vasimam anāsava ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_38:7.3",
        "original": "Visantaraṁ taṇhakkhaye vimuttaṁ,",
        "analyzed": "visantaram taṇhakkhaye ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_38:7.4",
        "original": "Taṁ ve muniṁ antimadehadhāriṁ;",
        "analyzed": "tam ve munim antima deha dhāri ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_38:7.5",
        "original": "Mārañjahaṁ brūmi jarāya pāraguṁ.",
        "analyzed": "māran aham brūmi jarāya pāragu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_38:8.1",
        "original": "Sele yathā pabbatamuddhaniṭṭhito,",
        "analyzed": "sele yathā pabbata muddha niṭṭhito ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_38:8.2",
        "original": "Yathāpi passe janataṁ samantato;",
        "analyzed": "yathā api passe janatam samantataḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_38:8.3",
        "original": "Tathūpamaṁ dhammamayaṁ sumedho,",
        "analyzed": "tathūpamam dhamma mayam sumedho ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_38:8.4",
        "original": "Pāsādamāruyha samantacakkhu;",
        "analyzed": "pāsādam āruyha samanta cakkhu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_38:8.5",
        "original": "Sokāvatiṇṇaṁ janatamapetasoko,",
        "analyzed": "soka avatiṇṇam janatam apeta sokaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_38:8.6",
        "original": "Avekkhati jātijarābhibhūtan”ti.",
        "analyzed": "avekkhati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_38:9.1",
        "original": "Ayampi attho vutto bhagavatā, iti me sutanti.",
        "analyzed": "ayam pi atthaḥ vuttaḥ bhagavatā iti me sutam ti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_38:9.2",
        "original": "Paṭhamaṁ.",
        "analyzed": "paṭhama ",
        "folio": "9"
    }
]