[
    {
        "segmentnr": "PA_ja_474:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_474:0.2",
        "original": "Terasakanipāta",
        "analyzed": "terasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_474:0.3",
        "original": "Ambavagga",
        "analyzed": "amba vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_474:0.4",
        "original": "1. Ambajātaka",
        "analyzed": "ambajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_474:1.1",
        "original": "“Ahāsi me ambaphalāni pubbe,",
        "analyzed": "ahāsi me amba phalāni pubbe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_474:1.2",
        "original": "Aṇūni thūlāni ca brahmacāri;",
        "analyzed": "aṇūni thūlāni ca brahmacāri ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_474:1.3",
        "original": "Teheva mantehi na dāni tuyhaṁ,",
        "analyzed": "tehā iva mantehi na dāni tuyha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_474:1.4",
        "original": "Dumapphalā pātubhavanti brahme”.",
        "analyzed": "pātubhavanti brahme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_474:2.1",
        "original": "“Nakkhattayogaṁ paṭimānayāmi,",
        "analyzed": "nakkhatta yogam paṭimānayāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_474:2.2",
        "original": "Khaṇaṁ muhuttañca mante na passaṁ;",
        "analyzed": "khaṇam muhuttañca mante na passa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_474:2.3",
        "original": "Nakkhattayogañca khaṇañca laddhā,",
        "analyzed": "nakkhatta yogañca khaṇañca laddhā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_474:2.4",
        "original": "Addhāharissambaphalaṁ pahūtaṁ”.",
        "analyzed": "addhā harissamba phalam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_474:3.1",
        "original": "“Nakkhattayogaṁ na pure abhāṇi,",
        "analyzed": "nakkhatta yogam na pure ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_474:3.2",
        "original": "Khaṇaṁ muhuttaṁ na pure asaṁsi;",
        "analyzed": "khaṇam muhuttam na pure asasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_474:3.3",
        "original": "Sayaṁ harī ambaphalaṁ pahūtaṁ,",
        "analyzed": "sayam harī amba phalam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_474:3.4",
        "original": "Vaṇṇena gandhena rasenupetaṁ.",
        "analyzed": "vaṇṇena gandhena rasena upeta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_474:4.1",
        "original": "Mantābhijappena pure hi tuyhaṁ,",
        "analyzed": "manta abhijappena pure hi tuyha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_474:4.2",
        "original": "Dumapphalā pātubhavanti brahme;",
        "analyzed": "pātubhavanti brahme ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_474:4.3",
        "original": "Svājja na pāresi jappampi mantaṁ,",
        "analyzed": "na pāresi manta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_474:4.4",
        "original": "Ayaṁ so ko nāma tavajja dhammo”.",
        "analyzed": "ayam saḥ kaḥ nāma tavajja dhammaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_474:5.1",
        "original": "“Caṇḍālaputto mama sampadāsi,",
        "analyzed": "caṇḍāla puttaḥ mama sampadā asi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_474:5.2",
        "original": "Dhammena mante pakatiñca saṁsi;",
        "analyzed": "dhammena mante pakatiñca sasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_474:5.3",
        "original": "Mā cassu me pucchito nāmagottaṁ,",
        "analyzed": "mā me pucchitaḥ nāma gotta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_474:5.4",
        "original": "Guyhittho atthaṁ vijaheyya manto.",
        "analyzed": "guyhitthaḥ attham vijaheyya manto ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_474:6.1",
        "original": "Sohaṁ janindena janamhi puṭṭho,",
        "analyzed": "saḥ aham janindena janamhi puṭṭhaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_474:6.2",
        "original": "Makkhābhibhūto alikaṁ abhāṇiṁ;",
        "analyzed": "makkha abhibhūtaḥ alikam abhāṇi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_474:6.3",
        "original": "‘Mantā ime brāhmaṇassā’ti micchā,",
        "analyzed": "mantāḥ ime micchā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_474:6.4",
        "original": "Pahīnamanto kapaṇo rudāmi”.",
        "analyzed": "kapaṇaḥ rudāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_474:7.1",
        "original": "“Eraṇḍā pucimandā vā,",
        "analyzed": "eraṇḍā pucimandā vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_474:7.2",
        "original": "atha vā pālibhaddakā;",
        "analyzed": "atha vā pālibhaddakā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_474:7.3",
        "original": "Madhuṁ madhutthiko vinde,",
        "analyzed": "madhum madhutthikaḥ vinde ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_474:7.4",
        "original": "so hi tassa dumuttamo.",
        "analyzed": "saḥ hi tassa dumuttamaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_474:8.1",
        "original": "Khattiyā brāhmaṇā vessā,",
        "analyzed": "khattiyāḥ brāhmaṇāḥ vessāḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_474:8.2",
        "original": "suddā caṇḍālapukkusā;",
        "analyzed": "suddā caṇḍālapukkusā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_474:8.3",
        "original": "Yamhā dhammaṁ vijāneyya,",
        "analyzed": "yamhā dhammam vijāneyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_474:8.4",
        "original": "so hi tassa naruttamo.",
        "analyzed": "saḥ hi tassa naruttamaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_474:9.1",
        "original": "Imassa daṇḍañca vadhañca datvā,",
        "analyzed": "imassa daṇḍañca vadhañca da tvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_474:9.2",
        "original": "Gale gahetvā khalayātha jammaṁ;",
        "analyzed": "gale gahetvā khalayā atha jamma ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_474:9.3",
        "original": "Yo uttamatthaṁ kasirena laddhaṁ,",
        "analyzed": "yaḥ uttamattham kasirena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_474:9.4",
        "original": "Mānātimānena vināsayittha”.",
        "analyzed": "māna ati mānena vināsayittha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_474:10.1",
        "original": "“Yathā samaṁ maññamāno pateyya,",
        "analyzed": "yathā samam maññamānaḥ pateyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_474:10.2",
        "original": "Sobbhaṁ guhaṁ narakaṁ pūtipādaṁ;",
        "analyzed": "guham narakam pūti pāda ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_474:10.3",
        "original": "Rajjūti vā akkame kaṇhasappaṁ,",
        "analyzed": "rajjū iti vai akkame kaṇhasappa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_474:10.4",
        "original": "Andho yathā jotimadhiṭṭhaheyya;",
        "analyzed": "andhaḥ yathā jotim adhiṭṭhaheyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_474:10.5",
        "original": "Evampi maṁ tvaṁ khalitaṁ sapañña,",
        "analyzed": "evam pi mam tvam khalitam sa pañña ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_474:10.6",
        "original": "Pahīnamantassa punappadāhi”.",
        "analyzed": "pahīnam antassa punappadāhi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_474:11.1",
        "original": "“Dhammena mantaṁ tava sampadāsiṁ,",
        "analyzed": "dhammena mantam tava sampadā asi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_474:11.2",
        "original": "Tuvampi dhammena pariggahesi;",
        "analyzed": "tuvam pi dhammena pariggahesi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_474:11.3",
        "original": "Pakatimpi te attamano asaṁsiṁ,",
        "analyzed": "pakatimpi te atta manaḥ asasi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_474:11.4",
        "original": "Dhamme ṭhitaṁ taṁ na jaheyya manto.",
        "analyzed": "dhamme ṭhitam tam na jaheyya manto ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_474:12.1",
        "original": "Yo bāla mantaṁ kasirena laddhaṁ,",
        "analyzed": "yaḥ bāla mantam kasirena ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_474:12.2",
        "original": "Yaṁ dullabhaṁ ajja manussaloke;",
        "analyzed": "yam dullabham ajja manussa loke ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_474:12.3",
        "original": "Kiñcāpi laddhā jīvituṁ appapañño,",
        "analyzed": "kiñca api laddhā jīvitum appapaññaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_474:12.4",
        "original": "Vināsayī alikaṁ bhāsamāno.",
        "analyzed": "vināsayī alikam bhāsamānaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_474:13.1",
        "original": "Bālassa mūḷhassa akataññuno ca,",
        "analyzed": "bālassa mūḷhassa a kataññuno ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_474:13.2",
        "original": "Musā bhaṇantassa asaññatassa;",
        "analyzed": "musā bhaṇantassa a saññatassa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_474:13.3",
        "original": "Mante mayaṁ tādisake na dema,",
        "analyzed": "mante mayam tādisake na dema ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_474:13.4",
        "original": "Kuto mantā gaccha na mayha ruccasī”ti.",
        "analyzed": "kutas mantā gaccha na mayha ruccasi iti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_474:14.1",
        "original": "Ambajātakaṁ paṭhamaṁ.",
        "analyzed": "amba jātakam ",
        "folio": "14"
    }
]