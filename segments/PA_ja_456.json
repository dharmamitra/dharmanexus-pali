[
    {
        "segmentnr": "PA_ja_456:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_456:0.2",
        "original": "Ekādasakanipāta",
        "analyzed": "ekādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_456:0.3",
        "original": "Mātuposakavagga",
        "analyzed": "mātu posaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_456:0.4",
        "original": "2. Juṇhajātaka",
        "analyzed": "juṇha jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_456:1.1",
        "original": "“Suṇohi mayhaṁ vacanaṁ janinda,",
        "analyzed": "suṇohi mayham vacanam janinda ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_456:1.2",
        "original": "Atthena juṇhamhi idhānupatto;",
        "analyzed": "atthena juṇhamhi idha anupatto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_456:1.3",
        "original": "Na brāhmaṇe addhike tiṭṭhamāne,",
        "analyzed": "na brāhmaṇe addhike tiṭṭhamāne ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_456:1.4",
        "original": "Gantabbamāhu dvipadindaseṭṭha”.",
        "analyzed": "gantabbam āhu dvipad inda seṭṭha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_456:2.1",
        "original": "“Suṇomi tiṭṭhāmi vadehi brahme,",
        "analyzed": "suṇomi tiṭṭhāmi vadehi brahme ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_456:2.2",
        "original": "Yenāsi atthena idhānupatto;",
        "analyzed": "yena asi atthena idha anupattaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_456:2.3",
        "original": "Kaṁ vā tvamatthaṁ mayi patthayāno,",
        "analyzed": "kam vā tvam attham mayi patthayānaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_456:2.4",
        "original": "Idhāgamā brahme tadiṅgha brūhi”.",
        "analyzed": "idha agama brahme tat iṅgha brūhi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_456:3.1",
        "original": "“Dadāhi me gāmavarāni pañca,",
        "analyzed": "dadāhi me gām avarāni pañca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_456:3.2",
        "original": "Dāsīsataṁ satta gavaṁsatāni;",
        "analyzed": "dāsī satam satta gava satāni ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_456:3.3",
        "original": "Parosahassañca suvaṇṇanikkhe,",
        "analyzed": "parosahassam ca suvaṇṇa nikkhe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_456:3.4",
        "original": "Bhariyā ca me sādisī dve dadāhi.",
        "analyzed": "bhariyā ca me sādisī dve dadāhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_456:4.1",
        "original": "Tapo nu te brāhmaṇa bhiṁsarūpo,",
        "analyzed": "tapaḥ nu te brāhmaṇa bhisarūpo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_456:4.2",
        "original": "Mantā nu te brāhmaṇa cittarūpā;",
        "analyzed": "mantā nu te brāhmaṇa citta rūpā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_456:4.3",
        "original": "Yakkhā nu te assavā santi keci,",
        "analyzed": "yakkhā nu te assavā santi keci ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_456:4.4",
        "original": "Atthaṁ vā me abhijānāsi kattaṁ.",
        "analyzed": "attham vā me abhijānāsi katta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_456:5.1",
        "original": "Na me tapo atthi na cāpi mantā,",
        "analyzed": "na me tapaḥ atthi na ca api mantā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_456:5.2",
        "original": "Yakkhāpi me assavā natthi keci;",
        "analyzed": "yakkhā api me assavā natthi keci ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_456:5.3",
        "original": "Atthampi te nābhijānāmi kattaṁ,",
        "analyzed": "te na abhijānāmi katta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_456:5.4",
        "original": "Pubbe ca kho saṅgatimattamāsi.",
        "analyzed": "pubbe ca kho saṅgatimattama asi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_456:6.1",
        "original": "Paṭhamaṁ idaṁ dassanaṁ jānato me,",
        "analyzed": "paṭhamam idam dassanam jānataḥ me ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_456:6.2",
        "original": "Na tābhijānāmi ito puratthā;",
        "analyzed": "na tā abhijānāmi itas puratthā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_456:6.3",
        "original": "Akkhāhi me pucchito etamatthaṁ,",
        "analyzed": "akkhāhi me pucchitaḥ etam attha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_456:6.4",
        "original": "Kadā kuhiṁ vā ahu saṅgamo no.",
        "analyzed": "kadā kuhim vai ahu saṅgamaḥ naḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_456:7.1",
        "original": "Gandhārarājassa puramhi ramme,",
        "analyzed": "gandhāra rājassa puramhi ramme ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_456:7.2",
        "original": "Avasimhase takkasīlāyaṁ deva;",
        "analyzed": "avasimhase takkasīlāyam deva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_456:7.3",
        "original": "Tatthandhakāramhi timīsikāyaṁ,",
        "analyzed": "tattha andhakāramhi timīsikāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_456:7.4",
        "original": "Aṁsena aṁsaṁ samaghaṭṭayimha.",
        "analyzed": "asena asam samaghaṭṭayimha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_456:8.1",
        "original": "Te tattha ṭhatvāna ubho janinda,",
        "analyzed": "te tattha ubhaḥ janinda ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_456:8.2",
        "original": "Sārāṇiyaṁ vītisārayimha tattha;",
        "analyzed": "sārāṇiyam vītisārayimha tattha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_456:8.3",
        "original": "Sāyeva no saṅgatimattamāsi,",
        "analyzed": "sāyā iva naḥ saṅgatimattamā asi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_456:8.4",
        "original": "Tato na pacchā na pure ahosi.",
        "analyzed": "tatas na pacchā na pure ahosi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_456:9.1",
        "original": "Yadā kadāci manujesu brahme,",
        "analyzed": "yadā kadāci manujesu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_456:9.2",
        "original": "Samāgamo sappurisena hoti;",
        "analyzed": "samāgamaḥ sappurisena hoti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_456:9.3",
        "original": "Na paṇḍitā saṅgatisanthavāni,",
        "analyzed": "na saṅgati santhavāni ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_456:9.4",
        "original": "Pubbe kataṁ vāpi vināsayanti.",
        "analyzed": "pubbe katam vā api vināsayanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_456:10.1",
        "original": "Bālāva kho saṅgatisanthavāni,",
        "analyzed": "saṅgati santhavāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_456:10.2",
        "original": "Pubbe kataṁ vāpi vināsayanti;",
        "analyzed": "pubbe katam vā api vināsayanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_456:10.3",
        "original": "Bahumpi bālesu kataṁ vinassati,",
        "analyzed": "bahum pi bālesu katam vinassati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_456:10.4",
        "original": "Tathā hi bālā akataññurūpā.",
        "analyzed": "tathā hi bālā a kataññu rūpā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_456:11.1",
        "original": "Dhīrā ca kho saṅgatisanthavāni,",
        "analyzed": "dhīrā ca kho saṅgati santhavāni ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_456:11.2",
        "original": "Pubbe kataṁ vāpi na nāsayanti;",
        "analyzed": "pubbe katam vā api na na āsayanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_456:11.3",
        "original": "Appampi dhīresu kataṁ na nassati,",
        "analyzed": "appam pi dhīresu katam na nassati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_456:11.4",
        "original": "Tathā hi dhīrā sukataññurūpā.",
        "analyzed": "tathā hi dhīrā su kataññu rūpā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_456:12.1",
        "original": "Dadāmi te gāmavarāni pañca,",
        "analyzed": "dadāmi te gām avarāni pañca ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_456:12.2",
        "original": "Dāsīsataṁ satta gavaṁsatāni;",
        "analyzed": "dāsī satam satta gava satāni ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_456:12.3",
        "original": "Parosahassañca suvaṇṇanikkhe,",
        "analyzed": "parosahassam ca suvaṇṇa nikkhe ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_456:12.4",
        "original": "Bhariyā ca te sādisī dve dadāmi.",
        "analyzed": "bhariyā ca te sādisī dve dadāmi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_456:13.1",
        "original": "Evaṁ sataṁ hoti samecca rāja,",
        "analyzed": "evam satam samet ca rāja ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_456:13.2",
        "original": "Nakkhattarājāriva tārakānaṁ;",
        "analyzed": "tārakāna ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_456:13.3",
        "original": "Āpūratī kāsipatī tathāhaṁ,",
        "analyzed": "āpūratī tathā āha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_456:13.4",
        "original": "Tayāpi me saṅgamo ajja laddho”ti.",
        "analyzed": "tayā api me saṅgamaḥ ajja laddhā uti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_456:14.1",
        "original": "Juṇhajātakaṁ dutiyaṁ.",
        "analyzed": "juṇha jātakam ",
        "folio": "14"
    }
]