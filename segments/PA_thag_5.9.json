        "analyzed": "kāma jāla sarīra ja ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_5.9:2.1",
        "original": "Tvaṁ olaggo na gacchasi,",
        "analyzed": "tvam olaggaḥ na gacchasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.9:2.2",
        "original": "Dvāravivaraṁ gajova alabhanto;",
        "analyzed": "dvāra vivaram ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.9:2.3",
        "original": "Na ca cittakali punappunaṁ,",
        "analyzed": "na ca citta kali punappuna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.9:2.4",
        "original": "Pasakka pāparato carissasi.",
        "analyzed": "pasakka pāpa rataḥ carissasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_5.9:3.1",
        "original": "Yathā kuñjaraṁ adantaṁ,",
        "analyzed": "yathā kuñjaram adanta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.9:3.2",
        "original": "Navaggahamaṅkusaggaho;",
        "analyzed": "nava ggaham aṅkusa ggahaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.9:3.3",
        "original": "Balavā āvatteti akāmaṁ,",
        "analyzed": "balavān āvattā iti akāma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.9:3.4",
        "original": "Evaṁ āvattayissaṁ taṁ.",
        "analyzed": "evam āvattayissam te ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_5.9:4.1",
        "original": "Yathā varahayadamakusalo,",
        "analyzed": "yathā vara haya dama kusalaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.9:4.2",
        "original": "Sārathi pavaro dameti ājaññaṁ;",
        "analyzed": "sārathi pavaraḥ dama iti ājañña ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.9:4.3",
        "original": "Evaṁ damayissaṁ taṁ,",
        "analyzed": "evam damayissam te ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.9:4.4",
        "original": "Patiṭṭhito pañcasu balesu.",
        "analyzed": "patiṭṭhitaḥ pañcasu balesu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_5.9:5.1",
        "original": "Satiyā taṁ nibandhissaṁ,",
        "analyzed": "satiyā tam nibandhissa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_5.9:5.2",
        "original": "Payutto te damessāmi;",
        "analyzed": "payuttaḥ te damessāmi ",
        "folio": "5"
    },
    {
