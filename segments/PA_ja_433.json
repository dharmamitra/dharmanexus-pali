        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_433:6.3",
        "original": "Balaṁ velā samuddassa,",
        "analyzed": "balam velā samuddassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_433:6.4",
        "original": "balātibalamitthiyo.",
