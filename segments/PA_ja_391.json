        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_391:1.2",
        "original": "Purakkhatvā pañjaliko namassasi;",
        "analyzed": "purakkhatvā pañjalikaḥ namassasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_391:1.3",
        "original": "Seyyo nu te so udavā sarikkho,",
        "analyzed": "seyyaḥ nu te saḥ udavā sarikkho ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_391:1.4",
        "original": "Nāmaṁ parassattano cāpi brūhi”.",
