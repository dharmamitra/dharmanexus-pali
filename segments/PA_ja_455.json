        "segmentnr": "PA_ja_455:4.3",
        "original": "Khāṇuṁ pādena ghaṭṭeti,",
        "analyzed": "khāṇum pādena ghaṭṭa iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_455:4.4",
        "original": "giriṁ caṇḍoraṇaṁ pati”.",
        "analyzed": "girim caṇḍoraṇam pati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_455:5.1",
        "original": "“Kā nu te sā mahānāga,",
        "analyzed": "kā nu te sā mahā nāga ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_455:5.2",
