[
    {
        "segmentnr": "PA_pv_30:0.1",
        "original": "Petavatthu",
        "analyzed": "petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_30:0.2",
        "original": "Cūḷavagga",
        "analyzed": "cūḷa vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_30:0.3",
        "original": "5. Kumārapetavatthu",
        "analyzed": "kumārapetavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_30:1.1",
        "original": "Accherarūpaṁ sugatassa ñāṇaṁ,",
        "analyzed": "sugatassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_30:1.2",
        "original": "Satthā yathā puggalaṁ byākāsi;",
        "analyzed": "satthā yathā puggalam byākāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_30:1.3",
        "original": "Ussannapuññāpi bhavanti heke,",
        "analyzed": "ussanna puññāḥ api bhavanti ha ike ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_30:1.4",
        "original": "Parittapuññāpi bhavanti heke.",
        "analyzed": "paritta puññā api bhavanti ha ike ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_30:2.1",
        "original": "Ayaṁ kumāro sīvathikāya chaḍḍito,",
        "analyzed": "ayam kumāraḥ sīvathikāya chaḍḍitaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_30:2.2",
        "original": "Aṅguṭṭhasnehena yāpeti rattiṁ;",
        "analyzed": "aṅguṭṭha snehena yāpa iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_30:2.3",
        "original": "Na yakkhabhūtā na sarīsapā vā,",
        "analyzed": "na yakkha bhūtāḥ na sarīsapāḥ vā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_30:2.4",
        "original": "Viheṭhayeyyuṁ katapuññaṁ kumāraṁ.",
        "analyzed": "viheṭhayeyyum kata puññam kumāra ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_30:3.1",
        "original": "Sunakhāpimassa palihiṁsu pāde,",
        "analyzed": "pāde ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_30:3.2",
        "original": "Dhaṅkā siṅgālā parivattayanti;",
        "analyzed": "dhaṅkā siṅgālā parivattayanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_30:3.3",
        "original": "Gabbhāsayaṁ pakkhigaṇā haranti,",
        "analyzed": "gabbhā sayam pakkhi gaṇāḥ haranti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_30:3.4",
        "original": "Kākā pana akkhimalaṁ haranti.",
        "analyzed": "kākāḥ pana akkhi malam haranti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_30:4.1",
        "original": "Nayimassa rakkhaṁ vidahiṁsu keci,",
        "analyzed": "rakkham vidahisu keci ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_30:4.2",
        "original": "Na osadhaṁ sāsapadhūpanaṁ vā;",
        "analyzed": "na osadham sāsapa dhūpanam vā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_30:4.3",
        "original": "Nakkhattayogampi na aggahesuṁ,",
        "analyzed": "nakkhatta yogam pi na aggahesu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_30:4.4",
        "original": "Na sabbadhaññānipi ākiriṁsu.",
        "analyzed": "na sabba dhañña anipi ākirisu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_30:5.1",
        "original": "Etādisaṁ uttamakicchapattaṁ,",
        "analyzed": "etādisam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_30:5.2",
        "original": "Rattābhataṁ sīvathikāya chaḍḍitaṁ;",
        "analyzed": "sīvathikāya chaḍḍita ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_30:5.3",
        "original": "Nonītapiṇḍaṁva pavedhamānaṁ,",
        "analyzed": "nonīta piṇḍava pavedhamāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_30:5.4",
        "original": "Sasaṁsayaṁ jīvitasāvasesaṁ.",
        "analyzed": "sa sa yam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_30:6.1",
        "original": "Tamaddasā devamanussapūjito,",
        "analyzed": "tam addasā deva manussa pūjito ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_30:6.2",
        "original": "Disvā ca taṁ byākari bhūripañño;",
        "analyzed": "disvā ca tam bhūri pañño ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_30:6.3",
        "original": "“Ayaṁ kumāro nagarassimassa,",
        "analyzed": "ayam kumāraḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_30:6.4",
        "original": "Aggakuliko bhavissati bhogato ca”.",
        "analyzed": "agga kulikaḥ bhavissati bhogato ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_30:7.1",
        "original": "“Kissa vataṁ kiṁ pana brahmacariyaṁ,",
        "analyzed": "kim pana ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_30:7.2",
        "original": "Kissa suciṇṇassa ayaṁ vipāko;",
        "analyzed": "kissa su ciṇṇassa ayam vipākaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_30:7.3",
        "original": "Etādisaṁ byasanaṁ pāpuṇitvā,",
        "analyzed": "etādisam byasanam pāpuṇitvā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_30:7.4",
        "original": "Taṁ tādisaṁ paccanubhossatiddhin”ti.",
        "analyzed": "tam tādisam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_30:8.1",
        "original": "Buddhapamukhassa bhikkhusaṅghassa,",
        "analyzed": "buddha pamukhassa bhikkhu saṅghassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_30:8.2",
        "original": "Pūjaṁ akāsi janatā uḷāraṁ;",
        "analyzed": "pūjam akāsi janatāḥ uḷāra ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_30:8.3",
        "original": "Tatrassa cittassahu aññathattaṁ,",
        "analyzed": "tatrassa cittassahu aññathatta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_30:8.4",
        "original": "Vācaṁ abhāsi pharusaṁ asabbhaṁ.",
        "analyzed": "vācam abhāsi pharusam asabbha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_30:9.1",
        "original": "So taṁ vitakkaṁ pavinodayitvā,",
        "analyzed": "saḥ tam vitakkam pavi nodayitvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_30:9.2",
        "original": "Pītiṁ pasādaṁ paṭiladdhā pacchā;",
        "analyzed": "pītim pasādam paṭiladdhā pacchā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_30:9.3",
        "original": "Tathāgataṁ jetavane vasantaṁ,",
        "analyzed": "tathāgatam jetavane vasanta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_30:9.4",
        "original": "Yāguyā upaṭṭhāsi sattarattaṁ.",
        "analyzed": "yāguyā upaṭṭhāsi sattaratta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_30:10.1",
        "original": "Tassa vataṁ taṁ pana brahmacariyaṁ,",
        "analyzed": "tassa vatam tam pana ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_30:10.2",
        "original": "Tassa suciṇṇassa ayaṁ vipāko;",
        "analyzed": "tassa su ciṇṇassa ayam vipākaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_30:10.3",
        "original": "Etādisaṁ byasanaṁ pāpuṇitvā,",
        "analyzed": "etādisam byasanam pāpuṇitvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_30:10.4",
        "original": "Taṁ tādisaṁ paccanubhossatiddhiṁ.",
        "analyzed": "tam tādisam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_30:11.1",
        "original": "Ṭhatvāna so vassasataṁ idheva,",
        "analyzed": "saḥ vassa satam idhā iva ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_30:11.2",
        "original": "Sabbehi kāmehi samaṅgibhūto;",
        "analyzed": "sabbehi kāmehi samaṅgi bhūtaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_30:11.3",
        "original": "Kāyassa bhedā abhisamparāyaṁ,",
        "analyzed": "kāyassa bhedāḥ abhisamparāya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_30:11.4",
        "original": "Sahabyataṁ gacchati vāsavassāti.",
        "analyzed": "sahabyatam gacchati vāsavaḥ sāti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_30:12.1",
        "original": "Kumārapetavatthu pañcamaṁ.",
        "analyzed": "kumārapetavatthu pañcama ",
        "folio": "12"
    }
]