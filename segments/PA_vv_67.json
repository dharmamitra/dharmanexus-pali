        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_67:6.1",
        "original": "Tavevāhaṁ mahāmuni,",
        "analyzed": "tava iva aham mahā muni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_67:6.2",
