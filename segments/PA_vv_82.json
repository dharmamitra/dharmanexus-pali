    },
    {
        "segmentnr": "PA_vv_82:0.4",
        "original": "8. Anekavaṇṇavimānavatthu",
        "analyzed": "aneka vaṇṇa vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_82:1.1",
        "original": "“Anekavaṇṇaṁ darasokanāsanaṁ,",
        "analyzed": "aneka vaṇṇam dara soka nāsana ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_82:1.2",
        "original": "Vimānamāruyha anekacittaṁ;",
        "analyzed": "vimānam āruyha anekacitta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_82:1.3",
        "original": "Parivārito accharāsaṅgaṇena,",
        "analyzed": "parivāritaḥ accha rāsaṅgaṇena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_82:1.4",
        "original": "Sunimmito bhūtapatīva modasi.",
        "analyzed": "su nimmitaḥ bhūtapati iva modasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_82:2.1",
        "original": "Samassamo natthi kuto panuttaro,",
        "analyzed": "samassamaḥ natthi kutas panuttaro ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_82:2.2",
        "original": "Yasena puññena ca iddhiyā ca;",
        "analyzed": "yasena puññena ca iddhiyā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_82:2.3",
        "original": "Sabbe ca devā tidasagaṇā samecca,",
        "analyzed": "sabbe ca devā tidasa gaṇā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_82:2.4",
        "original": "Taṁ taṁ namassanti sasiṁva devā;",
        "analyzed": "tam tam namassanti devā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_82:2.5",
        "original": "Imā ca te accharāyo samantato,",
        "analyzed": "imā ca te accharāyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_82:2.6",
        "original": "Naccanti gāyanti pamodayanti.",
        "analyzed": "naccanti gāyanti pamodayanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_82:3.1",
        "original": "Deviddhipattosi mahānubhāvo,",
        "analyzed": "deviddhipatto asi mahā anubhāvaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_82:3.2",
        "original": "Manussabhūto kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtaḥ kim akāsi puñña ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_82:3.3",
        "original": "Kenāsi evaṁ jalitānubhāvo,",
        "analyzed": "kena asi evam jalita anubhāvaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_82:3.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_82:4.1",
        "original": "So devaputto attamano,",
        "analyzed": "saḥ deva puttaḥ atta mano ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_82:4.2",
        "original": "…pe…",
        "analyzed": "pe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_82:4.3",
        "original": "yassa kammassidaṁ phalaṁ.",
        "analyzed": "yassa kammassa idam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_82:5.1",
        "original": "“Ahaṁ bhadante ahuvāsi pubbe,",
        "analyzed": "aham bhadante pubbe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_82:5.2",
        "original": "Sumedhanāmassa jinassa sāvako;",
        "analyzed": "sumedha nāmassa jinassa sāvako ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_82:5.3",
        "original": "Puthujjano ananubodhohamasmi,",
        "analyzed": "puthujjanaḥ ananubodhaḥ uham asmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_82:5.4",
        "original": "So satta vassāni paribbajissahaṁ.",
        "analyzed": "saḥ satta vassāni paribbajissaha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_82:6.1",
        "original": "Sohaṁ sumedhassa jinassa satthuno,",
        "analyzed": "so ham sumedhassa jinassa satthunaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_82:6.2",
        "original": "Parinibbutassoghatiṇṇassa tādino;",
        "analyzed": "tādinaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_82:6.3",
        "original": "Ratanuccayaṁ hemajālena channaṁ,",
        "analyzed": "rata nuccayam hema jālena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_82:6.4",
        "original": "Vanditvā thūpasmiṁ manaṁ pasādayiṁ.",
        "analyzed": "vanditvā thūpasmin ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_82:7.1",
        "original": "Na māsi dānaṁ na ca matthi dātuṁ,",
        "analyzed": "na māsi dānam na ca matthi dātu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_82:7.2",
        "original": "Pare ca kho tattha samādapesiṁ;",
        "analyzed": "pare ca kho tattha samādapesi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_82:7.3",
        "original": "Pūjetha naṁ pūjanīyassa dhātuṁ,",
        "analyzed": "pūjetha nam pūjanīyassa dhātu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_82:7.4",
        "original": "Evaṁ kira saggamito gamissatha.",
        "analyzed": "evam kira saggam itas gamissatha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_82:8.1",
        "original": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
        "analyzed": "tat eva kammam kusalam katam mayā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_82:8.2",
        "original": "Sukhañca dibbaṁ anubhomi attanā;",
        "analyzed": "sukhañca dibbam anubhomi attanā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_82:8.3",
        "original": "Modāmahaṁ tidasagaṇassa majjhe,",
        "analyzed": "modāmaham tidasa gaṇassa majjhe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_82:8.4",
        "original": "Na tassa puññassa khayampi ajjhagan”ti.",
        "analyzed": "na tassa puññassa khayampi ajjhaganti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_82:9.1",
        "original": "Anekavaṇṇavimānaṁ aṭṭhamaṁ.",
        "analyzed": "aneka vaṇṇa vimānam aṭṭhama ",
        "folio": "9"
    }
]