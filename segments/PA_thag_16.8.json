        "original": "abbhā muttova candimā.",
        "analyzed": "abbhā muttā uva candimā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_16.8:9.1",
        "original": "Disāpi me dhammakathaṁ suṇantu,",
        "analyzed": "disā api me dhamma katham suṇantu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.8:9.2",
        "original": "Disāpi me yuñjantu buddhasāsane;",
        "analyzed": "disā api me yuñjantu buddha sāsane ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.8:9.3",
        "original": "Disāpi me te manuje bhajantu,",
        "analyzed": "disā api me te manuje bhajantu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.8:9.4",
        "original": "Ye dhammamevādapayanti santo.",
        "analyzed": "ye dhammam eva ādapayanti santaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_16.8:10.1",
        "original": "Disā hi me khantivādānaṁ,",
        "analyzed": "disā hi me khantivādāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.8:10.2",
        "original": "avirodhappasaṁsinaṁ;",
        "analyzed": "a virodha ppasasina ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.8:10.3",
        "original": "Suṇantu dhammaṁ kālena,",
        "analyzed": "suṇantu dhammam kālena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.8:10.4",
        "original": "tañca anuvidhīyantu.",
        "analyzed": "tat ca anuvidhīyantu ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_16.8:11.1",
        "original": "Na hi jātu so mamaṁ hiṁse,",
        "analyzed": "na hi jātu saḥ mamam hise ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.8:11.2",
        "original": "aññaṁ vā pana kiñcanaṁ;",
        "analyzed": "aññam vā pana kiñcana ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.8:11.3",
        "original": "Pappuyya paramaṁ santiṁ,",
        "analyzed": "pappuyya paramam santi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.8:11.4",
        "original": "rakkheyya tasathāvare.",
        "analyzed": "rakkheyya tasathāvare ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_16.8:12.1",
        "original": "Udakañhi nayanti nettikā,",
        "analyzed": "udakam hi nayanti nettikā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.8:12.2",
        "original": "Usukārā namayanti tejanaṁ;",
        "analyzed": "usukārāḥ namayanti tejana ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.8:12.3",
        "original": "Dāruṁ namayanti tacchakā,",
        "analyzed": "dārum namayanti tat śakā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.8:12.4",
        "original": "Attānaṁ damayanti paṇḍitā.",
        "analyzed": "attānam damayanti paṇḍitā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_16.8:13.1",
        "original": "Daṇḍeneke damayanti,",
        "analyzed": "daṇḍena ike damayanti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.8:13.2",
        "original": "aṅkusebhi kasāhi ca;",
        "analyzed": "aṅkusebhi kasāhi ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.8:13.3",
        "original": "Adaṇḍena asatthena,",
        "analyzed": "a daṇḍena a satthena ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.8:13.4",
        "original": "ahaṁ dantomhi tādinā.",
        "analyzed": "aham tādinā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thag_16.8:14.1",
        "original": "‘Ahiṁsako’ti me nāmaṁ,",
        "analyzed": "ahisakoti me nāma ",
        "folio": "14"
    },
