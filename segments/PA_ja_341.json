        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_341:3.1",
        "original": "“Bakassa ca bāvarikassa rañño,",
        "analyzed": "bakassa ca bāvarikassa raññaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_341:3.2",
        "original": "Accantakāmānugatassa bhariyā;",
        "analyzed": "accanta kāma anugatassa bhariyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_341:3.3",
        "original": "Avācarī paṭṭhavasānugassa,",
        "analyzed": "avācarī paṭṭhavasa anugassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_341:3.4",
        "original": "Kaṁ vāpi itthī nāticare tadaññaṁ”.",
        "analyzed": "kam vā api itthī na aticare ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_341:4.1",
        "original": "“Piṅgiyānī sabbalokissarassa,",
        "analyzed": "piṅgiyānī sabba lokissarassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_341:4.2",
        "original": "Rañño piyā brahmadattassa bhariyā;",
        "analyzed": "raññaḥ piyā brahmadattassa bhariyā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_341:4.3",
