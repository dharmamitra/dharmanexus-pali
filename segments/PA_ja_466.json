[
    {
        "segmentnr": "PA_ja_466:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_466:0.2",
        "original": "Dvādasakanipāta",
        "analyzed": "dvādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_466:0.3",
        "original": "Cūḷakuṇālavagga",
        "analyzed": "cūḷa kuṇāla vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_466:0.4",
        "original": "3. Samuddavāṇijajātaka",
        "analyzed": "samudda vāṇija jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_466:1.1",
        "original": "“Kasanti vapanti te janā,",
        "analyzed": "kasanti vapanti te janā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_466:1.2",
        "original": "Manujā kammaphalūpajīvino;",
        "analyzed": "manujā kamma phala upajīvino ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_466:1.3",
        "original": "Nayimassa dīpakassa bhāgino,",
        "analyzed": "nayimassa dīpakassa bhāginaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_466:1.4",
        "original": "Jambudīpā idameva no varaṁ”.",
        "analyzed": "jambu dīpāḥ idam eva naḥ vara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_466:2.1",
        "original": "“Tipañcarattūpagatamhi cande,",
        "analyzed": "ti pañca rattu upagatamhi cande ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_466:2.2",
        "original": "Vego mahā hehiti sāgarassa;",
        "analyzed": "vegaḥ mahā sāgarassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_466:2.3",
        "original": "Uplavissaṁ dīpamimaṁ uḷāraṁ,",
        "analyzed": "uplavissam dīpam imam uḷāra ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_466:2.4",
        "original": "Mā vo vadhī gacchatha leṇamaññaṁ”.",
        "analyzed": "mā vaḥ vadhī gacchatha leṇamañña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_466:3.1",
        "original": "“Na jātuyaṁ sāgaravārivego,",
        "analyzed": "na jātuyam sāgara vāri vegaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_466:3.2",
        "original": "Uplavissaṁ dīpamimaṁ uḷāraṁ;",
        "analyzed": "uplavissam dīpam imam uḷāra ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_466:3.3",
        "original": "Taṁ me nimittehi bahūhi diṭṭhaṁ,",
        "analyzed": "tam me nimittehi bahūhi diṭṭha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_466:3.4",
        "original": "Mā bhetha kiṁ socatha modathavho.",
        "analyzed": "mā bhetha kim socatha modathavho ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_466:4.1",
        "original": "Pahūtabhakkhaṁ bahuannapānaṁ,",
        "analyzed": "pahūta bhakkham bahu anna pāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_466:4.2",
        "original": "Pattattha āvāsamimaṁ uḷāraṁ;",
        "analyzed": "pattatthe āvāsam imam uḷāra ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_466:4.3",
        "original": "Na vo bhayaṁ paṭipassāmi kiñci,",
        "analyzed": "na vaḥ bhayam paṭipassāmi kiñci ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_466:4.4",
        "original": "Āputtaputtehi pamodathavho”.",
        "analyzed": "āputta puttehi pamodathavhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_466:5.1",
        "original": "“Yo devoyaṁ dakkhiṇāyaṁ disāyaṁ,",
        "analyzed": "yaḥ devaḥ iyam dakkhiṇāyam disāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_466:5.2",
        "original": "Khemanti pakkosati tassa saccaṁ;",
        "analyzed": "khemanti pakkosati tassa sacca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_466:5.3",
        "original": "Na uttaro vedi bhayābhayassa,",
        "analyzed": "na uttaraḥ vedi bhaya abhayassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_466:5.4",
        "original": "Mā bhetha kiṁ socatha modathavho”.",
        "analyzed": "mā bhetha kim socatha modathavho ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_466:6.1",
        "original": "“Yathā ime vippavadanti yakkhā,",
        "analyzed": "yathā ime vippavadanti yakkhā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_466:6.2",
        "original": "Eko bhayaṁ saṁsati khemameko;",
        "analyzed": "ekaḥ bhayam sasati khemam ekaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_466:6.3",
        "original": "Tadiṅgha mayhaṁ vacanaṁ suṇātha,",
        "analyzed": "tat iṅgha vacanam suṇātha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_466:6.4",
        "original": "Khippaṁ lahuṁ mā vinassimha sabbe.",
        "analyzed": "khippam lahum mā vinassimha sabbe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_466:7.1",
        "original": "Sabbe samāgamma karoma nāvaṁ,",
        "analyzed": "sabbe samāgamma karoma nāva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_466:7.2",
        "original": "Doṇiṁ daḷhaṁ sabbayantūpapannaṁ;",
        "analyzed": "doṇim daḷham ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_466:7.3",
        "original": "Sace ayaṁ dakkhiṇo saccamāha,",
        "analyzed": "sace ayam dakkhiṇaḥ saccam āha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_466:7.4",
        "original": "Moghaṁ paṭikkosati uttaroyaṁ;",
        "analyzed": "mogham paṭikkosati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_466:7.5",
        "original": "Sā ceva no hehiti āpadatthā,",
        "analyzed": "sā ca iva naḥ hehiti āpadatthā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_466:7.6",
        "original": "Imañca dīpaṁ na pariccajema.",
        "analyzed": "imam ca dīpam na pariccajema ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_466:8.1",
        "original": "Sace ca kho uttaro saccamāha,",
        "analyzed": "sace ca kho uttaraḥ saccam āha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_466:8.2",
        "original": "Moghaṁ paṭikkosati dakkhiṇoyaṁ;",
        "analyzed": "mogham paṭikkosati dakkhiṇoya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_466:8.3",
        "original": "Tameva nāvaṁ abhiruyha sabbe,",
        "analyzed": "tam eva nāvam abhiruyha sabbe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_466:8.4",
        "original": "Evaṁ mayaṁ sotthi taremu pāraṁ.",
        "analyzed": "evam mayam taremu pāra ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_466:9.1",
        "original": "Na ve sugaṇhaṁ paṭhamena seṭṭhaṁ,",
        "analyzed": "na veḥ sugaṇham paṭhamena seṭṭha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_466:9.2",
        "original": "Kaniṭṭhamāpāthagataṁ gahetvā;",
        "analyzed": "kaniṭṭham āpātha gatam gahetvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_466:9.3",
        "original": "Yo cīdha tacchaṁ paviceyya gaṇhati,",
        "analyzed": "yaḥ ci idha taccham paviceyya gaṇhati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_466:9.4",
        "original": "Sa ve naro seṭṭhamupeti ṭhānaṁ.",
        "analyzed": "sa ve naraḥ seṭṭham upeti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_466:10.1",
        "original": "Yathāpi te sāgaravārimajjhe,",
        "analyzed": "yathā api te sāgara vāri majjhe ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_466:10.2",
        "original": "Sakammunā sotthi vahiṁsu vāṇijā;",
        "analyzed": "vahisu vāṇijā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_466:10.3",
        "original": "Anāgatatthaṁ paṭivijjhiyāna,",
        "analyzed": "anāgata ttham paṭivijjhiyāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_466:10.4",
        "original": "Appampi nācceti sa bhūripañño.",
        "analyzed": "na acceti sa bhūri paññaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_466:11.1",
        "original": "Bālā ca mohena rasānugiddhā,",
        "analyzed": "bālā ca mohena rasa anugiddhā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_466:11.2",
        "original": "Anāgataṁ appaṭivijjhiyatthaṁ;",
        "analyzed": "anāgatam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_466:11.3",
        "original": "Paccuppanne sīdanti atthajāte,",
        "analyzed": "paccuppanne sīdanti attha jāte ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_466:11.4",
        "original": "Samuddamajjhe yathā te manussā.",
        "analyzed": "samudda majjhe yathā te manussā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_466:12.1",
        "original": "Anāgataṁ paṭikayirātha kiccaṁ,",
        "analyzed": "anāgatam kit ca ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_466:12.2",
        "original": "‘Mā maṁ kiccaṁ kiccakāle byadhesi’;",
        "analyzed": "mā mama kiccam kicca kāle byadhesi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_466:12.3",
        "original": "Taṁ tādisaṁ paṭikatakiccakāriṁ,",
        "analyzed": "tam tādisam ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_466:12.4",
        "original": "Na taṁ kiccaṁ kiccakāle byadhetī”ti.",
        "analyzed": "na tam kiccam kicca kāle byadheti iti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_466:13.1",
        "original": "Samuddavāṇijajātakaṁ tatiyaṁ.",
        "analyzed": "samudda vāṇija jātakam ",
        "folio": "13"
    }
]