        "analyzed": "adāsim caturaḥ phale ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_67:7.1",
        "original": "Tasmā hi phalaṁ alameva dātuṁ,",
        "analyzed": "tasmai hi phalam alam eva dātu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_67:7.2",
        "original": "Niccaṁ manussena sukhatthikena;",
        "analyzed": "niccam manussena sukhatthikena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_67:7.3",
        "original": "Dibbāni vā patthayatā sukhāni,",
