    },
    {
        "segmentnr": "PA_ja_482:4.4",
        "original": "dūrato ajjhabhāsatha.",
        "analyzed": "dūratas ajjhabhāsatha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_482:5.1",
        "original": "Āgamehi mahārāja,",
        "analyzed": "āgama ihi mahā rāja ",
        "folio": "5"
    },
