        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_377:2.1",
        "original": "Agārino annadapānavatthadā,",
        "analyzed": "agārinaḥ anna da pānavattha dā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_377:2.2",
        "original": "Avhāyikā tampi disaṁ vadanti;",
