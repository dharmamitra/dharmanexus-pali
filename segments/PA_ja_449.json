    },
    {
        "segmentnr": "PA_ja_449:0.4",
        "original": "11. Maṭṭhakuṇḍalījātaka",
        "analyzed": "     ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_449:1.1",
        "original": "“Alaṅkato maṭṭhakuṇḍalī,",
        "analyzed": "alaṅkataḥ maṭṭha kuṇḍalī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_449:1.2",
        "original": "Māladhārī haricandanussado;",
        "analyzed": "      ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_449:1.3",
        "original": "Bāhā paggayha kandasi,",
        "analyzed": "bāhā paggayha kandasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_449:1.4",
        "original": "Vanamajjhe kiṁ dukkhito tuvaṁ”.",
        "analyzed": "vana majjhe kim dukkhitaḥ tuva ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_449:2.1",
        "original": "“Sovaṇṇamayo pabhassaro,",
        "analyzed": "sovaṇṇa mayaḥ pabhassaraḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_449:2.2",
        "original": "Uppanno rathapañjaro mama;",
        "analyzed": "uppannaḥ ratha pañjaraḥ mama ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_449:2.3",
        "original": "Tassa cakkayugaṁ na vindāmi,",
        "analyzed": "tassa cakka yugam na vindāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_449:2.4",
        "original": "Tena dukkhena jahāmi jīvitaṁ”.",
        "analyzed": "tena dukkhena jahāmi jīvita ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_449:3.1",
        "original": "“Sovaṇṇamayaṁ maṇīmayaṁ,",
        "analyzed": "sovaṇṇa mayam maṇī maya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_449:3.2",
        "original": "Lohamayaṁ atha rūpiyāmayaṁ;",
        "analyzed": "loha mayam atha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_449:3.3",
        "original": "Pāvada rathaṁ karissāmi te,",
        "analyzed": "pāvada ratham karissāmi te ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_449:3.4",
        "original": "Cakkayugaṁ paṭipādayāmi taṁ”.",
        "analyzed": "cakka yugam paṭipādayāmi te ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_449:4.1",
        "original": "“So māṇavo tassa pāvadi,",
        "analyzed": "saḥ māṇavaḥ tassa pāvadi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_449:4.2",
        "original": "Candasūriyā ubhayettha bhātaro;",
        "analyzed": "canda sūriyāḥ ubhayā ittha bhātaraḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_449:4.3",
        "original": "Sovaṇṇamayo ratho mama,",
        "analyzed": "sovaṇṇa mayaḥ rathaḥ mama ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_449:4.4",
        "original": "Tena cakkayugena sobhati”.",
        "analyzed": "tena cakka yugena sobhati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_449:5.1",
        "original": "“Bālo kho tvaṁsi māṇava,",
        "analyzed": "bālaḥ tu asi māṇava ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_449:5.2",
        "original": "Yo tvaṁ patthayase apatthiyaṁ;",
        "analyzed": "yaḥ tvam patthayase a patthiya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_449:5.3",
        "original": "Maññāmi tuvaṁ marissasi,",
        "analyzed": "maññāmi tuvam marissasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_449:5.4",
        "original": "Na hi tvaṁ lacchasi candasūriye”.",
        "analyzed": "na hi tvam lacchasi canda sūriye ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_449:6.1",
        "original": "“Gamanāgamanampi dissati,",
        "analyzed": "gamana āgamanam pi dissati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_449:6.2",
        "original": "Vaṇṇadhātu ubhayettha vīthiyo;",
        "analyzed": "vaṇṇa dhātu ubhayā ittha vīthiyaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_449:6.3",
        "original": "Peto pana neva dissati,",
        "analyzed": "petaḥ pana na iva dissati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_449:6.4",
        "original": "Ko nu kho kandataṁ bālyataro”.",
        "analyzed": "kaḥ nu khalu kandatam bālyataraḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_449:7.1",
        "original": "“Saccaṁ kho vadesi māṇava,",
        "analyzed": "saccam kho vadesi māṇava ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_449:7.2",
        "original": "Ahameva kandataṁ bālyataro;",
        "analyzed": "aham eva bālyataro ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_449:7.3",
        "original": "Candaṁ viya dārako rudaṁ,",
        "analyzed": "candam dārakaḥ ruda ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_449:7.4",
        "original": "Petaṁ kālakatābhipatthaye.",
        "analyzed": "petam kālakata abhipatthaye ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_449:8.1",
        "original": "Ādittaṁ vata maṁ santaṁ,",
        "analyzed": "ādittam vata mam santa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_449:8.2",
        "original": "ghatasittaṁva pāvakaṁ;",
        "analyzed": "pāvaka ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_449:8.3",
        "original": "Vārinā viya osiñcaṁ,",
        "analyzed": "vārinā viyaḥ osiñca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_449:8.4",
        "original": "sabbaṁ nibbāpaye daraṁ.",
        "analyzed": "sabbam nibbāpaye dara ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_449:9.1",
        "original": "Abbahī vata me sallaṁ,",
        "analyzed": "abbahī vata me salla ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_449:9.2",
        "original": "yamāsi hadayassitaṁ;",
        "analyzed": "yama asi hadayaḥ sita ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_449:9.3",
        "original": "Yo me sokaparetassa,",
        "analyzed": "yaḥ me soka paretassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_449:9.4",
        "original": "puttasokaṁ apānudi.",
        "analyzed": "putta sokam apānudi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_449:10.1",
        "original": "Sohaṁ abbūḷhasallosmi,",
        "analyzed": "saḥ aham abbūḍha sallaḥ asmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_449:10.2",
        "original": "vītasoko anāvilo;",
        "analyzed": "vīta sokaḥ anāvilaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_449:10.3",
        "original": "Na socāmi na rodāmi,",
        "analyzed": "na socāmi na rodāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_449:10.4",
        "original": "tava sutvāna māṇavā”ti.",
        "analyzed": "tava sutvāna māṇavāti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_449:11.1",
        "original": "Maṭṭhakuṇḍalījātakaṁ ekādasamaṁ.",
        "analyzed": "maṭṭha kuṇḍalī jātakam ",
        "folio": "11"
    }
]