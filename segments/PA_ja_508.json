        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_508:12.3",
        "original": "Bhariyāya rahogato asaṁsi,",
        "analyzed": "bhariyāya rahaḥ gataḥ asasi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_508:12.4",
        "original": "Guyhaṁ pātukataṁ sutaṁ mametaṁ”.",
        "analyzed": "guyham pātukatam sutam mameta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_508:13.1",
        "original": "“Yaṁ sālavanasmiṁ senako,",
        "analyzed": "yam sālavanasmin senakaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_508:13.2",
        "original": "Pāpakammaṁ akāsi asabbhirūpaṁ;",
