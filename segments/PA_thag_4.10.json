    {
        "segmentnr": "PA_thag_4.10:1.1",
        "original": "“Dhammo have rakkhati dhammacāriṁ,",
        "analyzed": "dhammaḥ have rakkhati dhamma cāri ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_4.10:1.2",
        "original": "Dhammo suciṇṇo sukhamāvahati;",
        "analyzed": "dhammaḥ su ciṇṇaḥ sukham āvahati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_4.10:1.3",
        "original": "Esānisaṁso dhamme suciṇṇe,",
        "analyzed": "dhamme su ciṇṇe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_4.10:1.4",
        "original": "Na duggatiṁ gacchati dhammacārī.",
