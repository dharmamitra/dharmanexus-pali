[
    {
        "segmentnr": "PA_snp_2.12:0.1",
        "original": "Sutta Nipāta 2.12",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.12:0.2",
        "original": "Nigrodhakappa (vaṅgīsa) sutta",
        "analyzed": "nigrodhakappa vaṅgīsa sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.12:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.2",
        "original": "ekaṁ samayaṁ bhagavā āḷaviyaṁ viharati aggāḷave cetiye.",
        "analyzed": "ekam samayam bhagavā āḷaviyam viharati aggāḷave cetiye ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.3",
        "original": "Tena kho pana samayena āyasmato vaṅgīsassa upajjhāyo nigrodhakappo nāma thero aggāḷave cetiye aciraparinibbuto hoti.",
        "analyzed": "tena kho pana samayena āyasmataḥ vaṅgīsassa upajjhāyaḥ nigrodhakappaḥ nāma theraḥ aggāḷave cetiye acira parinibbutaḥ hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.4",
        "original": "Atha kho āyasmato vaṅgīsassa rahogatassa paṭisallīnassa evaṁ cetaso parivitakko udapādi—",
        "analyzed": "atha kho āyasmataḥ vaṅgīsassa rahogatassa paṭisallīnassa evam cetasaḥ parivitakkaḥ udapādi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.5",
        "original": "parinibbuto nu kho me upajjhāyo udāhu no parinibbutoti?",
        "analyzed": "parinibbutaḥ nu kho me upajjhāyaḥ udāhu no parinibbutaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.6_0",
        "original": "Atha kho āyasmā vaṅgīso sāyanhasamayaṁ paṭisallānā vuṭṭhito yena bhagavā tenupasaṅkami;",
        "analyzed": "atha kho āyasmai vaṅgīsaḥ sāyanha samayam paṭisallānā vuṭṭhitaḥ yena bhagavā tena upasaṅkami ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.6_1",
        "original": "upasaṅkamitvā bhagavantaṁ abhivādetvā ekamantaṁ nisīdi. Ekamantaṁ nisinno kho āyasmā",
        "analyzed": "upasaṅkamitvā bhagavantam abhivādetvā ekamantam nisīdi ekamantam nisinnaḥ khalu āyasmā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.6_2",
        "original": "vaṅgīso bhagavantaṁ etadavoca—",
        "analyzed": "vaṅgīsaḥ bhagavantam etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.7",
        "original": "idha mayhaṁ, bhante, rahogatassa paṭisallīnassa evaṁ cetaso parivitakko udapādi—",
        "analyzed": "idha bhante rahogatassa paṭisallīnassa evam cetasaḥ parivitakkaḥ udapādi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.8",
        "original": "parinibbuto nu kho me upajjhāyo, udāhu no parinibbutoti.",
        "analyzed": "parinibbutaḥ nu kho me upajjhāyaḥ udāhu no parinibbutaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:1.9",
        "original": "Atha kho āyasmā vaṅgīso uṭṭhāyāsanā ekaṁsaṁ cīvaraṁ katvā yena bhagavā tenañjaliṁ paṇāmetvā bhagavantaṁ gāthāya ajjhabhāsi:",
        "analyzed": "atha kho āyasmā vaṅgīsaḥ uṭṭhāya āsanā ekasam cīvaram katvā yena bhagavā tena añjalim paṇāmetvā bhagavantam gāthāya ajjhabhāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.12:2.1",
        "original": "“Pucchāmi satthāramanomapaññaṁ,",
        "analyzed": "pucchāmi satthāram anomapañña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.12:2.2",
        "original": "Diṭṭheva dhamme yo vicikicchānaṁ chettā;",
        "analyzed": "diṭṭhā iva dhamme yaḥ vicikicchānam chettā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.12:2.3",
        "original": "Aggāḷave kālamakāsi bhikkhu,",
        "analyzed": "aggāḷave kālam akāsi bhikkhu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.12:2.4",
        "original": "Ñāto yasassī abhinibbutatto.",
        "analyzed": "ñātaḥ yasassī abhinibbutatto ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.12:3.1",
        "original": "Nigrodhakappo iti tassa nāmaṁ,",
        "analyzed": "nigrodhakappaḥ iti tassa nāma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.12:3.2",
        "original": "Tayā kataṁ bhagavā brāhmaṇassa;",
        "analyzed": "tayā katam bhagavā brāhmaṇassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.12:3.3",
        "original": "So taṁ namassaṁ acari mutyapekkho,",
        "analyzed": "saḥ tam namassam acari muti apekkhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.12:3.4",
        "original": "Āraddhaviriyo daḷhadhammadassī.",
        "analyzed": "āraddha viriyaḥ daḷha dhamma dassī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.12:4.1",
        "original": "Taṁ sāvakaṁ sakya mayampi sabbe,",
        "analyzed": "tam sāvakam sakya mayampi sabbe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.12:4.2",
        "original": "Aññātumicchāma samantacakkhu;",
        "analyzed": "aññātum icchāma samanta cakkhu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.12:4.3",
        "original": "Samavaṭṭhitā no savanāya sotā,",
        "analyzed": "samavaṭṭhitā naḥ savanāya sotā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.12:4.4",
        "original": "Tuvaṁ no satthā tvamanuttarosi.",
        "analyzed": "tuvam naḥ satthā tvam anuttarā asi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.12:5.1",
        "original": "Chindeva no vicikicchaṁ brūhi metaṁ,",
        "analyzed": "chindeva naḥ vicikiccham brūhi mā ita ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.12:5.2",
        "original": "Parinibbutaṁ vedaya bhūripañña;",
        "analyzed": "parinibbutam vedaya bhūri pañña ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.12:5.3",
        "original": "Majjheva no bhāsa samantacakkhu,",
        "analyzed": "majjhā iva naḥ bhāsa samanta cakkhu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.12:5.4",
        "original": "Sakkova devāna sahassanetto.",
        "analyzed": "sakkaḥ uva devāna sahassa nettaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.12:6.1",
        "original": "Ye keci ganthā idha mohamaggā,",
        "analyzed": "ye keci ganthāḥ idha moha maggā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.12:6.2",
        "original": "Aññāṇapakkhā vicikicchaṭhānā;",
        "analyzed": "aññāṇa pakkhā vicikiccha ṭhānā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.12:6.3",
        "original": "Tathāgataṁ patvā na te bhavanti,",
        "analyzed": "tathāgatam patvā na te bhavanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.12:6.4",
        "original": "Cakkhuñhi etaṁ paramaṁ narānaṁ.",
        "analyzed": "cakkhun hi etam paramam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.12:7.1",
        "original": "No ce hi jātu puriso kilese,",
        "analyzed": "no ce hi jātu purisaḥ kilese ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.12:7.2",
        "original": "Vāto yathā abbhaghanaṁ vihāne;",
        "analyzed": "vātaḥ yathā vihāne ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.12:7.3",
        "original": "Tamovassa nivuto sabbaloko,",
        "analyzed": "tamovassa nivutaḥ sabba loko ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.12:7.4",
        "original": "Na jotimantopi narā tapeyyuṁ.",
        "analyzed": "na jotimantaḥ api narā tapeyyu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.12:8.1",
        "original": "Dhīrā ca pajjotakarā bhavanti,",
        "analyzed": "dhīrāḥ ca pajjota karāḥ bhavanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.12:8.2",
        "original": "Taṁ taṁ ahaṁ vīra tatheva maññe;",
        "analyzed": "tam tam aham vīra tathā iva maññe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.12:8.3",
        "original": "Vipassinaṁ jānamupāgamumhā,",
        "analyzed": "vipassinam jānam upāgamumhā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.12:8.4",
        "original": "Parisāsu no āvikarohi kappaṁ.",
        "analyzed": "parisāsu naḥ āvikarohi kappa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.12:9.1",
        "original": "Khippaṁ giraṁ eraya vaggu vagguṁ,",
        "analyzed": "khippam giram eraya vaggu vaggu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.12:9.2",
        "original": "Haṁsova paggayha saṇikaṁ nikūja;",
        "analyzed": "hasā uva paggayha saṇikam nikūja ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.12:9.3",
        "original": "Bindussarena suvikappitena,",
        "analyzed": "bindussarena su vikappitena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.12:9.4",
        "original": "Sabbeva te ujjugatā suṇoma.",
        "analyzed": "sabbā iva te ujjugatā suṇoma ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.12:10.1",
        "original": "Pahīnajātimaraṇaṁ asesaṁ,",
        "analyzed": "pahīna jāti maraṇam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.12:10.2",
        "original": "Niggayha dhonaṁ vadessāmi dhammaṁ;",
        "analyzed": "niggayha dhonam vadessāmi dhamma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.12:10.3",
        "original": "Na kāmakāro hi puthujjanānaṁ,",
        "analyzed": "na kāma kāraḥ hi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.12:10.4",
        "original": "Saṅkheyyakāro ca tathāgatānaṁ.",
        "analyzed": "ca tathāgatāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.12:11.1",
        "original": "Sampannaveyyākaraṇaṁ tavedaṁ,",
        "analyzed": "sampanna veyyākaraṇam tava īda ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.12:11.2",
        "original": "Samujjupaññassa samuggahītaṁ;",
        "analyzed": "samujjupaññassa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.12:11.3",
        "original": "Ayamañjalī pacchimo suppaṇāmito,",
        "analyzed": "ayam añjalī pacchimaḥ su paṇāmito ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.12:11.4",
        "original": "Mā mohayī jānamanomapañña.",
        "analyzed": "mā mohayī jāna manaḥ mapañña ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.12:12.1",
        "original": "Parovaraṁ ariyadhammaṁ viditvā,",
        "analyzed": "parovaram ariya dhammam viditvā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.12:12.2",
        "original": "Mā mohayī jānamanomavīra;",
        "analyzed": "mā mohayī jāna manaḥ ma vīra ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.12:12.3",
        "original": "Vāriṁ yathā ghammani ghammatatto,",
        "analyzed": "vārim yathā ghammani ghamma tattaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.12:12.4",
        "original": "Vācābhikaṅkhāmi sutaṁ pavassa.",
        "analyzed": "vācā abhikaṅkhāmi sutam pavassa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.12:13.1",
        "original": "Yadatthikaṁ brahmacariyaṁ acarī,",
        "analyzed": "yadatthikam brahmacariyam acarī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.12:13.2",
        "original": "Kappāyano kaccissa taṁ amoghaṁ;",
        "analyzed": "kappāyano kaccissa tam amogha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.12:13.3",
        "original": "Nibbāyi so ādu saupādiseso,",
        "analyzed": "nibbāyi saḥ saupādisesaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.12:13.4",
        "original": "Yathā vimutto ahu taṁ suṇoma”.",
        "analyzed": "yathā vimuttaḥ ahu tam suṇoma ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.12:14.1",
        "original": "“Acchecchi taṇhaṁ idha nāmarūpe,",
        "analyzed": "nāma rūpe ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.12:14.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.12:14.3",
        "original": "Kaṇhassa sotaṁ dīgharattānusayitaṁ;",
        "analyzed": "kaṇhassa sotam ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.12:14.4",
        "original": "Atāri jātiṁ maraṇaṁ asesaṁ”,",
        "analyzed": "atāri jātim maraṇam asesa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.12:14.5",
        "original": "Iccabravī bhagavā pañcaseṭṭho.",
        "analyzed": "iccabravī bhagavā pañcaseṭṭhaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.12:15.1",
        "original": "“Esa sutvā pasīdāmi,",
        "analyzed": "esa sutvā pasīdāmi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.12:15.2",
        "original": "Vaco te isisattama;",
        "analyzed": "vacaḥ te isi sattama ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.12:15.3",
        "original": "Amoghaṁ kira me puṭṭhaṁ,",
        "analyzed": "amogham kira me puṭṭha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.12:15.4",
        "original": "Na maṁ vañcesi brāhmaṇo.",
        "analyzed": "na mama vañcesi brāhmaṇaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.12:16.1",
        "original": "Yathāvādī tathākārī,",
        "analyzed": "yathāvādī tathākārī ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.12:16.2",
        "original": "Ahu buddhassa sāvako;",
        "analyzed": "ahu buddhassa sāvakaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.12:16.3",
        "original": "Acchidā maccuno jālaṁ,",
        "analyzed": "acchidāḥ maccunaḥ jāla ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.12:16.4",
        "original": "Tataṁ māyāvino daḷhaṁ.",
        "analyzed": "tatam māyāvinaḥ daḷha ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.12:17.1",
        "original": "Addasā bhagavā ādiṁ,",
        "analyzed": "addasā bhagavā ādi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.12:17.2",
        "original": "Upādānassa kappiyo;",
        "analyzed": "upādānassa kappiyaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.12:17.3",
        "original": "Accagā vata kappāyano,",
        "analyzed": "accagāḥ vata kappāyano ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.12:17.4",
        "original": "Maccudheyyaṁ suduttaran”ti.",
        "analyzed": "maccudheyyam sut uttaranti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.12:17.5",
        "original": "Nigrodhakappasuttaṁ dvādasamaṁ.",
        "analyzed": "nigrodhakappasuttam dvādasama ",
        "folio": "17"
    }
]