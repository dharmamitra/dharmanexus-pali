[
    {
        "segmentnr": "PA_snp_2.2:0.1",
        "original": "Sutta Nipāta 2.2",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.2:0.2",
        "original": "Āmagandhasutta",
        "analyzed": "āmagandhasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.2:1.1",
        "original": "“Sāmākaciṅgūlakacīnakāni ca,",
        "analyzed": "sāmāka ciṅgūlaka cīnakāni ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.2:1.2",
        "original": "Pattapphalaṁ mūlaphalaṁ gavipphalaṁ;",
        "analyzed": "pattapphalam mūlaphalam gavipphala ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.2:1.3",
        "original": "Dhammena laddhaṁ satamasnamānā,",
        "analyzed": "dhammena laddham satamasnamānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.2:1.4",
        "original": "Na kāmakāmā alikaṁ bhaṇanti.",
        "analyzed": "na kāma kāmāḥ alikam bhaṇanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.2:2.1",
        "original": "Yadasnamāno sukataṁ suniṭṭhitaṁ,",
        "analyzed": "su katam su niṭṭhita ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.2:2.2",
        "original": "Parehi dinnaṁ payataṁ paṇītaṁ;",
        "analyzed": "parehi dinnam payatam paṇīta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.2:2.3",
        "original": "Sālīnamannaṁ paribhuñjamāno,",
        "analyzed": "sālīnam annam paribhuñjamānaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.2:2.4",
        "original": "So bhuñjasī kassapa āmagandhaṁ.",
        "analyzed": "saḥ bhuñjasi kassapa āma gandha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.2:3.1",
        "original": "Na āmagandho mama kappatīti,",
        "analyzed": "na āma gandhaḥ mama kappati iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.2:3.2",
        "original": "Icceva tvaṁ bhāsasi brahmabandhu;",
        "analyzed": "id ca iva tvam bhāsasi brahmabandhu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.2:3.3",
        "original": "Sālīnamannaṁ paribhuñjamāno,",
        "analyzed": "sālīnam annam paribhuñjamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.2:3.4",
        "original": "Sakuntamaṁsehi susaṅkhatehi;",
        "analyzed": "sakuntamasehi su saṅkhatehi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.2:3.5",
        "original": "Pucchāmi taṁ kassapa etamatthaṁ,",
        "analyzed": "pucchāmi tam kassapa etam attha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.2:3.6",
        "original": "Kathaṁpakāro tava āmagandho”.",
        "analyzed": "kathapakāraḥ tava āma gandhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.2:4.1",
        "original": "“Pāṇātipāto vadhachedabandhanaṁ,",
        "analyzed": "pāṇātipātaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.2:4.2",
        "original": "Theyyaṁ musāvādo nikativañcanāni ca;",
        "analyzed": "theyyam musāvādaḥ nikati vañcanāni ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.2:4.3",
        "original": "Ajjhenakuttaṁ paradārasevanā,",
        "analyzed": "ajjhenakuttam paradāra sevanā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.2:4.4",
        "original": "Esāmagandho na hi maṁsabhojanaṁ.",
        "analyzed": "na hi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.2:5.1",
        "original": "Ye idha kāmesu asaññatā janā,",
        "analyzed": "ye idha kāmesu asaññatā janā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.2:5.2",
        "original": "Rasesu giddhā asucibhāvamassitā;",
        "analyzed": "rasesu giddhā asuci bhāva massitā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.2:5.3",
        "original": "Natthikadiṭṭhī visamā durannayā,",
        "analyzed": "natthika diṭṭhī visamā durannayā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.2:5.4",
        "original": "Esāmagandho na hi maṁsabhojanaṁ.",
        "analyzed": "na hi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.2:6.1",
        "original": "Ye lūkhasā dāruṇā piṭṭhimaṁsikā,",
        "analyzed": "ye lūkhasā dāruṇā piṭṭhi masikā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.2:6.2",
        "original": "Mittadduno nikkaruṇātimānino;",
        "analyzed": "nikkaruṇa ati māninaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.2:6.3",
        "original": "Adānasīlā na ca denti kassaci,",
        "analyzed": "a dāna sīlāḥ na ca denti kassaci ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.2:6.4",
        "original": "Esāmagandho na hi maṁsabhojanaṁ.",
        "analyzed": "na hi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.2:7.1",
        "original": "Kodho mado thambho paccupaṭṭhāpanā,",
        "analyzed": "kodhaḥ madaḥ thambho paccupaṭṭhāpanā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.2:7.2",
        "original": "Māyā usūyā bhassasamussayo ca;",
        "analyzed": "māyā usūyā bhassa samussayaḥ ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.2:7.3",
        "original": "Mānātimāno ca asabbhi santhavo,",
        "analyzed": "māna atimānaḥ ca a sabbhi santhavaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.2:7.4",
        "original": "Esāmagandho na hi maṁsabhojanaṁ.",
        "analyzed": "na hi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.2:8.1",
        "original": "Ye pāpasīlā iṇaghātasūcakā,",
        "analyzed": "ye pāpa sīlāḥ iṇa ghāta sūcakā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.2:8.2",
        "original": "Vohārakūṭā idha pāṭirūpikā;",
        "analyzed": "pāṭirūpikā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.2:8.3",
        "original": "Narādhamā yedha karonti kibbisaṁ,",
        "analyzed": "nara adhamāḥ yedha karonti kibbisa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.2:8.4",
        "original": "Esāmagandho na hi maṁsabhojanaṁ.",
        "analyzed": "na hi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.2:9.1",
        "original": "Ye idha pāṇesu asaññatā janā,",
        "analyzed": "ye idha pāṇesu asaññatā janā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.2:9.2",
        "original": "Paresamādāya vihesamuyyutā;",
        "analyzed": "paresam ādāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.2:9.3",
        "original": "Dussīlaluddā pharusā anādarā,",
        "analyzed": "dussīla luddā pharusā anādarā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.2:9.4",
        "original": "Esāmagandho na hi maṁsabhojanaṁ.",
        "analyzed": "na hi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.2:10.1",
        "original": "Etesu giddhā viruddhātipātino,",
        "analyzed": "etesu giddhāḥ viruddha atipātinaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.2:10.2",
        "original": "Niccuyyutā pecca tamaṁ vajanti ye;",
        "analyzed": "niccuyyutā ped ca tamam vajanti ye ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.2:10.3",
        "original": "Patanti sattā nirayaṁ avaṁsirā,",
        "analyzed": "patanti sattāḥ nirayam avasirā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.2:10.4",
        "original": "Esāmagandho na hi maṁsabhojanaṁ.",
        "analyzed": "na hi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.2:11.1",
        "original": "Na macchamaṁsānamanāsakattaṁ,",
        "analyzed": "na mat śamasānam anāsakatta ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.2:11.2",
        "original": "Na naggiyaṁ na muṇḍiyaṁ jaṭājallaṁ;",
        "analyzed": "na naggiyam na muṇḍiyam jaṭājalla ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.2:11.3",
        "original": "Kharājināni nāggihuttassupasevanā,",
        "analyzed": "khara ajināni ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.2:11.4",
        "original": "Ye vāpi loke amarā bahū tapā;",
        "analyzed": "ye vā api loke amarāḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.2:11.5",
        "original": "Mantāhutī yaññamutūpasevanā,",
        "analyzed": "manta āhutī yañña mutu upasevanā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.2:11.6",
        "original": "Sodhenti maccaṁ avitiṇṇakaṅkhaṁ.",
        "analyzed": "sodhenti maccam a vitiṇṇa kaṅkha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.2:12.1",
        "original": "Sotesu gutto viditindriyo care,",
        "analyzed": "sotesu guttaḥ care ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.2:12.2",
        "original": "Dhamme ṭhito ajjavamaddave rato;",
        "analyzed": "dhamme ṭhitaḥ ajjava maddave rataḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.2:12.3",
        "original": "Saṅgātigo sabbadukkhappahīno,",
        "analyzed": "saṅga atigo sabba dukkha ppahīno ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.2:12.4",
        "original": "Na lippati diṭṭhasutesu dhīro”.",
        "analyzed": "na lippati diṭṭha sutesu dhīraḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.2:13.1",
        "original": "Iccetamatthaṁ bhagavā punappunaṁ,",
        "analyzed": "icca itam attham bhagavā punappuna ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.2:13.2",
        "original": "Akkhāsi naṁ vedayi mantapāragū;",
        "analyzed": "akkhāsi nam manta pāragū ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.2:13.3",
        "original": "Citrāhi gāthāhi munī pakāsayi,",
        "analyzed": "citrāhi gāthāhi munī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.2:13.4",
        "original": "Nirāmagandho asito durannayo.",
        "analyzed": "nirāma gandhaḥ asitaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.2:14.1",
        "original": "Sutvāna buddhassa subhāsitaṁ padaṁ,",
        "analyzed": "sutvāna buddhassa su bhāsitam pada ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.2:14.2",
        "original": "Nirāmagandhaṁ sabbadukkhappanūdanaṁ;",
        "analyzed": "nirāma gandham sabba dukkha ppanūdana ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.2:14.3",
        "original": "Nīcamano vandi tathāgatassa,",
        "analyzed": "nīca manaḥ vandi tathāgatassa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.2:14.4",
        "original": "Tattheva pabbajjamarocayitthāti.",
        "analyzed": "tattha iva pabbajjam arocayittha ati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.2:14.5",
        "original": "Āmagandhasuttaṁ dutiyaṁ.",
        "analyzed": "āmagandha suttam ",
        "folio": "14"
    }
]