        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_377:3.3",
        "original": "Kacci nu te mānusake payoge,",
        "analyzed": "kacci nu te mānusake payoge ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_377:3.4",
