        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_506:19.3",
        "original": "Maṅkāraṇā bandhanasmā pamutto,",
        "analyzed": "maṅkāraṇā bandhanasmā pamuttaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_506:19.4",
        "original": "Arahasi no jānituye katāni”.",
        "analyzed": "arahasi naḥ jānituye katāni ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_506:20.1",
        "original": "“So paccataṁ niraye ghorarūpe,",
