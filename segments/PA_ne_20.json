    },
    {
        "segmentnr": "PA_ne_20:0.2",
        "original": "Paṭiniddesavāra",
        "analyzed": "paṭiniddesa vāra ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ne_20:0.3",
        "original": "Sampāta 1",
        "analyzed": "sampāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ne_20:0.4",
        "original": "Desanāhārasampāta",
        "analyzed": "desanā āhāra sampāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ne_20:1.1",
        "original": "“Soḷasa hārā paṭhamaṁ,",
        "analyzed": "soḷasa hārā paṭhama ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ne_20:1.2",
        "original": "Disalocanato disā viloketvā;",
        "analyzed": "disa locanataḥ disā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ne_20:1.3",
        "original": "Saṅkhipiya aṅkusena hi,",
        "analyzed": "saṅkhipiya aṅkusena hi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ne_20:1.4",
        "original": "Nayehi tīhi niddise suttan”ti.",
        "analyzed": "nayehi tīhi niddise suttanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ne_20:2.1",
        "original": "Vuttā, tassā niddeso kuhiṁ daṭṭhabbo?",
        "analyzed": "vuttā tassā niddesaḥ kuhim daṭṭhabbo ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_20:2.2",
        "original": "Hārasampāte.",
        "analyzed": "hāra sampāte ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_20:2.3",
        "original": "Tattha katamo desanāhārasampāto?",
        "analyzed": "tattha katamaḥ desanā āhāra sampāto ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ne_20:3.1",
        "original": "“Arakkhitena cittena,",
        "analyzed": "a rakkhitena cittena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_20:3.2",
        "original": "micchādiṭṭhihatena ca;",
        "analyzed": "micchādiṭṭhi hatena ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_20:3.3",
        "original": "Thinamiddhābhibhūtena,",
        "analyzed": "thinamiddha abhibhūtena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_20:3.4",
        "original": "vasaṁ mārassa gacchatī”ti.",
        "analyzed": "vasam mārassa gacchati iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ne_20:4.1",
        "original": "Arakkhitena cittenāti kiṁ desayati, pamādaṁ taṁ maccuno padaṁ.",
        "analyzed": "a rakkhitena cittena ati kim desayati pamādam tam maccuno pada ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.2",
        "original": "Micchādiṭṭhihatena cāti micchādiṭṭhihataṁ nāma vuccati yadā “anicce niccan”ti passati, so vipallāso.",
        "analyzed": "micchādiṭṭhi hatena ca ati micchādiṭṭhi hatam nāma vuccati yadā anicce niccanti passati saḥ vipallāso ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.3",
        "original": "So pana vipallāso kiṁlakkhaṇo?",
        "analyzed": "so pana vipallāsaḥ kilakkhaṇo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.4",
        "original": "Viparītaggāhalakkhaṇo vipallāso.",
        "analyzed": "viparīta ggāha lakkhaṇaḥ vipallāsaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.5",
        "original": "So kiṁ vipallāsayati?",
        "analyzed": "sā u kim vipallāsayati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.6",
        "original": "Tayo dhamme saññaṁ cittaṁ diṭṭhimiti.",
        "analyzed": "tayaḥ dhamme saññam cittam diṭṭhim iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.7",
        "original": "So kuhiṁ vipallāsayati?",
        "analyzed": "saḥ kuhim vipallāsayati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.8",
        "original": "Catūsu attabhāvavatthūsu, rūpaṁ attato samanupassati, rūpavantaṁ vā attānaṁ, attani vā rūpaṁ, rūpasmiṁ vā attānaṁ.",
        "analyzed": "catūsu atta bhāva vatthūsu rūpam attataḥ samanupassati rūpavantam vā attāna attani vā rūpa rūpasmim vā attāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.9",
        "original": "Evaṁ vedanaṁ …pe…",
        "analyzed": "evam vedanam pe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.10",
        "original": "saññaṁ …pe…",
        "analyzed": "saññam pe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.11",
        "original": "saṅkhāre …pe…",
        "analyzed": "saṅkhāre pe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:4.12",
        "original": "viññāṇaṁ attato samanupassati, viññāṇavantaṁ vā attānaṁ, attani vā viññāṇaṁ, viññāṇasmiṁ vā attānaṁ.",
        "analyzed": "viññāṇam attataḥ samanupassati viññāṇavantam vā attāna attani vā viññāṇa viññāṇasmin vā attāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ne_20:5.1",
        "original": "Tattha rūpaṁ paṭhamaṁ vipallāsavatthu “asubhe subhan”ti.",
        "analyzed": "tattha rūpam paṭhamam vipallāsa vatthu asubhe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:5.2",
        "original": "Vedanā dutiyaṁ vipallāsavatthu “dukkhe sukhan”ti.",
        "analyzed": "vedanā dutiyam vipallāsa vatthu dukkhe sukhanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:5.3",
        "original": "Saññā saṅkhārā ca tatiyaṁ vipallāsavatthu “anattani attā”ti.",
        "analyzed": "saññā saṅkhārā ca tatiyam vipallāsa vatthu an attani attā ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:5.4",
        "original": "Viññāṇaṁ catutthaṁ vipallāsavatthu “anicce niccan”ti.",
        "analyzed": "viññāṇam catuttham vipallāsa vatthu anicce niccanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:5.5",
        "original": "Dve dhammā cittassa saṅkilesā—",
        "analyzed": "dve dhammā cittassa saṅkilesā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:5.6",
        "original": "taṇhā ca avijjā ca.",
        "analyzed": "taṇhā ca avijjā ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:5.7",
        "original": "Taṇhānivutaṁ cittaṁ dvīhi vipallāsehi vipallāsīyati “asubhe subhan”ti “dukkhe sukhan”ti.",
        "analyzed": "taṇhā nivutam cittam dvīhi vipallāsehi vipallāsīyati asubhe subhanti dukkhe sukhanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:5.8",
        "original": "Diṭṭhinivutaṁ cittaṁ dvīhi vipallāsehi vipallāsīyati “anicce niccan”ti “anattani attā”ti.",
        "analyzed": "diṭṭhi nivutam cittam dvīhi vipallāsehi vipallāsīyati anicce niccanti an attani attā ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ne_20:6.1",
        "original": "Tattha yo diṭṭhivipallāso, so atītaṁ rūpaṁ attato samanupassati,",
        "analyzed": "tattha yaḥ diṭṭhi vipallāso so atītam rūpam attataḥ samanupassati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.2",
        "original": "atītaṁ vedanaṁ …pe…",
        "analyzed": "atītam vedanam pe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.3",
        "original": "atītaṁ saññaṁ,",
        "analyzed": "atītam sañña ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.4",
        "original": "atīte saṅkhāre …",
        "analyzed": "atīte saṅkhāre ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.5",
        "original": "atītaṁ viññāṇaṁ attato samanupassati.",
        "analyzed": "atītam viññāṇam attataḥ samanupassati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.6",
        "original": "Tattha yo taṇhāvipallāso, so anāgataṁ rūpaṁ abhinandati,",
        "analyzed": "tattha yaḥ taṇhā vipallāso saḥ anāgatam rūpam abhinandati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.7",
        "original": "anāgataṁ vedanaṁ …pe…",
        "analyzed": "anāgatam vedanam pe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.8",
        "original": "anāgataṁ saññaṁ,",
        "analyzed": "anāgatam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.9",
        "original": "anāgate saṅkhāre,",
        "analyzed": "anāgate saṅkhāre ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.10",
        "original": "anāgataṁ viññāṇaṁ abhinandati.",
        "analyzed": "anāgatam viññāṇam abhinandati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.11",
        "original": "Dve dhammā cittassa upakkilesā—",
        "analyzed": "dve dhammā cittassa upakkilesā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.12",
        "original": "taṇhā ca avijjā ca.",
        "analyzed": "taṇhā ca avijjā ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.13",
        "original": "Tāhi visujjhantaṁ cittaṁ visujjhati.",
        "analyzed": "tāhi visujjhantam cittam visujjhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.14_0",
        "original": "Tesaṁ avijjānīvaraṇānaṁ taṇhāsaṁyojanānaṁ pubbā koṭi na paññāyati sandhāvantānaṁ",
        "analyzed": "tesam avijjā nīvaraṇānam taṇhā sayojanānam pubbā koṭi na paññāyati sandhāvantāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.14_1",
        "original": "saṁsarantānaṁ sakiṁ nirayaṁ sakiṁ tiracchānayoniṁ sakiṁ pettivisayaṁ sakiṁ asurakāyaṁ",
        "analyzed": "sa kim nirayam sakim tiracchāna yonim sakim petti visayam sakim asurakāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:6.14_2",
        "original": "sakiṁ deve sakiṁ manusse.",
        "analyzed": "sakim deve sakim manusse ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ne_20:7.1",
        "original": "Thinamiddhābhibhūtenāti.",
        "analyzed": "thinam iddha abhibhūtena ati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.2",
        "original": "Thinaṁ nāma yā cittassa akallatā akammaniyatā;",
        "analyzed": "thinam nāma yā cittassa a kalla tā a kamma niyatā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.3",
        "original": "middhaṁ nāma yaṁ kāyassa līnattaṁ.",
        "analyzed": "middham nāma yam kāyassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.4",
        "original": "Vasaṁ mārassa gacchatīti kilesamārassa ca sattamārassa ca vasaṁ gacchati, so hi nivuto saṁsārābhimukho hoti.",
        "analyzed": "vasam mārassa gacchati iti kilesa mārassa ca satta mārassa ca vasam gacchati saḥ hi nivutaḥ sa sāra abhimukhaḥ hoti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.5",
        "original": "Imāni bhagavatā dve saccāni desitāni dukkhaṁ samudayo ca.",
        "analyzed": "imāni bhagavatā dve saccāni desitāni dukkham samudayaḥ ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.6",
        "original": "Tesaṁ bhagavā pariññāya ca pahānāya ca dhammaṁ deseti dukkhassa pariññāya samudayassa pahānāya.",
        "analyzed": "tesam bhagavā pariññāya ca pahānāya ca dhammaṃ deseti dukkhassa pariññāya samudayassa pahānāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.7",
        "original": "Yena ca parijānāti yena ca pajahati, ayaṁ maggo.",
        "analyzed": "yena ca parijānāti yena ca pajahati ayam maggo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.8",
        "original": "Yaṁ taṇhāya avijjāya ca pahānaṁ, ayaṁ nirodho.",
        "analyzed": "yam taṇhāya avijjāya ca pahāna ayam nirodhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.9",
        "original": "Imāni cattāri saccāni.",
        "analyzed": "imāni cattāri saccāni ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.10",
        "original": "Tenāha bhagavā “arakkhitena cittenā”ti.",
        "analyzed": "tena āha bhagavā arakkhitena cittena ati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.11",
        "original": "Tenāhāyasmā mahākaccāyano “assādādīnavatā”ti.",
        "analyzed": "tena āha ayasmai mahākaccāyanaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ne_20:7.12",
        "original": "Niyutto desanāhārasampāto.",
        "analyzed": "niyuttaḥ desanā āhāra sampāto ",
        "folio": "7"
    }
]