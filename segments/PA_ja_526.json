        "folio": "50"
    },
    {
        "segmentnr": "PA_ja_526:50.3",
        "original": "Taṁ maṁ vanaṁ pāpaya tāta khippaṁ,",
        "analyzed": "tam mama vanam pāpaya tāta khippa ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_ja_526:50.4",
        "original": "Purā te pāṇaṁ vijahāmi assame”.",
        "analyzed": "purā te pāṇam vijahāmi assame ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_ja_526:51.1",
        "original": "“Imasmāhaṁ jotirase vanamhi,",
        "analyzed": "imasma aham jotiḥ ase vanamhi ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_ja_526:51.2",
        "original": "Gandhabbadevaccharasaṅghasevite;",
        "analyzed": "gandhabba deva chara saṅgha sevite ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_ja_526:51.3",
        "original": "Isīnamāvāse sanantanamhi,",
        "analyzed": "sanantanamhi ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_ja_526:51.4",
        "original": "Netādisaṁ aratiṁ pāpuṇetha.",
        "analyzed": "netā ādisam aratim pāpuṇetha ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_ja_526:52.1",
        "original": "Bhavanti mittāni atho na honti,",
        "analyzed": "bhavanti mittāni atho na honti ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_ja_526:52.2",
        "original": "Ñātīsu mittesu karonti pemaṁ;",
        "analyzed": "ñātīsu mittesu karonti pema ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_ja_526:52.3",
        "original": "Ayañca jammo kissa vā niviṭṭho,",
        "analyzed": "ayam ca jammaḥ kissa vā niviṭṭhaḥ ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_ja_526:52.4",
        "original": "Yo neva jānāti kutomhi āgato.",
        "analyzed": "yaḥ na iva jānāti kutas ahi āgataḥ ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_ja_526:53.1",
        "original": "Saṁvāsena hi mittāni,",
        "analyzed": "sa vāsena hi mittāni ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_ja_526:53.2",
        "original": "sandhīyanti punappunaṁ;",
        "analyzed": "sandhīyanti punar punar ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_ja_526:53.3",
        "original": "Sveva mitto asaṅgantu,",
        "analyzed": "su iva mittaḥ asaṅgantu ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_ja_526:53.4",
        "original": "asaṁvāsena jīrati.",
        "analyzed": "a sa vāsena jīrati ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_ja_526:54.1",
        "original": "Sace tuvaṁ dakkhasi brahmacāriṁ,",
        "analyzed": "sace tuvam dakkhasi brahmacāri ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_ja_526:54.2",
        "original": "Sace tuvaṁ sallape brahmacārinā;",
        "analyzed": "sace tuvam sallape brahmacārinā ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_ja_526:54.3",
        "original": "Sampannasassaṁva mahodakena,",
        "analyzed": "sampanna sassava mahā udakena ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_ja_526:54.4",
        "original": "Tapoguṇaṁ khippamimaṁ pahissasi.",
        "analyzed": "tapo guṇam khippam imam pahissasi ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_ja_526:55.1",
        "original": "Punapi ce dakkhasi brahmacāriṁ,",
        "analyzed": "punapi ce dakkhasi brahmacāri ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_ja_526:55.2",
        "original": "Punapi ce sallape brahmacārinā;",
        "analyzed": "punar api ce sallape brahmacārinā ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_ja_526:55.3",
        "original": "Sampannasassaṁva mahodakena,",
        "analyzed": "sampanna sassava mahā udakena ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_ja_526:55.4",
        "original": "Usmāgataṁ khippamimaṁ pahissasi.",
