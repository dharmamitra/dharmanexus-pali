        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_250:2.1",
        "original": "“Nāyaṁ isī upasamasaṁyame rato,",
        "analyzed": "na ayam upasama sayame ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_250:2.2",
