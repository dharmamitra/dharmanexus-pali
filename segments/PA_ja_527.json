        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_527:0.2",
        "original": "Paṇṇāsanipāta",
        "analyzed": "paṇṇāsa nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_527:0.3",
        "original": "Niḷinikāvagga",
        "analyzed": "niḷi nikā vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_527:0.4",
        "original": "2 Ummādantījātaka",
        "analyzed": "ummādantījātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_527:1.1",
        "original": "“Nivesanaṁ kassa nudaṁ sunanda,",
        "analyzed": "nivesanam kassa nudam su nanda ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_527:1.2",
        "original": "Pākārena paṇḍumayena guttaṁ;",
        "analyzed": "pākārena paṇḍu mayena gutta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_527:1.3",
        "original": "Kā dissati aggisikhāva dūre,",
        "analyzed": "kā dissati aggisikhā ava dūre ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_527:1.4",
        "original": "Vehāyasaṁ pabbataggeva acci.",
        "analyzed": "vehāyasam pabbataggā iva acci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_527:2.1",
        "original": "Dhītā nvayaṁ kassa sunanda hoti,",
        "analyzed": "dhītā nu ayam kassa su nanda hoti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_527:2.2",
        "original": "Suṇisā nvayaṁ kassa athopi bhariyā;",
        "analyzed": "suṇisā nu ayam kassa bhariyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_527:2.3",
        "original": "Akkhāhi me khippamidheva puṭṭho,",
        "analyzed": "akkhāhi me khippam idhā iva puṭṭhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_527:2.4",
        "original": "Avāvaṭā yadi vā atthi bhattā”.",
        "analyzed": "yadi vai atthi bhattā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_527:3.1",
        "original": "“Ahañhi jānāmi janinda etaṁ,",
        "analyzed": "ahañhi jānāmi janinde eta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_527:3.2",
        "original": "Matyā ca petyā ca athopi assā;",
        "analyzed": "matyā ca petyā ca athopi assā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_527:3.3",
        "original": "Taveva so puriso bhūmipāla,",
        "analyzed": "tava iva saḥ purisaḥ bhūmipāla ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_527:3.4",
        "original": "Rattindivaṁ appamatto tavatthe.",
        "analyzed": "rattindivam appamattaḥ tavatthe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_527:4.1",
        "original": "Iddho ca phīto ca suvaḍḍhito ca,",
        "analyzed": "iddhaḥ ca phītaḥ ca suvaḍḍhitaḥ ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_527:4.2",
        "original": "Amacco ca te aññataro janinda;",
        "analyzed": "amaccaḥ ca te aññataraḥ janinda ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_527:4.3",
        "original": "Tassesā bhariyābhipārakassa,",
        "analyzed": "tassa isā bhariyā abhipārakassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_527:4.4",
        "original": "Ummādantī nāmadheyyena rāja”.",
        "analyzed": "ummādantī nāmadheyyena rāja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_527:5.1",
        "original": "“Ambho ambho nāmamidaṁ imissā,",
        "analyzed": "ambhaḥ ambhaḥ nāmam idam imissā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_527:5.2",
        "original": "Matyā ca petyā ca kataṁ susādhu;",
        "analyzed": "matyā ca petyā ca katam su sādhu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_527:5.3",
        "original": "Tadā hi mayhaṁ avalokayantī,",
        "analyzed": "tadā hi mayham avalokayantī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_527:5.4",
        "original": "Ummattakaṁ ummadantī akāsi.",
        "analyzed": "ummattakam ummadantī akāsi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_527:6.1",
        "original": "Yā puṇṇamāse migamandalocanā,",
        "analyzed": "yā puṇṇamāse miga manda locanā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_527:6.2",
        "original": "Upāvisi puṇḍarīkattacaṅgī;",
        "analyzed": "upāvisi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_527:6.3",
        "original": "Dve puṇṇamāyo tadahū amaññahaṁ,",
        "analyzed": "dve puṇṇamāyo tad ahū amaññaha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_527:6.4",
        "original": "Disvāna pārāvatarattavāsiniṁ.",
        "analyzed": "disvāna pārāvata ratta vāsini ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_527:7.1",
        "original": "Aḷārapamhehi subhehi vaggubhi,",
        "analyzed": "aḷārapamhehi subhehi vaggubhi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_527:7.2",
        "original": "Palobhayantī maṁ yadā udikkhati;",
        "analyzed": "palobhayantī mama yadā udikkhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_527:7.3",
        "original": "Vijambhamānā harateva me mano,",
        "analyzed": "vijambhamānāḥ haratā iva me mano ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_527:7.4",
        "original": "Jātā vane kimpurisīva pabbate.",
        "analyzed": "jātā vane kimpurisī iva pabbate ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_527:8.1",
        "original": "Tadā hi brahatī sāmā,",
        "analyzed": "tadā hi brahatī sāmā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_527:8.2",
        "original": "āmuttamaṇikuṇḍalā;",
        "analyzed": "āmutta maṇi kuṇḍalā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_527:8.3",
        "original": "Ekaccavasanā nārī,",
        "analyzed": "ekacca vasanā nārī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_527:8.4",
        "original": "migī bhantāvudikkhati.",
        "analyzed": "migī bhantau udikkhati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_527:9.1",
        "original": "Kadāssu maṁ tambanakhā sulomā,",
        "analyzed": "kadā assu mama tamba nakhā su lomā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_527:9.2",
        "original": "Bāhāmudū candanasāralittā;",
        "analyzed": "bāhāmudū candana sāra littā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_527:9.3",
        "original": "Vaṭṭaṅgulī sannatadhīrakuttiyā,",
        "analyzed": "        ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_527:9.4",
        "original": "Nārī upaññissati sīsato subhā.",
        "analyzed": "nārī upaññissati sīsataḥ subhā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_527:10.1",
        "original": "Kadāssu maṁ kañcanajāluracchadā,",
        "analyzed": "kadā assu mam kañcana jāluracchadā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_527:10.2",
        "original": "Dhītā tirīṭissa vilaggamajjhā;",
        "analyzed": "dhītā tirīṭissa vilagga majjhā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_527:10.3",
        "original": "Mudūhi bāhāhi palissajissati,",
        "analyzed": "mudūhi bāhāhi palissajissati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_527:10.4",
        "original": "Brahāvane jātadumaṁva māluvā.",
        "analyzed": "brahāvane jāta dumava māluvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_527:11.1",
        "original": "Kadāssu lākhārasarattasucchavī,",
        "analyzed": "kadā assu lākhā rasa ratta su chavī ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_527:11.2",
        "original": "Bindutthanī puṇḍarīkattacaṅgī;",
        "analyzed": "       ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_527:11.3",
        "original": "Mukhaṁ mukhena upanāmayissati,",
        "analyzed": "mukham mukhena upanāmayissati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_527:11.4",
        "original": "Soṇḍova soṇḍassa surāya thālaṁ.",
        "analyzed": "soṇḍā uva soṇḍassa surāya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_527:12.1",
        "original": "Yadāddasaṁ taṁ tiṭṭhantiṁ,",
        "analyzed": "yadā addasam tam tiṭṭhanti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_527:12.2",
        "original": "Sabbabhaddaṁ manoramaṁ;",
        "analyzed": "sabbabhaddam manorama ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_527:12.3",
        "original": "Tato sakassa cittassa,",
        "analyzed": "tatas sakassa cittassa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_527:12.4",
        "original": "Nāvabodhāmi kañcinaṁ.",
        "analyzed": "na avabodhāmi kañcid na ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_527:13.1",
        "original": "Ummādantimahaṁ daṭṭhā,",
        "analyzed": "daṭṭhā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_527:13.2",
        "original": "āmuttamaṇikuṇḍalaṁ;",
        "analyzed": "āmutta maṇi kuṇḍala ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_527:13.3",
        "original": "Na supāmi divārattiṁ,",
        "analyzed": "na supāmi divāratti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_527:13.4",
        "original": "sahassaṁva parājito.",
        "analyzed": "sahassava parājitaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_527:14.1",
        "original": "Sakko ce me varaṁ dajjā,",
        "analyzed": "sakkaḥ ce me varam dajjā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_527:14.2",
        "original": "so ca labbhetha me varo;",
        "analyzed": "so ca labbhetha me varo ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_527:14.3",
        "original": "Ekarattaṁ dvirattaṁ vā,",
        "analyzed": "eka rattam dvi rattam vā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_527:14.4",
        "original": "bhaveyyaṁ abhipārako;",
        "analyzed": "bhaveyyam abhipārakaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_527:14.5",
        "original": "Ummādantyā ramitvāna,",
        "analyzed": "ummādantyā ramitvāna ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_527:14.6",
        "original": "sivirājā tato siyaṁ”.",
        "analyzed": "sivi rājā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_527:15.1",
        "original": "“Bhūtāni me bhūtapatī namassato,",
        "analyzed": "bhūtāni me bhūta patī namassataḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_527:15.2",
        "original": "Āgamma yakkho idametadabravi;",
        "analyzed": "āgamma yakkhaḥ idam etat abravi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_527:15.3",
        "original": "Rañño mano ummadantyā niviṭṭho,",
        "analyzed": "raññaḥ manaḥ ummadantyā niviṭṭhaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_527:15.4",
        "original": "Dadāmi te taṁ paricārayassu”.",
        "analyzed": "dadāmi te tam paricārayassu ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_527:16.1",
        "original": "“Puññā ca dhaṁse amaro na camhi,",
        "analyzed": "puññā ca dhase amaraḥ na ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_527:16.2",
        "original": "Jano ca me pāpamidañca jaññā;",
        "analyzed": "ca me pāpam idam ca jaññā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_527:16.3",
        "original": "Bhuso ca tyassa manaso vighāto,",
        "analyzed": "bhusaḥ ca tyassa manasaḥ vighātaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_527:16.4",
        "original": "Datvā piyaṁ ummadantiṁ adaṭṭhā”.",
        "analyzed": "datvā piyam ummadantim adaṭṭhā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_527:17.1",
        "original": "“Janinda nāññatra tayā mayā vā,",
        "analyzed": "janinda na aññatra tayā mayā vā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_527:17.2",
        "original": "Sabbāpi kammassa katassa jaññā;",
        "analyzed": "sabbā api kammassa katassa jaññā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_527:17.3",
        "original": "Yaṁ te mayā ummadantī padinnā,",
        "analyzed": "yam te mayā ummadantī padinnā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_527:17.4",
        "original": "Bhusehi rājā vanathaṁ sajāhi”.",
        "analyzed": "bhusehi rājā vanatham sajāhi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_527:18.1",
        "original": "“Yo pāpakaṁ kamma karaṁ manusso,",
        "analyzed": "yaḥ pāpakam kamma karam manussaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_527:18.2",
        "original": "So maññati māyida maññiṁsu aññe;",
        "analyzed": "saḥ maññati māyi da maññisu aññe ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_527:18.3",
        "original": "Passanti bhūtāni karontametaṁ,",
        "analyzed": "passanti bhūtāni karontam eta ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_527:18.4",
        "original": "Yuttā ca ye honti narā pathabyā.",
        "analyzed": "yuttā ca ye honti narā pathabyā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_527:19.1",
        "original": "Añño nu te koci naro pathabyā,",
        "analyzed": "aññaḥ nu te koci naraḥ pathabyā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_527:19.2",
        "original": "Saddheyya lokasmi na me piyāti;",
        "analyzed": "na me piyāti ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_527:19.3",
        "original": "Bhuso ca tyassa manaso vighāto,",
        "analyzed": "bhusaḥ ca tyassa manasaḥ vighātaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_527:19.4",
        "original": "Datvā piyaṁ ummadantiṁ adaṭṭhā”.",
        "analyzed": "datvā piyam ummadantim adaṭṭhā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_527:20.1",
        "original": "“Addhā piyā mayha janinda esā,",
        "analyzed": "addhā piyā mayha janinde esā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_527:20.2",
        "original": "Na sā mamaṁ appiyā bhūmipāla;",
        "analyzed": "na sā mamam appiyā bhūmipāla ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_527:20.3",
        "original": "Gaccheva tvaṁ ummadantiṁ bhadante,",
        "analyzed": "gaccha iva tvam ummadantim bhadante ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_527:20.4",
        "original": "Sīhova selassa guhaṁ upeti”.",
        "analyzed": "sīhaḥ uva selassa guham upeti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_527:21.1",
        "original": "“Na pīḷitā attadukhena dhīrā,",
        "analyzed": "na pīḍitā atta dukhena dhīrā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_527:21.2",
        "original": "Sukhapphalaṁ kamma pariccajanti;",
        "analyzed": "sukha pphalam kamma pariccajanti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_527:21.3",
        "original": "Sammohitā vāpi sukhena mattā,",
        "analyzed": "sammohitā vā api sukhena mattā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_527:21.4",
        "original": "Na pāpakammañca samācaranti”.",
        "analyzed": "na pāpa kammañca samācaranti ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_527:22.1",
        "original": "“Tuvañhi mātā ca pitā ca mayhaṁ,",
        "analyzed": "tuvan hi mātā ca pitā ca mayha ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_527:22.2",
        "original": "Bhattā patī posako devatā ca;",
        "analyzed": "bhattā patī posakaḥ devatā ca ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_527:22.3",
        "original": "Dāso ahaṁ tuyha saputtadāro,",
        "analyzed": "dāsaḥ aham sa putta dāro ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_527:22.4",
        "original": "Yathāsukhaṁ sāmi karohi kāmaṁ”.",
        "analyzed": "yathāsukham sāmi karohi kāma ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_527:23.1",
        "original": "“Yo issaromhīti karoti pāpaṁ,",
        "analyzed": "yaḥ issaromhi iti karoti pāpa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_527:23.2",
        "original": "Katvā ca so nuttasate paresaṁ;",
        "analyzed": "katvā ca saḥ paresa ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_527:23.3",
        "original": "Na tena so jīvati dīghamāyu,",
        "analyzed": "na tena saḥ jīvati dīgham āyu ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_527:23.4",
        "original": "Devāpi pāpena samekkhare naṁ.",
        "analyzed": "devāḥ api pāpena na ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_527:24.1",
        "original": "Aññātakaṁ sāmikehī padinnaṁ,",
        "analyzed": "aññātakam sāmikehī padinna ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_527:24.2",
        "original": "Dhamme ṭhitā ye paṭicchanti dānaṁ;",
        "analyzed": "dhamme ṭhitā ye paṭicchanti dāna ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_527:24.3",
        "original": "Paṭicchakā dāyakā cāpi tattha,",
        "analyzed": "paṭicchakāḥ dāyakāḥ ca api ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_527:24.4",
        "original": "Sukhapphalaññeva karonti kammaṁ.",
