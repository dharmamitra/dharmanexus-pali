        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_8.1:1.1",
        "original": "“Kammaṁ bahukaṁ na kāraye,",
        "analyzed": "kammam bahukam na kāraye ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_8.1:1.2",
        "original": "Parivajjeyya janaṁ na uyyame;",
        "analyzed": "parivajjeyya janam naḥ uyyame ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_8.1:1.3",
        "original": "So ussukko rasānugiddho,",
        "analyzed": "saḥ ussukkaḥ rasa anugiddhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_8.1:1.4",
        "original": "Atthaṁ riñcati yo sukhādhivāho.",
        "analyzed": "riñcati yaḥ sukha adhivāhaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_8.1:2.1",
        "original": "Paṅkoti hi naṁ avedayuṁ,",
        "analyzed": "paṅkoti hi naṃ avedayu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_8.1:2.2",
        "original": "Yāyaṁ vandanapūjanā kulesu;",
        "analyzed": "yā ayam vandana pūjanā kulesu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_8.1:2.3",
        "original": "Sukhumaṁ sallaṁ durubbahaṁ,",
        "analyzed": "sukhumam sallam durubbaha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_8.1:2.4",
        "original": "Sakkāro kāpurisena dujjaho.",
        "analyzed": "sakkāraḥ kāpurisena dujjahaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_8.1:3.1",
        "original": "Na parassupanidhāya,",
        "analyzed": "na paras upanidhāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_8.1:3.2",
        "original": "kammaṁ maccassa pāpakaṁ;",
        "analyzed": "kammam maccassa pāpaka ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_8.1:3.3",
        "original": "Attanā taṁ na seveyya,",
        "analyzed": "attana tam na seveyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_8.1:3.4",
        "original": "kammabandhū hi mātiyā.",
        "analyzed": "kamma bandhū hi mātiyā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_8.1:4.1",
        "original": "Na pare vacanā coro,",
        "analyzed": "na pare vacanā ca uro ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_8.1:4.2",
        "original": "na pare vacanā muni;",
        "analyzed": "na pare vacanā muni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_8.1:4.3",
        "original": "Attā ca naṁ yathāvedi,",
        "analyzed": "attā ca nam yathāvedi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_8.1:4.4",
        "original": "devāpi naṁ tathā vidū.",
        "analyzed": "devā api nam tathā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_8.1:5.1",
        "original": "Pare ca na vijānanti,",
        "analyzed": "pare ca na vijānanti ",
        "folio": "5"
    },
