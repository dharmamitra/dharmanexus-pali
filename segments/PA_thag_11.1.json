        "analyzed": "saṅkiccattheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_11.1:1.1",
        "original": "“Kiṁ tavattho vane tāta,",
        "analyzed": "kim tavatthaḥ vane tāta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_11.1:1.2",
        "original": "ujjuhānova pāvuse;",
        "analyzed": "ujjuhānā uva pāvuse ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_11.1:1.3",
        "original": "Verambhā ramaṇīyā te,",
        "analyzed": "verambhā ramaṇīyā te ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_11.1:1.4",
        "original": "paviveko hi jhāyinaṁ”.",
        "analyzed": "hi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_11.1:2.1",
        "original": "“Yathā abbhāni verambho,",
        "analyzed": "yathā abbhāni verambho ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_11.1:2.2",
        "original": "vāto nudati pāvuse;",
        "analyzed": "vātaḥ nudati pāvuse ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_11.1:2.3",
        "original": "Saññā me abhikiranti,",
        "analyzed": "saññā me abhikiranti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_11.1:2.4",
        "original": "vivekapaṭisaññutā.",
        "analyzed": "viveka paṭisaññu tā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_11.1:3.1",
        "original": "Apaṇḍaro aṇḍasambhavo,",
        "analyzed": "apaṇḍaraḥ aṇḍa sambhavaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_11.1:3.2",
        "original": "Sīvathikāya niketacāriko;",
        "analyzed": "sīvathikāya niketa cāriko ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_11.1:3.3",
        "original": "Uppādayateva me satiṁ,",
        "analyzed": "uppādayatā iva me sati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_11.1:3.4",
        "original": "Sandehasmiṁ virāganissitaṁ.",
        "analyzed": "sandehasmim virāga nissita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_11.1:4.1",
        "original": "Yañca aññe na rakkhanti,",
        "analyzed": "yañca aññe na rakkhanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_11.1:4.2",
        "original": "yo ca aññe na rakkhati;",
        "analyzed": "yaḥ ca aññe na rakkhati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_11.1:4.3",
        "original": "Sa ve bhikkhu sukhaṁ seti,",
        "analyzed": "sa ve bhikkhu sukham seti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_11.1:4.4",
        "original": "kāmesu anapekkhavā.",
        "analyzed": "kāmesu anapekkhavā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_11.1:5.1",
        "original": "Acchodikā puthusilā,",
        "analyzed": "acchodikā puthusilā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_11.1:5.2",
        "original": "gonaṅgulamigāyutā;",
        "analyzed": "gonaṅgulam igāyutā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_11.1:5.3",
        "original": "Ambusevālasañchannā,",
        "analyzed": "          ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_11.1:5.4",
        "original": "te selā ramayanti maṁ.",
        "analyzed": "te selāḥ ramayanti ma ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_11.1:6.1",
        "original": "Vasitaṁ me araññesu,",
        "analyzed": "vasitam me araññesu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_11.1:6.2",
        "original": "kandarāsu guhāsu ca;",
        "analyzed": "kandarāsu guhāsu ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_11.1:6.3",
        "original": "Senāsanesu pantesu,",
        "analyzed": "senāsanesu pantesu ",
        "folio": "6"
