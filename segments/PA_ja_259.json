        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_259:2.1",
        "original": "“Āpāsu me yuddhaparājitassa,",
        "analyzed": "me yuddha parājitassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_259:2.2",
        "original": "Ekassa katvā vivanasmi ghore;",
