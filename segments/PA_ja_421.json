        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_421:7.1",
        "original": "“Tapasā pajahanti pāpakammaṁ,",
        "analyzed": "tapasā pajahanti pāpa kamma ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_421:7.2",
        "original": "Tapasā nhāpitakumbhakārabhāvaṁ;",
