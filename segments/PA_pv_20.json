[
    {
        "segmentnr": "PA_pv_20:0.1",
        "original": "Petavatthu",
        "analyzed": "petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_20:0.2",
        "original": "Ubbarivagga",
        "analyzed": "ubbari vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_20:0.3",
        "original": "8. Cūḷaseṭṭhipetavatthu",
        "analyzed": "cūḷa seṭṭhi petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_20:1.1",
        "original": "“Naggo kiso pabbajitosi bhante,",
        "analyzed": "naggaḥ kisaḥ pabbajitaḥ asi bhante ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_20:1.2",
        "original": "Rattiṁ kuhiṁ gacchasi kissa hetu;",
        "analyzed": "rattim kuhim gacchasi kissa hetu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_20:1.3",
        "original": "Ācikkha me taṁ api sakkuṇemu,",
        "analyzed": "ācikkha me tam api sakkuṇemu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_20:1.4",
        "original": "Sabbena vittaṁ paṭipādaye tuvan”ti.",
        "analyzed": "sabbena vittam paṭipādaye tuvanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_20:2.1",
        "original": "“Bārāṇasī nagaraṁ dūraghuṭṭhaṁ,",
        "analyzed": "bārāṇasī nagaram dūraghuṭṭha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_20:2.2",
        "original": "Tatthāhaṁ gahapati aḍḍhako ahu dīno;",
        "analyzed": "tattha aham gahapati aḍḍhakaḥ ahu dīnaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_20:2.3",
        "original": "Adātā gedhitamano āmisasmiṁ,",
        "analyzed": "a dātā gedhita manaḥ āmisasmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_20:2.4",
        "original": "Dussīlyena yamavisayamhi patto.",
        "analyzed": "dussīlyena yama visayamhi patto ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_20:3.1",
        "original": "So sūcikāya kilamito tehi,",
        "analyzed": "saḥ sūcikāya kilamitaḥ tehi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_20:3.2",
        "original": "Teneva ñātīsu yāmi āmisakiñcikkhahetu;",
        "analyzed": "tena iva ñātīsu yāmi āmisa kiñcikkha hetu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_20:3.3",
        "original": "Adānasīlā na ca saddahanti,",
        "analyzed": "a dāna sīlāḥ na ca sat dahanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_20:3.4",
        "original": "Dānaphalaṁ hoti paramhi loke.",
        "analyzed": "dāna phalam hoti paramhi loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_20:4.1",
        "original": "Dhītā ca mayhaṁ lapate abhikkhaṇaṁ,",
        "analyzed": "dhītā ca mayham lapate abhikkhaṇa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_20:4.2",
        "original": "‘Dassāmi dānaṁ pitūnaṁ pitāmahānaṁ’;",
        "analyzed": "dassāmi dānam pitūnam pitāmahāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_20:4.3",
        "original": "Tamupakkhaṭaṁ parivisayanti brāhmaṇā,",
        "analyzed": "tam upakkhaṭam brāhmaṇāḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_20:4.4",
        "original": "‘Yāmi ahaṁ andhakavindaṁ bhottun’”ti.",
        "analyzed": "yāmi aham andhakavindam bhottunti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_20:5.1",
        "original": "Tamavoca rājā “anubhaviyāna tampi,",
        "analyzed": "tam avoca rājā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_20:5.2",
        "original": "Eyyāsi khippaṁ ahamapi kassaṁ pūjaṁ;",
        "analyzed": "eyyā asi khippam aham api kassam pūja ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_20:5.3",
        "original": "Ācikkha me taṁ yadi atthi hetu,",
        "analyzed": "ācikkha me tam yadi atthi hetu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_20:5.4",
        "original": "Saddhāyitaṁ hetuvaco suṇomā”ti.",
        "analyzed": "sat hāyitam hetu vacaḥ suṇoma ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_20:6.1",
        "original": "“Tathā”ti vatvā agamāsi tattha,",
        "analyzed": "tathā ati vatvā agamāsi tattha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_20:6.2",
        "original": "Bhuñjiṁsu bhattaṁ na ca dakkhiṇārahā;",
        "analyzed": "bhuñjisu bhattam na ca dakkhiṇā rahā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_20:6.3",
        "original": "Paccāgami rājagahaṁ punāparaṁ,",
        "analyzed": "paccā agami rājagaham ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_20:6.4",
        "original": "Pāturahosi purato janādhipassa.",
        "analyzed": "pātuḥ ahosi puratas janādhipassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_20:7.1",
        "original": "Disvāna petaṁ punadeva āgataṁ,",
        "analyzed": "disvāna petam punar devaḥ āgata ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_20:7.2",
        "original": "Rājā avoca “ahamapi kiṁ dadāmi;",
        "analyzed": "rājā avoca aham api kim dadāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_20:7.3",
        "original": "Ācikkha me taṁ yadi atthi hetu,",
        "analyzed": "ācikkha me tam yadi atthi hetu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_20:7.4",
        "original": "Yena tuvaṁ cirataraṁ pīṇito siyā”ti.",
        "analyzed": "yena tuvam cirataram pīṇitaḥ siyāti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_20:8.1",
        "original": "“Buddhañca saṅghaṁ parivisiyāna rāja,",
        "analyzed": "buddham ca saṅgham rāja ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_20:8.2",
        "original": "Annena pānena ca cīvarena;",
        "analyzed": "annena pānena ca cīvarena ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_20:8.3",
        "original": "Taṁ dakkhiṇaṁ ādisa me hitāya,",
        "analyzed": "tam dakkhiṇam ādisa me hitāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_20:8.4",
        "original": "Evaṁ ahaṁ cirataraṁ pīṇito siyā”ti.",
        "analyzed": "evam aham cirataram pīṇitaḥ siyāti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_20:9.1",
        "original": "Tato ca rājā nipatitvā tāvade,",
        "analyzed": "ca rājā nipatitvā tāvade ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_20:9.2",
        "original": "Dānaṁ sahatthā atulaṁ daditvā saṅghe;",
        "analyzed": "dānam sahatthāḥ atulam daditvā saṅghe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_20:9.3",
        "original": "Ārocesi pakataṁ tathāgatassa,",
        "analyzed": "ārocesi pakatam tathāgatassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_20:9.4",
        "original": "Tassa ca petassa dakkhiṇaṁ ādisittha.",
        "analyzed": "tassa ca petassa dakkhiṇam ādisittha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_20:10.1",
        "original": "So pūjito ativiya sobhamāno,",
        "analyzed": "saḥ pūjitaḥ ativiya sobhamānaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_20:10.2",
        "original": "Pāturahosi purato janādhipassa;",
        "analyzed": "pātuḥ ahosi puratas janādhipassa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_20:10.3",
        "original": "“Yakkhohamasmi paramiddhipatto,",
        "analyzed": "yakkhaḥ ham asmi param iddhi pattaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_20:10.4",
        "original": "Na mayhamatthi samā sadisā mānusā.",
        "analyzed": "na mayham atthi samā sadisā mānusā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_20:11.1",
        "original": "Passānubhāvaṁ aparimitaṁ mamayidaṁ,",
        "analyzed": "aparimitam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_20:11.2",
        "original": "Tayānudiṭṭhaṁ atulaṁ datvā saṅghe;",
        "analyzed": "tayā anudiṭṭham atulam da tvā saṅghe ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_20:11.3",
        "original": "Santappito satataṁ sadā bahūhi,",
        "analyzed": "santappitaḥ satatam sadā bahūhi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_20:11.4",
        "original": "Yāmi ahaṁ sukhito manussadevā”ti.",
        "analyzed": "yāmi aham sukhitaḥ manussadeva ati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_20:12.1",
        "original": "Cūḷaseṭṭhipetavatthu aṭṭhamaṁ.",
        "analyzed": "cūḷa seṭṭhi petavatthu aṭṭhama ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_20:12.2",
        "original": "Bhāṇavāraṁ paṭhamaṁ.",
        "analyzed": "bhāṇa vāram ",
        "folio": "12"
    }
]