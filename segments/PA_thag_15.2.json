        "analyzed": "udāyittheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_15.2:1.1",
        "original": "“Manussabhūtaṁ sambuddhaṁ,",
        "analyzed": "manus sa bhūtam sambuddha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_15.2:1.2",
        "original": "Attadantaṁ samāhitaṁ;",
        "analyzed": "samāhita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_15.2:1.3",
        "original": "Iriyamānaṁ brahmapathe,",
        "analyzed": "iriyamānam brahma pathe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_15.2:1.4",
        "original": "Cittassūpasame rataṁ.",
        "analyzed": "rata ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_15.2:2.1",
        "original": "Yaṁ manussā namassanti,",
        "analyzed": "yam manussāḥ namassanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_15.2:2.2",
        "original": "sabbadhammāna pāraguṁ;",
        "analyzed": "sabba dhammāna pāragu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_15.2:2.3",
        "original": "Devāpi taṁ namassanti,",
        "analyzed": "devāḥ api tam namassanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_15.2:2.4",
        "original": "iti me arahato sutaṁ.",
        "analyzed": "iti me arahataḥ suta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_15.2:3.1",
        "original": "Sabbasaṁyojanātītaṁ,",
        "analyzed": "sabba sayojana atīta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_15.2:3.2",
        "original": "vanā nibbanamāgataṁ;",
        "analyzed": "vanā nibbanam āgata ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_15.2:3.3",
        "original": "Kāmehi nekkhammarataṁ,",
        "analyzed": "kāmehi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_15.2:3.4",
        "original": "muttaṁ selāva kañcanaṁ.",
        "analyzed": "muttam kañcana ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_15.2:4.1",
        "original": "Sa ve accaruci nāgo,",
        "analyzed": "sa ve accaruci nāgaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_15.2:4.2",
        "original": "himavāvaññe siluccaye;",
        "analyzed": "himavā vaññe siluccaye ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_15.2:4.3",
        "original": "Sabbesaṁ nāganāmānaṁ,",
        "analyzed": "sabbesam nāga nāmāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_15.2:4.4",
        "original": "saccanāmo anuttaro.",
        "analyzed": "anuttaraḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_15.2:5.1",
        "original": "Nāgaṁ vo kittayissāmi,",
        "analyzed": "nāgam vaḥ kittayissāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_15.2:5.2",
        "original": "na hi āguṁ karoti so;",
        "analyzed": "na hi āgum karoti saḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_15.2:5.3",
        "original": "Soraccaṁ avihiṁsā ca,",
        "analyzed": "soraccam a vihisā ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_15.2:5.4",
        "original": "pādā nāgassa te duve.",
        "analyzed": "pādā nāgassa te duve ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_15.2:6.1",
        "original": "Sati ca sampajaññañca,",
        "analyzed": "sati ca sampajaññan ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_15.2:6.2",
        "original": "caraṇā nāgassa tepare;",
        "analyzed": "caraṇāḥ nāgassa te pare ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_15.2:6.3",
        "original": "Saddhāhattho mahānāgo,",
        "analyzed": "saddhā hatthaḥ mahā nāgaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_15.2:6.4",
        "original": "upekkhāsetadantavā.",
        "analyzed": "upekkhā setadantavā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_15.2:7.1",
        "original": "Sati gīvā siro paññā,",
        "analyzed": "sati gīvā siro paññā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_15.2:7.2",
        "original": "vīmaṁsā dhammacintanā;",
        "analyzed": "vīmasā dhamma cintanā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_15.2:7.3",
        "original": "Dhammakucchisamāvāso,",
        "analyzed": "dhammakucchi samāvāso ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_15.2:7.4",
        "original": "viveko tassa vāladhi.",
        "analyzed": "vivekaḥ tassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_15.2:8.1",
        "original": "So jhāyī assāsarato,",
        "analyzed": "saḥ jhāyī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_15.2:8.2",
        "original": "ajjhattaṁ susamāhito;",
        "analyzed": "ajjhattam su samāhitaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_15.2:8.3",
        "original": "Gacchaṁ samāhito nāgo,",
        "analyzed": "gaccham samāhitaḥ nāgaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_15.2:8.4",
        "original": "ṭhito nāgo samāhito.",
        "analyzed": "ṭhitaḥ nāgaḥ samāhitaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_15.2:9.1",
        "original": "Sayaṁ samāhito nāgo,",
        "analyzed": "sayam samāhitaḥ nāgaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_15.2:9.2",
        "original": "nisinnopi samāhito;",
        "analyzed": "nisinnaḥ api samāhitaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_15.2:9.3",
        "original": "Sabbattha saṁvuto nāgo,",
        "analyzed": "sabbattha sa vutaḥ nāgaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_15.2:9.4",
        "original": "esā nāgassa sampadā.",
        "analyzed": "esā nāgassa sampadā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_15.2:10.1",
        "original": "Bhuñjati anavajjāni,",
        "analyzed": "bhuñjati anavajjāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_15.2:10.2",
        "original": "sāvajjāni na bhuñjati;",
        "analyzed": "sāvajjāni na bhuñjati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_15.2:10.3",
        "original": "Ghāsamacchādanaṁ laddhā,",
        "analyzed": "ghāsam acchādanam laddhā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_15.2:10.4",
        "original": "sannidhiṁ parivajjayaṁ.",
        "analyzed": "sannidhim parivajjaya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_15.2:11.1",
        "original": "Saṁyojanaṁ aṇuṁ thūlaṁ,",
        "analyzed": "sa yojanam aṇum ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_15.2:11.2",
        "original": "sabbaṁ chetvāna bandhanaṁ;",
        "analyzed": "sabbam chetvāna bandhana ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_15.2:11.3",
        "original": "Yena yeneva gacchati,",
        "analyzed": "yena yena iva gacchati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_15.2:11.4",
        "original": "anapekkhova gacchati.",
        "analyzed": "anapekkhaḥ uva gacchati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_15.2:12.1",
        "original": "Yathāpi udake jātaṁ,",
        "analyzed": "yathā api udake jāta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_15.2:12.2",
        "original": "puṇḍarīkaṁ pavaḍḍhati;",
        "analyzed": "puṇḍarīkam pavaḍḍhati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_15.2:12.3",
        "original": "Nopalippati toyena,",
        "analyzed": "na upalippati toyena ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_15.2:12.4",
        "original": "sucigandhaṁ manoramaṁ.",
        "analyzed": "suci gandham manorama ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thag_15.2:13.1",
        "original": "Tatheva ca loke jāto,",
        "analyzed": "tathā iva ca loke jātaḥ ",
        "folio": "13"
