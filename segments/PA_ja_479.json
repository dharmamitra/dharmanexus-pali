    },
    {
        "segmentnr": "PA_ja_479:0.4",
        "original": "6. Kāliṅgabodhijātaka",
        "analyzed": "     ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_479:1.1",
        "original": "Rājā kāliṅgo cakkavatti,",
        "analyzed": "rājā kāliṅgaḥ cakkavatti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_479:1.2",
        "original": "Dhammena pathavimanusāsaṁ;",
        "analyzed": "dhammena pathavim anusāsa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_479:1.3",
        "original": "Agamā bodhisamīpaṁ,",
        "analyzed": "agamā bodhisamīpa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_479:1.4",
        "original": "Nāgena mahānubhāvena.",
        "analyzed": "nāgena mahā anubhāvena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_479:2.1",
        "original": "Kāliṅgo bhāradvājo ca,",
        "analyzed": "kāliṅgaḥ bhāradvājaḥ ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_479:2.2",
        "original": "Rājānaṁ kāliṅgaṁ samaṇakolaññaṁ;",
        "analyzed": "rājānam kāliṅgam samaṇa kolañña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_479:2.3",
        "original": "Cakkaṁ vattayato pariggahetvā,",
        "analyzed": "cakkam vattayataḥ pariggahetvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_479:2.4",
        "original": "Pañjalī idamavoca.",
        "analyzed": "pañjalī idam avoca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_479:3.1",
        "original": "“Paccoroha mahārāja,",
        "analyzed": "paccoroha mahā rāja ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_479:3.2",
        "original": "Bhūmibhāgo yathā samaṇuggato;",
        "analyzed": "bhūmi bhāgaḥ yathā samaṇuggataḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_479:3.3",
        "original": "Idha anadhivarā buddhā,",
        "analyzed": "idha anadhivarā buddhā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_479:3.4",
        "original": "Abhisambuddhā virocanti.",
        "analyzed": "abhisambuddhāḥ virocanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_479:4.1",
        "original": "Padakkhiṇato āvaṭṭā,",
        "analyzed": "padakkhiṇatas āvaṭṭā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_479:4.2",
        "original": "Tiṇalatā asmiṁ bhūmibhāgasmiṁ;",
        "analyzed": "tiṇa latā asmin ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_479:4.3",
        "original": "Pathaviyā nābhiyaṁ maṇḍo,",
        "analyzed": "pathaviyā nābhiyam maṇḍo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_479:4.4",
        "original": "Iti no sutaṁ mante mahārāja.",
        "analyzed": "iti naḥ sutam mante mahā rāja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_479:5.1",
        "original": "Sāgarapariyantāya,",
        "analyzed": "sāgara pariyantāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_479:5.2",
        "original": "Mediniyā sabbabhūtadharaṇiyā;",
        "analyzed": "mediniyā sabba bhūta dharaṇiyā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_479:5.3",
        "original": "Pathaviyā ayaṁ maṇḍo,",
        "analyzed": "pathaviyāḥ ayam maṇḍaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_479:5.4",
        "original": "Orohitvā namo karohi.",
        "analyzed": "orohitvā namaḥ karohi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_479:6.1",
        "original": "Ye te bhavanti nāgā ca,",
        "analyzed": "ye te bhavanti nāgā ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_479:6.2",
        "original": "abhijātā ca kuñjarā;",
        "analyzed": "abhijātā ca kuñjarā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_479:6.3",
        "original": "Ettāvatā padesaṁ te,",
        "analyzed": "ettāvatā padesam te ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_479:6.4",
        "original": "nāgā neva mupayanti.",
        "analyzed": "nāgāḥ na iva mupayanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_479:7.1",
        "original": "Abhijāto nāgo kāmaṁ,",
        "analyzed": "abhijātaḥ nāgaḥ kāma ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_479:7.2",
        "original": "Pesehi kuñjaraṁ dantiṁ;",
        "analyzed": "pesehi kuñjaram danti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_479:7.3",
        "original": "Ettāvatā padeso,",
        "analyzed": "ettāvatā padesaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_479:7.4",
        "original": "Sakkā nāgena mupagantuṁ.",
        "analyzed": "sakkā nāgena mā upagantu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_479:8.1",
        "original": "Taṁ sutvā rājā kāliṅgo,",
        "analyzed": "tam sutvā rājā kāliṅgaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_479:8.2",
        "original": "Veyyañjanikavaco nisāmetvā;",
        "analyzed": "veyyañjanika vacaḥ nisām etvā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_479:8.3",
        "original": "Sampesesi nāgaṁ ñassāma,",
        "analyzed": "sampesesi nāgam ñassāma ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_479:8.4",
        "original": "Mayaṁ yathimassidaṁ vacanaṁ”.",
        "analyzed": "mayam vacana ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_479:9.1",
        "original": "Sampesito ca raññā,",
        "analyzed": "sampesitaḥ ca raññā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_479:9.2",
        "original": "Nāgo koñcova abhinaditvāna;",
        "analyzed": "nāgaḥ abhinaditvāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_479:9.3",
        "original": "Paṭisakkitvā nisīdi,",
        "analyzed": "paṭisakkitvā nisīdi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_479:9.4",
        "original": "Garuṁva bhāraṁ asahamāno.",
        "analyzed": "garuva bhāram a sahamānaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_479:10.1",
        "original": "Kāliṅgabhāradvājo,",
        "analyzed": "kāliṅga bhāradvājaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_479:10.2",
        "original": "Nāgaṁ khīṇāyukaṁ viditvāna;",
        "analyzed": "nāgam khīṇāyukam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_479:10.3",
        "original": "Rājānaṁ kāliṅgaṁ,",
        "analyzed": "rājānam kāliṅga ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_479:10.4",
        "original": "Taramāno ajjhabhāsittha;",
        "analyzed": "taramānaḥ ajjhabhāsittha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_479:10.5",
        "original": "“Aññaṁ saṅkama nāgaṁ,",
        "analyzed": "aññam saṅkama nāga ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_479:10.6",
        "original": "Nāgo khīṇāyuko mahārāja”.",
        "analyzed": "nāgaḥ khīṇāyukaḥ mahā rāja ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_479:11.1",
        "original": "Taṁ sutvā kāliṅgo,",
        "analyzed": "tam sutvā kāliṅgaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_479:11.2",
        "original": "taramāno saṅkamī nāgaṁ;",
        "analyzed": "taramānaḥ saṅkamī nāga ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_479:11.3",
        "original": "Saṅkanteva raññe,",
        "analyzed": "saṅkantā iva raññe ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_479:11.4",
        "original": "nāgo tattheva pati bhumyā;",
        "analyzed": "nāgaḥ tattha iva pati bhumyā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_479:11.5",
        "original": "Veyyañjanikavaco,",
        "analyzed": "veyyañjanika vacaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_479:11.6",
        "original": "yathā tathā ahu nāgo.",
        "analyzed": "yathā tathā ahu nāgo ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_479:12.1",
        "original": "Kāliṅgo rājā kāliṅgaṁ,",
        "analyzed": "kāliṅgaḥ rājā kāliṅga ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_479:12.2",
        "original": "brāhmaṇaṁ etadavoca;",
        "analyzed": "brāhmaṇam etat avoca ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_479:12.3",
        "original": "“Tvameva asi sambuddho,",
        "analyzed": "tvam eva asi sambuddhaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_479:12.4",
        "original": "sabbaññū sabbadassāvī”.",
        "analyzed": "sabbaññū sabba dassāvī ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_479:13.1",
        "original": "Taṁ anadhivāsento kāliṅgaṁ,",
        "analyzed": "tam kāliṅga ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_479:13.2",
        "original": "Brāhmaṇo idamavoca;",
        "analyzed": "brāhmaṇaḥ idam avoca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_479:13.3",
        "original": "“Veyyañjanikā hi mayaṁ,",
        "analyzed": "veyyañjanikāḥ hi maya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_479:13.4",
        "original": "Buddhā sabbaññuno mahārāja.",
        "analyzed": "buddhāḥ sabbaññunaḥ mahā rāja ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_479:14.1",
        "original": "Sabbaññū sabbavidū ca,",
        "analyzed": "sabbaññū sabba vidū ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_479:14.2",
        "original": "Buddhā na lakkhaṇena jānanti;",
        "analyzed": "buddhāḥ na lakkhaṇena jānanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_479:14.3",
        "original": "Āgamabalasā hi mayaṁ,",
        "analyzed": "āgama balasāḥ hi maya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_479:14.4",
        "original": "Buddhā sabbaṁ pajānanti”.",
        "analyzed": "buddhāḥ sabbam pajānanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_479:15.1",
        "original": "Mahayitvā sambodhiṁ,",
        "analyzed": "mahayitvā sambodhi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_479:15.2",
        "original": "Nānāturiyehi vajjamānehi;",
        "analyzed": "nānā turiyehi vajjamānehi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_479:15.3",
        "original": "Mālāvilepanaṁ abhiharitvā,",
        "analyzed": "mālā vilepanam abhiharitvā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_479:15.4",
        "original": "Atha rājā manupāyāsi.",
        "analyzed": "atha rājā manupāyāsi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_479:16.1",
        "original": "Saṭṭhi vāhasahassāni,",
        "analyzed": "saṭṭhi vāha sahassāni ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_479:16.2",
        "original": "pupphānaṁ sannipātayi;",
        "analyzed": "pupphānam sannipātayi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_479:16.3",
        "original": "Pūjesi rājā kāliṅgo,",
        "analyzed": "pūjesi rājā kāliṅgaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_479:16.4",
        "original": "bodhimaṇḍamanuttaranti.",
        "analyzed": "bodhimaṇḍam anuttaranti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_479:17.1",
        "original": "Kāliṅgabodhijātakaṁ chaṭṭhaṁ.",
        "analyzed": "kāliṅgabodhijātakam chaṭṭha ",
        "folio": "17"
    }
]