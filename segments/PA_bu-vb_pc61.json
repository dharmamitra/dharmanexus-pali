        "folio": "0"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.2",
        "original": "Tena kho pana samayena āyasmā udāyī issāso hoti, kākā cassa amanāpā honti.",
        "analyzed": "tena kho pana samayena āyasmai udāyī issāsaḥ hoti kākā cassa amanāpāḥ honti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.3",
        "original": "So kāke vijjhitvā vijjhitvā sīsaṁ chinditvā sūle paṭipāṭiyā ṭhapesi.",
        "analyzed": "saḥ kāke vijjhitvā vijjhitvā sīsam chinditvā sūle paṭipāṭiyā ṭhapesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.4",
        "original": "Bhikkhū evamāhaṁsu—",
        "analyzed": "bhikkhū evam āhasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.5",
        "original": "“kenime, āvuso, kākā jīvitā voropitā”ti?",
        "analyzed": "kenime āvuso kākāḥ jīvitā voropitā ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.6",
        "original": "“Mayā, āvuso.",
        "analyzed": "mayā āvuso ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.7",
        "original": "Amanāpā me kākā”ti.",
        "analyzed": "amanāpā me kākāti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.8",
        "original": "Ye te bhikkhū appicchā …pe… te ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "ye te bhikkhū appicchā pe te ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.9",
        "original": "“kathañhi nāma āyasmā udāyī sañcicca pāṇaṁ jīvitā voropessatī”ti …pe…",
        "analyzed": "katham hi nāma āyasmai udāyī sañcit ca pāṇam jīvitā voropessati iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.10",
        "original": "“saccaṁ kira tvaṁ, udāyi, sañcicca pāṇaṁ jīvitā voropesī”ti?",
        "analyzed": "saccam kira tvam udāyi sañcit ca pāṇam jīvitā voropesi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.11",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.12",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.13",
        "original": "kathañhi nāma tvaṁ, moghapurisa, sañcicca pāṇaṁ jīvitā voropessasi.",
        "analyzed": "katham hi nāma tva mogha purisa sañcit ca pāṇam jīvitā voropessasi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.14",
        "original": "Netaṁ, moghapurisa, appasannānaṁ vā pasādāya …pe…",
        "analyzed": "neta moghapurisa appasannānam vā pasādāya pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.15",
        "original": "evañca pana, bhikkhave, imaṁ sikkhāpadaṁ uddiseyyātha—",
        "analyzed": "evam ca pana bhikkhave imam sikkhāpadam uddiseyyātha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_pc61:1.16.1",
        "original": "“Yo pana bhikkhu sañcicca pāṇaṁ jīvitā voropeyya, pācittiyan”ti.",
        "analyzed": "yaḥ pana bhikkhu sañcicca pāṇam jīvitā voropeyya pācittiyanti ",
        "folio": "1"
    },
    {
