[
    {
        "segmentnr": "PA_snp_3.8:0.1",
        "original": "Sutta Nipāta 3.8",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.8:0.2",
        "original": "Sallasutta",
        "analyzed": "sallasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_3.8:1.1",
        "original": "Animittamanaññātaṁ,",
        "analyzed": "a nimittam an aññāta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.8:1.2",
        "original": "maccānaṁ idha jīvitaṁ;",
        "analyzed": "matcānam idha jīvita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.8:1.3",
        "original": "Kasirañca parittañca,",
        "analyzed": "kasirañca parittañca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.8:1.4",
        "original": "tañca dukkhena saṁyutaṁ.",
        "analyzed": "tat ca dukkhena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_3.8:2.1",
        "original": "Na hi so upakkamo atthi,",
        "analyzed": "na hi so upakkamaḥ atthi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.8:2.2",
        "original": "yena jātā na miyyare;",
        "analyzed": "yena jātāḥ na miyyare ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.8:2.3",
        "original": "Jarampi patvā maraṇaṁ,",
        "analyzed": "jarampi patvā maraṇa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.8:2.4",
        "original": "evaṁdhammā hi pāṇino.",
        "analyzed": "eva dhammāḥ hi pāṇinaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_3.8:3.1",
        "original": "Phalānamiva pakkānaṁ,",
        "analyzed": "phalānam iva pakkāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.8:3.2",
        "original": "pāto patanato bhayaṁ;",
        "analyzed": "pāto patanataḥ bhaya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.8:3.3",
        "original": "Evaṁ jātāna maccānaṁ,",
        "analyzed": "evam jātāna madcāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.8:3.4",
        "original": "niccaṁ maraṇato bhayaṁ.",
        "analyzed": "niccam maraṇataḥ bhaya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_3.8:4.1",
        "original": "Yathāpi kumbhakārassa,",
        "analyzed": "yathā api kumbhakārassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.8:4.2",
        "original": "katā mattikabhājanā;",
        "analyzed": "katā mattika bhājanā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.8:4.3",
        "original": "Sabbe bhedanapariyantā,",
        "analyzed": "sabbe bhedana pariyantā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.8:4.4",
        "original": "evaṁ maccāna jīvitaṁ.",
        "analyzed": "evam jīvita ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_3.8:5.1",
        "original": "Daharā ca mahantā ca,",
        "analyzed": "daharā ca mahantā ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.8:5.2",
        "original": "ye bālā ye ca paṇḍitā;",
        "analyzed": "ye bālāḥ ye ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.8:5.3",
        "original": "Sabbe maccuvasaṁ yanti,",
        "analyzed": "sabbe maccu vasam yanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.8:5.4",
        "original": "sabbe maccuparāyaṇā.",
        "analyzed": "sabbe maccu parāyaṇā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_3.8:6.1",
        "original": "Tesaṁ maccuparetānaṁ,",
        "analyzed": "tesam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.8:6.2",
        "original": "gacchataṁ paralokato;",
        "analyzed": "gacchatam para lokataḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.8:6.3",
        "original": "Na pitā tāyate puttaṁ,",
        "analyzed": "na pitā tāyate putta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.8:6.4",
        "original": "ñātī vā pana ñātake.",
        "analyzed": "ñātī vā pana ñātake ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_3.8:7.1",
        "original": "Pekkhataṁyeva ñātīnaṁ,",
        "analyzed": "pekkhatayā iva ñātīna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.8:7.2",
        "original": "passa lālapataṁ puthu;",
        "analyzed": "passa lālapatam puthu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.8:7.3",
        "original": "Ekamekova maccānaṁ,",
        "analyzed": "ekam ekā uva mat cāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.8:7.4",
        "original": "govajjho viya nīyati.",
        "analyzed": "go vajjhaḥ nīyati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.8:8.1",
        "original": "Evamabbhāhato loko,",
        "analyzed": "evam abbhāhataḥ lokaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.8:8.2",
        "original": "Maccunā ca jarāya ca;",
        "analyzed": "maccunā ca jarāya ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.8:8.3",
        "original": "Tasmā dhīrā na socanti,",
        "analyzed": "tasmai dhīrāḥ na socanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.8:8.4",
        "original": "Viditvā lokapariyāyaṁ.",
        "analyzed": "viditvā loka pariyāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_3.8:9.1",
        "original": "Yassa maggaṁ na jānāsi,",
        "analyzed": "yassa maggam na jānāsi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.8:9.2",
        "original": "āgatassa gatassa vā;",
        "analyzed": "āgatassa gatassa vā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.8:9.3",
        "original": "Ubho ante asampassaṁ,",
        "analyzed": "ubho ante a sampassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.8:9.4",
        "original": "niratthaṁ paridevasi.",
        "analyzed": "nirattham paridevasi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_3.8:10.1",
        "original": "Paridevayamāno ce,",
        "analyzed": "paridevayamānā ce ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.8:10.2",
        "original": "Kiñcidatthaṁ udabbahe;",
        "analyzed": "kiñcidattham ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.8:10.3",
        "original": "Sammūḷho hiṁsamattānaṁ,",
        "analyzed": "sammūḍhaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.8:10.4",
        "original": "Kayirā ce naṁ vicakkhaṇo.",
        "analyzed": "kayirā ce nam vicakkhaṇo ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_3.8:11.1",
        "original": "Na hi ruṇṇena sokena,",
        "analyzed": "na hi ruṇṇena sokena ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.8:11.2",
        "original": "Santiṁ pappoti cetaso;",
        "analyzed": "santim pappoti cetasaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.8:11.3",
        "original": "Bhiyyassuppajjate dukkhaṁ,",
        "analyzed": "bhiyyassu uppajjate dukkha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.8:11.4",
        "original": "Sarīraṁ cupahaññati.",
        "analyzed": "sarīram ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_3.8:12.1",
        "original": "Kiso vivaṇṇo bhavati,",
        "analyzed": "kisaḥ vivaṇṇaḥ bhavati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.8:12.2",
        "original": "Hiṁsamattānamattanā;",
        "analyzed": "hisamatta anamattanā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.8:12.3",
        "original": "Na tena petā pālenti,",
        "analyzed": "na tena petā pālenti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.8:12.4",
        "original": "Niratthā paridevanā.",
        "analyzed": "niratthā paridevanā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_3.8:13.1",
        "original": "Sokamappajahaṁ jantu,",
        "analyzed": "sokam appajaham jantu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.8:13.2",
        "original": "Bhiyyo dukkhaṁ nigacchati;",
        "analyzed": "bhiyyas dukkham nigacchati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.8:13.3",
        "original": "Anutthunanto kālaṅkataṁ,",
        "analyzed": "an utthunantaḥ kālaṅkata ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.8:13.4",
        "original": "Sokassa vasamanvagū.",
        "analyzed": "sokaḥ sa vasamanvagū ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_3.8:14.1",
        "original": "Aññepi passa gamine,",
        "analyzed": "aññe pi passa gamine ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.8:14.2",
        "original": "Yathākammupage nare;",
        "analyzed": "yathā kammupage nare ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.8:14.3",
        "original": "Maccuno vasamāgamma,",
        "analyzed": "maccunaḥ vasam āgamma ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.8:14.4",
        "original": "Phandantevidha pāṇino.",
        "analyzed": "phandantevidha pāṇinaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_3.8:15.1",
        "original": "Yena yena hi maññanti,",
        "analyzed": "yena yena hi maññanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.8:15.2",
        "original": "Tato taṁ hoti aññathā;",
        "analyzed": "tatas tam hoti aññathā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.8:15.3",
        "original": "Etādiso vinābhāvo,",
        "analyzed": "etādisaḥ vinābhāvaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.8:15.4",
        "original": "Passa lokassa pariyāyaṁ.",
        "analyzed": "passa lokassa pariyāya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_3.8:16.1",
        "original": "Api vassasataṁ jīve,",
        "analyzed": "api vassa satam jīve ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.8:16.2",
        "original": "Bhiyyo vā pana māṇavo;",
        "analyzed": "bhiyyaḥ vā pana māṇavaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.8:16.3",
        "original": "Ñātisaṅghā vinā hoti,",
        "analyzed": "ñāti saṅghāḥ vinā hoti ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.8:16.4",
        "original": "Jahāti idha jīvitaṁ.",
        "analyzed": "jahāti idha jīvita ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_3.8:17.1",
        "original": "Tasmā arahato sutvā,",
        "analyzed": "tasmai arahataḥ sutvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.8:17.2",
        "original": "Vineyya paridevitaṁ;",
        "analyzed": "vineyya paridevita ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.8:17.3",
        "original": "Petaṁ kālaṅkataṁ disvā,",
        "analyzed": "petam kālaṅkatam disvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.8:17.4",
        "original": "Neso labbhā mayā iti.",
        "analyzed": "na iso labbhā mayā iti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_3.8:18.1",
        "original": "Yathā saraṇamādittaṁ,",
        "analyzed": "yathā saraṇam āditta ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.8:18.2",
        "original": "Vārinā parinibbaye;",
        "analyzed": "vārinā parinibbaye ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.8:18.3",
        "original": "Evampi dhīro sapañño,",
        "analyzed": "evam pi dhīraḥ sa pañño ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.8:18.4",
        "original": "Paṇḍito kusalo naro;",
        "analyzed": "paṇḍito kusalaḥ naraḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.8:18.5",
        "original": "Khippamuppatitaṁ sokaṁ,",
        "analyzed": "khippam uppatitam soka ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.8:18.6",
        "original": "Vāto tūlaṁva dhaṁsaye.",
        "analyzed": "vāto tūlava dhasaye ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_3.8:19.1",
        "original": "Paridevaṁ pajappañca,",
        "analyzed": "paridevam ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.8:19.2",
        "original": "Domanassañca attano;",
        "analyzed": "domanassañca attanaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.8:19.3",
        "original": "Attano sukhamesāno,",
        "analyzed": "attanaḥ sukham esānaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.8:19.4",
        "original": "Abbahe sallamattano.",
        "analyzed": "ap bahe salla mattano ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_3.8:20.1",
        "original": "Abbūḷhasallo asito,",
        "analyzed": "abbūḍha sallaḥ asitaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.8:20.2",
        "original": "santiṁ pappuyya cetaso;",
        "analyzed": "santim pappuyya cetasaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.8:20.3",
        "original": "Sabbasokaṁ atikkanto,",
        "analyzed": "sabba sokam atikkanto ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.8:20.4",
        "original": "asoko hoti nibbutoti.",
        "analyzed": "a sokaḥ hoti nibbuto ti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_3.8:20.5",
        "original": "Sallasuttaṁ aṭṭhamaṁ.",
        "analyzed": "salla suttam aṭṭhama ",
        "folio": "20"
    }
]