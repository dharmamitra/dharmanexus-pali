        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_471:0.2",
        "original": "Dvādasakanipāta",
        "analyzed": "dvādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_471:0.3",
        "original": "Cūḷakuṇālavagga",
        "analyzed": "cūḷa kuṇāla vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_471:0.4",
        "original": "8. Meṇḍakapañhajātaka",
        "analyzed": "     ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_471:1.1",
        "original": "“Yesaṁ na kadāci bhūtapubbaṁ,",
        "analyzed": "yesam na kadāci bhūta pubba ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_471:1.2",
        "original": "Sakhyaṁ sattapadampimasmi loke;",
        "analyzed": "sakhyam loke ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_471:1.3",
        "original": "Jātā amittā duve sahāyā,",
        "analyzed": "jātā amittā duve sahāyā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_471:1.4",
        "original": "Paṭisandhāya caranti kissa hetu”.",
        "analyzed": "paṭisandhāya caranti kissa hetu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_471:2.1",
        "original": "“Yadi me ajja pātarāsakāle,",
        "analyzed": "yadi me ajja pāta rāsa kāle ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_471:2.2",
        "original": "Pañhaṁ na sakkuṇeyyātha vattumetaṁ;",
        "analyzed": "pañham na vattum eta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_471:2.3",
        "original": "Raṭṭhā pabbājayissāmi vo sabbe,",
        "analyzed": "raṭṭhā pabbājayissāmi vaḥ sabbe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_471:2.4",
        "original": "Na hi mattho duppaññajātikehi”.",
        "analyzed": "na hi matthaḥ duppañña jātikehi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_471:3.1",
        "original": "“Mahājanasamāgamamhi ghore,",
        "analyzed": "ghore ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_471:3.2",
        "original": "Janakolāhalasaṅgamamhi jāte;",
        "analyzed": "jana kolāhala saṅgamamhi jāte ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_471:3.3",
        "original": "Vikkhittamanā anekacittā,",
        "analyzed": "vikkhitta manāḥ aneka cittā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_471:3.4",
        "original": "Pañhaṁ na sakkuṇoma vattumetaṁ.",
        "analyzed": "pañham na sakkuṇoma vattum eta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_471:4.1",
        "original": "Ekaggacittāva ekamekā,",
        "analyzed": "ekagga cittā avaḥ ekam ekā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_471:4.2",
        "original": "Rahasi gatā atthaṁ nicintayitvā;",
        "analyzed": "rahasi gatāḥ attham nicintayitvā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_471:4.3",
        "original": "Paviveke sammasitvāna dhīrā,",
        "analyzed": "paviveke sammasitvāna dhīrā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_471:4.4",
        "original": "Atha vakkhanti janinda etamatthaṁ”.",
        "analyzed": "atha vakkhanti janindaḥ etam attha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_471:5.1",
        "original": "“Uggaputtarājaputtiyānaṁ,",
        "analyzed": "ugga putta rāja puttiyāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_471:5.2",
        "original": "Urabbhassa maṁsaṁ piyaṁ manāpaṁ;",
        "analyzed": "urabbhassa masam piyam manāpa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_471:5.3",
        "original": "Na sunakhassa te adenti maṁsaṁ,",
        "analyzed": "na su nakhassa te adenti masa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_471:5.4",
        "original": "Atha meṇḍassa suṇena sakhyamassa”.",
        "analyzed": "atha meṇḍassa suṇena sakhyamassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_471:6.1",
        "original": "“Cammaṁ vihananti eḷakassa,",
        "analyzed": "cammam vihananti eḷakassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_471:6.2",
        "original": "Assapiṭṭhattharassukhassa hetu;",
        "analyzed": "hetu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_471:6.3",
        "original": "Na ca te sunakhassa attharanti,",
        "analyzed": "na ca te sunakhassa attharanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_471:6.4",
        "original": "Atha meṇḍassa suṇena sakhyamassa”.",
        "analyzed": "atha meṇḍassa suṇena sakhyamassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_471:7.1",
        "original": "“Āvellitasiṅgiko hi meṇḍo,",
        "analyzed": "āvellita siṅgikaḥ hi meṇḍaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_471:7.2",
        "original": "Na ca sunakhassa visāṇakāni atthi;",
        "analyzed": "na ca sunakhassa visāṇakāni atthi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_471:7.3",
        "original": "Tiṇabhakkho maṁsabhojano ca,",
        "analyzed": "tiṇabhakkhaḥ masabhojano ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_471:7.4",
        "original": "Atha meṇḍassa suṇena sakhyamassa”.",
        "analyzed": "atha meṇḍassa suṇena sakhyamassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_471:8.1",
        "original": "“Tiṇamāsi palāsamāsi meṇḍo,",
        "analyzed": "tiṇa māsi palāsa māsi meṇḍaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_471:8.2",
        "original": "Na ca sunakho tiṇamāsi no palāsaṁ;",
        "analyzed": "na ca su nakhaḥ tiṇamāsi na u palāsa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_471:8.3",
        "original": "Gaṇheyya suṇo sasaṁ biḷāraṁ,",
        "analyzed": "gaṇheyya suṇo sasam biḷāra ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_471:8.4",
        "original": "Atha meṇḍassa suṇena sakhyamassa”.",
        "analyzed": "atha meṇḍassa suṇena sakhyamassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_471:9.1",
        "original": "“Aṭṭhaḍḍhapado catuppadassa,",
        "analyzed": "aṭṭhaḍḍha padaḥ catuppadassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_471:9.2",
        "original": "Meṇḍo aṭṭhanakho adissamāno;",
        "analyzed": "meṇḍaḥ aṭṭha nakhaḥ a dissamānaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_471:9.3",
        "original": "Chādiyamāharatī ayaṁ imassa,",
        "analyzed": "chādiyam āharatī ayam imassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_471:9.4",
        "original": "Maṁsaṁ āharatī ayaṁ amussa.",
        "analyzed": "masam āharatī ayam amussa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_471:10.1",
        "original": "Pāsādavaragato videhaseṭṭho,",
        "analyzed": "pāsāda vara gataḥ videha seṭṭhaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_471:10.2",
        "original": "Vitihāraṁ aññamaññabhojanānaṁ;",
        "analyzed": "vitihāram aññamañña bhojanāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_471:10.3",
        "original": "Addakkhi kira sakkhikaṁ janindo,",
        "analyzed": "addakkhi kira sakkhikam janindo ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_471:10.4",
        "original": "Bubhukkassa puṇṇaṁmukhassa cetaṁ”.",
        "analyzed": "bubhukkassa puṇṇa mukhassa ceta ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_471:11.1",
        "original": "“Lābhā vata me anapparūpā,",
        "analyzed": "lābhā vata me anapparūpā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_471:11.2",
        "original": "Yassa medisā paṇḍitā kulamhi;",
        "analyzed": "yassa medisā paṇḍitā kulamhi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_471:11.3",
        "original": "Pañhassa gambhīragataṁ nipuṇamatthaṁ,",
        "analyzed": "pañhassa gambhīra gatam nipuṇam attha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_471:11.4",
        "original": "Paṭivijjhanti subhāsitena dhīrā.",
        "analyzed": "paṭivijjhanti su bhāsitena dhīrā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_471:12.1",
        "original": "Assatarirathañca ekamekaṁ,",
        "analyzed": "assatari rathañca ekam eka ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_471:12.2",
        "original": "Phītaṁ gāmavarañca ekamekaṁ;",
        "analyzed": "phītam gām avaram ca ekam eka ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_471:12.3",
        "original": "Sabbesaṁ vo dammi paṇḍitānaṁ,",
        "analyzed": "sabbesam vaḥ dammi paṇḍitāna ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_471:12.4",
        "original": "Paramappatītamano subhāsitenā”ti.",
        "analyzed": "su bhāsitena ati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_471:13.1",
        "original": "Meṇḍakapañhajātakaṁ aṭṭhamaṁ.",
        "analyzed": "meṇḍaka pañha jātakam aṭṭhama ",
        "folio": "13"
    }
]