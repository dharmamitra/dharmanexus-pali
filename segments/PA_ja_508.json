        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_508:15.4",
        "original": "Guyhaṁ pātukataṁ sutaṁ mametaṁ.",
        "analyzed": "guyham pātukatam sutam mameta ",
        "folio": "15"
    },
    {
