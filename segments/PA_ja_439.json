    },
    {
        "segmentnr": "PA_ja_439:5.4",
        "original": "cakkaṁ bhamati matthake.",
        "analyzed": "cakkam bhamati matthake ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_439:6.1",
        "original": "Uparivisālā duppūrā,",
        "analyzed": "upari visālā duppūrā ",
