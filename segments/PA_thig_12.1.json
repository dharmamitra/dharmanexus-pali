    },
    {
        "segmentnr": "PA_thig_12.1:11.2",
        "original": "nāhamicchāmi sāṭakaṁ;",
        "analyzed": "na aham icchāmi sāṭaka ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_12.1:11.3",
        "original": "Sace bhāyasi dukkhassa,",
        "analyzed": "sace bhāyasi dukkhassa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_12.1:11.4",
        "original": "sace te dukkhamappiyaṁ.",
        "analyzed": "sace te dukkham appiya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_12.1:12.1",
        "original": "Mākāsi pāpakaṁ kammaṁ,",
        "analyzed": "mā akāsi pāpakam kamma ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_12.1:12.2",
        "original": "āvi vā yadi vā raho;",
        "analyzed": "āvi vā yadi vā raho ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_12.1:12.3",
        "original": "Sace ca pāpakaṁ kammaṁ,",
        "analyzed": "sace ca pāpakam kamma ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_12.1:12.4",
        "original": "karissasi karosi vā.",
        "analyzed": "karissasi karosi vā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_12.1:13.1",
        "original": "Na te dukkhā pamutyatthi,",
        "analyzed": "na te dukkhā pamuti atthi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_12.1:13.2",
        "original": "upeccāpi palāyato;",
        "analyzed": "palāyato ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_12.1:13.3",
        "original": "Sace bhāyasi dukkhassa,",
        "analyzed": "sace bhāyasi dukkhassa ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_12.1:13.4",
        "original": "sace te dukkhamappiyaṁ.",
        "analyzed": "sace te dukkham appiya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_12.1:14.1",
