    },
    {
        "segmentnr": "PA_pv_28:0.3",
        "original": "3. Rathakārapetivatthu",
        "analyzed": "rathakāra petivatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_28:1.1",
        "original": "“Veḷuriyathambhaṁ ruciraṁ pabhassaraṁ,",
        "analyzed": "ruciram pabhassara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_28:1.2",
        "original": "Vimānamāruyha anekacittaṁ;",
        "analyzed": "vimānam āruyha anekacitta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_28:1.3",
        "original": "Tatthacchasi devi mahānubhāve,",
        "analyzed": "devi mahā anubhāve ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_28:1.4",
        "original": "Pathaddhani pannaraseva cando.",
        "analyzed": "cando ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_28:2.1",
        "original": "Vaṇṇo ca te kanakassa sannibho,",
        "analyzed": "vaṇṇaḥ ca te kanakassa sannibhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_28:2.2",
        "original": "Uttattarūpo bhusa dassaneyyo;",
        "analyzed": "uttatta rūpaḥ bhusa dassaneyyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_28:2.3",
        "original": "Pallaṅkaseṭṭhe atule nisinnā,",
        "analyzed": "pallaṅka seṭṭhe atule nisinnā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_28:2.4",
        "original": "Ekā tuvaṁ natthi ca tuyha sāmiko.",
        "analyzed": "ekā tuvam natthi ca tuyha sāmiko ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_28:3.1",
        "original": "Imā ca te pokkharaṇī samantā,",
        "analyzed": "imā ca te pokkharaṇī samantā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_28:3.2",
        "original": "Pahūtamalyā bahupuṇḍarīkā;",
        "analyzed": "bahu puṇḍarīkā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_28:3.3",
        "original": "Suvaṇṇacuṇṇehi samantamotthatā,",
        "analyzed": "suvaṇṇa cuṇṇehi samantama uttha tā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_28:3.4",
        "original": "Na tattha paṅko paṇako ca vijjati.",
        "analyzed": "na tattha paṅko paṇako ca vijjati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_28:4.1",
        "original": "Haṁsā cime dassanīyā manoramā,",
        "analyzed": "hasā cime dassanīyā manoramā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_28:4.2",
        "original": "Udakasmimanupariyanti sabbadā;",
        "analyzed": "udakasmim anupariyanti sabbadā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_28:4.3",
        "original": "Samayya vaggūpanadanti sabbe,",
        "analyzed": "samayya vaggū upanadanti sabbe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_28:4.4",
        "original": "Bindussarā dundubhīnaṁva ghoso.",
        "analyzed": "bindu ssarā dundubhī nava ghosaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_28:5.1",
        "original": "Daddallamānā yasasā yasassinī,",
        "analyzed": "daddallamānā yasasā yasassinī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_28:5.2",
        "original": "Nāvāya ca tvaṁ avalamba tiṭṭhasi;",
        "analyzed": "nāvāya ca tvam avalamba tiṭṭhasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_28:5.3",
        "original": "Āḷārapamhe hasite piyaṁvade,",
        "analyzed": "āḷārapamhe hasite piyavade ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_28:5.4",
        "original": "Sabbaṅgakalyāṇi bhusaṁ virocasi.",
        "analyzed": "sabbaṅga kalyāṇi bhusam virocasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_28:6.1",
        "original": "Idaṁ vimānaṁ virajaṁ same ṭhitaṁ,",
        "analyzed": "idam vimānam virajam same ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_28:6.2",
        "original": "Uyyānavantaṁ ratinandivaḍḍhanaṁ;",
        "analyzed": "uyyānavantam rati nandi vaḍḍhana ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_28:6.3",
        "original": "Icchāmahaṁ nāri anomadassane,",
        "analyzed": "icchāmaham nāri anomadassane ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_28:6.4",
        "original": "Tayā saha nandane idha moditun”ti.",
        "analyzed": "tayā saha nandane idha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_28:7.1",
        "original": "“Karohi kammaṁ idha vedanīyaṁ,",
        "analyzed": "karohi kammam idha vedanīya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_28:7.2",
        "original": "Cittañca te idha nihitaṁ bhavatu;",
        "analyzed": "cittam ca te idha nihitam bhavatu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_28:7.3",
        "original": "Katvāna kammaṁ idha vedanīyaṁ,",
        "analyzed": "katvāna kammam idha vedanīya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_28:7.4",
        "original": "Evaṁ mamaṁ lacchasi kāmakāminin”ti.",
        "analyzed": "evam lacchasi kāma kāminin ti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_28:8.1",
        "original": "“Sādhū”ti so tassā paṭissuṇitvā,",
        "analyzed": "sādhu iti saḥ tassā paṭissuṇitvā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_28:8.2",
        "original": "Akāsi kammaṁ tahiṁ vedanīyaṁ;",
        "analyzed": "kammam tahim vedanīya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_28:8.3",
        "original": "Katvāna kammaṁ tahiṁ vedanīyaṁ,",
        "analyzed": "katvāna kammam tahim ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_28:8.4",
        "original": "Upapajji so māṇavo tassā sahabyatanti.",
        "analyzed": "upapajji saḥ māṇavaḥ tassā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_28:9.1",
        "original": "Rathakārapetivatthu tatiyaṁ.",
        "analyzed": "rathakāra petivatthu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_28:9.2",
        "original": "Bhāṇavāraṁ dutiyaṁ.",
        "analyzed": "bhāṇa vāram ",
        "folio": "9"
    }
]