        "analyzed": "santaritvā mahā udadhi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_tha-ap_538:15.3",
        "original": "Suppārapaṭṭanavaraṁ,",
        "analyzed": "      ",
        "folio": "15"
    },
