    },
    {
        "segmentnr": "PA_thag_6.7:0.4",
        "original": "Kātiyānattheragāthā",
        "analyzed": "kātiyānattheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_6.7:1.1",
        "original": "“Uṭṭhehi nisīda kātiyāna,",
        "analyzed": "uṭṭhehi nisīda kātiyāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_6.7:1.2",
        "original": "Mā niddābahulo ahu jāgarassu;",
        "analyzed": "mā niddā bahulaḥ ahu jāgarassu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_6.7:1.3",
        "original": "Mā taṁ alasaṁ pamattabandhu,",
        "analyzed": "mā tam alasam pamatta bandhu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_6.7:1.4",
        "original": "Kūṭeneva jinātu maccurājā.",
        "analyzed": "kūṭena iva jinātu maccu rājā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_6.7:2.1",
        "original": "Seyyathāpi mahāsamuddavego,",
        "analyzed": "seyyathā api mahā samudda vegaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_6.7:2.2",
        "original": "Evaṁ jātijarātivattate taṁ;",
        "analyzed": "evam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_6.7:2.3",
        "original": "So karohi sudīpamattano tvaṁ,",
        "analyzed": "saḥ karohi su dīpam attano tva ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_6.7:2.4",
        "original": "Na hi tāṇaṁ tava vijjateva aññaṁ.",
        "analyzed": "na hi tāṇam tava vijjatā iva añña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_6.7:3.1",
        "original": "Satthā hi vijesi maggametaṁ,",
        "analyzed": "satthā hi vijesi maggam eta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_6.7:3.2",
        "original": "Saṅgā jātijarābhayā atītaṁ;",
        "analyzed": "saṅgāḥ jāti jarā bhayāḥ atīta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_6.7:3.3",
        "original": "Pubbāpararattamappamatto,",
        "analyzed": "pubba apara rattam appamattaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_6.7:3.4",
        "original": "Anuyuñjassu daḷhaṁ karohi yogaṁ.",
        "analyzed": "anuyuñjassu daḷham karohi yoga ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_6.7:4.1",
        "original": "Purimāni pamuñca bandhanāni,",
        "analyzed": "purimāni pamuñca bandhanāni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_6.7:4.2",
        "original": "Saṅghāṭikhuramuṇḍabhikkhabhojī;",
        "analyzed": "saṅghāṭi khura muṇḍa bhikkha bhojī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_6.7:4.3",
        "original": "Mā khiḍḍāratiñca mā niddaṁ,",
        "analyzed": "mā khiḍḍā ratiñca mā nidda ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_6.7:4.4",
        "original": "Anuyuñjittha jhāya kātiyāna.",
        "analyzed": "anuyuñjittha jhāya kātiyāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_6.7:5.1",
        "original": "Jhāyāhi jināhi kātiyāna,",
        "analyzed": "jhāyāhi jināhi kātiyāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_6.7:5.2",
        "original": "Yogakkhemapathesu kovidosi;",
        "analyzed": "yogakkhema pathesu kovidosi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_6.7:5.3",
        "original": "Pappuyya anuttaraṁ visuddhiṁ,",
        "analyzed": "pappuyya anuttaram ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_6.7:5.4",
        "original": "Parinibbāhisi vārināva joti.",
        "analyzed": "parinibbāhisi vārinā ava joti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_6.7:6.1",
        "original": "Pajjotakaro parittaraṁso,",
        "analyzed": "pajjota karaḥ paritta rasaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_6.7:6.2",
        "original": "Vātena vinamyate latāva;",
        "analyzed": "vātena vinamyate ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_6.7:6.3",
        "original": "Evampi tuvaṁ anādiyāno,",
        "analyzed": "evam pi tuvam an ādiyānaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_6.7:6.4",
        "original": "Māraṁ indasagotta niddhunāhi;",
        "analyzed": "mā aram indasagotta niddhunāhi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_6.7:6.5",
        "original": "So vedayitāsu vītarāgo,",
        "analyzed": "saḥ vedayitāsu vīta rāgaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_6.7:6.6",
        "original": "Kālaṁ kaṅkha idheva sītibhūto”ti.",
        "analyzed": "kālam kaṅkhaḥ idhā iva sīti bhūta uti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_6.7:6.7",
        "original": "… Kātiyāno thero ….",
        "analyzed": "kātiyānaḥ theraḥ ",
        "folio": "6"
    }
