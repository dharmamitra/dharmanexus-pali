    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.2",
        "original": "Tena kho pana samayena chabbaggiyā bhikkhuniyo gandhavaṇṇakena nahāyanti.",
        "analyzed": "tena kho pana samayena chabbaggiyāḥ bhikkhuniyaḥ gandha vaṇṇakena nahāyanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.3",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.4",
        "original": "“kathañhi nāma bhikkhuniyo gandhavaṇṇakena nahāyissanti, seyyathāpi gihiniyo kāmabhoginiyo”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ gandha vaṇṇakena nahāyissanti seyyathā api gihiniyaḥ kāmabhoginiyaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.5",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.6",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.7",
        "original": "“kathañhi nāma chabbaggiyā bhikkhuniyo gandhavaṇṇakena nahāyissantī”ti …pe…",
        "analyzed": "katham hi nāma chabbaggiyāḥ bhikkhuniyaḥ gandha vaṇṇakena nahāyissanti iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.8",
        "original": "“saccaṁ kira, bhikkhave, chabbaggiyā bhikkhuniyo gandhavaṇṇakena nahāyantī”ti?",
        "analyzed": "saccam kira bhikkhave chabbaggiyāḥ bhikkhuniyaḥ gandha vaṇṇakena nahāyanti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc88:1.9",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
