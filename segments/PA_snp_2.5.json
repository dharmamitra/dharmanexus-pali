[
    {
        "segmentnr": "PA_snp_2.5:0.1",
        "original": "Sutta Nipāta 2.5",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.5:0.2",
        "original": "Sūcilomasutta",
        "analyzed": "sūcilomasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.5:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.5:1.2",
        "original": "ekaṁ samayaṁ bhagavā gayāyaṁ viharati ṭaṅkitamañce sūcilomassa yakkhassa bhavane.",
        "analyzed": "ekam samayam bhagavā gayā ayam viharati ṭaṅkita mañce sūcilomassa yakkhassa bhavane ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.5:1.3",
        "original": "Tena kho pana samayena kharo ca yakkho sūcilomo ca yakkho bhagavato avidūre atikkamanti.",
        "analyzed": "tena kho pana samayena kharaḥ ca yakkhaḥ sūcilomaḥ ca yakkhaḥ bhagavataḥ avidūre atikkamanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.5:1.4",
        "original": "Atha kho kharo yakkho sūcilomaṁ yakkhaṁ etadavoca:",
        "analyzed": "atha kho kharaḥ yakkhaḥ sūcilomam yakkham etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.5:1.5",
        "original": "“eso samaṇo”ti.",
        "analyzed": "esaḥ samaṇaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.5:1.6",
        "original": "“Neso samaṇo, samaṇako eso.",
        "analyzed": "na isaḥ samaṇaḥ samaṇakaḥ esaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.5:1.7",
        "original": "Yāvāhaṁ jānāmi yadi vā so samaṇo, yadi vā so samaṇako”ti.",
        "analyzed": "yāvā aham jānāmi yadi vā saḥ samaṇaḥ yadi vā saḥ samaṇakaḥ iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.5:2.1",
        "original": "Atha kho sūcilomo yakkho yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavato kāyaṁ upanāmesi.",
        "analyzed": "atha kho sūcilomaḥ yakkhaḥ yena bhagavā tena upasaṅkami upasaṅkamitvā bhagavataḥ kāyam upanāmesi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.5:2.2",
        "original": "Atha kho bhagavā kāyaṁ apanāmesi.",
        "analyzed": "atha khalu bhagavā kāyam apanāmesi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.5:2.3",
        "original": "Atha kho sūcilomo yakkho bhagavantaṁ etadavoca:",
        "analyzed": "atha kho sūcilomaḥ yakkhaḥ bhagavantam etat avoca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.5:2.4",
        "original": "“bhāyasi maṁ, samaṇā”ti?",
        "analyzed": "bhāyasi ma samaṇāti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.5:2.5",
        "original": "“Na khvāhaṁ taṁ, āvuso, bhāyāmi;",
        "analyzed": "na khvā aham te āvusaḥ bhāyāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.5:2.6",
        "original": "api ca te samphasso pāpako”ti.",
        "analyzed": "api ca te samphassaḥ pāpakaḥ ti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.5:3.1",
        "original": "“Pañhaṁ taṁ, samaṇa, pucchissāmi.",
        "analyzed": "pañham ta samaṇa pucchissāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.5:3.2",
        "original": "Sace me na byākarissasi, cittaṁ vā te khipissāmi, hadayaṁ vā te phālessāmi, pādesu vā gahetvā pāragaṅgāya khipissāmī”ti.",
        "analyzed": "sace me na byākarissasi cittam vā te khipissāmi hadayam vā te phālessāmi pādesu vā gahetvā pāragaṅgāya khipissāmi iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.5:4.1_0",
        "original": "“Na khvāhaṁ taṁ, āvuso, passāmi sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā",
        "analyzed": "na khvā aham te āvuso passāmi sa devake loke sa mārake sa brahmake sa ssamaṇa brāhmaṇiyā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.5:4.1_1",
        "original": "pajāya sadevamanussāya yo me cittaṁ vā khipeyya hadayaṁ vā phāleyya pādesu vā gahetvā",
        "analyzed": "pajāya sa deva manussāya yaḥ me cittam vā khipeyya hadayam vā phāleyya pādesu vā gahetvā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.5:4.1_2",
        "original": "pāragaṅgāya khipeyya.",
        "analyzed": "pāragaṅgāya khipeyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.5:4.2",
        "original": "Api ca tvaṁ, āvuso, puccha yadākaṅkhasī”ti.",
        "analyzed": "api ca tvai āvuso puccha yat ākaṅkhasi iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.5:4.3",
        "original": "Atha kho sūcilomo yakkho bhagavantaṁ gāthāya ajjhabhāsi:",
        "analyzed": "atha kho sūcilomaḥ yakkhaḥ bhagavantam gāthāya ajjhabhāsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.5:5.1",
        "original": "“Rāgo ca doso ca kutonidānā,",
        "analyzed": "rāgaḥ ca dosaḥ ca kutonidānā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.5:5.2",
        "original": "Aratī ratī lomahaṁso kutojā;",
        "analyzed": "aratī ratī loma hasaḥ kutojā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.5:5.3",
        "original": "Kuto samuṭṭhāya manovitakkā,",
        "analyzed": "kutas samuṭṭhāya manaḥ vitakkā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.5:5.4",
        "original": "Kumārakā dhaṅkamivossajanti”.",
        "analyzed": "kumārakāḥ dhaṅkam iva ussajanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.5:6.1",
        "original": "“Rāgo ca doso ca itonidānā,",
        "analyzed": "rāgaḥ ca dosaḥ ca itas nidānā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.5:6.2",
        "original": "Aratī ratī lomahaṁso itojā;",
        "analyzed": "aratī ratī loma hasaḥ ita ojā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.5:6.3",
        "original": "Ito samuṭṭhāya manovitakkā,",
        "analyzed": "itas samuṭṭhāya manaḥ vitakkā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.5:6.4",
        "original": "Kumārakā dhaṅkamivossajanti.",
        "analyzed": "kumārakāḥ dhaṅkam iva ussajanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.5:7.1",
        "original": "Snehajā attasambhūtā,",
        "analyzed": "sneha jā atta sambhūtā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.5:7.2",
        "original": "Nigrodhasseva khandhajā;",
        "analyzed": "nigrodhaḥ sā iva khandha jā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.5:7.3",
        "original": "Puthū visattā kāmesu,",
        "analyzed": "puthū visattā kāmesu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.5:7.4",
        "original": "Māluvāva vitatāvane.",
        "analyzed": "māluvāva vitata avane ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.5:8.1",
        "original": "Ye naṁ pajānanti yatonidānaṁ,",
        "analyzed": "ye nam pajānanti yatonidāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.5:8.2",
        "original": "Te naṁ vinodenti suṇohi yakkha;",
        "analyzed": "te nam vinodenti suṇohi yakkha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.5:8.3",
        "original": "Te duttaraṁ oghamimaṁ taranti,",
        "analyzed": "te duttaram ogham imam taranti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.5:8.4",
        "original": "Atiṇṇapubbaṁ apunabbhavāyā”ti.",
        "analyzed": "a tiṇṇa pubbam apunabbhavāya ati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.5:8.5",
        "original": "Sūcilomasuttaṁ pañcamaṁ.",
        "analyzed": "sūcilomasuttam pañcama ",
        "folio": "8"
    }
]