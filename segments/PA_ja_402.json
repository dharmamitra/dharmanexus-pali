    {
        "segmentnr": "PA_ja_402:0.4",
        "original": "7. Sattubhastajātaka",
        "analyzed": "sattu bhasta jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_402:1.1",
        "original": "“Vibbhantacitto kupitindriyosi,",
        "analyzed": "kupita indriyaḥ asi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_402:1.2",
        "original": "Nettehi te vārigaṇā savanti;",
        "analyzed": "te vāri gaṇāḥ savanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_402:1.3",
        "original": "Kiṁ te naṭṭhaṁ kiṁ pana patthayāno,",
        "analyzed": "kim te naṭṭham kim pana patthayāno ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_402:1.4",
        "original": "Idhāgamā brahme tadiṅgha brūhi”.",
        "analyzed": "idha agama brahme tat iṅgha brūhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_402:2.1",
        "original": "“Miyyetha bhariyā vajato mamajja,",
        "analyzed": "miyyetha bhariyā vajataḥ mamajja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_402:2.2",
        "original": "Agacchato maraṇamāha yakkho;",
        "analyzed": "a gacchataḥ maraṇam āha yakkhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_402:2.3",
        "original": "Etena dukkhena pavedhitosmi,",
        "analyzed": "etena dukkhena pavedhitaḥ asmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_402:2.4",
        "original": "Akkhāhi me senaka etamatthaṁ”.",
        "analyzed": "akkhāhi me senaka etam attha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_402:3.1",
        "original": "“Bahūni ṭhānāni vicintayitvā,",
        "analyzed": "bahūni ṭhānāni vicintayitvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_402:3.2",
        "original": "Yamettha vakkhāmi tadeva saccaṁ;",
        "analyzed": "yam ettha vakkhāmi tat eva sat ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_402:3.3",
        "original": "Maññāmi te brāhmaṇa sattubhastaṁ,",
        "analyzed": "maññāmi te brāhmaṇa sattubhasta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_402:3.4",
        "original": "Ajānato kaṇhasappo paviṭṭho.",
        "analyzed": "a jānataḥ kaṇha sappaḥ paviṭṭhaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_402:4.1",
        "original": "Ādāya daṇḍaṁ parisumbha bhastaṁ,",
        "analyzed": "ādāya daṇḍam parisumbha bhasta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_402:4.2",
        "original": "Passeḷamūgaṁ uragaṁ dujivhaṁ;",
        "analyzed": "uragam dujivha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_402:4.3",
        "original": "Chindajja kaṅkhaṁ vicikicchitāni,",
        "analyzed": "chindat ja kaṅkham vicikicchitāni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_402:4.4",
        "original": "Bhujaṅgamaṁ passa pamuñca bhastaṁ”.",
        "analyzed": "bhujaṅgamam passa pamuñca bhasta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_402:5.1",
        "original": "Saṁviggarūpo parisāya majjhe,",
        "analyzed": "sa vigga rūpaḥ parisāya majjhe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_402:5.2",
        "original": "So brāhmaṇo sattubhastaṁ pamuñci;",
        "analyzed": "saḥ brāhmaṇaḥ sattubhaḥ tam pamuñci ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_402:5.3",
        "original": "Atha nikkhami urago uggatejo,",
        "analyzed": "atha nikkhami uragaḥ ugga tejaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_402:5.4",
        "original": "Āsīviso sappo phaṇaṁ karitvā.",
        "analyzed": "āsīvisaḥ sappaḥ phaṇam karitvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_402:6.1",
        "original": "“Suladdhalābhā janakassa rañño,",
        "analyzed": "su laddha lābhā janakassa rañño ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_402:6.2",
        "original": "Yo passatī senakaṁ sādhupaññaṁ;",
        "analyzed": "yaḥ passati senakam sādhu pañña ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_402:6.3",
        "original": "Vivaṭṭachaddo nusi sabbadassī,",
        "analyzed": "vivaṭṭa chaddaḥ nusi sabbadassī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_402:6.4",
        "original": "Ñāṇaṁ nu te brāhmaṇa bhiṁsarūpaṁ”.",
        "analyzed": "nu te brāhmaṇa bhisarūpa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_402:7.1",
        "original": "“Imāni me sattasatāni atthi,",
        "analyzed": "imāni me satta satāni atthi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_402:7.2",
        "original": "Gaṇhāhi sabbāni dadāmi tuyhaṁ;",
        "analyzed": "gaṇhāhi sabbāni dadāmi tuyha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_402:7.3",
        "original": "Tayā hi me jīvitamajja laddhaṁ,",
        "analyzed": "tayā hi me jīvita majja laddha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_402:7.4",
        "original": "Athopi bhariyāya makāsi sotthiṁ”.",
        "analyzed": "bhariyāya makāsi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_402:8.1",
        "original": "“Na paṇḍitā vetanamādiyanti,",
        "analyzed": "na paṇḍitāḥ vetanam ādiyanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_402:8.2",
        "original": "Citrāhi gāthāhi subhāsitāhi;",
        "analyzed": "citrāhi gāthāhi su bhāsitāhi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_402:8.3",
        "original": "Itopi te brahme dadantu vittaṁ,",
        "analyzed": "itas upi te brahme dadantu vitta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_402:8.4",
        "original": "Ādāya tvaṁ gaccha sakaṁ niketan”ti.",
        "analyzed": "ādāya tvam gaccha sakam ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_402:9.1",
        "original": "Sattubhastajātakaṁ sattamaṁ.",
        "analyzed": "sattubhastajātakam sattama ",
        "folio": "9"
    }
]