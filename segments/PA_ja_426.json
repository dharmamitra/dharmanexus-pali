        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_426:1.1",
        "original": "“Khamanīyaṁ yāpanīyaṁ,",
        "analyzed": "khamanīyam yāpanīya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_426:1.2",
        "original": "kacci mātula te sukhaṁ;",
        "analyzed": "kacci mātula te sukha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_426:1.3",
        "original": "Sukhaṁ te ammā avaca,",
        "analyzed": "sukham te ammai avaca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_426:1.4",
        "original": "sukhakāmāva te mayaṁ”.",
        "analyzed": "sukha kāmāva te maya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_426:2.1",
        "original": "“Naṅguṭṭhaṁ me avakkamma,",
        "analyzed": "naṅguṭṭham me avakkamma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_426:2.2",
        "original": "Heṭhayitvāna eḷike;",
        "analyzed": "heṭhayitvānaḥ eḷike ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_426:2.3",
        "original": "Sājja mātulavādena,",
        "analyzed": "sā ajja mātula vādena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_426:2.4",
        "original": "Muñcitabbā nu maññasi”.",
        "analyzed": "muñcitabbā nu maññasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_426:3.1",
        "original": "“Puratthāmukho nisinnosi,",
        "analyzed": "puratthāmukhaḥ nisinnaḥ asi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_426:3.2",
        "original": "Ahaṁ te mukhamāgatā;",
        "analyzed": "aham te mukham āgatā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_426:3.3",
        "original": "Pacchato tuyhaṁ naṅguṭṭhaṁ,",
        "analyzed": "pacchatas tuyham naṅguṭṭha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_426:3.4",
        "original": "Kathaṁ khvāhaṁ avakkamiṁ”.",
        "analyzed": "katham khvā aham avakkami ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_426:4.1",
        "original": "“Yāvatā caturo dīpā,",
        "analyzed": "yāvatā caturaḥ dīpā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_426:4.2",
        "original": "sasamuddā sapabbatā;",
        "analyzed": "sa samuddā sa pabbatā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_426:4.3",
        "original": "Tāvatā mayhaṁ naṅguṭṭhaṁ,",
        "analyzed": "tāvatā mayham naṅguṭṭha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_426:4.4",
        "original": "kathaṁ kho tvaṁ vivajjayi”.",
        "analyzed": "katham khalu tvam vivajjayi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_426:5.1",
        "original": "“Pubbeva metamakkhiṁsu,",
        "analyzed": "pubbā iva mā itam akkhisu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_426:5.2",
        "original": "mātā pitā ca bhātaro;",
        "analyzed": "mātā pitā ca bhātaro ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_426:5.3",
        "original": "Dīghaṁ duṭṭhassa naṅguṭṭhaṁ,",
        "analyzed": "dīgham duṭṭhassa naṅguṭṭha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_426:5.4",
        "original": "sāmhi vehāyasāgatā”.",
        "analyzed": "sāmhi vehāyasā āgatā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_426:6.1",
        "original": "“Tañca disvāna āyantiṁ,",
        "analyzed": "tat ca disvānaḥ āyanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_426:6.2",
        "original": "antalikkhasmi eḷike;",
        "analyzed": "antalikkhasmi eḷike ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_426:6.3",
        "original": "Migasaṅgho palāyittha,",
        "analyzed": "miga saṅghaḥ palāyittha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_426:6.4",
        "original": "bhakkho me nāsito tayā”.",
        "analyzed": "bhakkhaḥ me na āsito tayā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_426:7.1",
        "original": "“Iccevaṁ vilapantiyā,",
        "analyzed": "iccā ivam vilapantiyā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_426:7.2",
        "original": "eḷakiyā ruhagghaso;",
        "analyzed": "eḷakiyāḥ ruhak hasaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_426:7.3",
        "original": "Galakaṁ anvāvamaddi,",
        "analyzed": "galakam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_426:7.4",
        "original": "natthi duṭṭhe subhāsitaṁ.",
        "analyzed": "na atthi duṭṭhe subhāsita ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_426:8.1",
        "original": "Neva duṭṭhe nayo atthi,",
        "analyzed": "na iva duṭṭhe nayaḥ atthi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_426:8.2",
        "original": "Na dhammo na subhāsitaṁ;",
        "analyzed": "na dhammaḥ na ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_426:8.3",
        "original": "Nikkamaṁ duṭṭhe yuñjetha,",
        "analyzed": "nikkamam duṭṭhe yuñjetha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_426:8.4",
        "original": "So ca sabbhiṁ na rañjatī”ti.",
        "analyzed": "saḥ ca sabbhim na rañjati iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_426:9.1",
        "original": "Dīpijātakaṁ dasamaṁ.",
        "analyzed": "dīpi jātakam dasama ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_426:10.1",
        "original": "Aṭṭhakanipātaṁ niṭṭhitaṁ.",
        "analyzed": "aṭṭhaka nipātam niṭṭhita ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_426:11.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_426:12.1",
        "original": "Parisuddhā manāvilā vatthadharā,",
        "analyzed": "parisuddhā manāvilā vattha dharā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_426:12.2",
        "original": "Bakarājassa kāyuraṁ daṇḍavaro;",
        "analyzed": "bakarājassa kāyuram daṇḍavaraḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_426:12.3",
        "original": "Atha aṅgāra cetiya devilinā,",
        "analyzed": "atha aṅgāra cetiya devilinā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_426:12.4",
        "original": "Atha āditta gaṅgā daseḷakināti.",
        "analyzed": "atha āditta gaṅgā daseḍakināti ",
        "folio": "12"
    }
]