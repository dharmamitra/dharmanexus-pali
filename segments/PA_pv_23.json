    },
    {
        "segmentnr": "PA_pv_23:0.3",
        "original": "11. Suttapetavatthu",
        "analyzed": "sutta petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_23:1.1",
        "original": "“Ahaṁ pure pabbajitassa bhikkhuno,",
        "analyzed": "aham pure pabbajitassa bhikkhuno ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_23:1.2",
        "original": "Suttaṁ adāsiṁ upasaṅkamma yācitā;",
        "analyzed": "suttam adāsim upasaṅkamma yācitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_23:1.3",
        "original": "Tassa vipāko vipulaphalūpalabbhati,",
        "analyzed": "tassa vipākaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_23:1.4",
        "original": "Bahukā ca me uppajjare vatthakoṭiyo.",
        "analyzed": "bahukā ca me uppajjare vattha koṭiyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_23:2.1",
        "original": "Pupphābhikiṇṇaṁ ramitaṁ vimānaṁ,",
        "analyzed": "puppha abhikiṇṇam ramitam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_23:2.2",
        "original": "Anekacittaṁ naranārisevitaṁ;",
        "analyzed": "aneka cittam nara nāri sevita ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_23:2.3",
        "original": "Sāhaṁ bhuñjāmi ca pārupāmi ca,",
        "analyzed": "sā aham bhuñjāmi ca pārupāmi ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_23:2.4",
        "original": "Pahūtavittā na ca tāva khīyati.",
        "analyzed": "pahūta vittā na ca tāva khīyati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_23:3.1",
        "original": "Tasseva kammassa vipākamanvayā,",
        "analyzed": "tassa iva kammassa vipākam anvayā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_23:3.2",
        "original": "Sukhañca sātañca idhūpalabbhati;",
        "analyzed": "sukham ca sātam ca idhu upalabbhati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_23:3.3",
        "original": "Sāhaṁ gantvā punadeva mānusaṁ,",
        "analyzed": "sā aham gantvā punar eva mānusa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_23:3.4",
        "original": "Kāhāmi puññāni nayayyaputta man”ti.",
        "analyzed": "puññāni nayayya putta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_23:4.1",
        "original": "“Satta tuvaṁ vassasatā idhāgatā,",
        "analyzed": "satta tuvam vassa satā idha āgatā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_23:4.2",
        "original": "Jiṇṇā ca vuḍḍhā ca tahiṁ bhavissasi;",
        "analyzed": "jiṇṇā ca vuḍḍhā ca tahim bhavissasi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_23:4.3",
        "original": "Sabbeva te kālakatā ca ñātakā,",
        "analyzed": "sabbe iva te kālakatā ca ñātakā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_23:4.4",
        "original": "Kiṁ tattha gantvāna ito karissasī”ti.",
        "analyzed": "kim tattha gantvānaḥ itas karissasi iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_23:5.1",
        "original": "“Satteva vassāni idhāgatāya me,",
        "analyzed": "satta iva vassāni idha āgatāya me ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_23:5.2",
        "original": "Dibbañca sukhañca samappitāya;",
        "analyzed": "dibbañca sukhañca samappitāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_23:5.3",
        "original": "Sāhaṁ gantvāna punadeva mānusaṁ,",
        "analyzed": "sā aham punar deva mānusa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_23:5.4",
        "original": "Kāhāmi puññāni nayayyaputta man”ti.",
        "analyzed": "puññāni nayayya putta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_23:6.1",
        "original": "So taṁ gahetvāna pasayha bāhāyaṁ,",
        "analyzed": "saḥ tam gahetvāna bāhāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_23:6.2",
        "original": "Paccānayitvāna theriṁ sudubbalaṁ;",
        "analyzed": "paccānayitvāna therim su dubbala ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_23:6.3",
        "original": "“Vajjesi aññampi janaṁ idhāgataṁ,",
        "analyzed": "vajjesi aññam pi janam idha āgata ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_23:6.4",
        "original": "‘Karotha puññāni sukhūpalabbhati’.",
        "analyzed": "karotha puññāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_23:7.1",
        "original": "Diṭṭhā mayā akatena sādhunā,",
        "analyzed": "diṭṭhā mayā a katena sādhunā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_23:7.2",
        "original": "Petā vihaññanti tatheva manussā;",
        "analyzed": "petā vihaññanti tathā iva manussā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_23:7.3",
        "original": "Kammañca katvā sukhavedanīyaṁ,",
        "analyzed": "kamman ca katvā sukha vedanīya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_23:7.4",
        "original": "Devā manussā ca sukhe ṭhitā pajā”ti.",
        "analyzed": "devā manussā ca sukhe ṭhitā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_23:8.1",
        "original": "Suttapetavatthu ekādasamaṁ.",
        "analyzed": "sutta petavatthu ekādasama ",
        "folio": "8"
    }
]