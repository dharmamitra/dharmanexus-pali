        "analyzed": "bhāvitattaḥ nāma bhikkhave paccekasambuddhaḥ imasmin isigilismim pabbate cira nivāsī ahosi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_mn_116:5.1",
        "original": "Ye sattasārā anīghā nirāsā,",
        "analyzed": "ye satta sārāḥ anīghāḥ nirāsā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:5.2",
        "original": "Paccekamevajjhagamaṁsu bodhiṁ;",
        "analyzed": "bodhi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:5.3",
        "original": "Tesaṁ visallāna naruttamānaṁ,",
        "analyzed": "tesam visallāna naruttamāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:5.4",
        "original": "Nāmāni me kittayato suṇātha.",
        "analyzed": "nāmāni me kittayataḥ suṇātha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:5.5",
        "original": "Ariṭṭho upariṭṭho tagarasikhī yasassī,",
        "analyzed": "ariṭṭhaḥ upariṭṭhaḥ tagarasikhī yasassī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:5.6",
        "original": "Sudassano piyadassī ca susambuddho;",
        "analyzed": "sudassanaḥ piya dassī ca su sambuddhaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:5.7",
        "original": "Gandhāro piṇḍolo upāsabho ca,",
        "analyzed": "gandhāraḥ piṇḍolaḥ upāsabhaḥ ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:5.8",
        "original": "Nīto tatho sutavā bhāvitatto.",
        "analyzed": "nītaḥ tatho sutavā bhāvitatto ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_mn_116:6.1",
        "original": "Sumbho subho matulo aṭṭhamo ca,",
        "analyzed": "sumbhaḥ subhaḥ matulaḥ aṭṭhamo ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.2",
        "original": "Athassumegho anīgho sudāṭho;",
        "analyzed": "athassu meghaḥ anīghaḥ su dāṭhaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.3",
        "original": "Paccekabuddhā bhavanettikhīṇā,",
        "analyzed": "paccekabuddhāḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.4",
        "original": "Hiṅgū ca hiṅgo ca mahānubhāvā.",
        "analyzed": "hiṅgū ca hiṅgo ca mahā anubhāvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.5",
        "original": "Dve jālino munino aṭṭhako ca,",
        "analyzed": "dve jālinaḥ muninaḥ ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.6",
        "original": "Atha kosallo buddho atho subāhu;",
        "analyzed": "atha kosallaḥ buddhaḥ atho subāhu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.7",
        "original": "Upanemiso nemiso santacitto,",
        "analyzed": "upanemisaḥ nemisaḥ santa cittaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.8",
        "original": "Sacco tatho virajo paṇḍito ca.",
        "analyzed": "sacco virajaḥ paṇḍito ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.9",
        "original": "Kāḷūpakāḷā vijito jito ca,",
        "analyzed": "kāḷūpakāḷāḥ vijitaḥ jitaḥ ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.10",
        "original": "Aṅgo ca paṅgo ca guttijito ca;",
        "analyzed": "aṅgo ca paṅgo ca gutti jito ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.11",
        "original": "Passi jahi upadhidukkhamūlaṁ,",
        "analyzed": "passi jahi upadhi dukkha mūla ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.12",
        "original": "Aparājito mārabalaṁ ajesi.",
        "analyzed": "aparājitaḥ māra balam ajesi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.13",
        "original": "Satthā pavattā sarabhaṅgo lomahaṁso,",
        "analyzed": "satthā pavattā sarabhaṅgaḥ lomahasaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.14",
        "original": "Uccaṅgamāyo asito anāsavo;",
        "analyzed": "uccaṅga māyaḥ asitaḥ anāsavaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.15",
        "original": "Manomayo mānacchido ca bandhumā,",
        "analyzed": "manaḥ mayaḥ ca bandhumā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.16",
        "original": "Tadādhimutto vimalo ca ketumā.",
        "analyzed": "tadā adhimuttaḥ vimalaḥ ca ketumā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.17",
        "original": "Ketumbharāgo ca mātaṅgo ariyo,",
        "analyzed": "ketumbha rāgaḥ ca mātaṅgaḥ ariyaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.18",
        "original": "Athaccuto accutagāmabyāmako;",
        "analyzed": "athaccutaḥ accuta gāma byāmakaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.19",
        "original": "Sumaṅgalo dabbilo supatiṭṭhito,",
        "analyzed": "su maṅgalaḥ dabbilaḥ su patiṭṭhito ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.20",
        "original": "Asayho khemābhirato ca sorato.",
        "analyzed": "a sayhaḥ khema abhirataḥ ca sorataḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.21",
        "original": "Durannayo saṅgho athopi ujjayo,",
        "analyzed": "saṅghaḥ ujjayaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.22",
        "original": "Aparo muni sayho anomanikkamo;",
        "analyzed": "aparaḥ muni sayhaḥ anomanikkamaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.23",
        "original": "Ānando nando upanando dvādasa,",
        "analyzed": "ānandaḥ nandaḥ upanandaḥ dvādasa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.24",
        "original": "Bhāradvājo antimadehadhārī.",
        "analyzed": "bhāradvājaḥ antima dehadhārī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.25",
        "original": "Bodhi mahānāmo athopi uttaro,",
        "analyzed": "bodhi mahānāmaḥ uttaro ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.26",
        "original": "Kesī sikhī sundaro dvārabhājo;",
        "analyzed": "kesī sikhī sundaraḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.27",
        "original": "Tissūpatissā bhavabandhanacchidā,",
        "analyzed": "tissu upatissā bhava bandhana chidā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.28",
        "original": "Upasikhi taṇhacchido ca sikhari.",
        "analyzed": "upa sikhi taṇha chidaḥ ca sikhari ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.29",
        "original": "Buddho ahu maṅgalo vītarāgo,",
        "analyzed": "buddhaḥ ahu maṅgalaḥ vīta rāgaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.30",
        "original": "Usabhacchidā jāliniṁ dukkhamūlaṁ;",
        "analyzed": "usabha chidā jālinim dukkha mūla ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_mn_116:6.31",
