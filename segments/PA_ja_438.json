    },
    {
        "segmentnr": "PA_ja_438:0.4",
        "original": "12. Daddarajātaka",
        "analyzed": "daddarajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_438:1.1",
        "original": "“Yo te puttake akhādi,",
        "analyzed": "yaḥ te puttake akhādi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_438:1.2",
        "original": "dinnabhatto adūsake;",
        "analyzed": "dinna bhattaḥ a dūsake ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_438:1.3",
        "original": "Tasmiṁ dāṭhaṁ nipātehi,",
        "analyzed": "tasmin ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_438:1.4",
        "original": "mā te muccittha jīvato”.",
        "analyzed": "mā te muccittha jīvataḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_438:2.1",
        "original": "“Ākiṇṇaluddo puriso,",
        "analyzed": "ākiṇṇa luddaḥ purisaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_438:2.2",
        "original": "dhāticelaṁva makkhito;",
        "analyzed": "makkhito ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_438:2.3",
        "original": "Padesaṁ taṁ na passāmi,",
        "analyzed": "padesam tam na passāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_438:2.4",
        "original": "yattha dāṭhaṁ nipātaye.",
        "analyzed": "yattha dāṭham nipātaye ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_438:3.1",
        "original": "Akataññussa posassa,",
        "analyzed": "akataññussa posassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_438:3.2",
        "original": "niccaṁ vivaradassino;",
        "analyzed": "niccam vivara dassinaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_438:3.3",
        "original": "Sabbañce pathaviṁ dajjā,",
        "analyzed": "sabbañce pathavim dajjā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_438:3.4",
        "original": "neva naṁ abhirādhaye”.",
        "analyzed": "na iva nam abhirādhaye ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_438:4.1",
        "original": "“Kiṁ nu subāhu taramānarūpo,",
        "analyzed": "kim nu su bāhu taramāna rūpaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_438:4.2",
        "original": "Paccāgatosi saha māṇavena;",
        "analyzed": "paccāgataḥ asi saha māṇavena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_438:4.3",
        "original": "Kiṁ kiccamatthaṁ idhamatthi tuyhaṁ,",
        "analyzed": "kim kiccam attham idham atthi tuyha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_438:4.4",
        "original": "Akkhāhi me pucchito etamatthaṁ”.",
        "analyzed": "akkhāhi me pucchitaḥ etam attha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_438:5.1",
        "original": "“Yo te sakhā daddaro sādhurūpo,",
        "analyzed": "yaḥ te sakhā daddaraḥ sādhu rūpaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_438:5.2",
        "original": "Tassa vadhaṁ parisaṅkāmi ajja;",
        "analyzed": "tassa vadham ajja ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_438:5.3",
        "original": "Purisassa kammāyatanāni sutvā,",
        "analyzed": "purisassa kamma āyatanāni sutvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_438:5.4",
        "original": "Nāhaṁ sukhiṁ daddaraṁ ajja maññe”.",
        "analyzed": "na aham sukhim daddaram ajja maññe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_438:6.1",
        "original": "“Kānissa kammāyatanāni assu,",
        "analyzed": "kānissa kamma āyatanāni assu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_438:6.2",
        "original": "Purisassa vuttisamodhānatāya;",
        "analyzed": "purisassa vutti samodhāna tāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_438:6.3",
        "original": "Kaṁ vā paṭiññaṁ purisassa sutvā,",
        "analyzed": "kam vā paṭiññam purisassa sutvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_438:6.4",
        "original": "Parisaṅkasi daddaraṁ māṇavena”.",
        "analyzed": "parisaṅkasi daddaram māṇavena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_438:7.1",
        "original": "“Ciṇṇā kaliṅgā caritā vaṇijjā,",
        "analyzed": "ciṇṇā kaliṅgā caritā vaṇijjā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_438:7.2",
        "original": "Vettācaro saṅkupathopi ciṇṇo;",
        "analyzed": "vettā caraḥ saṅku pathaḥ pi ciṇṇaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_438:7.3",
        "original": "Naṭehi ciṇṇaṁ saha vākurehi,",
        "analyzed": "naṭehi ciṇṇam saha vākurehi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_438:7.4",
        "original": "Daṇḍena yuddhampi samajjamajjhe.",
        "analyzed": "daṇḍena yuddham pi sa majja majjhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_438:8.1",
        "original": "Baddhā kulīkā mitamāḷhakena,",
        "analyzed": "baddhā kulīkā mita māḍhakena ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_438:8.2",
        "original": "Akkhā jitā saṁyamo abbhatīto;",
        "analyzed": "akkhā jitā sayamaḥ abbhatītaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_438:8.3",
        "original": "Abbāhitaṁ pubbakaṁ aḍḍharattaṁ,",
        "analyzed": "abbāhitam pubbakam aḍḍharatta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_438:8.4",
        "original": "Hatthā daḍḍhā piṇḍapaṭiggahena.",
        "analyzed": "hatthā daḍḍhā piṇḍapaṭiggahena ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_438:9.1",
        "original": "Tānissa kammāyatanāni assu,",
        "analyzed": "tāni issa kamma āyatanāni assu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_438:9.2",
        "original": "Purisassa vuttisamodhānatāya;",
        "analyzed": "purisassa vutti samodhāna tāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_438:9.3",
        "original": "Yathā ayaṁ dissati lomapiṇḍo,",
        "analyzed": "yathā ayam dissati loma piṇḍaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_438:9.4",
        "original": "Gāvo hatā kiṁ pana daddarassā”ti.",
        "analyzed": "gāvaḥ hatāḥ kim pana daddarassa ati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_438:10.1",
        "original": "Daddarajātakaṁ dvādasamaṁ.",
        "analyzed": "daddarajātakam dvādasama ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_438:11.1",
        "original": "Navakanipātaṁ niṭṭhitaṁ.",
        "analyzed": "navaka nipātam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_438:12.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_438:13.1",
        "original": "Varagijjha samajjana haṁsavaro,",
        "analyzed": "varagijjha samajjana hasavaro ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_438:13.2",
        "original": "Nidhisavhaya hārita pāṭaliko;",
        "analyzed": "hārita ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_438:13.3",
        "original": "Ajarāmara dhaṅka titikkha kuto,",
        "analyzed": "ajara amara dhaṅka titikkha kutas ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_438:13.4",
        "original": "Atha dvādasa pekkhana daddaribhīti.",
        "analyzed": "atha dvādasa pekkhana daddaribhi iti ",
        "folio": "13"
    }
]