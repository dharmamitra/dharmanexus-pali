    },
    {
        "segmentnr": "PA_iti_90:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_90:2.1",
        "original": "“Tayome, bhikkhave, aggappasādā.",
        "analyzed": "tayo me bhikkhave agga ppasādā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_90:2.2",
        "original": "Katame tayo?",
        "analyzed": "katame tayaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_90:2.3_0",
        "original": "Yāvatā, bhikkhave, sattā apadā vā dvipadā vā catuppadā vā bahuppadā vā rūpino vā",
        "analyzed": "yāvatā bhikkhave sattāḥ apadāḥ vā dvipadāḥ vā catuppadāḥ vā bahuppadāḥ vā rūpinaḥ vā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_90:2.3_1",
        "original": "arūpino vā saññino vā asaññino vā nevasaññināsaññino vā, tathāgato tesaṁ aggamakkhāyati",
        "analyzed": "arūpinaḥ vā saññinaḥ vā asaññinaḥ vā nevasaññinā asaññinaḥ vā tathāgataḥ tesam aggam akkhāyati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_90:2.3_2",
        "original": "arahaṁ sammāsambuddho.",
        "analyzed": "araham sammāsambuddhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_90:2.4",
        "original": "Ye, bhikkhave, buddhe pasannā, agge te pasannā.",
        "analyzed": "ye bhikkhave buddhe pasannāḥ agge te pasannāḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_90:2.5",
        "original": "Agge kho pana pasannānaṁ aggo vipāko hoti.",
        "analyzed": "agge kho pana pasannānam aggaḥ vipākaḥ hoti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_90:3.1_0",
        "original": "Yāvatā, bhikkhave, dhammā saṅkhatā vā asaṅkhatā vā, virāgo tesaṁ aggamakkhāyati,",
        "analyzed": "yāvatā bhikkhave dhammā saṅkhatāḥ vā asaṅkhatāḥ vā virāgo tesam aggam akkhāyati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_90:3.1_1",
        "original": "yadidaṁ madanimmadano pipāsavinayo ālayasamugghāto vaṭṭupacchedo taṇhakkhayo virāgo nirodho nibbānaṁ.",
        "analyzed": "yat idam mada nimmadanaḥ pipāsa vinayaḥ ālaya samugghātaḥ vaṭṭa upacchedo taṇha khayaḥ virāgaḥ nirodhaḥ nibbāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_90:3.2",
        "original": "Ye, bhikkhave, virāge dhamme pasannā, agge te pasannā.",
        "analyzed": "ye bhikkhave virāge dhamme pasannāḥ agge te pasannā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_90:3.3",
        "original": "Agge kho pana pasannānaṁ aggo vipāko hoti.",
        "analyzed": "agge kho pana pasannānam aggaḥ vipākaḥ hoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_90:4.1_0",
        "original": "Yāvatā, bhikkhave, saṅghā vā gaṇā vā, tathāgatasāvakasaṅgho tesaṁ aggamakkhāyati,",
        "analyzed": "yāvatā bhikkhave saṅghāḥ vā gaṇāḥ vā tathāgata sāvaka saṅghaḥ tesam aggam akkhāyati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:4.1_1",
        "original": "yadidaṁ cattāri purisayugāni aṭṭha purisapuggalā esa bhagavato sāvakasaṅgho āhuneyyo",
        "analyzed": "yat idam cattāri purisa yugāni aṭṭha purisa puggalāḥ esa bhagavataḥ sāvaka saṅghaḥ āhuneyyaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:4.1_2",
        "original": "pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṁ puññakkhettaṁ lokassa.",
        "analyzed": "pāhuneyyaḥ dakkhiṇeyyaḥ añjali karaṇīyaḥ anuttaram puññakkhettam lokassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:4.2",
        "original": "Ye, bhikkhave, saṅghe pasannā, agge te pasannā.",
        "analyzed": "ye bhikkhave saṅghe pasannāḥ agge te pasannā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:4.3",
        "original": "Agge kho pana pasannānaṁ aggo vipāko hoti.",
        "analyzed": "agge kho pana pasannānam aggaḥ vipākaḥ hoti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:4.4",
        "original": "Ime kho, bhikkhave, tayo aggappasādā”ti.",
        "analyzed": "ime kho bhikkhave tayo agga ppasādā ti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:4.5",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:4.6",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_90:5.1",
        "original": "“Aggato ve pasannānaṁ,",
