[
    {
        "segmentnr": "PA_snp_1.11:0.1",
        "original": "Sutta Nipāta 1.11",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.11:0.2",
        "original": "Vijayasutta",
        "analyzed": "vijayasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.11:1.1",
        "original": "Caraṁ vā yadi vā tiṭṭhaṁ,",
        "analyzed": "caram vā yadi vā tiṭṭha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.11:1.2",
        "original": "nisinno uda vā sayaṁ;",
        "analyzed": "nisinnaḥ uda vā saya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.11:1.3",
        "original": "Samiñjeti pasāreti,",
        "analyzed": "samiñja iti pasāra iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.11:1.4",
        "original": "esā kāyassa iñjanā.",
        "analyzed": "esā kāyassa iñjanā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.11:2.1",
        "original": "Aṭṭhinahārusaṁyutto,",
        "analyzed": "aṭṭhi nahāru sayuttaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.11:2.2",
        "original": "tacamaṁsāvalepano;",
        "analyzed": "tad camasa avalepanā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.11:2.3",
        "original": "Chaviyā kāyo paṭicchanno,",
        "analyzed": "chaviyā kāyo paṭicchanno ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.11:2.4",
        "original": "yathābhūtaṁ na dissati.",
        "analyzed": "yathābhūtam na dissati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.11:3.1",
        "original": "Antapūro udarapūro,",
        "analyzed": "anta pūro udara pūro ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.11:3.2",
        "original": "yakanapeḷassa vatthino;",
        "analyzed": "yakana peḷassa vatthinaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.11:3.3",
        "original": "Hadayassa papphāsassa,",
        "analyzed": "hadayassa papphāsassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.11:3.4",
        "original": "vakkassa pihakassa ca.",
        "analyzed": "vakkassa pihakassa ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.11:4.1",
        "original": "Siṅghāṇikāya kheḷassa,",
        "analyzed": "siṅghāṇikāya kheḷassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.11:4.2",
        "original": "sedassa ca medassa ca;",
        "analyzed": "sedassa ca medassa ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.11:4.3",
        "original": "Lohitassa lasikāya,",
        "analyzed": "lohitassa lasikāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.11:4.4",
        "original": "pittassa ca vasāya ca.",
        "analyzed": "pittassa ca vasāya ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.11:5.1",
        "original": "Athassa navahi sotehi,",
        "analyzed": "athassa navahi sotehi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.11:5.2",
        "original": "asucī savati sabbadā;",
        "analyzed": "asucī savati sabbadā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.11:5.3",
        "original": "Akkhimhā akkhigūthako,",
        "analyzed": "akkhimhā akkhigūthakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.11:5.4",
        "original": "kaṇṇamhā kaṇṇagūthako.",
        "analyzed": "kaṇṇamhā kaṇṇagūthakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.11:6.1",
        "original": "Siṅghāṇikā ca nāsato,",
        "analyzed": "siṅghāṇikā ca nāsato ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.11:6.2",
        "original": "mukhena vamatekadā;",
        "analyzed": "mukhena vamatekadā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.11:6.3",
        "original": "Pittaṁ semhañca vamati,",
        "analyzed": "pittam semhan ca vamati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.11:6.4",
        "original": "kāyamhā sedajallikā.",
        "analyzed": "sedajallikā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.11:7.1",
        "original": "Athassa susiraṁ sīsaṁ,",
        "analyzed": "su siram ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.11:7.2",
        "original": "matthaluṅgassa pūritaṁ;",
        "analyzed": "mattha luṅgassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.11:7.3",
        "original": "Subhato naṁ maññati bālo,",
        "analyzed": "subhataḥ nam maññati bālaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.11:7.4",
        "original": "avijjāya purakkhato.",
        "analyzed": "avijjāya purakkhataḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.11:8.1",
        "original": "Yadā ca so mato seti,",
        "analyzed": "yadā ca saḥ mataḥ sā iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.11:8.2",
        "original": "Uddhumāto vinīlako;",
        "analyzed": "vinīlakaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.11:8.3",
        "original": "Apaviddho susānasmiṁ,",
        "analyzed": "apaviddhaḥ su sānasmi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.11:8.4",
        "original": "Anapekkhā honti ñātayo.",
        "analyzed": "anapekkhāḥ honti ñātayaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.11:9.1",
        "original": "Khādanti naṁ suvānā ca,",
        "analyzed": "khādanti nam suvānā ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.11:9.2",
        "original": "siṅgālā ca vakā kimī;",
        "analyzed": "siṅgālā ca vakā kimī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.11:9.3",
        "original": "Kākā gijjhā ca khādanti,",
        "analyzed": "kākāḥ gijjhā ca khādanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.11:9.4",
        "original": "ye caññe santi pāṇino.",
        "analyzed": "ye caññe santi pāṇinaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.11:10.1",
        "original": "Sutvāna buddhavacanaṁ,",
        "analyzed": "sutvāna buddha vacana ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.11:10.2",
        "original": "Bhikkhu paññāṇavā idha;",
        "analyzed": "bhikkhu paññāṇavā idha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.11:10.3",
        "original": "So kho naṁ parijānāti,",
        "analyzed": "saḥ khalu nam parijānāti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.11:10.4",
        "original": "Yathābhūtañhi passati.",
        "analyzed": "yathābhūtam hi passati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.11:11.1",
        "original": "Yathā idaṁ tathā etaṁ,",
        "analyzed": "yathā idam tathā eta ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.11:11.2",
        "original": "Yathā etaṁ tathā idaṁ;",
        "analyzed": "yathā etam tathā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.11:11.3",
        "original": "Ajjhattañca bahiddhā ca,",
        "analyzed": "ajjhattañca bahiddhā ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.11:11.4",
        "original": "Kāye chandaṁ virājaye.",
        "analyzed": "kāye chandam virājaye ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.11:12.1",
        "original": "Chandarāgaviratto so,",
        "analyzed": "chanda rāga virattaḥ saḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.11:12.2",
        "original": "Bhikkhu paññāṇavā idha;",
        "analyzed": "bhikkhu paññāṇavā idha ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.11:12.3",
        "original": "Ajjhagā amataṁ santiṁ,",
        "analyzed": "ajjhagāḥ amatam santi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.11:12.4",
        "original": "Nibbānaṁ padamaccutaṁ.",
        "analyzed": "nibbānam padamat cuta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.11:13.1",
        "original": "Dvipādakoyaṁ asuci,",
        "analyzed": "asuci ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.11:13.2",
        "original": "Duggandho parihīrati;",
        "analyzed": "duggandhaḥ parihīrati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.11:13.3",
        "original": "Nānākuṇapaparipūro,",
        "analyzed": "nānā kuṇapa paripūraḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.11:13.4",
        "original": "Vissavanto tato tato.",
        "analyzed": "vissavanto tatas tatas ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.11:14.1",
        "original": "Etādisena kāyena,",
        "analyzed": "etādisena kāyena ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.11:14.2",
        "original": "Yo maññe uṇṇametave;",
        "analyzed": "yaḥ maññe uṇṇametave ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.11:14.3",
        "original": "Paraṁ vā avajāneyya,",
        "analyzed": "param vai avajāneyya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.11:14.4",
        "original": "Kimaññatra adassanāti.",
        "analyzed": "kim aññatra ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.11:14.5",
        "original": "Vijayasuttaṁ ekādasamaṁ.",
        "analyzed": "vijaya suttam ekādasama ",
        "folio": "14"
    }
]