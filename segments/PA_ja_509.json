        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_509:11.4",
        "original": "Sohaṁ kathaṁ na gaveseyyaṁ rāja.",
        "analyzed": "sā uham katham na gaveseyyam rāja ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_509:12.1",
        "original": "Hiyyoti hiyyati poso,",
        "analyzed": "hiyyoti hiyyati poso ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_509:12.2",
        "original": "Pareti parihāyati;",
        "analyzed": "pareti parihāyati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_509:12.3",
        "original": "Anāgataṁ netamatthīti ñatvā,",
        "analyzed": "anāgatam ñatvā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_509:12.4",
        "original": "Uppannachandaṁ ko panudeyya dhīro”.",
        "analyzed": "uppanna chandam kaḥ panudeyya dhīraḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_509:13.1",
        "original": "“Passāmi vohaṁ daharaṁ kumāriṁ,",
        "analyzed": "passāmi voham daharam kumāri ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_509:13.2",
