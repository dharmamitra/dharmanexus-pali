        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pd2-8:2.1.1.2",
        "original": "Tena kho pana samayena chabbaggiyā bhikkhuniyo telaṁ viññāpetvā bhuñjanti.",
        "analyzed": "tena kho pana samayena chabbaggiyāḥ bhikkhuniyaḥ telam viññāpetvā bhuñjanti ",
        "folio": "2"
    },
