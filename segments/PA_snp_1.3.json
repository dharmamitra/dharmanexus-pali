[
    {
        "segmentnr": "PA_snp_1.3:0.1",
        "original": "Sutta Nipāta 1.3",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.3:0.2",
        "original": "Khaggavisāṇasutta",
        "analyzed": "khagga visāṇa sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.3:1.1",
        "original": "“Sabbesu bhūtesu nidhāya daṇḍaṁ,",
        "analyzed": "sabbesu bhūtesu nidhāya daṇḍa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.3:1.2",
        "original": "Aviheṭhayaṁ aññatarampi tesaṁ;",
        "analyzed": "aviheṭhayam aññataram pi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.3:1.3",
        "original": "Na puttamiccheyya kuto sahāyaṁ,",
        "analyzed": "na puttam iccheyya kutas sahāya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.3:1.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.3:2.1",
        "original": "“Saṁsaggajātassa bhavanti snehā,",
        "analyzed": "sa sagga jātassa bhavanti snehāḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.3:2.2",
        "original": "Snehanvayaṁ dukkhamidaṁ pahoti;",
        "analyzed": "dukkham idam pahoti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.3:2.3",
        "original": "Ādīnavaṁ snehajaṁ pekkhamāno,",
        "analyzed": "ādīnavam sneha jam pekkhamāno ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.3:2.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.3:3.1",
        "original": "“Mitte suhajje anukampamāno,",
        "analyzed": "mitte su hajje anukampamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.3:3.2",
        "original": "Hāpeti atthaṁ paṭibaddhacitto;",
        "analyzed": "hāpeti attham paṭibaddha cittaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.3:3.3",
        "original": "Etaṁ bhayaṁ santhave pekkhamāno,",
        "analyzed": "etam bhayam santhave pekkhamāno ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.3:3.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.3:4.1",
        "original": "“Vaṁso visālova yathā visatto,",
        "analyzed": "vaso visālaḥ uva yathā visattaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.3:4.2",
        "original": "Puttesu dāresu ca yā apekkhā;",
        "analyzed": "puttesu dāresu ca yā apekkhā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.3:4.3",
        "original": "Vaṁsakkaḷīrova asajjamāno,",
        "analyzed": "vasakkaḷīraḥ iva a sajjamānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.3:4.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.3:5.1",
        "original": "“Migo araññamhi yathā abaddho,",
        "analyzed": "migaḥ araññamhi yathā a baddhaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.3:5.2",
        "original": "Yenicchakaṁ gacchati gocarāya;",
        "analyzed": "yenicchakam gacchati gocarāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.3:5.3",
        "original": "Viññū naro seritaṁ pekkhamāno,",
        "analyzed": "viññū naraḥ sa īritam pekkhamāno ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.3:5.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.3:6.1",
        "original": "“Āmantanā hoti sahāyamajjhe,",
        "analyzed": "āmantanā hoti sahāya majjhe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.3:6.2",
        "original": "Vāse ṭhāne gamane cārikāya;",
        "analyzed": "vāse ṭhāne gamane cārikāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.3:6.3",
        "original": "Anabhijjhitaṁ seritaṁ pekkhamāno,",
        "analyzed": "an abhijjhitam sa īritam pekkhamānaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.3:6.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.3:7.1",
        "original": "“Khiḍḍā ratī hoti sahāyamajjhe,",
        "analyzed": "khiḍḍā ratī hoti sahāya majjhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.3:7.2",
        "original": "Puttesu ca vipulaṁ hoti pemaṁ;",
        "analyzed": "puttesu ca vipulam hoti pema ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.3:7.3",
        "original": "Piyavippayogaṁ vijigucchamāno,",
        "analyzed": "piya vippayogam vijigucchamānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.3:7.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.3:8.1",
        "original": "“Cātuddiso appaṭigho ca hoti,",
        "analyzed": "cātuddisaḥ appaṭighaḥ ca hoti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.3:8.2",
        "original": "Santussamāno itarītarena;",
        "analyzed": "itarītarena ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.3:8.3",
        "original": "Parissayānaṁ sahitā achambhī,",
        "analyzed": "parissayānam sahitāḥ a chambhī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.3:8.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.3:9.1",
        "original": "“Dussaṅgahā pabbajitāpi eke,",
        "analyzed": "dussaṅgahā pabbajitā api eke ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.3:9.2",
        "original": "Atho gahaṭṭhā gharamāvasantā;",
        "analyzed": "atho gahaṭṭhā gharam āvasantā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.3:9.3",
        "original": "Appossukko paraputtesu hutvā,",
        "analyzed": "appossukkaḥ paraputtesu hutvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.3:9.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_1.3:10.1",
        "original": "“Oropayitvā gihibyañjanāni,",
        "analyzed": "āropayitvā gihi byañjanāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.3:10.2",
        "original": "Sañchinnapatto yathā koviḷāro;",
        "analyzed": "sañchinna pattaḥ yathā koviḍāraḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.3:10.3",
        "original": "Chetvāna vīro gihibandhanāni,",
        "analyzed": "chetvāna vīraḥ gihibandhanāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.3:10.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_1.3:11.1",
        "original": "“Sace labhetha nipakaṁ sahāyaṁ,",
        "analyzed": "sace labhetha nipakam sahāya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.3:11.2",
        "original": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
        "analyzed": "saddhim caram sādhu vihāri dhīra ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.3:11.3",
        "original": "Abhibhuyya sabbāni parissayāni,",
        "analyzed": "abhibhuyya sabbāni parissayāni ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.3:11.4",
        "original": "Careyya tenattamano satīmā”.",
        "analyzed": "careyya satī imā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_1.3:12.1",
        "original": "“No ce labhetha nipakaṁ sahāyaṁ,",
        "analyzed": "na u ce labhetha nipakam sahāya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.3:12.2",
        "original": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
        "analyzed": "saddhim caram sādhu vihāri dhīra ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.3:12.3",
        "original": "Rājāva raṭṭhaṁ vijitaṁ pahāya,",
        "analyzed": "rājā ava raṭṭham vijitam pahāya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.3:12.4",
        "original": "Eko care mātaṅgaraññeva nāgo”.",
        "analyzed": "ekaḥ care mātaṅga raññā iva nāgaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_1.3:13.1",
        "original": "“Addhā pasaṁsāma sahāyasampadaṁ,",
        "analyzed": "addhā pasasāma sahāya sampada ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.3:13.2",
        "original": "Seṭṭhā samā sevitabbā sahāyā;",
        "analyzed": "seṭṭhā samā sevitabbā sahāyā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.3:13.3",
        "original": "Ete aladdhā anavajjabhojī,",
        "analyzed": "ete a laddhā an avajja bhojī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.3:13.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_1.3:14.1",
        "original": "“Disvā suvaṇṇassa pabhassarāni,",
        "analyzed": "disvā suvaṇṇassa pabhassarāni ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.3:14.2",
        "original": "Kammāraputtena suniṭṭhitāni;",
        "analyzed": "kammāraputtena su niṭṭhitāni ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.3:14.3",
        "original": "Saṅghaṭṭamānāni duve bhujasmiṁ,",
        "analyzed": "saṅghaṭṭamānāni duve bhujasmi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.3:14.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_1.3:15.1",
        "original": "“Evaṁ dutīyena sahā mamassa,",
        "analyzed": "evam dutīyena sahā mamassa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.3:15.2",
        "original": "Vācābhilāpo abhisajjanā vā;",
        "analyzed": "vācā abhilāpaḥ abhisajjanā vā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.3:15.3",
        "original": "Etaṁ bhayaṁ āyatiṁ pekkhamāno,",
        "analyzed": "etam bhayam āyatim pekkhamāno ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.3:15.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_1.3:16.1",
        "original": "“Kāmā hi citrā madhurā manoramā,",
        "analyzed": "kāmā hi citrā madhurā manoramā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.3:16.2",
        "original": "Virūparūpena mathenti cittaṁ;",
        "analyzed": "virūpa rūpena mathenti citta ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.3:16.3",
        "original": "Ādīnavaṁ kāmaguṇesu disvā,",
        "analyzed": "ādīnavam kāma guṇesu disvā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.3:16.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_1.3:17.1",
        "original": "“Ītī ca gaṇḍo ca upaddavo ca,",
        "analyzed": "ītī ca gaṇḍo ca upaddavo ca ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.3:17.2",
        "original": "Rogo ca sallañca bhayañca metaṁ;",
        "analyzed": "rogo ca sallañca bhayañca mā ita ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.3:17.3",
        "original": "Etaṁ bhayaṁ kāmaguṇesu disvā,",
        "analyzed": "etam bhayam kāma guṇesu disvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.3:17.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_1.3:18.1",
        "original": "“Sītañca uṇhañca khudaṁ pipāsaṁ,",
        "analyzed": "sītam ca uṇham ca khudam pipāsa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_1.3:18.2",
        "original": "Vātātape ḍaṁsasarīsape ca;",
        "analyzed": "vāta ātape ḍasa sarīsape ca ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_1.3:18.3",
        "original": "Sabbānipetāni abhisambhavitvā,",
        "analyzed": "sabbānipetāni abhisambhavitvā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_1.3:18.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_1.3:19.1",
        "original": "“Nāgova yūthāni vivajjayitvā,",
        "analyzed": "nāgā uva yūthāni vivajjayitvā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_1.3:19.2",
        "original": "Sañjātakhandho padumī uḷāro;",
        "analyzed": "sañjāta khandhaḥ padumī uḷāraḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_1.3:19.3",
        "original": "Yathābhirantaṁ viharaṁ araññe,",
        "analyzed": "yathā abhirantam viharam araññe ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_1.3:19.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_1.3:20.1",
        "original": "“Aṭṭhānataṁ saṅgaṇikāratassa,",
        "analyzed": "a ṭṭhāna tam saṅgaṇikā ratassa ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_1.3:20.2",
        "original": "Yaṁ phassaye sāmayikaṁ vimuttiṁ;",
        "analyzed": "yam phassaye sāmayikam vimutti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_1.3:20.3",
        "original": "Ādiccabandhussa vaco nisamma,",
        "analyzed": "ādiccabandhussa vacaḥ nisamma ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_1.3:20.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_1.3:21.1",
        "original": "“Diṭṭhīvisūkāni upātivatto,",
        "analyzed": "diṭṭhīvisūkāni upātivatto ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_1.3:21.2",
        "original": "Patto niyāmaṁ paṭiladdhamaggo;",
        "analyzed": "pattaḥ niyāmam paṭiladdha maggaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_1.3:21.3",
        "original": "Uppannañāṇomhi anaññaneyyo,",
        "analyzed": "uppanna ñāṇaḥ umhi an añña neyyaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_1.3:21.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_snp_1.3:22.1",
        "original": "“Nillolupo nikkuho nippipāso,",
        "analyzed": "nillolupaḥ nikkuhaḥ nippipāsaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_1.3:22.2",
        "original": "Nimmakkho niddhantakasāvamoho;",
        "analyzed": "nimmakkhaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_1.3:22.3",
        "original": "Nirāsayo sabbaloke bhavitvā,",
        "analyzed": "nirāsayo sabba loke bhavitvā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_1.3:22.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_snp_1.3:23.1",
        "original": "“Pāpaṁ sahāyaṁ parivajjayetha,",
        "analyzed": "pāpam sahāyam parivajjayetha ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.3:23.2",
        "original": "Anatthadassiṁ visame niviṭṭhaṁ;",
        "analyzed": "anattha dassim visame niviṭṭha ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.3:23.3",
        "original": "Sayaṁ na seve pasutaṁ pamattaṁ,",
        "analyzed": "sayam na seve pasutam pamatta ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.3:23.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_snp_1.3:24.1",
        "original": "“Bahussutaṁ dhammadharaṁ bhajetha,",
        "analyzed": "bahu ssutam dhamma dharam bhajetha ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.3:24.2",
        "original": "Mittaṁ uḷāraṁ paṭibhānavantaṁ;",
        "analyzed": "mittam uḷāram ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.3:24.3",
        "original": "Aññāya atthāni vineyya kaṅkhaṁ,",
        "analyzed": "aññāya atthāni vineyya kaṅkha ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.3:24.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_snp_1.3:25.1",
        "original": "“Khiḍḍaṁ ratiṁ kāmasukhañca loke,",
        "analyzed": "khiḍḍam ratim kāma sukham ca loke ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.3:25.2",
        "original": "Analaṅkaritvā anapekkhamāno;",
        "analyzed": "an alaṅkaritvā an apekkhamānaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.3:25.3",
        "original": "Vibhūsanaṭṭhānā virato saccavādī,",
        "analyzed": "vibhūsana ṭṭhānā virataḥ sacca vādī ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.3:25.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_snp_1.3:26.1",
        "original": "“Puttañca dāraṁ pitarañca mātaraṁ,",
        "analyzed": "puttam ca dāram pitaram ca mātara ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.3:26.2",
        "original": "Dhanāni dhaññāni ca bandhavāni;",
        "analyzed": "dhanāni dhaññāni ca bandhavāni ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.3:26.3",
        "original": "Hitvāna kāmāni yathodhikāni,",
        "analyzed": "hitvāna kāmāni yathā udhikāni ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.3:26.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_snp_1.3:27.1",
        "original": "“Saṅgo eso parittamettha sokhyaṁ,",
        "analyzed": "saṅgaḥ sokhya ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.3:27.2",
        "original": "Appassādo dukkhamettha bhiyyo;",
        "analyzed": "appassādaḥ dukkham ettha bhiyyaḥ ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.3:27.3",
        "original": "Gaḷo eso iti ñatvā matīmā,",
        "analyzed": "iti ñatvā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.3:27.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_snp_1.3:28.1",
        "original": "“Sandālayitvāna saṁyojanāni,",
        "analyzed": "sandālayitvāna sa yojanāni ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.3:28.2",
        "original": "Jālaṁva bhetvā salilambucārī;",
        "analyzed": "bhetvā salilambu cārī ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.3:28.3",
        "original": "Aggīva daḍḍhaṁ anivattamāno,",
