        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_509:14.3",
        "original": "Hitvāna kāme paṭikacca gehaṁ,",
        "analyzed": "hitvāna kāme paṭikacca geha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_509:14.4",
        "original": "Anujāna maṁ pabbajissāmi deva”.",
        "analyzed": "anujāna mama pabbajissāmi deva ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_509:15.1",
        "original": "“Sākhāhi rukkho labhate samaññaṁ,",
        "analyzed": "sākhāhi rukkhaḥ labhate samañña ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_509:15.2",
