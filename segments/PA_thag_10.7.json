        "analyzed": "paṭhama vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.7:0.4",
        "original": "(Apara) Gotamattheragāthā",
        "analyzed": "aparā gotamatthera gāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.7:1.1",
        "original": "“Vijāneyya sakaṁ atthaṁ,",
        "analyzed": "vijāneyya sakam attha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.7:1.2",
        "original": "Avalokeyyātha pāvacanaṁ;",
        "analyzed": "avalokeyyā atha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.7:1.3",
        "original": "Yañcettha assa patirūpaṁ,",
        "analyzed": "yat ca itthā assa patirūpa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.7:1.4",
        "original": "Sāmaññaṁ ajjhupagatassa.",
        "analyzed": "sāmaññam ajjhupagatassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.7:2.1",
        "original": "Mittaṁ idha ca kalyāṇaṁ,",
        "analyzed": "mittam ca kalyāṇa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.7:2.2",
        "original": "Sikkhā vipulaṁ samādānaṁ;",
        "analyzed": "sikkha vipulam samādāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.7:2.3",
        "original": "Sussūsā ca garūnaṁ,",
        "analyzed": "sussūsā ca garūna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.7:2.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.7:3.1",
        "original": "Buddhesu sagāravatā,",
        "analyzed": "buddhesu sagārava tā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.7:3.2",
        "original": "Dhamme apaciti yathābhūtaṁ;",
        "analyzed": "dhamme apaciti yathābhūta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.7:3.3",
        "original": "Saṅghe ca cittikāro,",
        "analyzed": "saṅghe ca citti kāraḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.7:3.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.7:4.1",
        "original": "Ācāragocare yutto,",
        "analyzed": "ācāra gocare ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.7:4.2",
        "original": "Ājīvo sodhito agārayho;",
        "analyzed": "ājīvaḥ sodhitaḥ agārayhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.7:4.3",
        "original": "Cittassa ca saṇṭhapanaṁ,",
        "analyzed": "cittassa ca saṇṭhapana ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.7:4.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.7:5.1",
        "original": "Cārittaṁ atha vārittaṁ,",
        "analyzed": "cārittam atha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.7:5.2",
        "original": "Iriyāpathiyaṁ pasādaniyaṁ;",
        "analyzed": "iriyāpathiyam pasādaniya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.7:5.3",
        "original": "Adhicitte ca āyogo,",
        "analyzed": "adhicitte ca āyogaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.7:5.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.7:6.1",
        "original": "Āraññakāni senāsanāni,",
        "analyzed": "āraññakāni senāsanāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.7:6.2",
        "original": "Pantāni appasaddāni;",
        "analyzed": "pantāni appasaddāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.7:6.3",
        "original": "Bhajitabbāni muninā,",
        "analyzed": "bhajitabbāni muninā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.7:6.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.7:7.1",
        "original": "Sīlañca bāhusaccañca,",
        "analyzed": "sīlam ca bāhusaccañca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.7:7.2",
        "original": "Dhammānaṁ pavicayo yathābhūtaṁ;",
        "analyzed": "dhammānam pavicayaḥ yathā bhūta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.7:7.3",
        "original": "Saccānaṁ abhisamayo,",
        "analyzed": "abhisamayaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.7:7.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.7:8.1",
        "original": "Bhāveyya ca aniccanti,",
        "analyzed": "bhāveyya ca aniccanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.7:8.2",
        "original": "Anattasaññaṁ asubhasaññañca;",
        "analyzed": "anatta saññam asubha saññañca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.7:8.3",
        "original": "Lokamhi ca anabhiratiṁ,",
        "analyzed": "lokamhi ca anabhirati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.7:8.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.7:9.1",
        "original": "Bhāveyya ca bojjhaṅge,",
        "analyzed": "bhāveyya ca bojjhaṅge ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.7:9.2",
        "original": "Iddhipādāni indriyāni balāni;",
        "analyzed": "iddhipādāni indriyāni balāni ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.7:9.3",
        "original": "Aṭṭhaṅgamaggamariyaṁ,",
        "analyzed": "aṭṭhaṅga magga mariya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.7:9.4",
        "original": "Etaṁ samaṇassa patirūpaṁ.",
        "analyzed": "etam samaṇassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.7:10.1",
        "original": "Taṇhaṁ pajaheyya muni,",
        "analyzed": "taṇham pajaheyya muni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.7:10.2",
        "original": "Samūlake āsave padāleyya;",
        "analyzed": "sa mūlake āsave padāleyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.7:10.3",
        "original": "Vihareyya vippamutto,",
        "analyzed": "vihareyya vippamuttaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.7:10.4",
        "original": "Etaṁ samaṇassa patirūpan”ti.",
        "analyzed": "etam samaṇassa patirūpanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.7:10.5",
        "original": "… Gotamo thero …",
        "analyzed": "gotamaḥ theraḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.7:10.6",
        "original": "Dasakanipāto niṭṭhito.",
        "analyzed": "dasaka nipātaḥ niṭṭhito ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.7:11.0",
        "original": "Tatruddānaṁ",
        "analyzed": "tatra uddāna ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_10.7:11.1",
        "original": "Kāḷudāyī ca so thero,",
        "analyzed": "kāḷudāyī ca saḥ theraḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_10.7:11.2",
        "original": "ekavihārī ca kappino;",
        "analyzed": "eka vihārī ca kappino ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_10.7:11.3",
        "original": "Cūḷapanthako kappo ca,",
        "analyzed": "cūḍapanthako kappo ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thag_10.7:11.4",
        "original": "upaseno ca gotamo;",
        "analyzed": "upasenaḥ ca gotamaḥ ",
        "folio": "11"
    },
