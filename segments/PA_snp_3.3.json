        "original": "Saccaṁ ve amatā vācā,",
        "analyzed": "saccam veḥ amatā vācā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.3:7.2",
        "original": "esa dhammo sanantano;",
        "analyzed": "esa dhammo sanantanaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_3.3:7.3",
        "original": "Sacce atthe ca dhamme ca,",
        "analyzed": "sacce atthe ca dhamme ca ",
        "folio": "7"
    },
    {
