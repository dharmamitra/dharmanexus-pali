[
    {
        "segmentnr": "PA_ja_481:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_481:0.2",
        "original": "Terasakanipāta",
        "analyzed": "terasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_481:0.3",
        "original": "Ambavagga",
        "analyzed": "amba vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_481:0.4",
        "original": "8. Takkāriyajātaka",
        "analyzed": "takkāriyajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_481:1.1",
        "original": "“Ahameva dubbhāsitaṁ bhāsi bālo,",
        "analyzed": "aham eva dubbhāsitam bhāsi bālaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_481:1.2",
        "original": "Bhekovaraññe ahimavhāyamāno;",
        "analyzed": "bhekovaraññe ahimavhāyamāno ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_481:1.3",
        "original": "Takkāriye sobbhamimaṁ patāmi,",
        "analyzed": "takkāriye sobbham imam patāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_481:1.4",
        "original": "Na kireva sādhu ativelabhāṇī”.",
        "analyzed": "na kirā iva sādhu ativelabhāṇī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_481:2.1",
        "original": "“Pappoti macco ativelabhāṇī,",
        "analyzed": "maccaḥ ativela bhāṇī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_481:2.2",
        "original": "Bandhaṁ vadhaṁ sokapariddavañca;",
        "analyzed": "bandham vadham soka pariddavañca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_481:2.3",
        "original": "Attānameva garahāsi ettha,",
        "analyzed": "attānam eva garahā asi ettha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_481:2.4",
        "original": "Ācera yaṁ taṁ nikhaṇanti sobbhe”.",
        "analyzed": "ācera yam tam nikhaṇanti sa ubbhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_481:3.1",
        "original": "“Kimevahaṁ tuṇḍilamanupucchiṁ,",
        "analyzed": "kim eva aham tuṇḍilam anupucchi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_481:3.2",
        "original": "Kareyya saṁ bhātaraṁ kāḷikāyaṁ;",
        "analyzed": "kareyya sam bhātaram kāḷikāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_481:3.3",
        "original": "Naggovahaṁ vatthayugañca jīno,",
        "analyzed": "naggo vaham vattha yugañca jīno ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_481:3.4",
        "original": "Ayampi attho bahutādisova”.",
        "analyzed": "ayam pi atthaḥ bahu tādisaḥ uva ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_481:4.1",
        "original": "“Yo yujjhamānānamayujjhamāno,",
        "analyzed": "yaḥ yujjhamānānam a yujjhamānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_481:4.2",
        "original": "Meṇḍantaraṁ accupatī kuliṅgo;",
        "analyzed": "meṇḍantaram accupatī kuliṅgo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_481:4.3",
        "original": "So piṁsito meṇḍasirehi tattha,",
        "analyzed": "saḥ pisitaḥ meṇḍa sirehi tattha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_481:4.4",
        "original": "Ayampi attho bahutādisova.",
        "analyzed": "ayam pi atthaḥ bahu tādisaḥ uva ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_481:5.1",
        "original": "Caturo janā potthakamaggahesuṁ,",
        "analyzed": "caturaḥ janā potthaka magga hesu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_481:5.2",
        "original": "Ekañca posaṁ anurakkhamānā;",
        "analyzed": "ekam ca posam anurakkhamānā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_481:5.3",
        "original": "Sabbeva te bhinnasirā sayiṁsu,",
        "analyzed": "sabbā iva te bhinna sirāḥ sayisu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_481:5.4",
        "original": "Ayampi attho bahutādisova.",
        "analyzed": "ayam pi atthaḥ bahu tādisaḥ uva ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_481:6.1",
        "original": "Ajā yathā veḷugumbasmiṁ baddhā,",
        "analyzed": "ajā yathā baddhā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_481:6.2",
        "original": "Avakkhipantī asimajjhagacchi;",
        "analyzed": "avakkhipantī asim ajjhagacchi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_481:6.3",
        "original": "Teneva tassā galakāvakantaṁ,",
        "analyzed": "tena iva tassāḥ galakau akanta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_481:6.4",
        "original": "Ayampi attho bahutādisova”.",
        "analyzed": "ayam pi atthaḥ bahu tādisaḥ uva ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_481:7.1",
        "original": "“Ime na devā na gandhabbaputtā,",
        "analyzed": "ime na devāḥ na gandhabba puttā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_481:7.2",
        "original": "Migā ime atthavasaṁ gatā me;",
        "analyzed": "migāḥ ime atthavasam gatāḥ me ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_481:7.3",
        "original": "Ekañca naṁ sāyamāse pacantu,",
        "analyzed": "ekam ca nam sāyamāse pacantu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_481:7.4",
        "original": "Ekaṁ punappātarāse pacantu”.",
        "analyzed": "ekam punappāta rāse pacantu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_481:8.1",
        "original": "“Sataṁ sahassāni dubhāsitāni,",
        "analyzed": "satam sahassāni dubhāsitāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_481:8.2",
        "original": "Kalampi nāgghanti subhāsitassa;",
        "analyzed": "na agghanti subhāsitassa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_481:8.3",
        "original": "Dubbhāsitaṁ saṅkamāno kileso,",
        "analyzed": "dubbhāsitam saṅkamānaḥ kilesaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_481:8.4",
        "original": "Tasmā tuṇhī kimpurisā na bālyā”.",
        "analyzed": "tasmā tuṇhī kimpurisāḥ na bālyā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_481:9.1",
        "original": "“Yā mesā byāhāsi pamuñcathetaṁ,",
        "analyzed": "yā mesā byāhāsi pamuñcatha ita ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_481:9.2",
        "original": "Giriñca naṁ himavantaṁ nayantu;",
        "analyzed": "girin ca nam himavantam nayantu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_481:9.3",
        "original": "Imañca kho dentu mahānasāya,",
        "analyzed": "iman ca khaḥ dentu mahānasāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_481:9.4",
        "original": "Pātova naṁ pātarāse pacantu”.",
        "analyzed": "nam pātarāse pacantu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_481:10.1",
        "original": "“Pajjunnanāthā pasavo,",
        "analyzed": "pajjunnanāthā pasavaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_481:10.2",
        "original": "pasunāthā ayaṁ pajā;",
        "analyzed": "pasu nāthāḥ ayam pajā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_481:10.3",
        "original": "Tvaṁ nāthosi mahārāja,",
        "analyzed": "tvam nāthaḥ asi mahā rāja ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_481:10.4",
        "original": "nāthohaṁ bhariyāya me;",
        "analyzed": "bhariyāya me ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_481:10.5",
        "original": "Dvinnamaññataraṁ ñatvā,",
        "analyzed": "dvinnam aññataram ñatvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_481:10.6",
        "original": "mutto gaccheyya pabbataṁ.",
        "analyzed": "muttaḥ gaccheyya pabbata ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_481:11.1",
        "original": "Na ve nindā suparivajjayetha,",
        "analyzed": "na ve nindā su parivajjayetha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_481:11.2",
        "original": "Nānā janā sevitabbā janinda;",
        "analyzed": "nānā janā sevitabbā janinda ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_481:11.3",
        "original": "Yeneva eko labhate pasaṁsaṁ,",
        "analyzed": "yena iva ekaḥ labhate pasasa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_481:11.4",
        "original": "Teneva añño labhate ninditāraṁ.",
        "analyzed": "tena iva aññaḥ labhate ninditāra ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_481:12.1",
        "original": "Sabbo loko paricitto aticitto,",
        "analyzed": "sabbo lokaḥ paricittaḥ aticittaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_481:12.2",
        "original": "Sabbo loko cittavā samhi citte;",
        "analyzed": "sabbaḥ loko cittavā samhi citte ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_481:12.3",
        "original": "Paccekacittā puthu sabbasattā,",
        "analyzed": "paccekacittā puthu sabba sattā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_481:12.4",
        "original": "Kassīdha cittassa vasena vatte”.",
        "analyzed": "kassi idha cittassa vasena vatte ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_481:13.1",
        "original": "“Tuṇhī ahū kimpuriso sabhariyo,",
        "analyzed": "tuṇhī ahū kimpurisaḥ sa bhariyaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_481:13.2",
        "original": "Yo dāni byāhāsi bhayassa bhīto;",
        "analyzed": "yaḥ dāni byāhāsi bhayassa bhītaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_481:13.3",
        "original": "So dāni mutto sukhito arogo,",
        "analyzed": "saḥ dāni muttaḥ sukhitaḥ arogaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_481:13.4",
        "original": "Vācākirevatthavatī narānan”ti.",
        "analyzed": "vācākirevatthavatī narā ananti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_481:14.1",
        "original": "Takkāriyajātakaṁ aṭṭhamaṁ.",
        "analyzed": "takkāriya jātakam aṭṭhama ",
        "folio": "14"
    }
]