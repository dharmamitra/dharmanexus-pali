    },
    {
        "segmentnr": "PA_ja_472:0.4",
        "original": "9. Mahāpadumajātaka",
        "analyzed": "mahāpadumajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_472:1.1",
        "original": "“Nādaṭṭhā parato dosaṁ,",
        "analyzed": "na a daṭṭhā paratas dosa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_472:1.2",
        "original": "aṇuṁthūlāni sabbaso;",
        "analyzed": "aṇu thūlāni sabbaso ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_472:1.3",
        "original": "Issaro paṇaye daṇḍaṁ,",
        "analyzed": "issaraḥ paṇaye daṇḍa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_472:1.4",
        "original": "sāmaṁ appaṭivekkhiya.",
        "analyzed": "sāmam a paṭivekkhiya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_472:2.1",
        "original": "Yo ca appaṭivekkhitvā,",
        "analyzed": "yaḥ ca a paṭivekkhitvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_472:2.2",
        "original": "daṇḍaṁ kubbati khattiyo;",
        "analyzed": "daṇḍam kubbati khattiyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_472:2.3",
        "original": "Sakaṇṭakaṁ so gilati,",
        "analyzed": "sa kaṇṭakam saḥ gilati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_472:2.4",
        "original": "jaccandhova samakkhikaṁ.",
        "analyzed": "samakkhika ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_472:3.1",
        "original": "Adaṇḍiyaṁ daṇḍayati,",
        "analyzed": "adaṇḍiyam daṇḍayati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_472:3.2",
        "original": "daṇḍiyañca adaṇḍiyaṁ;",
        "analyzed": "daṇḍiyañca a daṇḍiya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_472:3.3",
        "original": "Andhova visamaṁ maggaṁ,",
        "analyzed": "andhaḥ uva visamam magga ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_472:3.4",
        "original": "na jānāti samāsamaṁ.",
        "analyzed": "na jānāti sama asama ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_472:4.1",
        "original": "Yo ca etāni ṭhānāni,",
        "analyzed": "yaḥ ca etāni ṭhānāni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_472:4.2",
        "original": "Aṇuṁthūlāni sabbaso;",
        "analyzed": "aṇu thūlāni sabbaso ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_472:4.3",
        "original": "Sudiṭṭhamanusāseyya,",
        "analyzed": "su diṭṭham anusāseyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_472:4.4",
        "original": "Sa ve voharitumarahati.",
        "analyzed": "sa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_472:5.1",
        "original": "Nekantamudunā sakkā,",
        "analyzed": "na ikantam udunā sakkā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_472:5.2",
        "original": "Ekantatikhiṇena vā;",
        "analyzed": "vā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_472:5.3",
        "original": "Attaṁ mahante ṭhapetuṁ,",
        "analyzed": "attam mahante ṭhapetu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_472:5.4",
        "original": "Tasmā ubhayamācare.",
        "analyzed": "tasmai ubhayam ācare ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_472:6.1",
        "original": "Paribhūto mudu hoti,",
        "analyzed": "paribhūtaḥ mudu hoti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_472:6.2",
        "original": "atitikkho ca veravā;",
        "analyzed": "atitikkhaḥ ca veravā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_472:6.3",
        "original": "Etañca ubhayaṁ ñatvā,",
        "analyzed": "etat ca ubhayam ñatvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_472:6.4",
        "original": "anumajjhaṁ samācare.",
        "analyzed": "anumajjham samācare ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_472:7.1",
        "original": "Bahumpi ratto bhāseyya,",
        "analyzed": "rattaḥ bhāseyya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_472:7.2",
        "original": "duṭṭhopi bahu bhāsati;",
        "analyzed": "duṭṭhaḥ api bahu bhāsati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_472:7.3",
        "original": "Na itthikāraṇā rāja,",
        "analyzed": "na itthi kāraṇā rāja ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_472:7.4",
        "original": "puttaṁ ghātetumarahasi”.",
        "analyzed": "puttam ghātetum arahasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_472:8.1",
        "original": "“Sabbova loko ekato,",
        "analyzed": "sabbaḥ uva lokaḥ ekatas ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_472:8.2",
        "original": "Itthī ca ayamekikā;",
        "analyzed": "itthī ca ayam ekikā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_472:8.3",
        "original": "Tenāhaṁ paṭipajjissaṁ,",
        "analyzed": "tena aham paṭipajjissa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_472:8.4",
        "original": "Gacchatha pakkhipatheva taṁ”.",
        "analyzed": "gacchatha pakkhipatha iva ta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_472:9.1",
        "original": "“Anekatāle narake,",
        "analyzed": "aneka tāle narake ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_472:9.2",
        "original": "gambhīre ca suduttare;",
        "analyzed": "gambhīre ca suduttare ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_472:9.3",
        "original": "Pātito giriduggasmiṁ,",
        "analyzed": "pātitaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_472:9.4",
        "original": "kena tvaṁ tattha nāmari”.",
        "analyzed": "kena tvam tattha nāmari ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_472:10.1",
        "original": "“Nāgo jātaphaṇo tattha,",
        "analyzed": "nāgaḥ jāta phaṇaḥ tattha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_472:10.2",
        "original": "thāmavā girisānujo;",
        "analyzed": "thāmavā girisānujaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_472:10.3",
        "original": "Paccaggahi maṁ bhogehi,",
        "analyzed": "paccaggahi mama bhogehi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_472:10.4",
        "original": "tenāhaṁ tattha nāmariṁ”.",
        "analyzed": "tena aham tattha nāmari ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_472:11.1",
        "original": "“Ehi taṁ paṭinessāmi,",
        "analyzed": "ehi tam paṭinessāmi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_472:11.2",
        "original": "rājaputta sakaṁ gharaṁ;",
        "analyzed": "rāja putta sakam ghara ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_472:11.3",
        "original": "Rajjaṁ kārehi bhaddante,",
        "analyzed": "rajjam kārehi bhaddante ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_472:11.4",
        "original": "kiṁ araññe karissasi”.",
        "analyzed": "kim araññe karissasi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_472:12.1",
        "original": "“Yathā gilitvā baḷisaṁ,",
        "analyzed": "yathā gilitvā baḷisa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_472:12.2",
        "original": "Uddhareyya salohitaṁ;",
        "analyzed": "uddhareyya sa lohita ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_472:12.3",
        "original": "Uddharitvā sukhī assa,",
        "analyzed": "uddharitvā sukhī assa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_472:12.4",
        "original": "Evaṁ passāmi attanaṁ”.",
        "analyzed": "evam passāmi attana ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_472:13.1",
        "original": "“Kiṁ nu tvaṁ baḷisaṁ brūsi,",
        "analyzed": "kim nu tvam baḷisam brūsi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_472:13.2",
        "original": "kiṁ tvaṁ brūsi salohitaṁ;",
        "analyzed": "kim tvam brūsi salohita ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_472:13.3",
        "original": "Kiṁ nu tvaṁ ubbhataṁ brūsi,",
        "analyzed": "kim nu tvam ubbhatam brūsi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_472:13.4",
        "original": "taṁ me akkhāhi pucchito”.",
        "analyzed": "tam me akkhāhi pucchitaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_472:14.1",
        "original": "“Kāmāhaṁ baḷisaṁ brūmi,",
        "analyzed": "kāma aham baḷisam brūmi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_472:14.2",
        "original": "hatthiassaṁ salohitaṁ;",
        "analyzed": "hatthi assam sa lohita ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_472:14.3",
        "original": "Cattāhaṁ ubbhataṁ brūmi,",
        "analyzed": "cattā aham ubbhatam brūmi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_472:14.4",
        "original": "evaṁ jānāhi khattiya”.",
        "analyzed": "evam jānāhi khattiya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_472:15.1",
        "original": "“Ciñcamāṇavikā mātā,",
        "analyzed": "ciñcamāṇavikā mātā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_472:15.2",
        "original": "devadatto ca me pitā;",
        "analyzed": "devadatto ca me pitā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_472:15.3",
        "original": "Ānando paṇḍito nāgo,",
        "analyzed": "ānandaḥ paṇḍitaḥ nāgaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_472:15.4",
        "original": "sāriputto ca devatā;",
        "analyzed": "sāriputto ca devatā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_472:15.5",
        "original": "Rājaputto ahaṁ āsiṁ,",
        "analyzed": "rāja puttaḥ aham āsi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_472:15.6",
        "original": "evaṁ dhāretha jātakan”ti.",
        "analyzed": "evam dhāretha jātakanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_472:16.1",
        "original": "Mahāpadumajātakaṁ navamaṁ.",
        "analyzed": "mahāpaduma jātakam navama ",
        "folio": "16"
    }
