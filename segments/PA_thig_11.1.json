[
    {
        "segmentnr": "PA_thig_11.1:0.1",
        "original": "Therīgāthā 11.1",
        "analyzed": "therīgāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_11.1:0.2",
        "original": "Dvādasakanipāta",
        "analyzed": "dvādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_11.1:0.3",
        "original": "Uppalavaṇṇātherīgāthā",
        "analyzed": "uppalavaṇṇā therīgāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thig_11.1:1.1",
        "original": "“Ubho mātā ca dhītā ca,",
        "analyzed": "ubhau mātā ca dhītā ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_11.1:1.2",
        "original": "mayaṁ āsuṁ sapattiyo;",
        "analyzed": "mayam āsum sapattiyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_11.1:1.3",
        "original": "Tassā me ahu saṁvego,",
        "analyzed": "tassā me ahu sa vegaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_11.1:1.4",
        "original": "abbhuto lomahaṁsano.",
        "analyzed": "abbhutaḥ loma hasanaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thig_11.1:2.1",
        "original": "Dhiratthu kāmā asucī,",
        "analyzed": "dhiratthu kāmā asucī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_11.1:2.2",
        "original": "duggandhā bahukaṇṭakā;",
        "analyzed": "duggandhā bahukaṇṭakā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_11.1:2.3",
        "original": "Yattha mātā ca dhītā ca,",
        "analyzed": "yattha mātā ca dhītā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_11.1:2.4",
        "original": "sabhariyā mayaṁ ahuṁ.",
        "analyzed": "sabhariyā mayam ahu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thig_11.1:3.1",
        "original": "Kāmesvādīnavaṁ disvā,",
        "analyzed": "kāmesva ādīnavam disvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_11.1:3.2",
        "original": "nekkhammaṁ daṭṭhu khemato;",
        "analyzed": "nekkhammam daṭṭhu khemataḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_11.1:3.3",
        "original": "Sā pabbajjiṁ rājagahe,",
        "analyzed": "sā pabbajjim rājagahe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_11.1:3.4",
        "original": "agārasmānagāriyaṁ.",
        "analyzed": "agārasmān anagāriya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thig_11.1:4.1",
        "original": "Pubbenivāsaṁ jānāmi,",
        "analyzed": "pubbenivāsam jānāmi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_11.1:4.2",
        "original": "dibbacakkhu visodhitaṁ;",
        "analyzed": "dibba cakkhu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_11.1:4.3",
        "original": "Cetopariccañāṇañca,",
        "analyzed": "cetaḥ uparicca ñāṇañca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_11.1:4.4",
        "original": "sotadhātu visodhitā.",
        "analyzed": "sotadhātu visodhitā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thig_11.1:5.1",
        "original": "Iddhīpi me sacchikatā,",
        "analyzed": "id hi ipi me sacchikatā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_11.1:5.2",
        "original": "patto me āsavakkhayo;",
        "analyzed": "pattaḥ me āsava kkhayaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_11.1:5.3",
        "original": "Chaḷabhiññā sacchikatā,",
        "analyzed": "chaḍabhiññā sacchikatā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_11.1:5.4",
        "original": "kataṁ buddhassa sāsanaṁ.",
        "analyzed": "katam buddhassa sāsana ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thig_11.1:6.1",
        "original": "Iddhiyā abhinimmitvā,",
        "analyzed": "iddhiyai abhinimmitvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_11.1:6.2",
        "original": "caturassaṁ rathaṁ ahaṁ;",
        "analyzed": "caturassam ratham aha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_11.1:6.3",
        "original": "Buddhassa pāde vanditvā,",
        "analyzed": "buddhassa pāde vanditvā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_11.1:6.4",
        "original": "lokanāthassa tādino”.",
        "analyzed": "loka nāthassa tādinaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thig_11.1:7.1",
        "original": "“Supupphitaggaṁ upagamma pādapaṁ,",
        "analyzed": "su pupphita aggam upagamma pādapa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_11.1:7.2",
        "original": "Ekā tuvaṁ tiṭṭhasi sālamūle;",
        "analyzed": "ekā tuvam tiṭṭhasi sāla mūle ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_11.1:7.3",
        "original": "Na cāpi te dutiyo atthi koci,",
        "analyzed": "na ca api te dutiyaḥ atthi koci ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_11.1:7.4",
        "original": "Bāle na tvaṁ bhāyasi dhuttakānaṁ”.",
        "analyzed": "bāle na tvam bhāyasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thig_11.1:8.1",
        "original": "“Sataṁ sahassānipi dhuttakānaṁ,",
        "analyzed": "satam ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_11.1:8.2",
        "original": "Samāgatā edisakā bhaveyyuṁ;",
        "analyzed": "samāgatāḥ edisakāḥ bhaveyyu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_11.1:8.3",
        "original": "Lomaṁ na iñje napi sampavedhe,",
        "analyzed": "lomam naḥ iñje napi sampavedhe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_11.1:8.4",
        "original": "Kiṁ me tuvaṁ māra karissaseko.",
        "analyzed": "kim me tuvam māra karissasekaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thig_11.1:9.1",
        "original": "Esā antaradhāyāmi,",
        "analyzed": "esā antaradhāyāmi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_11.1:9.2",
        "original": "kucchiṁ vā pavisāmi te;",
        "analyzed": "kucchim vā pavisāmi te ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_11.1:9.3",
        "original": "Bhamukantare tiṭṭhāmi,",
        "analyzed": "bhamuka antare tiṭṭhāmi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_11.1:9.4",
        "original": "tiṭṭhantiṁ maṁ na dakkhasi.",
        "analyzed": "tiṭṭhantim mama na dakkhasi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thig_11.1:10.1",
        "original": "Cittamhi vasībhūtāhaṁ,",
        "analyzed": "cittamhi vasībhūtā āha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_11.1:10.2",
        "original": "iddhipādā subhāvitā;",
        "analyzed": "iddhipādā su bhāvitā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_11.1:10.3",
        "original": "Chaḷabhiññā sacchikatā,",
        "analyzed": "chaḍabhiññā sacchikatā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_11.1:10.4",
        "original": "kataṁ buddhassa sāsanaṁ.",
        "analyzed": "katam buddhassa sāsana ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thig_11.1:11.1",
        "original": "Sattisūlūpamā kāmā,",
        "analyzed": "kāmā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_11.1:11.2",
        "original": "khandhāsaṁ adhikuṭṭanā;",
        "analyzed": "khandhāsam adhikuṭṭanā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_11.1:11.3",
        "original": "Yaṁ tvaṁ ‘kāmaratiṁ’ brūsi,",
        "analyzed": "yam tvam kāmarati brūsi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_11.1:11.4",
        "original": "‘āratī’ dāni sā mama.",
        "analyzed": "āratī dāni sā mama ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_thig_11.1:12.1",
        "original": "Sabbattha vihatā nandī,",
        "analyzed": "sabbattha vihatā nandī ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_11.1:12.2",
        "original": "Tamokhandho padālito;",
        "analyzed": "tamaḥ khandhaḥ padālitaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_11.1:12.3",
        "original": "Evaṁ jānāhi pāpima,",
        "analyzed": "evam jānāhi pāpima ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_11.1:12.4",
        "original": "Nihato tvamasi antakā”ti.",
        "analyzed": "nihataḥ tvam asi antaka ati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_thig_11.1:13.1",
        "original": "… Uppalavaṇṇā therī ….",
        "analyzed": "uppalavaṇṇā therī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_thig_11.1:14.1",
        "original": "Dvādasakanipāto niṭṭhito.",
        "analyzed": "dvādasaka nipātaḥ niṭṭhito ",
        "folio": "14"
    }
]