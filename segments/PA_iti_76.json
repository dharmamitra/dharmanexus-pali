    {
        "segmentnr": "PA_iti_76:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_76:2.1",
        "original": "“Tīṇimāni, bhikkhave, sukhāni patthayamāno sīlaṁ rakkheyya paṇḍito.",
        "analyzed": "tīṇi imāni bhikkhave sukhāni patthayamānaḥ sīlam rakkheyya paṇḍitaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_76:2.2",
        "original": "Katamāni tīṇi?",
        "analyzed": "katamāni tīṇi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_76:2.3_0",
        "original": "Pasaṁsā me āgacchatūti sīlaṁ rakkheyya paṇḍito, bhogā me uppajjantūti sīlaṁ rakkheyya",
        "analyzed": "pasasā me āgacchatu uti sīlam rakkheyya paṇḍitaḥ bhogāḥ me uppajjantu uti sīlam rakkheyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_76:2.3_1",
        "original": "paṇḍito, kāyassa bhedā paraṁ maraṇā sugatiṁ saggaṁ lokaṁ upapajjissāmīti sīlaṁ rakkheyya paṇḍito.",
        "analyzed": "paṇḍitaḥ kāyassa bhedā param maraṇā sugatim saggam lokam upapajjissāmi iti sīlam rakkheyya paṇḍitaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_76:2.4",
        "original": "Imāni kho, bhikkhave, tīṇi sukhāni patthayamāno sīlaṁ rakkheyya paṇḍito”ti.",
        "analyzed": "imāni kho bhikkhave tīṇi sukhāni patthayamānaḥ sīlam rakkheyya paṇḍitaḥ ti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_76:2.5",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_76:2.6",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_76:3.1",
        "original": "“Sīlaṁ rakkheyya medhāvī,",
        "analyzed": "sīlam rakkheyya medhāvī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_76:3.2",
        "original": "patthayāno tayo sukhe;",
        "analyzed": "patthayānaḥ tayo sukhe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_76:3.3",
