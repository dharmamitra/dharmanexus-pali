        "analyzed": "ajitā ati bhagavā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.2:2.3",
        "original": "Vevicchā pamādā nappakāsati;",
        "analyzed": "vevicchā pamādā nappakāsati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.2:2.4",
        "original": "Jappābhilepanaṁ brūmi,",
        "analyzed": "jappa abhilepanam brūmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.2:2.5",
