    },
    {
        "segmentnr": "PA_ja_512:0.4",
        "original": "2. Kumbhajātaka",
        "analyzed": "kumbhajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_512:1.1",
        "original": "“Ko pāturāsī tidivā nabhamhi,",
        "analyzed": "kaḥ pāturāsī tidivā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_512:1.2",
        "original": "Obhāsayaṁ saṁvariṁ candimāva;",
        "analyzed": "obhāsayam savarim candim āva ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_512:1.3",
        "original": "Gattehi te rasmiyo niccharanti,",
        "analyzed": "gattehi te rasmiyaḥ niccharanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_512:1.4",
        "original": "Sateratā vijjurivantalikkhe.",
        "analyzed": "sateratā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_512:2.1",
        "original": "So chinnavātaṁ kamasī aghamhi,",
        "analyzed": "saḥ chinna vātam kamasī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_512:2.2",
        "original": "Vehāyasaṁ gacchasi tiṭṭhasī ca;",
        "analyzed": "vehāyasam gacchasi tiṭṭhasi ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_512:2.3",
        "original": "Iddhī nu te vatthukatā subhāvitā,",
        "analyzed": "iddhi nu te vatthukatā su bhāvitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_512:2.4",
        "original": "Anaddhagūnaṁ api devatānaṁ.",
        "analyzed": "anaddhagūnam api devatāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_512:3.1",
        "original": "Vehāyasaṁ gammamāgamma tiṭṭhasi,",
        "analyzed": "vehāyasam gammam āgamma tiṭṭhasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_512:3.2",
        "original": "Kumbhaṁ kiṇāthāti yametamatthaṁ;",
        "analyzed": "kumbham kiṇātha ati yam etam attha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_512:3.3",
        "original": "Ko vā tuvaṁ kissa vā tāya kumbho,",
        "analyzed": "kaḥ vā tuvam kissa vā tāya kumbho ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_512:3.4",
        "original": "Akkhāhi me brāhmaṇa etamatthaṁ”.",
        "analyzed": "akkhāhi me brāhmaṇa etam attha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_512:4.1",
        "original": "“Na sappikumbho napi telakumbho,",
        "analyzed": "na sappi kumbhaḥ tela kumbhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_512:4.2",
        "original": "Na phāṇitassa na madhussa kumbho;",
        "analyzed": "na phāṇitassa na madhussa kumbhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_512:4.3",
        "original": "Kumbhassa vajjāni anappakāni,",
        "analyzed": "kumbhassa vajjāni anappakāni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_512:4.4",
        "original": "Dose bahū kumbhagate suṇātha.",
        "analyzed": "dose bahū kumbha gate suṇātha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_512:5.1",
        "original": "Gaḷeyya yaṁ pitvā pate papātaṁ,",
        "analyzed": "gaḷeyya yam pitvā pate papāta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_512:5.2",
        "original": "Sobbhaṁ guhaṁ candaniyoḷigallaṁ;",
        "analyzed": "       ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_512:5.3",
        "original": "Bahumpi bhuñjeyya abhojaneyyaṁ,",
        "analyzed": "bahumpi bhuñjeyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_512:5.4",
        "original": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
        "analyzed": "tassā puṇṇam kumbham imam kiṇātha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_512:6.1",
        "original": "Yaṁ pitva cittasmimanesamāno,",
        "analyzed": "yam pitva cittasmimanesamānaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_512:6.2",
        "original": "Āhiṇḍatī goriva bhakkhasādī;",
        "analyzed": "āhiṇḍatī goḥ iva bhakkha sādī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_512:6.3",
        "original": "Anāthamāno upagāyati naccati ca,",
        "analyzed": "an āthamānaḥ upagāyati naccati ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_512:6.4",
        "original": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
        "analyzed": "tassā puṇṇam kumbham imam kiṇātha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_512:7.1",
        "original": "Yaṁ ve pivitvā acelova naggo,",
        "analyzed": "yam veḥ pivitvā acelaḥ uva naggaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_512:7.2",
        "original": "Careyya gāme visikhantarāni;",
        "analyzed": "careyya gāme visikhantarāni ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_512:7.3",
        "original": "Sammūḷhacitto ativelasāyī,",
        "analyzed": "sammūḍha cittaḥ ativela sāyī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_512:7.4",
        "original": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
        "analyzed": "tassā puṇṇam kumbham imam kiṇātha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_512:8.1",
        "original": "Yaṁ pitva uṭṭhāya pavedhamāno,",
        "analyzed": "yam pitve uṭṭhāya pavedhamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_512:8.2",
        "original": "Sīsañca bāhuñca pacālayanto;",
        "analyzed": "sīsam ca bāhum ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_512:8.3",
        "original": "So naccatī dārukaṭallakova,",
        "analyzed": "saḥ naccatī dāru kaṭallakā uva ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_512:8.4",
        "original": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
        "analyzed": "tassā puṇṇam kumbham imam kiṇātha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_512:9.1",
        "original": "Yaṁ ve pivitvā aggidaḍḍhā sayanti,",
