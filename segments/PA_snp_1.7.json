[
    {
        "segmentnr": "PA_snp_1.7:0.1",
        "original": "Sutta Nipāta 1.7",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.7:0.2",
        "original": "Vasalasutta",
        "analyzed": "vasalasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_1.7:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.7:1.2",
        "original": "ekaṁ samayaṁ bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "ekam samayam bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.7:1.3",
        "original": "Atha kho bhagavā pubbaṇhasamayaṁ nivāsetvā pattacīvaramādāya sāvatthiṁ piṇḍāya pāvisi.",
        "analyzed": "atha kho bhagavā pubbaṇha samayam nivāsetvā patta cīvaram ādāya sāvatthim piṇḍāya pāvisi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.7:1.4",
        "original": "Tena kho pana samayena aggikabhāradvājassa brāhmaṇassa nivesane aggi pajjalito hoti āhuti paggahitā.",
        "analyzed": "tena kho pana samayena aggikabhāradvājassa brāhmaṇassa nivesane aggi pajjalitaḥ hoti āhuti paggahitā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.7:1.5",
        "original": "Atha kho bhagavā sāvatthiyaṁ sapadānaṁ piṇḍāya caramāno yena aggikabhāradvājassa brāhmaṇassa nivesanaṁ tenupasaṅkami.",
        "analyzed": "atha khalu bhagavā sāvatthiyam sa padānam piṇḍāya caramānaḥ yena aggikabhāradvājassa brāhmaṇassa nivesanam tena upasaṅkami ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_1.7:2.1",
        "original": "Addasā kho aggikabhāradvājo brāhmaṇo bhagavantaṁ dūratova āgacchantaṁ.",
        "analyzed": "addasā khalu aggikabhāradvājaḥ brāhmaṇaḥ bhagavantam dūratas u vaḥ āgacchanta ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.7:2.2",
        "original": "Disvāna bhagavantaṁ etadavoca:",
        "analyzed": "disvāna bhagavantam etat avoca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.7:2.3",
        "original": "“tatreva, muṇḍaka;",
        "analyzed": "tatra iva muṇḍaka ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.7:2.4",
        "original": "tatreva, samaṇaka;",
        "analyzed": "tatra iva samaṇaka ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.7:2.5",
        "original": "tatreva, vasalaka, tiṭṭhāhī”ti.",
        "analyzed": "tatra iva vasalaka tiṭṭhāhi iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_1.7:3.1",
        "original": "Evaṁ vutte, bhagavā aggikabhāradvājaṁ brāhmaṇaṁ etadavoca:",
        "analyzed": "evam vutte bhagavān aggikabhāradvājam brāhmaṇam etat avoca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.7:3.2",
        "original": "“jānāsi pana tvaṁ, brāhmaṇa, vasalaṁ vā vasalakaraṇe vā dhamme”ti?",
        "analyzed": "jānāsi pana tva brāhmaṇa vasalam vā vasala karaṇe vā dhamma iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.7:3.3",
        "original": "“Na khvāhaṁ, bho gotama, jānāmi vasalaṁ vā vasalakaraṇe vā dhamme;",
        "analyzed": "na khvā aha bho gotama jānāmi vasalam vā vasala karaṇe vā dhamme ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.7:3.4",
        "original": "sādhu me bhavaṁ gotamo tathā dhammaṁ desetu, yathāhaṁ jāneyyaṁ vasalaṁ vā vasalakaraṇe vā dhamme”ti.",
        "analyzed": "sādhu me bhavam gotamaḥ tathā dhammam desetu yathā aham jāneyyam vasalam vā vasala karaṇe vā dhamma iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.7:3.5",
        "original": "“Tena hi, brāhmaṇa, suṇāhi, sādhukaṁ manasi karohi; bhāsissāmī”ti.",
        "analyzed": "tena hi brāhmaṇa suṇāhi sādhukam manasi karohi bhāsissāmi iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.7:3.6",
        "original": "“Evaṁ, bho”ti kho aggikabhāradvājo brāhmaṇo bhagavato paccassosi.",
        "analyzed": "eva bhoti kho aggikabhāradvājaḥ brāhmaṇaḥ bhagavataḥ paccassosi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.7:3.7",
        "original": "Bhagavā etadavoca:",
        "analyzed": "bhagavān etat avoca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_1.7:4.1",
        "original": "“Kodhano upanāhī ca,",
        "analyzed": "kodhanaḥ upanāhī ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.7:4.2",
        "original": "pāpamakkhī ca yo naro;",
        "analyzed": "pāpa makkhī ca yaḥ naraḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.7:4.3",
        "original": "Vipannadiṭṭhi māyāvī,",
        "analyzed": "vipanna diṭṭhi māyāvī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.7:4.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_1.7:5.1",
        "original": "Ekajaṁ vā dvijaṁ vāpi,",
        "analyzed": "ekajam vā dvijam vā api ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.7:5.2",
        "original": "yodha pāṇaṁ vihiṁsati;",
        "analyzed": "yodha pāṇam vihisati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.7:5.3",
        "original": "Yassa pāṇe dayā natthi,",
        "analyzed": "yassa pāṇe dayā natthi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.7:5.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_1.7:6.1",
        "original": "Yo hanti parirundhati,",
        "analyzed": "yaḥ hanti parirundhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.7:6.2",
        "original": "gāmāni nigamāni ca;",
        "analyzed": "gāmāni nigamāni ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.7:6.3",
        "original": "Niggāhako samaññāto,",
        "analyzed": "niggāhakaḥ samaññātaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.7:6.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_1.7:7.1",
        "original": "Gāme vā yadi vā raññe,",
        "analyzed": "gāme vā yadi vā raññe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.7:7.2",
        "original": "yaṁ paresaṁ mamāyitaṁ;",
        "analyzed": "yam parā isam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.7:7.3",
        "original": "Theyyā adinnamādeti,",
        "analyzed": "theyyā a dinna mādā iti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.7:7.4",
        "original": "taṁ jaññā vasalo iti.",
        "analyzed": "tam jaññā vasalo iti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_1.7:8.1",
        "original": "Yo have iṇamādāya,",
        "analyzed": "yaḥ have iṇam ādāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_1.7:8.2",
