        "analyzed": "petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_33:0.2",
        "original": "Cūḷavagga",
        "analyzed": "cūḷa vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_33:0.3",
        "original": "8. Dutiyamigaluddakapetavatthu",
        "analyzed": "dutiya miga luddaka peta vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_33:1.1",
        "original": "“Kūṭāgāre ca pāsāde,",
        "analyzed": "kūṭāgāre ca pāsāde ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_33:1.2",
        "original": "pallaṅke gonakatthate;",
        "analyzed": "pallaṅke ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_33:1.3",
        "original": "Pañcaṅgikena turiyena,",
        "analyzed": "pañcaṅgikena turiyena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_33:1.4",
        "original": "ramasi suppavādite.",
        "analyzed": "ramasi suppa vādite ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_33:2.1",
        "original": "Tato ratyā vivasāne,",
        "analyzed": "tatas ratyā vivasāne ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_33:2.2",
        "original": "sūriyuggamanaṁ pati;",
        "analyzed": "sūri yuj gamanam pati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_33:2.3",
        "original": "Apaviddho susānasmiṁ,",
        "analyzed": "apaviddhaḥ su sānasmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_33:2.4",
        "original": "bahudukkhaṁ nigacchasi.",
        "analyzed": "bahu dukkham nigacchasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_33:3.1",
        "original": "Kiṁ nu kāyena vācāya,",
        "analyzed": "kim nu kāyena vācāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_33:3.2",
        "original": "manasā dukkaṭaṁ kataṁ;",
        "analyzed": "manasā dukkaṭam kata ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_33:3.3",
        "original": "Kissa kammavipākena,",
        "analyzed": "kissa kamma vipākena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_33:3.4",
        "original": "idaṁ dukkhaṁ nigacchasi”.",
        "analyzed": "idam dukkham nigacchasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_33:4.1",
        "original": "“Ahaṁ rājagahe ramme,",
        "analyzed": "aham rājagahe ramme ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_33:4.2",
        "original": "ramaṇīye giribbaje;",
        "analyzed": "ramaṇīye giribbaje ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_33:4.3",
        "original": "Migaluddo pure āsiṁ,",
        "analyzed": "migaluddaḥ pure āsi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_33:4.4",
        "original": "luddo cāsimasaññato.",
        "analyzed": "luddaḥ ca a si ma saññataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_33:5.1",
        "original": "Tassa me sahāyo suhadayo,",
        "analyzed": "tassa me sahāyaḥ su hadayaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_33:5.2",
        "original": "Saddho āsi upāsako;",
        "analyzed": "saddhaḥ āsi upāsakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_33:5.3",
        "original": "Tassa kulupako bhikkhu,",
        "analyzed": "tassa kulupakaḥ bhikkhu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_33:5.4",
        "original": "Āsi gotamasāvako;",
        "analyzed": "āsi gotama sāvakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_33:5.5",
        "original": "Sopi maṁ anukampanto,",
        "analyzed": "saḥ upi mam anukampantaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_33:5.6",
        "original": "Nivāresi punappunaṁ.",
        "analyzed": "nivāresi punappuna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_33:6.1",
        "original": "‘Mākāsi pāpakaṁ kammaṁ,",
        "analyzed": "mā akāsi pāpakam kamma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_33:6.2",
        "original": "Mā tāta duggatiṁ agā;",
        "analyzed": "mā tāta duggatim agāḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_33:6.3",
        "original": "Sace icchasi pecca sukhaṁ,",
        "analyzed": "sace icchasi pecca sukha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_33:6.4",
        "original": "Virama pāṇavadhā asaṁyamā’.",
        "analyzed": "virama pāṇa vadhā a sa yamā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_33:7.1",
        "original": "Tassāhaṁ vacanaṁ sutvā,",
        "analyzed": "tassa aham vacanam sutvā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_33:7.2",
        "original": "Sukhakāmassa hitānukampino;",
        "analyzed": "sukha kāmassa hita anukampino ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_33:7.3",
        "original": "Nākāsiṁ sakalānusāsaniṁ,",
        "analyzed": "nāka asim sakala anusāsani ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_33:7.4",
        "original": "Cirapāpābhirato abuddhimā.",
        "analyzed": "cira pāpa abhirataḥ abuddhimā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_33:8.1",
        "original": "So maṁ puna bhūrisumedhaso,",
        "analyzed": "saḥ mam punar bhūri sumedhasaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_33:8.2",
        "original": "Anukampāya saṁyame nivesayi;",
        "analyzed": "anukampāya sayame ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_33:8.3",
        "original": "‘Sace divā hanasi pāṇino,",
        "analyzed": "sace divā hanasi pāṇinaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_33:8.4",
        "original": "Atha te rattiṁ bhavatu saṁyamo’.",
        "analyzed": "atha te rattim bhavatu sayamaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_33:9.1",
        "original": "Svāhaṁ divā hanitvā pāṇino,",
        "analyzed": "svāham divā hanitvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_33:9.2",
        "original": "Virato rattimahosi saññato;",
        "analyzed": "virataḥ ratti mahosi saññataḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_33:9.3",
        "original": "Rattāhaṁ paricāremi,",
        "analyzed": "rattā aham paricāremi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_33:9.4",
        "original": "Divā khajjāmi duggato.",
        "analyzed": "divā khajjāmi duggataḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_33:10.1",
        "original": "Tassa kammassa kusalassa,",
        "analyzed": "tassa kammassa kusalassa ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_33:10.2",
        "original": "Anubhomi rattiṁ amānusiṁ;",
        "analyzed": "anubhomi rattim amānusi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_33:10.3",
        "original": "Divā paṭihatāva kukkurā,",
        "analyzed": "divā paṭihatā ava kukkurā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_33:10.4",
        "original": "Upadhāvanti samantā khādituṁ.",
        "analyzed": "upadhāvanti samantā khāditu ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_33:11.1",
        "original": "Ye ca te satatānuyogino,",
        "analyzed": "ye ca te satata anuyoginaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_33:11.2",
        "original": "Dhuvaṁ payuttā sugatassa sāsane;",
        "analyzed": "dhuvam payuttā sugatassa sāsane ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_33:11.3",
        "original": "Maññāmi te amatameva kevalaṁ,",
        "analyzed": "maññāmi te amatam eva kevala ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_33:11.4",
        "original": "Adhigacchanti padaṁ asaṅkhatan”ti.",
        "analyzed": "adhigacchanti padam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_33:12.1",
        "original": "Dutiyamigaluddakapetavatthu aṭṭhamaṁ.",
        "analyzed": "dutiya miga luddaka peta vatthu aṭṭhama ",
        "folio": "12"
    }
]