        "analyzed": "nandāti bhagavā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.8:6.3",
        "original": "Jātijarāya nivutāti brūmi;",
        "analyzed": "jāti jarāya brūmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.8:6.4",
        "original": "Ye sīdha diṭṭhaṁ va sutaṁ mutaṁ vā,",
        "analyzed": "ye sīdha diṭṭham va sutam mutam vā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.8:6.5",
        "original": "Sīlabbataṁ vāpi pahāya sabbaṁ.",
        "analyzed": "sīlabbatam vā api pahāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.8:6.6",
        "original": "Anekarūpampi pahāya sabbaṁ,",
        "analyzed": "aneka rūpam pi pahāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.8:6.7",
        "original": "Taṇhaṁ pariññāya anāsavāse;",
        "analyzed": "taṇham pariññāya anāsavāse ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.8:6.8",
        "original": "Te ve narā oghatiṇṇāti brūmi”.",
        "analyzed": "te ve narāḥ oghatiṇṇāḥ ati brūmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_5.8:7.1",
        "original": "“Etābhinandāmi vaco mahesino,",
        "analyzed": "vacaḥ mahā īsinaḥ ",
        "folio": "7"
    },
    {
