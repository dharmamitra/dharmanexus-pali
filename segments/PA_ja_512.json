        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_512:11.2",
        "original": "Mameva sabbā pathavīti maññe;",
        "analyzed": "mama eva sabbā pathavī iti maññe ",
        "folio": "11"
    },
    {
