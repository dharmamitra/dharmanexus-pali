[
    {
        "segmentnr": "PA_snp_4.13:0.1",
        "original": "Sutta Nipāta 4.13",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.13:0.2",
        "original": "Mahābyūhasutta",
        "analyzed": "mahābyūhasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.13:1.1",
        "original": "“Ye kecime diṭṭhiparibbasānā,",
        "analyzed": "ye keci ime diṭṭhi paribbasānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.13:1.2",
        "original": "Idameva saccanti vivādayanti;",
        "analyzed": "idam eva vivādayanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.13:1.3",
        "original": "Sabbeva te nindamanvānayanti,",
        "analyzed": "te nindam anvānayanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.13:1.4",
        "original": "Atho pasaṁsampi labhanti tattha”.",
        "analyzed": "atho pasasampi labhanti tattha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.13:2.1",
        "original": "“Appañhi etaṁ na alaṁ samāya,",
        "analyzed": "etam na alam ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.13:2.2",
        "original": "Duve vivādassa phalāni brūmi;",
        "analyzed": "duve vivādassa phalāni brūmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.13:2.3",
        "original": "Etampi disvā na vivādayetha,",
        "analyzed": "etam pi disvā na vivādayetha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.13:2.4",
        "original": "Khemābhipassaṁ avivādabhūmiṁ”.",
        "analyzed": "a vivāda bhūmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.13:3.1",
        "original": "“Yā kācimā sammutiyo puthujjā,",
        "analyzed": "yā kācimā sammutiyo puthujjā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.13:3.2",
        "original": "Sabbāva etā na upeti vidvā;",
        "analyzed": "etāḥ naḥ upa iti vidvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.13:3.3",
        "original": "Anūpayo so upayaṁ kimeyya,",
        "analyzed": "saḥ upayam kim eyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.13:3.4",
        "original": "Diṭṭhe sute khantimakubbamāno”.",
        "analyzed": "diṭṭhe sute khantim akubbamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.13:4.1",
        "original": "“Sīluttamā saññamenāhu suddhiṁ,",
        "analyzed": "sīluttamā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.13:4.2",
        "original": "Vataṁ samādāya upaṭṭhitāse;",
        "analyzed": "vatam samādāya upaṭṭhitāse ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.13:4.3",
        "original": "Idheva sikkhema athassa suddhiṁ,",
        "analyzed": "idhā iva sikkhema athassa suddhi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.13:4.4",
        "original": "Bhavūpanītā kusalāvadānā.",
        "analyzed": "bhavūpanītā kusala avadānā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.13:5.1",
        "original": "Sace cuto sīlavatato hoti,",
        "analyzed": "sace cutaḥ sīlavatataḥ hoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.13:5.2",
        "original": "Pavedhatī kamma virādhayitvā;",
        "analyzed": "pavedhatī kamma virādhayitvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.13:5.3",
        "original": "Pajappatī patthayatī ca suddhiṁ,",
        "analyzed": "pajappatī patthayatī ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.13:5.4",
        "original": "Satthāva hīno pavasaṁ gharamhā.",
        "analyzed": "satthāva hīnaḥ pavasam gharamhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.13:6.1",
        "original": "Sīlabbataṁ vāpi pahāya sabbaṁ,",
        "analyzed": "sīlabbatam vā api pahāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.13:6.2",
        "original": "Kammañca sāvajjanavajjametaṁ;",
        "analyzed": "kamman ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.13:6.3",
        "original": "Suddhiṁ asuddhinti apatthayāno,",
        "analyzed": "suddhim a suddhim ti a patthayānaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.13:6.4",
        "original": "Virato care santimanuggahāya.",
        "analyzed": "virataḥ care santim anuggahāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.13:7.1",
        "original": "Tamūpanissāya jigucchitaṁ vā,",
        "analyzed": "tam ūpanissāya jigucchitam vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.13:7.2",
        "original": "Atha vāpi diṭṭhaṁ va sutaṁ mutaṁ vā;",
        "analyzed": "atha vā api diṭṭham sutam mutam vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.13:7.3",
        "original": "Uddhaṁsarā suddhimanutthunanti,",
        "analyzed": "suddhim an utthunanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.13:7.4",
        "original": "Avītataṇhāse bhavābhavesu.",
        "analyzed": "a vīta taṇhā se bhava abhavesu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.13:8.1",
        "original": "Patthayamānassa hi jappitāni,",
        "analyzed": "patthayamānassa hi jappitāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.13:8.2",
        "original": "Pavedhitaṁ vāpi pakappitesu;",
        "analyzed": "pavedhitam vā api pakappitesu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.13:8.3",
        "original": "Cutūpapāto idha yassa natthi,",
        "analyzed": "cutūpapāto idha yassa na atthi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.13:8.4",
        "original": "Sa kena vedheyya kuhiṁ va jappe”.",
        "analyzed": "sa kena vedheyya kuhim va jappe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.13:9.1",
        "original": "“Yamāhu dhammaṁ paramanti eke,",
        "analyzed": "yam āhu dhammam paramanti eke ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.13:9.2",
        "original": "Tameva hīnanti panāhu aññe;",
        "analyzed": "tam eva hīnan ti pana āhu aññe ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.13:9.3",
        "original": "Sacco nu vādo katamo imesaṁ,",
        "analyzed": "saccaḥ nu vādaḥ katamaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.13:9.4",
        "original": "Sabbeva hīme kusalāvadānā”.",
        "analyzed": "sabbe iva hi ime kusala avadānā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.13:10.1",
        "original": "“Sakañhi dhammaṁ paripuṇṇamāhu,",
        "analyzed": "sakan hi dhammam paripuṇṇam āhu ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.13:10.2",
        "original": "Aññassa dhammaṁ pana hīnamāhu;",
        "analyzed": "aññassa dhammam pana hīnam āhu ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.13:10.3",
        "original": "Evampi viggayha vivādayanti,",
        "analyzed": "evam pi viggayha vivādayanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.13:10.4",
        "original": "Sakaṁ sakaṁ sammutimāhu saccaṁ.",
        "analyzed": "sakam sakam sammutim āhuḥ sat ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.13:11.1",
        "original": "Parassa ce vambhayitena hīno,",
        "analyzed": "parassa ce vambhayitena hīnaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.13:11.2",
        "original": "Na koci dhammesu visesi assa;",
        "analyzed": "na koci dhammesu visesī assa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.13:11.3",
        "original": "Puthū hi aññassa vadanti dhammaṁ,",
        "analyzed": "puthū hi aññassa vadanti dhamma ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.13:11.4",
        "original": "Nihīnato samhi daḷhaṁ vadānā.",
        "analyzed": "samhi daḷham vadānā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.13:12.1",
        "original": "Saddhammapūjāpi nesaṁ tatheva,",
        "analyzed": "saddhamma pūjā api na īsam tathā iva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.13:12.2",
        "original": "Yathā pasaṁsanti sakāyanāni;",
        "analyzed": "yathā pasasanti sa kāyanāni ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.13:12.3",
        "original": "Sabbeva vādā tathiyā bhaveyyuṁ,",
        "analyzed": "sabbe iva vādāḥ tathiyāḥ bhaveyyu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.13:12.4",
        "original": "Suddhī hi nesaṁ paccattameva.",
        "analyzed": "suddhī hi na isam paccattam eva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.13:13.1",
        "original": "Na brāhmaṇassa paraneyyamatthi,",
        "analyzed": "na brāhmaṇassa paraneyyam atthi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.13:13.2",
        "original": "Dhammesu niccheyya samuggahītaṁ;",
        "analyzed": "dhammesu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.13:13.3",
        "original": "Tasmā vivādāni upātivatto,",
        "analyzed": "tasmai vivādāni upātivattaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.13:13.4",
        "original": "Na hi seṭṭhato passati dhammamaññaṁ.",
        "analyzed": "na hi seṭṭhataḥ passati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.13:14.1",
        "original": "Jānāmi passāmi tatheva etaṁ,",
        "analyzed": "jānāmi passāmi tathā iva eta ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.13:14.2",
        "original": "Diṭṭhiyā eke paccenti suddhiṁ;",
        "analyzed": "diṭṭhiyā eke paccenti suddhi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.13:14.3",
        "original": "Addakkhi ce kiñhi tumassa tena,",
        "analyzed": "addakkhi ce kiñhi tumassa tena ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.13:14.4",
        "original": "Atisitvā aññena vadanti suddhiṁ.",
        "analyzed": "atisitvā aññena vadanti suddhi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.13:15.1",
        "original": "Passaṁ naro dakkhati nāmarūpaṁ,",
        "analyzed": "passam naraḥ dakkhati nāma rūpa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.13:15.2",
        "original": "Disvāna vā ñassati tānimeva;",
        "analyzed": "disvāna vai ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.13:15.3",
        "original": "Kāmaṁ bahuṁ passatu appakaṁ vā,",
        "analyzed": "kāmam bahum passatu appakam vā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.13:15.4",
        "original": "Na hi tena suddhiṁ kusalā vadanti.",
        "analyzed": "na hi tena suddhim kusalāḥ vadanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.13:16.1",
        "original": "Nivissavādī na hi subbināyo,",
        "analyzed": "nivissa vādī na hi subbināyo ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.13:16.2",
        "original": "Pakappitaṁ diṭṭhi purakkharāno;",
        "analyzed": "pakappitam diṭṭhi purakkharāno ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.13:16.3",
        "original": "Yaṁ nissito tattha subhaṁ vadāno,",
        "analyzed": "yam nissitaḥ tattha subham vadānaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.13:16.4",
        "original": "Suddhiṁ vado tattha tathaddasā so.",
        "analyzed": "suddhim vado tattha so ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.13:17.1",
        "original": "Na brāhmaṇo kappamupeti saṅkhā,",
        "analyzed": "na brāhmaṇaḥ kappam upeti saṅkhā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.13:17.2",
        "original": "Na diṭṭhisārī napi ñāṇabandhu;",
        "analyzed": "na diṭṭhi sārī na pi ñāṇa bandhu ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.13:17.3",
        "original": "Ñatvā ca so sammutiyo puthujjā,",
        "analyzed": "ñatvā ca saḥ sammutiyaḥ puthujjā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.13:17.4",
        "original": "Upekkhatī uggahaṇanti maññe.",
        "analyzed": "upekkhatī uggahaṇanti maññe ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.13:18.1",
        "original": "Vissajja ganthāni munīdha loke,",
        "analyzed": "vissajja ganthāni munīdha loke ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.13:18.2",
        "original": "Vivādajātesu na vaggasārī;",
        "analyzed": "vivāda jātesu na vagga sārī ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.13:18.3",
        "original": "Santo asantesu upekkhako so,",
        "analyzed": "santaḥ a santesu upekkhako so ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.13:18.4",
        "original": "Anuggaho uggahaṇanti maññe.",
        "analyzed": "an uggahaḥ uggahaṇanti maññe ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.13:19.1",
        "original": "Pubbāsave hitvā nave akubbaṁ,",
        "analyzed": "pubba asave hitvā nave akubba ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.13:19.2",
        "original": "Na chandagū nopi nivissavādī;",
        "analyzed": "na chandagū na upi nivissavādī ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.13:19.3",
        "original": "Sa vippamutto diṭṭhigatehi dhīro,",
        "analyzed": "sa vippamuttaḥ diṭṭhi gatehi dhīraḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.13:19.4",
        "original": "Na lippati loke anattagarahī.",
        "analyzed": "na lippati loke a natta garahī ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.13:20.1",
        "original": "Sa sabbadhammesu visenibhūto,",
        "analyzed": "sa sabba dhammesu visenibhūtaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.13:20.2",
        "original": "Yaṁ kiñci diṭṭhaṁ va sutaṁ mutaṁ vā;",
        "analyzed": "yam kiñci diṭṭham vā sutam mutam vā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.13:20.3",
        "original": "Sa pannabhāro muni vippamutto,",
        "analyzed": "sa panna bhāraḥ muni vippamuttaḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.13:20.4",
        "original": "Na kappiyo nūparato na patthiyo”ti.",
        "analyzed": "na kappiyaḥ nu uparataḥ na patthiyaḥ ti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.13:20.5",
        "original": "Mahābyūhasuttaṁ terasamaṁ.",
        "analyzed": "mahābyūhasuttam terasama ",
        "folio": "20"
    }
]