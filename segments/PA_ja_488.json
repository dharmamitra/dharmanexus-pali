[
    {
        "segmentnr": "PA_ja_488:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_488:0.2",
        "original": "Pakiṇṇakanipāta",
        "analyzed": "pakiṇṇaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_488:0.3",
        "original": "Sālikedāravagga",
        "analyzed": "sāli kedāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_488:0.4",
        "original": "5. Bhisajātaka",
        "analyzed": "bhisajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_488:1.1",
        "original": "“Assaṁ gavaṁ rajataṁ jātarūpaṁ,",
        "analyzed": "rajatam jātarūpa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_488:1.2",
        "original": "Bhariyañca so idha labhataṁ manāpaṁ;",
        "analyzed": "bhariyañca saḥ u idha labhatam manāpa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_488:1.3",
        "original": "Puttehi dārehi samaṅgi hotu,",
        "analyzed": "puttehi dārehi samaṅgi hotu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_488:1.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi”.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_488:2.1",
        "original": "“Mālañca so kāsikacandanañca,",
        "analyzed": "mālañca saḥ kāsikacandanañca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_488:2.2",
        "original": "Dhāretu puttassa bahū bhavantu;",
        "analyzed": "dhāretu puttassa bahū bhavantu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_488:2.3",
        "original": "Kāmesu tibbaṁ kurutaṁ apekkhaṁ,",
        "analyzed": "kāmesu tibbam kurutam apekkha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_488:2.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi”.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_488:3.1",
        "original": "“Pahūtadhañño kasimā yasassī,",
        "analyzed": "pahūta dhaññaḥ kasimā yasassī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_488:3.2",
        "original": "Putte gihī dhanimā sabbakāme;",
        "analyzed": "putte gihī dhanimā sabba kāme ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_488:3.3",
        "original": "Vayaṁ apassaṁ gharamāvasātu,",
        "analyzed": "vayam apassam gharam āvasātu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_488:3.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_488:4.1",
        "original": "So khattiyo hotu pasayhakārī,",
        "analyzed": "saḥ khattiyaḥ hotu pasayha kārī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_488:4.2",
        "original": "Rājābhirājā balavā yasassī;",
        "analyzed": "rājā abhirājā balavā yasassī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_488:4.3",
        "original": "Sa cāturantaṁ mahimāvasātu,",
        "analyzed": "sa cāturantam mahimā avasātu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_488:4.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_488:5.1",
        "original": "So brāhmaṇo hotu avītarāgo,",
        "analyzed": "saḥ brāhmaṇaḥ hotu a vīta rāgaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_488:5.2",
        "original": "Muhuttanakkhattapathesu yutto;",
        "analyzed": "muhutta nakkhatta pathesu yuttaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_488:5.3",
        "original": "Pūjetu naṁ raṭṭhapatī yasassī,",
        "analyzed": "pūjetu nam raṭṭhapatī yasassī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_488:5.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_488:6.1",
        "original": "Ajjhāyakaṁ sabbasamantavedaṁ,",
        "analyzed": "ajjhāyakam sabba samanta veda ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_488:6.2",
        "original": "Tapassīnaṁ maññatu sabbaloko;",
        "analyzed": "tapassīnam maññatu sabba lokaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_488:6.3",
        "original": "Pūjentu naṁ jānapadā samecca,",
        "analyzed": "pūjentu nam jānapadā samet ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_488:6.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_488:7.1",
        "original": "Catussadaṁ gāmavaraṁ samiddhaṁ,",
        "analyzed": "catur sadam gām avaram samiddha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_488:7.2",
        "original": "Dinnañhi so bhuñjatu vāsavena;",
        "analyzed": "saḥ bhuñjatu vāsavena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_488:7.3",
        "original": "Avītarāgo maraṇaṁ upetu,",
        "analyzed": "a vīta rāgaḥ maraṇam upetu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_488:7.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_488:8.1",
        "original": "So gāmaṇī hotu sahāyamajjhe,",
        "analyzed": "saḥ hotu sahāya majjhe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_488:8.2",
        "original": "Naccehi gītehi pamodamāno;",
        "analyzed": "naccehi gītehi pamodamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_488:8.3",
        "original": "So rājato byasana mālattha kiñci,",
        "analyzed": "saḥ rājataḥ byasana mā alattha kiñci ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_488:8.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_488:9.1",
        "original": "Yaṁ ekarājā pathaviṁ vijetvā,",
        "analyzed": "yam eka rājā pathavim vijetvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_488:9.2",
        "original": "Itthīsahassāna ṭhapetu aggaṁ;",
        "analyzed": "itthī sahassāna ṭhapetu agga ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_488:9.3",
        "original": "Sīmantinīnaṁ pavarā bhavātu,",
        "analyzed": "sīmantinīnam pavarā bhavātu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_488:9.4",
        "original": "Bhisāni te brāhmaṇa yā ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yāḥ ahāsi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_488:10.1",
        "original": "Isīnañhi sā sabbasamāgatānaṁ,",
        "analyzed": "isīnam hi sā sabba samāgatāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_488:10.2",
        "original": "Bhuñjeyya sāduṁ avikampamānā;",
        "analyzed": "bhuñjeyya sādum a vikampamānā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_488:10.3",
        "original": "Carātu lābhena vikatthamānā,",
        "analyzed": "carātu lābhena vikatthamānā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_488:10.4",
        "original": "Bhisāni te brāhmaṇa yā ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yāḥ ahāsi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_488:11.1",
        "original": "Āvāsiko hotu mahāvihāre,",
        "analyzed": "āvāsikaḥ hotu mahāvihāre ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_488:11.2",
        "original": "Navakammiko hotu gajaṅgalāyaṁ;",
        "analyzed": "nava kammikaḥ hotu gajaṅgalāya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_488:11.3",
        "original": "Ālokasandhiṁ divasaṁ karotu,",
        "analyzed": "āloka sandhim divasam karotu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_488:11.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_488:12.1",
        "original": "So bajjhatū pāsasatehi chabbhi,",
        "analyzed": "saḥ bajjhatū pāsa satehi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_488:12.2",
        "original": "Rammā vanā niyyatu rājadhāniṁ;",
        "analyzed": "rammā vanā niyyatu rājadhāni ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_488:12.3",
        "original": "Tuttehi so haññatu pācanehi,",
        "analyzed": "tuttehi saḥ haññatu pācanehi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_488:12.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_488:13.1",
        "original": "Alakkamālī tipukaṇṇaviddho,",
        "analyzed": "alakkamālī tipu kaṇṇa viddhaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_488:13.2",
        "original": "Laṭṭhīhato sappamukhaṁ upetu;",
        "analyzed": "laṭṭhī hataḥ sa pamukham upetu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_488:13.3",
        "original": "Sakacchabandho visikhaṁ carātu,",
        "analyzed": "sa kaccha bandhaḥ visikham carātu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_488:13.4",
        "original": "Bhisāni te brāhmaṇa yo ahāsi”.",
        "analyzed": "bhisāni te brāhmaṇa yaḥ ahā asi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_488:14.1",
        "original": "“Yo ve anaṭṭhaṁva naṭṭhanti cāha,",
        "analyzed": "yaḥ naṭṭhanti ca āha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_488:14.2",
        "original": "Kāmeva so labhataṁ bhuñjatañca;",
        "analyzed": "kāmā iva saḥ labhatam bhuñjatam ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_488:14.3",
        "original": "Agāramajjhe maraṇaṁ upetu,",
        "analyzed": "agāra majjhe maraṇam upetu ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_488:14.4",
        "original": "Yo vā bhonto saṅkati kañcideva”.",
        "analyzed": "yaḥ vā bhontaḥ kañcid eva ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_488:15.1",
        "original": "“Yadesamānā vicaranti loke,",
        "analyzed": "yadā isamānāḥ vicaranti loke ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_488:15.2",
        "original": "Iṭṭhañca kantañca bahūnametaṁ;",
        "analyzed": "iṭṭhan ca kantan ca bahūn ameta ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_488:15.3",
        "original": "Piyaṁ manuññaṁ cidha jīvaloke,",
        "analyzed": "piyam manuññam cidha jīva loke ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_488:15.4",
        "original": "Kasmā isayo nappasaṁsanti kāme”.",
        "analyzed": "kasmai isayaḥ na pasasanti kāme ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_488:16.1",
        "original": "“Kāmesu ve haññare bajjhare ca,",
        "analyzed": "kāmesu ve haññare bajjhare ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_488:16.2",
        "original": "Kāmesu dukkhañca bhayañca jātaṁ;",
        "analyzed": "kāmesu dukkhañca bhayañca jāta ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_488:16.3",
        "original": "Kāmesu bhūtādhipatī pamattā,",
        "analyzed": "kāmesu bhūta adhipatī pamattā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_488:16.4",
        "original": "Pāpāni kammāni karonti mohā.",
        "analyzed": "pāpāni kammāni karonti mohā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_488:17.1",
        "original": "Te pāpadhammā pasavetva pāpaṁ,",
        "analyzed": "te pāpa dhammā pasavetva pāpa ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_488:17.2",
        "original": "Kāyassa bhedā nirayaṁ vajanti;",
        "analyzed": "kāyassa bhedāḥ nirayam vajanti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_488:17.3",
        "original": "Ādīnavaṁ kāmaguṇesu disvā,",
        "analyzed": "ādīnavam kāma guṇesu disvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_488:17.4",
        "original": "Tasmā isayo nappasaṁsanti kāme”.",
        "analyzed": "tasmai isayaḥ na pasasanti kāme ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_488:18.1",
        "original": "“Vīmaṁsamāno isino bhisāni,",
        "analyzed": "vīmasamānaḥ isinaḥ bhisāni ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_488:18.2",
        "original": "Tīre gahetvāna thale nidhesiṁ;",
        "analyzed": "tīre gahetvāna thale nidhesi ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_488:18.3",
        "original": "Suddhā apāpā isayo vasanti,",
        "analyzed": "suddhāḥ apāpāḥ isayaḥ vasanti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_488:18.4",
        "original": "Etāni te brahmacārī bhisāni”.",
        "analyzed": "etāni te brahmacārī bhisāni ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_488:19.1",
        "original": "“Na te naṭā no pana kīḷaneyyā,",
        "analyzed": "na te naṭā na u pana kīḍaneyyā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_488:19.2",
        "original": "Na bandhavā no pana te sahāyā;",
        "analyzed": "na bandhavā na u pana te sahāyā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_488:19.3",
        "original": "Kismiṁ vupatthambha sahassanetta,",
        "analyzed": "kismin vupatthambha sahassa netta ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_488:19.4",
        "original": "Isīhi tvaṁ kīḷasi devarāja”.",
        "analyzed": "isīhi tvam kīḷasi deva rāja ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_488:20.1",
        "original": "“Ācariyo mesi pitā ca mayhaṁ,",
        "analyzed": "ācariyaḥ mā isi pitā ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_488:20.2",
        "original": "Esā patiṭṭhā khalitassa brahme;",
        "analyzed": "esā patiṭṭhā khalitassa brahme ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_488:20.3",
        "original": "Ekāparādhaṁ khama bhūripañña,",
        "analyzed": "eka aparādham khama bhūri pañña ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_488:20.4",
        "original": "Na paṇḍitā kodhabalā bhavanti”.",
        "analyzed": "na paṇḍitā kodha balāḥ bhavanti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_488:21.1",
        "original": "“Suvāsitaṁ isinaṁ ekarattaṁ,",
        "analyzed": "su vāsitam isinam ekaratta ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_488:21.2",
        "original": "Yaṁ vāsavaṁ bhūtapatiddasāma;",
        "analyzed": "yam vāsavam bhūtapatit dasāma ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_488:21.3",
        "original": "Sabbeva bhonto sumanā bhavantu,",
        "analyzed": "sabbe iva bhonto sumanāḥ bhavantu ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_488:21.4",
        "original": "Yaṁ brāhmaṇo paccupādī bhisāni”.",
        "analyzed": "yam brāhmaṇaḥ paccupādī bhisāni ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_488:22.1",
        "original": "“Ahañca sāriputto ca,",
        "analyzed": "aham ca sāriputto ca ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_488:22.2",
        "original": "moggallāno ca kassapo;",
        "analyzed": "moggallānaḥ ca kassapaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_488:22.3",
        "original": "Anuruddho puṇṇo ānando,",
        "analyzed": "anuruddhaḥ puṇṇaḥ ānandaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_488:22.4",
        "original": "tadāsuṁ satta bhātaro.",
        "analyzed": "tadā āsum satta bhātaraḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_488:23.1",
        "original": "Bhaginī uppalavaṇṇā ca,",
        "analyzed": "bhaginī uppala vaṇṇā ca ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_488:23.2",
        "original": "dāsī khujjuttarā tadā;",
        "analyzed": "dāsī khujjuttarā tadā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_488:23.3",
        "original": "Citto gahapati dāso,",
        "analyzed": "cittaḥ gahapati dāsaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_488:23.4",
        "original": "yakkho sātāgiro tadā.",
        "analyzed": "yakkhaḥ sātāgiraḥ tadā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_488:24.1",
        "original": "Pālileyyo tadā nāgo,",
        "analyzed": "pālileyyaḥ tadā nāgaḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_488:24.2",
        "original": "madhudo seṭṭhavānaro;",
        "analyzed": "madhu daḥ seṭṭhavānaraḥ ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_488:24.3",
        "original": "Kāḷudāyī tadā sakko,",
