        "analyzed": "tā api tam saraṇam gatā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_506:9.1",
        "original": "Dhammena mocehi asāhasena,",
        "analyzed": "dhammena mocehi asāhasena ",
        "folio": "9"
    },
