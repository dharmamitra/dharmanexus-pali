    {
        "segmentnr": "PA_pp_1.2:1.1",
        "original": "Dve puggalā—",
        "analyzed": "dve puggalā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pp_1.2:2.1",
        "original": "Kodhano ca, upanāhī ca.",
        "analyzed": "kodhanaḥ ca upanāhī ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pp_1.2:3.1",
        "original": "Makkhī ca, paḷāsī ca.",
        "analyzed": "makkhī ca paḷāsī ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pp_1.2:4.1",
        "original": "Issukī ca, maccharī ca.",
        "analyzed": "issukī ca maccharī ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pp_1.2:5.1",
        "original": "Saṭho ca, māyāvī ca.",
        "analyzed": "ca māyāvī ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pp_1.2:6.1",
        "original": "Ahiriko ca, anottappī ca.",
        "analyzed": "ahirikaḥ ca anottappī ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pp_1.2:7.1",
        "original": "Dubbaco ca, pāpamitto ca.",
        "analyzed": "dubbacaḥ ca pāpa mitto ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pp_1.2:8.1",
        "original": "Indriyesu aguttadvāro ca, bhojane amattaññū ca.",
        "analyzed": "indriyesu agutta dvāraḥ ca bhojane amattaññū ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pp_1.2:9.1",
        "original": "Muṭṭhassati ca, asampajāno ca.",
        "analyzed": "muṭṭhaḥ sati ca asampajāno ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pp_1.2:10.1",
        "original": "Sīlavipanno ca, diṭṭhivipanno ca.",
        "analyzed": "sīla vipanno ca diṭṭhi vipanno ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pp_1.2:11.1",
        "original": "Ajjhattasaṁyojano ca, bahiddhāsaṁyojano ca.",
        "analyzed": "ajjhatta sayojanaḥ ca bahiddhā sayojanaḥ ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pp_1.2:12.1",
        "original": "Akkodhano ca, anupanāhī ca.",
        "analyzed": "akkodhanaḥ ca anupanāhī ca ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pp_1.2:13.1",
        "original": "Amakkhī ca, apaḷāsī ca.",
        "analyzed": "a makkhī ca a paḷāsī ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_pp_1.2:14.1",
        "original": "Anissukī ca, amaccharī ca.",
        "analyzed": "a nissukī ca a maccharī ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_pp_1.2:15.1",
        "original": "Asaṭho ca, amāyāvī ca.",
        "analyzed": "a saṭhaḥ ca a māyāvī ca ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_pp_1.2:16.1",
        "original": "Hirimā ca, ottappī ca.",
        "analyzed": "hirimā ca ottappī ca ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_pp_1.2:17.1",
        "original": "Suvaco ca, kalyāṇamitto ca.",
        "analyzed": "su vacaḥ ca kalyāṇa mitto ca ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_pp_1.2:18.1",
        "original": "Indriyesu guttadvāro ca, bhojane mattaññū ca.",
        "analyzed": "indriyesu ca bhojane mattaññū ca ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_pp_1.2:19.1",
        "original": "Upaṭṭhitassati ca, sampajāno ca.",
        "analyzed": "upaṭṭhitaḥ sati ca sampajānaḥ ca ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_pp_1.2:20.1",
        "original": "Sīlasampanno ca, diṭṭhisampanno ca.",
        "analyzed": "sīla sampanno ca diṭṭhi sampanno ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_pp_1.2:21.1",
        "original": "Dve puggalā dullabhā lokasmiṁ.",
        "analyzed": "dve puggalā dullabhā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_pp_1.2:22.1",
        "original": "Dve puggalā duttappayā.",
        "analyzed": "dve puggalāḥ duttappayā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_pp_1.2:23.1",
        "original": "Dve puggalā sutappayā.",
        "analyzed": "dve puggalā su tappayā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_pp_1.2:24.1",
        "original": "Dvinnaṁ puggalānaṁ āsavā vaḍḍhanti.",
        "analyzed": "dvinnam puggalānam āsavāḥ vaḍḍhanti ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_pp_1.2:25.1",
        "original": "Dvinnaṁ puggalānaṁ āsavā na vaḍḍhanti.",
        "analyzed": "dvinnam puggalānam āsavāḥ na vaḍḍhanti ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_pp_1.2:26.1",
        "original": "Hīnādhimutto ca, paṇītādhimutto ca.",
        "analyzed": "hīna adhimuttaḥ ca paṇīta adhimuttaḥ ca ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_pp_1.2:27.1",
        "original": "Titto ca, tappetā ca.",
        "analyzed": "tittaḥ ca tappetā ca ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_pp_1.2:28.1",
        "original": "Dukaṁ.",
        "analyzed": "duka ",
        "folio": "28"
    }
