        "segmentnr": "PA_ja_512:24.2",
        "original": "sappaṁ ghoravisamiva;",
        "analyzed": "sappam ghora visam iva ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_512:24.3",
        "original": "Taṁ loke visasamānaṁ,",
        "analyzed": "tam loke visasamāna ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_512:24.4",
        "original": "ko naro pātumarahati.",
        "analyzed": "kaḥ pātum arahati ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_512:25.1",
        "original": "Yaṁ ve pitvā andhakaveṇḍaputtā,",
        "analyzed": "yam ve pitvā andhaka veṇḍa puttā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_512:25.2",
        "original": "Samuddatīre paricārayantā;",
        "analyzed": "samudda tīre paricārayantā ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_512:25.3",
        "original": "Upakkamuṁ musalehaññamaññaṁ,",
        "analyzed": "upakkamum musale haññamañña ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_512:25.4",
        "original": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
        "analyzed": "tassā puṇṇam kumbham imam kiṇātha ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_512:26.1",
        "original": "Yaṁ ve pitvā pubbadevā pamattā,",
        "analyzed": "yam ve pitvā pubbadevā pamattā ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_512:26.2",
        "original": "Tidivā cutā sassatiyā samāya;",
        "analyzed": "tidivā cutā sassatiyā samāya ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_512:26.3",
        "original": "Taṁ tādisaṁ majjamimaṁ niratthakaṁ,",
        "analyzed": "tam tādisam majjam imam niratthaka ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_512:26.4",
        "original": "Jānaṁ mahārāja kathaṁ piveyya.",
        "analyzed": "jānam mahā rāja katham piveyya ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_512:27.1",
        "original": "Nayimasmiṁ kumbhasmiṁ dadhi vā madhu vā,",
        "analyzed": "dadhi vā madhu vā ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_512:27.2",
        "original": "Evaṁ abhiññāya kiṇāhi rāja;",
        "analyzed": "evam abhiññāya kiṇāhi rāja ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_512:27.3",
        "original": "Evañhimaṁ kumbhagatā mayā te,",
        "analyzed": "evam hi imam kumbha gatāḥ mayā te ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_512:27.4",
        "original": "Akkhātarūpaṁ tava sabbamitta”.",
        "analyzed": "akkhāta rūpam tava sabba mitta ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_512:28.1",
        "original": "“Na me pitā vā athavāpi mātā,",
        "analyzed": "na me pitā vā athavā api mātā ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_512:28.2",
        "original": "Etādisā yādisako tuvaṁsi;",
        "analyzed": "etādisā tuvasi ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_512:28.3",
        "original": "Hitānukampī paramatthakāmo,",
        "analyzed": "hita anukampī paramattha kāmaḥ ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_512:28.4",
        "original": "Sohaṁ karissaṁ vacanaṁ tavajja.",
        "analyzed": "sā uham karissam vacanam tavajja ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_512:29.1",
        "original": "Dadāmi te gāmavarāni pañca,",
        "analyzed": "dadāmi te gām avarāni pañca ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_512:29.2",
        "original": "Dāsīsataṁ satta gavaṁsatāni;",
        "analyzed": "dāsī satam satta gava satāni ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_512:29.3",
        "original": "Ājaññayutte ca rathe dasa ime,",
        "analyzed": "ājañña yutte ca rathe dasa ime ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_512:29.4",
        "original": "Ācariyo hosi mamatthakāmo”.",
        "analyzed": "ācariyaḥ hosi mamattha kāmo ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_512:30.1",
        "original": "“Taveva dāsīsatamatthu rāja,",
        "analyzed": "tava iva rāja ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_512:30.2",
        "original": "Gāmā ca gāvo ca taveva hontu;",
        "analyzed": "gāmā ca gāvaḥ ca tava iva hontu ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_512:30.3",
        "original": "Ājaññayuttā ca rathā taveva,",
        "analyzed": "ājañña yuttā ca rathā tava iva ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_512:30.4",
        "original": "Sakkohamasmī tidasānamindo.",
        "analyzed": "sakkā uham asmi tidasānam indo ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_512:31.1",
        "original": "Maṁsodanaṁ sappipāyāsaṁ bhuñja,",
        "analyzed": "masodanam sappi pāyāsam bhuñja ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_512:31.2",
        "original": "Khādassu ca tvaṁ madhumāsapūve;",
        "analyzed": "khādassu ca tvam madhu māsa pūve ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_512:31.3",
        "original": "Evaṁ tuvaṁ dhammarato janinda,",
        "analyzed": "evam tuvam dhamma rataḥ janinda ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_512:31.4",
        "original": "Anindito saggamupehi ṭhānan”ti.",
        "analyzed": "aninditaḥ saggam upehi ṭhānan ti ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_512:32.1",
        "original": "Kumbhajātakaṁ dutiyaṁ.",
        "analyzed": "kumbha jātakam ",
        "folio": "32"
    }
]