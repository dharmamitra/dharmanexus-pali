        "segmentnr": "PA_ja_454:13.4",
        "original": "puttasokaṁ apānudi.",
        "analyzed": "putta sokam apānudi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_454:14.1",
        "original": "Sohaṁ abbūḷhasallosmi,",
        "analyzed": "saḥ aham abbūḍha sallaḥ asmi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_454:14.2",
        "original": "vītasoko anāvilo;",
        "analyzed": "vīta sokaḥ anāvilaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_454:14.3",
        "original": "Na socāmi na rodāmi,",
        "analyzed": "na socāmi na rodāmi ",
        "folio": "14"
    },
