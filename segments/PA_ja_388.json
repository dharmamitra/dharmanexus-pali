        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_388:2.4",
        "original": "Maṁsatthāya hi positāmhase.",
        "analyzed": "masatthāya hi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_388:3.1",
        "original": "Ogaha rahadaṁ akaddamaṁ,",
