[
    {
        "segmentnr": "PA_anya-e12_04n:0",
        "original": "Dhātupāṭha vilāsiniyā",
        "analyzed": "dhātupāṭha vilāsiniyā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_04n:1",
        "original": "Namo tassa bhagavato arahato sammāsambuddhassa",
        "analyzed": "namaḥ tassa bhagavataḥ arahataḥ sammāsambuddhassa ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_04n:3",
        "original": "Sammāsambuddha sūriyo yo sambodho dayo dito;",
        "analyzed": "sammāsambuddha sūriyaḥ yaḥ sambodhaḥ dayaḥ ditaḥ ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_04n:4",
        "original": "Jagu paṅkaja saṅghāte bodhayī paṇamāmi taṃ.",
        "analyzed": "jagu paṅkaja saṅghāte bodhayī paṇamāmi tam ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_04n:6",
        "original": "Saddhammabhānu yo lokā lokaṃ katvāna dhī tamaṃ;",
        "analyzed": "yaḥ lokāḥ lokam katvāna tamam ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_04n:7",
        "original": "Dhaṃsayī muninā sammā pātubhūto namāmi taṃ.",
        "analyzed": "dhaṃsayī muninā pātu bhūtaḥ namāmi tam ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_04n:9",
        "original": "Silagandhasamākiṇṇo buddho saddhammahāya yo;",
        "analyzed": "sila gandha samākiṇṇaḥ buddhaḥ saddhamma hāya yaḥ ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_anya-e12_04n:10",
        "original": "Saṅghatoyaruho pāṇa lī tosesi namāmi taṃ-",
        "analyzed": "saṅgha toyaruhaḥ namāmi tam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:12",
        "original": "Natvā mama garuṃvāsi padumārāma nāmakaṃ;",
        "analyzed": "natvā mama nāmakam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:13",
        "original": "Pāḷiṃ nissāya kassā haṃ dhātupāṭhavilāsini-",
        "analyzed": "pāḷim nissāya kassā ham dhātu pāṭha vilāsini ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:15",
        "original": "Imañhi ganthakaraṇaṃ satthāgamanaye rato;",
        "analyzed": "iman hi gantha karaṇam satthā āgamanaye rataḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:16",
        "original": "Mūkalaṃgamu saṅkhāte gāme sajjanakārite-",
        "analyzed": "saṅkhāte gāme sat jana kārite ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:18",
        "original": "Suvisuddhārāmanāma vihāramhi nivāsako;",
        "analyzed": "suvisuddha ārāma nāma vihāramhi nivāsakaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:19",
        "original": "Tasmiṃ padhāna thero si kataññū santavutti yo-",
        "analyzed": "tasmin theraḥ si kataññū santavutti yaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:21",
        "original": "Guṇālaṅkāranāmo so thero theranvaye rato;",
        "analyzed": "saḥ theraḥ theḥ anvaye rataḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:22",
        "original": "Yāci maṃ abhigantvāna mitto me vaṅkamānaso.",
        "analyzed": "yāci mam abhigantvāna mittaḥ me vaṅka mānasaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:24",
        "original": "Buddho hessaṃ yadā loke niddeso haṃ tadā iti;",
        "analyzed": "buddhaḥ hessam yadā loke niddesaḥ ham tadā iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:25",
        "original": "Pāpuṇissa mahaṅkāraṃ ko vādo pani hantare-",
        "analyzed": "pāpuṇissa mahaṅkāram kaḥ vādaḥ hantare ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:26",
        "original": "Appaccayo paro hoti bhūvādi gaṇato sati;",
        "analyzed": "appaccayaḥ paraḥ hoti bhūva ādi gaṇataḥ sati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:27",
        "original": "Suddhakattu kirayākhyāne sabbadhātuka nissitaṃ-",
        "analyzed": "suddha kattu kiraya ākhyāne sabba dhātuka nissitam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:28",
        "original": "Payutto kattunā yoge ṭhito yevā ppadhāniye;",
        "analyzed": "payuttaḥ kattunā yoge ṭhitaḥ ye vā ppadhāniye ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:29",
        "original": "Kirayaṃ sādheti etassa dīpakaṃ sāsane padaṃ-",
        "analyzed": "sādha iti etassa dīpakam sāsane padam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_anya-e12_04n:30",
        "original": "Karaṇa vacanaṃyeva yebhuyyena padissati;",
        "analyzed": "karaṇa vacanaṃyā iva yebhuyyena padissati ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:31",
        "original": "Ākhyāte kāritaṭṭhānaṃ sandhāya kathitaṃ idaṃ;",
        "analyzed": "ākhyāte kārita ṭṭhānam sandhāya kathitam idam ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:32",
        "original": "Na nāme kārataṭṭhānaṃ bodhetā itiādikaṃ-",
        "analyzed": "na iti ādikam ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:33",
        "original": "Sunakhehipi khādāpenti iccādini padānitu;",
        "analyzed": "khādāpenti it ca ādini ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:34",
        "original": "Āharitvāna dīpeyya payoga kusalo budho-yī.",
        "analyzed": "āharitvāna dīpeyya payoga kusalaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:35",
        "original": "Kathito sacca saṅkhepe paccanta vacanena ve;",
        "analyzed": "kathitaḥ sacca saṅkhepe paccanta vacanena ve ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:36",
        "original": "Bhuyyate iti saddassa sambandho bhāvadīpano-",
        "analyzed": "bhuyyate iti saddassa sambandhaḥ bhāva dīpanaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:37",
        "original": "Niddesapāḷiyaṃ rūpaṃ vihoti vihavīyati;",
        "analyzed": "niddesa pāḷiyam rūpam vihoti vihavīyati ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:38",
        "original": "Iti dassanatovāpi paccattavacanaṃ thiraṃ-",
        "analyzed": "iti dassanataḥ vā api paccatta vacanam thiram ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:39",
        "original": "Tathā dhajaggasuttante muninā hacca bhāsite;",
        "analyzed": "tathā dhajagga suttante muninā hac ca bhāsite ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_anya-e12_04n:40",
        "original": "So pahīyissati iti pāḷidassanatopica-",
        "analyzed": "saḥ pahīyissati iti ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:41",
        "original": "Pāramitānu bhāvena mahesīnaṃva dehato;",
        "analyzed": "bhāvena ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:42",
        "original": "Sanni nipphādanā neva sakkaṭādi vaco viya-",
        "analyzed": "sanni nipphādanā na iva sakkaṭa ādi vacaḥ ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:43",
        "original": "Paccatta dassaneneva purisattaya yojanaṃ;",
        "analyzed": "paccatta dassanena iva purisattaya yojanam ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:44",
        "original": "Ekavacanikañcāpi bahuvacanikampica;",
        "analyzed": "ekavacanikam ca api bahuvacanikam pi ca ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:45",
        "original": "Kātabba miti no khantī parassapadaādike-",
        "analyzed": "kātabba m iti no khantī parassa pada ādike ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:46",
        "original": "Bhāve kirayāpadaṃ nāma pāḷiyā atiduddasaṃ;",
        "analyzed": "bhāve kirayāpadam nāma pāḷiyāḥ ati duddasam ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:47",
        "original": "Tasmā taggahaṇūpāyo vutto ettāvatā mayā-yī.",
        "analyzed": "tasmā tad gahaṇa upāyaḥ vuttaḥ ettāvatā ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:48",
        "original": "Yaṃ tikālaṃ tipurisaṃ kirayāvāci tikārakaṃ;",
        "analyzed": "yam ti kālam ti purisam ti kārakam ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:49",
        "original": "Attiliṅgaṃ dvivacanaṃ ta dākhyātanti vuccati-yī.",
        "analyzed": "atti liṅgam dvivacanam ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_anya-e12_04n:50",
        "original": "Ākhyāta sāgara matha jjatani taraṅgaṃ,",
        "analyzed": "ākhyāta sāgara taraṅgam ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:51",
        "original": "Dhātujjalaṃ vikaraṇa gama kālamīnaṃ;",
        "analyzed": "vikaraṇa gama kāla mīnam ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:52",
        "original": "Lopā nubandha raya mattha vibhāgatīraṃ,",
        "analyzed": "lopa nubandha raya mattha vibhāga tīram ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:53",
        "original": "Dhīrā taranti kavino puthu buddhi nāvā-yī.",
        "analyzed": "dhīrā taranti kavino puthu buddhi na āvāyī ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:54",
        "original": "Cakkhakkhī nayanaṃ nettaṃ locanaṃ diṭṭhi dassanaṃ;",
        "analyzed": "cakkha kkhī nayanam nettam locanam diṭṭhi dassanam ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:55",
        "original": "Pekkhanaṃ acchi pamhantu pakhumanti pavuccati-yi.",
        "analyzed": "pekkhanam acchi pamhantu pakhumanti ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:56",
        "original": "“Pabbājito sakā raṭṭhā, aññaṃ janapadaṃ gato,",
        "analyzed": "pabbājitaḥ sakā raṭṭhā aññam janapadam gataḥ ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:57",
        "original": "Mahantaṃ koṭṭhaṃ kayirātha, duruttānaṃ nivetave”-",
        "analyzed": "mahantam koṭṭham kayirā atha duruttānam nivetave ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:58",
        "original": "Porāṇa metaṃ atula netaṃ ajjatanāmiva,",
        "analyzed": "porāṇa metam atula netam ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:59",
        "original": "Nindanti tuṇhi māsīnaṃ nindanti bahubhāṇinaṃ;",
        "analyzed": "nindanti tuṇhi māsīnam nindanti bahu bhāṇinam ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_anya-e12_04n:60",
        "original": "Mitabhāṇinampi nindanti natthi loke anindito-",
        "analyzed": "mita bhāṇinam pi nindanti natthi loke anindito ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:61",
        "original": "Nagaraṃ yathā paccantaṃ “guttaṃ” santarabāhiyaṃ,",
        "analyzed": "nagaram yathā paccantam guttam santarabāhiyam ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:62",
        "original": "Evaṃ ‘gopetha’ attānaṃ, khaṇe ve mā upaccagā-",
        "analyzed": "evam gopetha attānam khaṇe ve mā upaccagā ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:63",
        "original": "Dhiratthu taṃ visavantaṃ, yamahaṃ jīvita kāraṇā,",
        "analyzed": "tam visavantam yam aham jīvita kāraṇā ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:64",
        "original": "Vantaṃ pacchā vamissāmi, mataṃ me jivitaṃ varaṃ-",
        "analyzed": "vantam pacchā vamissāmi matam me jivitam varam ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:65",
        "original": "Viluppateva puriso, yāvassa upakappati,",
        "analyzed": "viluppatā iva purisaḥ yāva assa upakappati ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:66",
        "original": "Yadā caññe vilumpantī, so vilutto vilumpatī-",
        "analyzed": "yadā caññe vilumpantī saḥ viluttaḥ vilumpatī ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:67",
        "original": "“Appamādo amatapadaṃ, pamādo maccuno padaṃ,",
        "analyzed": "appamādaḥ a mata padam pamādaḥ maccuno padam ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:68",
        "original": "Appamattā na mīyanti, ye pamattā yathāmatā”.",
        "analyzed": "appamattāḥ na mīyanti ye pamattāḥ yathā amatā ",
        "folio": "60"
    },
    {
        "segmentnr": "PA_anya-e12_04n:70",
        "original": "Ñāṇavimala tissākhyo, yo mahāsaṅgha nāyako,",
        "analyzed": "yaḥ ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_04n:71",
        "original": "Marammavaṃsaṃ ādoca, dīpe saṇṭhāpayī idha-",
        "analyzed": "dīpe saṇṭhāpayī idha ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_04n:73",
        "original": "Tassa padhāna sissosi, pāḷi yaṭṭhakathā vidū,",
        "analyzed": "tassa padhāna sissosi pāḷi yaṭṭhakathā vidū ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_04n:74",
        "original": "Dhammadhāra samaññāto, yo mahā saṅghasāmiko-",
        "analyzed": "dhammadhāra samaññātaḥ yaḥ mahā saṅgha sāmikaḥ ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_04n:76",
        "original": "Yo tassa mukhyasiṃssā si, dhamme sattheva kovido,",
        "analyzed": "yaḥ tassa mukhya siṃssā si dhamme satthā iva kovidaḥ ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_04n:77",
        "original": "Ñāṇānanda mahāthero, khe mā viya supākaṭo-",
        "analyzed": "mahā theraḥ khe mā supākaṭaḥ ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_04n:79",
        "original": "Vimalasāra tissākhyo, mahāsaṃsādhipo kavi,",
        "analyzed": "vimala sāra kavi ",
        "folio": "70"
    },
    {
        "segmentnr": "PA_anya-e12_04n:80",
        "original": "Sissosi dutiyo tassa, pariyatti visārado-",
        "analyzed": "dutiyaḥ tassa pariyatti visāradaḥ ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:82",
        "original": "Padumārāma nāmo me, ācero therapuṅgavo,",
        "analyzed": "nāmaḥ me āceraḥ thera puṅgavaḥ ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:83",
        "original": "Tatiyo tassa sisso si sikkhāgārava saññuto-",
        "analyzed": "tatiyo tassa sissaḥ si sikkhāgārava saññuto ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:85",
        "original": "Saṅghādhipoca vimala, sārākhyo therakuñjaro,",
        "analyzed": "saṅgha adhipaḥ ca vimala sāra ākhyaḥ thera kuñjaraḥ ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:86",
        "original": "Padumārāma vikhyāta, mahāthero cime duve-",
        "analyzed": "vikhyāta cime duve ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:88",
        "original": "Dhammādhāra mahāsaṅgha, sāminoca upantike,",
        "analyzed": "dhamma ādhāra mahā saṅgha sāmi noce upantike ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:89",
        "original": "Ñāṇānanda mahāthera, ssantikeva samuggahuṃ-",
        "analyzed": "ñāṇa ānanda mahā thera ssantikā iva samuggahum ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:91",
        "original": "Tesu kho padumārāma mahāthero avaṃ mamaṃ,",
        "analyzed": "tesu kho mahā theraḥ ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:92",
        "original": "Sikkhayi sadda sattheca, pāḷiyaṭṭhakathāsu ca-",
        "analyzed": "sikkhayi sadda satthe ca pāḷiya ṭṭhakathāsu ca ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:94",
        "original": "Tasmiṃ divaṅgate pacchā, chando vyākaraṇādikaṃ,",
        "analyzed": "tasmin divaṅgate pacchā chandaḥ vyākaraṇa ādikam ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:95",
        "original": "Vimalasāra mahāthera, ssantikeca samuggahiṃ-",
        "analyzed": "vimala sāra mahā thera ssantike ca samuggahiṃ ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:97",
        "original": "Tassa kho padumārāma mahātherassa dhīmato,",
        "analyzed": "tassa kho padumārāma mahā therassa dhīmataḥ ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:98",
        "original": "Sissena ñāṇatilaka therena saṃsasāminā-",
        "analyzed": "sissena ñāṇatilaka therena saṃsasāminā ",
        "folio": "80"
    },
    {
        "segmentnr": "PA_anya-e12_04n:100",
        "original": "Buddhassa parinibbāṇa vīsahasse catussate,",
        "analyzed": "buddhassa parinibbāṇa vīsahasse catussate ",
        "folio": "100"
    },
    {
        "segmentnr": "PA_anya-e12_04n:101",
        "original": "Sa sattatyādhike vasse jeṭṭhamāse manorame-",
        "analyzed": "sa sattatya adhike vasse jeṭṭha māse manorame ",
        "folio": "100"
    },
    {
        "segmentnr": "PA_anya-e12_04n:103",
        "original": "Aṭṭhamiyaṃ kāḷapakkhe, katāyaṃ matisūdanī,",
        "analyzed": "aṭṭhamiyam kāḷapakkhe katā ayam mati sūdanī ",
        "folio": "100"
    },
    {
        "segmentnr": "PA_anya-e12_04n:104",
