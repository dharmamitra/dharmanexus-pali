        "analyzed": "kaṇha dīpāyanaḥ isi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_cp_31:1.3",
        "original": "Paropaññāsavassāni,",
        "analyzed": "paropaññāsa vassāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_cp_31:1.4",
        "original": "anabhiratocariṁ ahaṁ.",
        "analyzed": "an abhirata ucarim aha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_cp_31:2.1",
        "original": "Na koci etaṁ jānāti,",
        "analyzed": "na koci etam jānāti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cp_31:2.2",
        "original": "Anabhiratimanaṁ mama;",
        "analyzed": "an abhirati manam mama ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cp_31:2.3",
        "original": "Ahañhi kassaci nācikkhiṁ,",
        "analyzed": "aham hi kassaci na ācikkhi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cp_31:2.4",
        "original": "Arati me carati mānase.",
        "analyzed": "arati me carati mānase ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_cp_31:3.1",
        "original": "Sabrahmacārī maṇḍabyo,",
        "analyzed": "sabrahmacārī maṇḍabyo ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cp_31:3.2",
        "original": "sahāyo me mahāisi;",
        "analyzed": "sahāyaḥ me mahā aisi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cp_31:3.3",
        "original": "Pubbakammasamāyutto,",
        "analyzed": "pubba kamma samāyuttaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cp_31:3.4",
        "original": "sūlamāropanaṁ labhi.",
        "analyzed": "sūlam āropanam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_cp_31:4.1",
        "original": "Tamahaṁ upaṭṭhahitvāna,",
        "analyzed": "tam aham upaṭṭhahitvāna ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_31:4.2",
        "original": "ārogyamanupāpayiṁ;",
        "analyzed": "ārogyam anupāpayi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_31:4.3",
        "original": "Āpucchitvāna āgañchiṁ,",
        "analyzed": "āpucchitvānaḥ āgañchi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_31:4.4",
        "original": "yaṁ mayhaṁ sakamassamaṁ.",
        "analyzed": "yam mayham sakamassama ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_cp_31:5.1",
        "original": "Sahāyo brāhmaṇo mayhaṁ,",
        "analyzed": "sahāyaḥ brāhmaṇaḥ mayha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_cp_31:5.2",
        "original": "bhariyaṁ ādāya puttakaṁ;",
        "analyzed": "bhariyam ādāya puttaka ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_cp_31:5.3",
        "original": "Tayo janā samāgantvā,",
        "analyzed": "tayoḥ janā samāgantvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_cp_31:5.4",
        "original": "āgañchuṁ pāhunāgataṁ.",
        "analyzed": "āgañchum pāhunā āgata ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_cp_31:6.1",
        "original": "Sammodamāno tehi saha,",
        "analyzed": "sammodamānaḥ tehi saha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_cp_31:6.2",
        "original": "nisinno sakamassame;",
        "analyzed": "nisinnaḥ sa kamassame ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_cp_31:6.3",
        "original": "Dārako vaṭṭamanukkhipaṁ,",
        "analyzed": "dārakaḥ vaṭṭam anukkhipa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_cp_31:6.4",
        "original": "āsīvisamakopayi.",
        "analyzed": "āsīvi sama kopayi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_cp_31:7.1",
        "original": "Tato so vaṭṭagataṁ maggaṁ,",
        "analyzed": "tatas saḥ vaṭṭa gatam magga ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_cp_31:7.2",
        "original": "anvesanto kumārako;",
        "analyzed": "anvesantaḥ kumārakaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_cp_31:7.3",
        "original": "Āsīvisassa hatthena,",
        "analyzed": "āsīvisassa hatthena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_cp_31:7.4",
        "original": "uttamaṅgaṁ parāmasi.",
        "analyzed": "uttamaṅgam parāmasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_cp_31:8.1",
        "original": "Tassa āmasane kuddho,",
        "analyzed": "tassa āmasane kuddhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_cp_31:8.2",
        "original": "sappo visabalassito;",
        "analyzed": "sappaḥ visa bala ssitaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_cp_31:8.3",
        "original": "Kupito paramakopena,",
        "analyzed": "kupitaḥ parama kopena ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_cp_31:8.4",
        "original": "aḍaṁsi dārakaṁ khaṇe.",
        "analyzed": "aḍasi dārakam khaṇe ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_cp_31:9.1",
        "original": "Sahadaṭṭho āsīvisena,",
        "analyzed": "saha daṭṭhaḥ āsīvisena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_cp_31:9.2",
        "original": "dārako papati bhūmiyaṁ;",
        "analyzed": "dārako papati bhūmiya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_cp_31:9.3",
        "original": "Tenāhaṁ dukkhito āsiṁ,",
        "analyzed": "tena aham dukkhitaḥ āsi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_cp_31:9.4",
        "original": "mama vāhasi taṁ dukkhaṁ.",
        "analyzed": "mama tam dukkha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_cp_31:10.1",
        "original": "Tyāhaṁ assāsayitvāna,",
        "analyzed": "tyā aham ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_cp_31:10.2",
        "original": "dukkhite sokasallite;",
        "analyzed": "dukkhite soka sallite ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_cp_31:10.3",
        "original": "Paṭhamaṁ akāsiṁ kiriyaṁ,",
        "analyzed": "paṭhamam a kāsim ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_cp_31:10.4",
        "original": "aggaṁ saccaṁ varuttamaṁ.",
        "analyzed": "aggam saccam varuttama ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_cp_31:11.1",
        "original": "‘Sattāhamevāhaṁ pasannacitto,",
        "analyzed": "sattāham eva aham pasanna cittaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_cp_31:11.2",
        "original": "Puññatthiko acariṁ brahmacariyaṁ;",
        "analyzed": "puññatthikaḥ acarim brahmacariya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_cp_31:11.3",
        "original": "Athāparaṁ yaṁ caritaṁ mamedaṁ,",
        "analyzed": "atha aparam yam caritam mama idam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_cp_31:11.4",
        "original": "Vassāni paññāsasamādhikāni.",
        "analyzed": "vassāni paññāsa sama adhikāni ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_cp_31:12.1",
        "original": "Akāmako vāhi ahaṁ carāmi,",
        "analyzed": "akāmakaḥ vāhi aham carāmi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_cp_31:12.2",
        "original": "Etena saccena suvatthi hotu;",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_cp_31:12.3",
        "original": "Hataṁ visaṁ jīvatu yaññadatto’.",
        "analyzed": "hatam visam jīvatu yañña dattaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_cp_31:13.1",
        "original": "Saha sacce kate mayhaṁ,",
        "analyzed": "saha sacce kate mayha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_cp_31:13.2",
        "original": "visavegena vedhito;",
        "analyzed": "visa vegena vedhitaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_cp_31:13.3",
        "original": "Abujjhitvāna vuṭṭhāsi,",
        "analyzed": "a bujjhitvāna vuṭṭhāsi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_cp_31:13.4",
        "original": "arogo cāsi māṇavo;",
        "analyzed": "arogaḥ ca asi māṇavaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_cp_31:13.5",
        "original": "Saccena me samo natthi,",
        "analyzed": "saccena me samaḥ natthi ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_cp_31:13.6",
        "original": "esā me saccapāramī”ti.",
        "analyzed": "esā me sacca pāramī ti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_cp_31:14.1",
