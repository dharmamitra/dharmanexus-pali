[
    {
        "segmentnr": "PA_ja_457:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_457:0.2",
        "original": "Ekādasakanipāta",
        "analyzed": "ekādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_457:0.3",
        "original": "Mātuposakavagga",
        "analyzed": "mātu posaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_457:0.4",
        "original": "3. Dhammadevaputtajātaka",
        "analyzed": "dhammadeva putta jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_457:1.1",
        "original": "“Yasokaro puññakarohamasmi,",
        "analyzed": "yasokaro puñña karaḥ ham asmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_457:1.2",
        "original": "Sadātthuto samaṇabrāhmaṇānaṁ;",
        "analyzed": "samaṇa brāhmaṇāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_457:1.3",
        "original": "Maggāraho devamanussapūjito,",
        "analyzed": "        ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_457:1.4",
        "original": "Dhammo ahaṁ dehi adhamma maggaṁ”.",
        "analyzed": "dhammaḥ aham dehi adhamma magga ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_457:2.1",
        "original": "“Adhammayānaṁ daḷhamāruhitvā,",
        "analyzed": "adhamma yānam daḷham āruhitvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_457:2.2",
        "original": "Asantasanto balavāhamasmi;",
        "analyzed": "asanta santaḥ balavāham asmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_457:2.3",
        "original": "Sa kissa hetumhi tavajja dajjaṁ,",
        "analyzed": "sa kissa hetumhi tavajja dajja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_457:2.4",
        "original": "Maggaṁ ahaṁ dhamma adinnapubbaṁ”.",
        "analyzed": "maggam aham dhamma adinnapubba ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_457:3.1",
        "original": "“Dhammo have pāturahosi pubbe,",
        "analyzed": "dhammaḥ have pāturahosi pubbe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_457:3.2",
        "original": "Pacchā adhammo udapādi loke;",
        "analyzed": "pacchā adhammaḥ udapādi loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_457:3.3",
        "original": "Jeṭṭho ca seṭṭho ca sanantano ca,",
        "analyzed": "jeṭṭhaḥ ca seṭṭhaḥ ca sanantano ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_457:3.4",
        "original": "Uyyāhi jeṭṭhassa kaniṭṭha maggā”.",
        "analyzed": "uyyāhi jeṭṭhassa kaniṭṭha maggā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_457:4.1",
        "original": "“Na yācanāya napi pātirūpā,",
        "analyzed": "na yācanāya na pi pātirūpā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_457:4.2",
        "original": "Na arahatā tehaṁ dadeyyaṁ maggaṁ;",
        "analyzed": "na arahatā dadeyyam magga ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_457:4.3",
        "original": "Yuddhañca no hotu ubhinnamajja,",
        "analyzed": "yuddham ca naḥ hotu ubhin amajja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_457:4.4",
        "original": "Yuddhamhi yo jessati tassa maggo”.",
        "analyzed": "yuddhamhi yaḥ jessati tassa maggo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_457:5.1",
        "original": "“Sabbā disā anuvisaṭohamasmi,",
        "analyzed": "sabbāḥ disāḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_457:5.2",
        "original": "Mahabbalo amitayaso atulyo;",
        "analyzed": "mahat balaḥ amita yasaḥ atulyaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_457:5.3",
        "original": "Guṇehi sabbehi upetarūpo,",
        "analyzed": "guṇehi sabbehi upeta rūpaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_457:5.4",
        "original": "Dhammo adhamma tvaṁ kathaṁ vijessasi.",
        "analyzed": "dhammaḥ adhamma tvam katham vijessasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_457:6.1",
        "original": "Lohena ve haññati jātarūpaṁ,",
        "analyzed": "lohena jātarūpa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_457:6.2",
        "original": "Na jātarūpena hananti lohaṁ;",
        "analyzed": "na jātarūpena hananti loha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_457:6.3",
        "original": "Sace adhammo hañchati dhammamajja,",
        "analyzed": "sace adhammaḥ hañchati dhamma majja ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_457:6.4",
        "original": "Ayo suvaṇṇaṁ viya dassaneyyaṁ.",
        "analyzed": "ayaḥ suvaṇṇam viya dassaneyya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_457:7.1",
        "original": "Sace tuvaṁ yuddhabalo adhamma,",
        "analyzed": "sace tuvam yuddha balaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_457:7.2",
        "original": "Na tuyha vuḍḍhā ca garū ca atthi;",
        "analyzed": "na tuyha vuḍḍhā ca garū ca atthi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_457:7.3",
        "original": "Maggañca te dammi piyāppiyena,",
        "analyzed": "maggam ca te dammi piya appiyena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_457:7.4",
        "original": "Vācāduruttānipi te khamāmi”.",
        "analyzed": "te khamāmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_457:8.1",
        "original": "“Idañca sutvā vacanaṁ adhammo,",
        "analyzed": "idam ca sutvā vacanam ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_457:8.2",
        "original": "Avaṁsiro patito uddhapādo;",
        "analyzed": "avasiraḥ patitaḥ uddha pādaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_457:8.3",
        "original": "‘Yuddhatthiko ce na labhāmi yuddhaṁ’,",
        "analyzed": "yuddhatthikaḥ ce na labhāmi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_457:8.4",
        "original": "Ettāvatā hoti hato adhammo.",
        "analyzed": "ettāvatā hoti hataḥ adhammo ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_457:9.1",
        "original": "Khantībalo yuddhabalaṁ vijetvā,",
        "analyzed": "khantī balaḥ yuddha balam vijetvā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_457:9.2",
        "original": "Hantvā adhammaṁ nihanitva bhūmyā;",
        "analyzed": "hantvā adhammam nihanitvā bhūmyā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_457:9.3",
        "original": "Pāyāsi vitto abhiruyha sandanaṁ,",
        "analyzed": "pāyāsi vittaḥ abhiruyha sandana ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_457:9.4",
        "original": "Maggeneva atibalo saccanikkamo.",
        "analyzed": "maggena iva atibalaḥ sacca nikkamaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_457:10.1",
        "original": "Mātā pitā samaṇabrāhmaṇā ca,",
        "analyzed": "mātā pitā samaṇa brāhmaṇā ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_457:10.2",
        "original": "Asammānitā yassa sake agāre;",
        "analyzed": "a sammānitā yassa sake agāre ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_457:10.3",
        "original": "Idheva nikkhippa sarīradehaṁ,",
        "analyzed": "idha iva nikkhippa sarīra deha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_457:10.4",
        "original": "Kāyassa bhedā nirayaṁ vajanti te;",
        "analyzed": "kāyassa bhedāḥ nirayam vajanti te ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_457:10.5",
        "original": "Yathā adhammo patito avaṁsiro.",
        "analyzed": "yathā adhammaḥ patitaḥ avasiraḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_457:11.1",
        "original": "Mātā pitā samaṇabrāhmaṇā ca,",
        "analyzed": "mātā pitā samaṇa brāhmaṇā ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_457:11.2",
        "original": "Susammānitā yassa sake agāre;",
        "analyzed": "su sammānitā yassa sake agāre ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_457:11.3",
        "original": "Idheva nikkhippa sarīradehaṁ,",
        "analyzed": "idha iva nikkhippa sarīra deha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_457:11.4",
        "original": "Kāyassa bhedā sugatiṁ vajanti te;",
        "analyzed": "kāyassa bhedā sugatim vajanti te ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_457:11.5",
        "original": "Yathāpi dhammo abhiruyha sandanan”ti.",
        "analyzed": "yathā api dhammaḥ abhiruyha sandananti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_457:12.1",
        "original": "Dhammadevaputtajātakaṁ tatiyaṁ.",
        "analyzed": "dhammadeva putta jātakam ",
        "folio": "12"
    }
]