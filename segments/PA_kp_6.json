    {
        "segmentnr": "PA_kp_6:1.1",
        "original": "Yānīdha bhūtāni samāgatāni,",
        "analyzed": "yāni idha bhūtāni samāgatāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_6:1.2",
        "original": "Bhummāni vā yāni va antalikkhe;",
        "analyzed": "bhummāni vā yāni va antalikkhe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_6:1.3",
        "original": "Sabbeva bhūtā sumanā bhavantu,",
        "analyzed": "sabbā iva bhūtā sumanāḥ bhavantu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_6:1.4",
        "original": "Athopi sakkacca suṇantu bhāsitaṁ.",
        "analyzed": "suṇantu bhāsita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_6:2.1",
        "original": "Tasmā hi bhūtā nisāmetha sabbe,",
        "analyzed": "tasmā hi bhūtāḥ nisāmetha sabbe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_6:2.2",
        "original": "Mettaṁ karotha mānusiyā pajāya;",
        "analyzed": "mā idtam karotha mānusiyā pajāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_6:2.3",
        "original": "Divā ca ratto ca haranti ye baliṁ,",
        "analyzed": "divā ca ca haranti ye bali ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_6:2.4",
        "original": "Tasmā hi ne rakkhatha appamattā.",
        "analyzed": "tasmā hi ne rakkhatha appamattā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_6:3.1",
        "original": "Yaṁ kiñci vittaṁ idha vā huraṁ vā,",
        "analyzed": "yam kiñci vittam vā huram vā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_6:3.2",
        "original": "Saggesu vā yaṁ ratanaṁ paṇītaṁ;",
        "analyzed": "saggesu vā yam ratanam paṇīta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_6:3.3",
        "original": "Na no samaṁ atthi tathāgatena,",
        "analyzed": "na no samam atthi tathāgatena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_6:3.4",
        "original": "Idampi buddhe ratanaṁ paṇītaṁ;",
        "analyzed": "idam api buddhe ratanam paṇīta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_6:3.5",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_6:4.1",
        "original": "Khayaṁ virāgaṁ amataṁ paṇītaṁ,",
        "analyzed": "khayam virāgam amatam paṇīta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_6:4.2",
        "original": "Yadajjhagā sakyamunī samāhito;",
        "analyzed": "sakya munī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_6:4.3",
        "original": "Na tena dhammena samatthi kiñci,",
        "analyzed": "na tena dhammena samatthi kiñci ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_6:4.4",
        "original": "Idampi dhamme ratanaṁ paṇītaṁ;",
        "analyzed": "idam pi dhamme ratanam paṇīta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_6:4.5",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_6:5.1",
        "original": "Yaṁ buddhaseṭṭho parivaṇṇayī suciṁ,",
        "analyzed": "yam buddha seṭṭhaḥ parivaṇṇayī suci ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_6:5.2",
        "original": "Samādhimānantarikaññamāhu;",
        "analyzed": "samādhim ānantarikaññam āhu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_6:5.3",
        "original": "Samādhinā tena samo na vijjati,",
        "analyzed": "samādhinā tena samaḥ na vijjati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_6:5.4",
        "original": "Idampi dhamme ratanaṁ paṇītaṁ;",
        "analyzed": "idam pi dhamme ratanam paṇīta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_6:5.5",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_6:6.1",
        "original": "Ye puggalā aṭṭha sataṁ pasatthā,",
        "analyzed": "ye puggalāḥ aṭṭha satam pasatthā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_6:6.2",
        "original": "Cattāri etāni yugāni honti;",
        "analyzed": "cattāri etāni yugāni honti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_6:6.3",
        "original": "Te dakkhiṇeyyā sugatassa sāvakā,",
        "analyzed": "te dakkhiṇeyyā sugatassa sāvakā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_6:6.4",
        "original": "Etesu dinnāni mahapphalāni;",
        "analyzed": "etesu dinnāni mahapphalāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_6:6.5",
        "original": "Idampi saṅghe ratanaṁ paṇītaṁ,",
        "analyzed": "idam api saṅghe ratanam paṇīta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_6:6.6",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_6:7.1",
        "original": "Ye suppayuttā manasā daḷhena,",
        "analyzed": "ye su payuttāḥ manasā daḷhena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_6:7.2",
        "original": "Nikkāmino gotamasāsanamhi;",
        "analyzed": "nikkāminaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_6:7.3",
        "original": "Te pattipattā amataṁ vigayha,",
        "analyzed": "te patti pattāḥ amatam vigayha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_6:7.4",
        "original": "Laddhā mudhā nibbutiṁ bhuñjamānā;",
        "analyzed": "laddhā mudhā nibbutim bhuñjamānā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_6:7.5",
        "original": "Idampi saṅghe ratanaṁ paṇītaṁ,",
        "analyzed": "idam api saṅghe ratanam paṇīta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_6:7.6",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_6:8.1",
        "original": "Yathindakhīlo pathavissito siyā,",
        "analyzed": "yathā inda khīlaḥ patha vissitaḥ siyā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_6:8.2",
        "original": "Catubbhi vātehi asampakampiyo;",
        "analyzed": "catubbhi vātehi asampakampiyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_6:8.3",
        "original": "Tathūpamaṁ sappurisaṁ vadāmi,",
        "analyzed": "tathūpamam sappurisam vadāmi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_6:8.4",
        "original": "Yo ariyasaccāni avecca passati;",
        "analyzed": "yaḥ ariya saccāni avecca passati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_6:8.5",
        "original": "Idampi saṅghe ratanaṁ paṇītaṁ,",
        "analyzed": "idam api saṅghe ratanam paṇīta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_6:8.6",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_6:9.1",
        "original": "Ye ariyasaccāni vibhāvayanti,",
        "analyzed": "ye ariya saccāni vibhāvayanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_6:9.2",
        "original": "Gambhīrapaññena sudesitāni;",
        "analyzed": "gambhīra paññena su desitāni ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_6:9.3",
        "original": "Kiñcāpi te honti bhusaṁ pamattā,",
        "analyzed": "kiñca api te honti bhusam pamattā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_6:9.4",
        "original": "Na te bhavaṁ aṭṭhamamādiyanti;",
        "analyzed": "na te bhavam aṭṭhamam ādiyanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_6:9.5",
        "original": "Idampi saṅghe ratanaṁ paṇītaṁ,",
        "analyzed": "idam api saṅghe ratanam paṇīta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_6:9.6",
        "original": "Etena saccena suvatthi hotu.",
        "analyzed": "etena saccena suvatthi hotu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_6:10.1",
        "original": "Sahāvassa dassanasampadāya,",
        "analyzed": "sahāvassa dassana sampadāya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_6:10.2",
        "original": "Tayassu dhammā jahitā bhavanti;",
        "analyzed": "tayassu dhammā jahitāḥ bhavanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_6:10.3",
        "original": "Sakkāyadiṭṭhī vicikicchitañca,",
        "analyzed": "sa kāya diṭṭhī vicikicchitañca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_6:10.4",
        "original": "Sīlabbataṁ vāpi yadatthi kiñci.",
        "analyzed": "sīlabbatam vā api yat atthi kiñci ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_6:11.1",
        "original": "Catūhapāyehi ca vippamutto,",
        "analyzed": "catūhapāyehi ca vippamuttaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_kp_6:11.2",
