        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_11:1.1",
        "original": "“Koñcā mayūrā diviyā ca haṁsā,",
        "analyzed": "koñcā mayūrā diviyā ca hasā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_11:1.2",
        "original": "Vaggussarā kokilā sampatanti;",
        "analyzed": "vaggussarā kokilā sampatanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_11:1.3",
        "original": "Pupphābhikiṇṇaṁ rammamidaṁ vimānaṁ,",
        "analyzed": "puppha abhikiṇṇam rammam idam vimāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_11:1.4",
        "original": "Anekacittaṁ naranārisevitaṁ.",
        "analyzed": "aneka cittam nara nāri sevita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_11:2.1",
        "original": "Tatthacchasi devi mahānubhāve,",
        "analyzed": "devi mahā anubhāve ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_11:2.2",
        "original": "Iddhī vikubbanti anekarūpā;",
        "analyzed": "iddhī vikubbanti aneka rūpā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_11:2.3",
        "original": "Imā ca te accharāyo samantato,",
        "analyzed": "imā ca te accharāyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_11:2.4",
        "original": "Naccanti gāyanti pamodayanti ca.",
        "analyzed": "naccanti gāyanti pamodayanti ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_11:3.1",
        "original": "Deviddhipattāsi mahānubhāve,",
        "analyzed": "devi iddhipattā asi mahā anubhāve ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_11:3.2",
        "original": "Manussabhūtā kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtā kim akāsi puñña ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_11:3.3",
        "original": "Kenāsi evaṁ jalitānubhāvā,",
        "analyzed": "kena asi evam jalita anubhāvā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_11:3.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_11:4.1",
        "original": "Sā devatā attamanā,",
        "analyzed": "sā devatā attamanā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_11:4.2",
        "original": "moggallānena pucchitā;",
        "analyzed": "moggallānena pucchitā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_11:4.3",
        "original": "Pañhaṁ puṭṭhā viyākāsi,",
        "analyzed": "pañham puṭṭhā viyākā asi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_11:4.4",
        "original": "yassa kammassidaṁ phalaṁ.",
        "analyzed": "yassa kammassa idam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_11:5.1",
        "original": "“Ahaṁ manussesu manussabhūtā,",
        "analyzed": "aham manussesu manussa bhūtā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_11:5.2",
        "original": "Patibbatānaññamanā ahosiṁ;",
        "analyzed": "ahosi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_11:5.3",
        "original": "Mātāva puttaṁ anurakkhamānā,",
        "analyzed": "mātā ava puttam anurakkhamānā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_11:5.4",
        "original": "Kuddhāpihaṁ nappharusaṁ avocaṁ.",
        "analyzed": "kuddha apiham nappharusam avoca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_11:6.1",
        "original": "Sacce ṭhitā mosavajjaṁ pahāya,",
        "analyzed": "sacce ṭhitāḥ mosa vajjam pahāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_11:6.2",
        "original": "Dāne ratā saṅgahitattabhāvā;",
        "analyzed": "dāne ratā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_11:6.3",
        "original": "Annañca pānañca pasannacittā,",
        "analyzed": "annam ca pānam ca pasanna cittā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_11:6.4",
        "original": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
        "analyzed": "sakkacca dānam vipulam adāsi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_11:7.1",
        "original": "Tena metādiso vaṇṇo,",
        "analyzed": "tena mā itādisaḥ vaṇṇaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_11:7.2",
        "original": "tena me idha mijjhati;",
        "analyzed": "tena me idha mijjhati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_11:7.3",
        "original": "Uppajjanti ca me bhogā,",
        "analyzed": "uppajjanti ca me bhogāḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_11:7.4",
        "original": "ye keci manaso piyā.",
        "analyzed": "ye keci manasaḥ piyā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_11:8.1",
        "original": "Akkhāmi te bhikkhu mahānubhāva,",
        "analyzed": "akkhāmi te bhikkhu mahā anubhāva ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_11:8.2",
        "original": "Manussabhūtā yamakāsi puññaṁ;",
        "analyzed": "manussa bhūtā yamakā asi puñña ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_11:8.3",
        "original": "Tenamhi evaṁ jalitānubhāvā,",
        "analyzed": "evam jalita anubhāvā ",
        "folio": "8"
    },
    {
