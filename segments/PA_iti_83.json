    {
        "segmentnr": "PA_iti_83:0.4",
        "original": "Pañcapubbanimittasutta",
        "analyzed": "pañca pubba nimitta sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_iti_83:1.1",
        "original": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
        "analyzed": "vuttam ha etam bhagavatā vuttam arahatā iti me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_iti_83:2.1",
        "original": "“Yadā, bhikkhave, devo devakāyā cavanadhammo hoti, pañcassa pubbanimittāni pātubhavanti—",
        "analyzed": "yadā bhikkhave devaḥ deva kāyā cavana dhammaḥ hoti pañcassa pubba nimittāni pātubhavanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_83:2.2",
        "original": "mālā milāyanti, vatthāni kilissanti, kacchehi sedā muccanti, kāye dubbaṇṇiyaṁ okkamati, sake devo devāsane nābhiramatīti.",
        "analyzed": "mālāḥ milāyanti vatthāni kilissanti kacchehi sedāḥ muccanti kāye dubbaṇṇiyam okkamati sake devaḥ devāsane na abhiramati iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_83:2.3",
        "original": "Tamenaṁ, bhikkhave, devā ‘cavanadhammo ayaṁ devaputto’ti iti viditvā tīhi vācāhi anumodenti:",
        "analyzed": "tam ena bhikkhave devā cavana dhammo ayam deva putto ti iti viditvā tīhi vācāhi anumodenti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_83:2.4",
        "original": "‘ito, bho, sugatiṁ gaccha.",
        "analyzed": "itas bho sugatim gaccha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_iti_83:3.1",
        "original": "Sugatiṁ gantvā suladdhalābhaṁ labha.",
        "analyzed": "sugatim gantvā su laddha lābham labha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_iti_83:4.1",
        "original": "Suladdhalābhaṁ labhitvā suppatiṭṭhito bhavāhī’”ti.",
        "analyzed": "su laddha lābham labhitvā su patiṭṭhitaḥ bhavāhi iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_iti_83:5.1",
        "original": "Evaṁ vutte, aññataro bhikkhu bhagavantaṁ etadavoca:",
        "analyzed": "evam vutte aññataraḥ bhikkhu bhagavantam etat avoca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_83:5.2",
        "original": "“kiṁ nu kho, bhante, devānaṁ sugatigamanasaṅkhātaṁ.",
        "analyzed": "kim nu khalu bhante sugati gamana saṅkhāta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_iti_83:6.1",
        "original": "Kiñca, bhante, devānaṁ suladdhalābhasaṅkhātaṁ.",
        "analyzed": "kiñca bhante su laddha lābha saṅkhāta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_iti_83:7.1",
        "original": "Kiṁ pana, bhante, devānaṁ suppatiṭṭhitasaṅkhātan”ti?",
        "analyzed": "kim pana bhante devānam suppatiṭṭhita saṅkhātanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_iti_83:8.1",
        "original": "“Manussattaṁ kho, bhikkhu, devānaṁ sugatigamanasaṅkhātaṁ.",
        "analyzed": "manussa ttam kho bhikkhu devānam sugati gamana saṅkhāta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_iti_83:9.1",
        "original": "Yaṁ manussabhūto samāno tathāgatappavedite dhammavinaye saddhaṁ paṭilabhati;",
        "analyzed": "yam manussa bhūtaḥ samānaḥ tathāgata ppavedite dhamma vinaye saddham paṭilabhati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_83:9.2",
        "original": "idaṁ kho, bhikkhu, devānaṁ suladdhalābhasaṅkhātaṁ.",
        "analyzed": "idam kho bhikkhu devānam su laddha lābha saṅkhāta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_iti_83:10.1",
        "original": "Sā kho panassa saddhā niviṭṭhā hoti mūlajātā patiṭṭhitā daḷhā asaṁhāriyā samaṇena vā brāhmaṇena vā devena vā mārena vā brahmunā vā kenaci vā lokasmiṁ;",
        "analyzed": "sā kho panassa saddhā niviṭṭhā hoti mūla jātā patiṭṭhitā daḷhā a sahāriyā samaṇena vā brāhmaṇena vā devena vā mārena vā brahmunā vā kenacid vā lokasmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_83:10.2",
        "original": "idaṁ kho, bhikkhu, devānaṁ suppatiṭṭhitasaṅkhātan”ti.",
        "analyzed": "idam kho bhikkhu devānam su patiṭṭhita saṅkhātanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_iti_83:11.1",
        "original": "Etamatthaṁ bhagavā avoca.",
        "analyzed": "etam attham bhagavān avoca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_iti_83:11.2",
        "original": "Tatthetaṁ iti vuccati:",
        "analyzed": "tattha itam iti vuccati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_iti_83:12.1",
        "original": "“Yadā devo devakāyā,",
        "analyzed": "yadā devaḥ deva kāyā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_iti_83:12.2",
        "original": "cavati āyusaṅkhayā;",
        "analyzed": "cavati āyu saṅkhayā ",
        "folio": "12"
    },
