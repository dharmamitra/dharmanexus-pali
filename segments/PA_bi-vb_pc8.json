    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.2",
        "original": "Tena kho pana samayena aññataro brāhmaṇo nibbiṭṭharājabhaṭo “taññeva bhaṭapathaṁ yācissāmī”ti sīsaṁ nahāyitvā bhikkhunupassayaṁ nissāya rājakulaṁ gacchati.",
        "analyzed": "tena kho pana samayena aññataraḥ brāhmaṇaḥ nibbiṭṭha rāja bhaṭaḥ taññeva bhaṭa patham yācissāmi iti sīsam nahāyitvā bhikkhun upassayam nissāya rājakulam gacchati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.3",
        "original": "Aññatarā bhikkhunī kaṭāhe vaccaṁ katvā tirokuṭṭe chaḍḍentī tassa brāhmaṇassa matthake āsumbhi.",
        "analyzed": "aññatarā bhikkhunī kaṭāhe vaccam katvā tirokuṭṭe chaḍḍentī tassa brāhmaṇassa matthake ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.4",
        "original": "Atha kho so brāhmaṇo ujjhāyati khiyyati vipāceti—",
        "analyzed": "atha kho saḥ brāhmaṇaḥ ujjhāyati khiyyati vipāca iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.5",
        "original": "“assamaṇiyo imā muṇḍā bandhakiniyo.",
        "analyzed": "assamaṇiyaḥ imāḥ muṇḍāḥ bandhakiniyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.6",
        "original": "Kathañhi nāma gūthakaṭāhaṁ matthake āsumbhissanti.",
        "analyzed": "katham hi nāma gūtha kaṭāham matthake āsumbhissanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.7",
        "original": "Imāsaṁ upassayaṁ jhāpessāmī”ti.",
        "analyzed": "imāsam upassayam jhāpessāmi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.8",
        "original": "Ummukaṁ gahetvā upassayaṁ pavisati.",
        "analyzed": "ummukam gahetvā upassayam pavisati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.9",
        "original": "Aññataro upāsako upassayā nikkhamanto addasa taṁ brāhmaṇaṁ ummukaṁ gahetvā upassayaṁ pavisantaṁ.",
        "analyzed": "aññataraḥ upāsakaḥ upassayā nikkhamantaḥ addasa tam brāhmaṇam ummukam gahetvā upassayam pavisanta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.10",
        "original": "Disvāna taṁ brāhmaṇaṁ etadavoca—",
        "analyzed": "disvāna tam brāhmaṇam etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.11",
        "original": "“kissa tvaṁ, bho, ummukaṁ gahetvā upassayaṁ pavisasī”ti?",
        "analyzed": "kissa tva bho ummukam gahetvā upassayam pavisasi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.12",
        "original": "“Imā maṁ, bho, muṇḍā bandhakiniyo gūthakaṭāhaṁ matthake āsumbhiṁsu.",
        "analyzed": "imāḥ ma bho muṇḍāḥ bandhakiniyaḥ gūtha kaṭāham matthake ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.13",
        "original": "Imāsaṁ upassayaṁ jhāpessāmī”ti.",
        "analyzed": "imāsam upassayam jhāpessāmi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.14",
        "original": "“Gaccha, bho brāhmaṇa, maṅgalaṁ etaṁ.",
        "analyzed": "gaccha bho brāhmaṇa maṅgalam eta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.15",
        "original": "Sahassaṁ lacchasi tañca bhaṭapathan”ti.",
        "analyzed": "sahassam lacchasi tam ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.16",
        "original": "Atha kho so brāhmaṇo sīsaṁ nahāyitvā rājakulaṁ gantvā sahassaṁ alattha tañca bhaṭapathaṁ.",
        "analyzed": "atha kho saḥ brāhmaṇaḥ sīsam nahāyitvā rājakulam gantvā sahassam alattha tat ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.17",
        "original": "Atha kho so upāsako upassayaṁ pavisitvā bhikkhunīnaṁ etamatthaṁ ārocetvā paribhāsi.",
        "analyzed": "atha kho saḥ upāsakaḥ upassayam pavisitvā bhikkhunīnam etam attham ārocetvā paribhāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.18",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.19",
        "original": "“kathañhi nāma bhikkhuniyo uccāraṁ tirokuṭṭe chaḍḍessantī”ti …pe…",
        "analyzed": "katham hi nāma bhikkhuniyaḥ uccāram tirokuṭṭe chaḍḍessanti iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc8:1.20",
        "original": "“saccaṁ kira, bhikkhave, bhikkhuniyo uccāraṁ tirokuṭṭe chaḍḍentī”ti?",
