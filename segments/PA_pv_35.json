        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_35:1.4",
        "original": "Khādanti kiṁ kammamakāsi pubbe.",
        "analyzed": "khādanti kim kammam akāsi pubbe ",
        "folio": "1"
    },
    {
