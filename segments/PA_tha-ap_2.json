        "analyzed": "sandālayitvāna sa yojanāni ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_tha-ap_2:49.3",
        "original": "Asantasaṁ jīvitasaṅkhayamhi,",
        "analyzed": "asantasam jīvita saṅkhayamhi ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_tha-ap_2:49.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "49"
    },
    {
        "segmentnr": "PA_tha-ap_2:50.1",
        "original": "‘Bhajanti sevanti ca kāraṇatthā,",
        "analyzed": "bhajanti sevanti ca kāraṇatthā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_tha-ap_2:50.2",
        "original": "Nikkāraṇā dullabhā ajja mittā;",
        "analyzed": "nikkāraṇā dullabhā ajja mittā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_tha-ap_2:50.3",
        "original": "Attatthapaññā asucīmanussā,",
        "analyzed": "atta ttha paññāḥ asucī manussā ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_tha-ap_2:50.4",
        "original": "Eko care khaggavisāṇakappo’.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "50"
    },
    {
        "segmentnr": "PA_tha-ap_2:51.1",
        "original": "Visuddhasīlā suvisuddhapaññā,",
        "analyzed": "visuddha sīlā su visuddha paññā ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_tha-ap_2:51.2",
        "original": "Samāhitā jāgariyānuyuttā;",
        "analyzed": "samāhitā jāgariyā anuyuttā ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_tha-ap_2:51.3",
        "original": "Vipassakā dhammavisesadassī,",
        "analyzed": "vipassakā dhamma visesa dassī ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_tha-ap_2:51.4",
        "original": "Maggaṅgabojjhaṅgagate vijaññā.",
        "analyzed": "maggaṅga bojjhaṅga gate vijaññā ",
        "folio": "51"
    },
    {
        "segmentnr": "PA_tha-ap_2:52.1",
        "original": "Suññappaṇidhiñca tathānimittaṁ,",
        "analyzed": "suñña ppaṇidhim ca tathā nimitta ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_tha-ap_2:52.2",
        "original": "Āsevayitvā jinasāsanamhi;",
        "analyzed": "āsevayitvā ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_tha-ap_2:52.3",
        "original": "Ye sāvakattaṁ na vajanti dhīrā,",
        "analyzed": "ye sāvakattam na vajanti dhīrā ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_tha-ap_2:52.4",
        "original": "Bhavanti paccekajinā sayambhū.",
        "analyzed": "bhavanti sayambhū ",
        "folio": "52"
    },
    {
        "segmentnr": "PA_tha-ap_2:53.1",
        "original": "Mahantadhammā bahudhammakāyā,",
        "analyzed": "mahanta dhammā bahu dhamma kāyā ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_tha-ap_2:53.2",
        "original": "Cittissarā sabbadukkhoghatiṇṇā;",
        "analyzed": "citti ssarā sabba dukkha ogha tiṇṇā ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_tha-ap_2:53.3",
        "original": "Udaggacittā paramatthadassī,",
        "analyzed": "udagga cittā paramattha dassī ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_tha-ap_2:53.4",
        "original": "Sīhopamā khaggavisāṇakappā.",
        "analyzed": "sīha upamā khagga visāṇa kappā ",
        "folio": "53"
    },
    {
        "segmentnr": "PA_tha-ap_2:54.1",
        "original": "Santindriyā santamanā samādhī,",
        "analyzed": "santindriyā santamanā ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_tha-ap_2:54.2",
        "original": "Paccantasattesu patippacārā;",
        "analyzed": "paccanta sattesu patippacārā ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_tha-ap_2:54.3",
        "original": "Dīpā parattha idha vijjalantā,",
        "analyzed": "dīpā parattha idha vijjalantā ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_tha-ap_2:54.4",
        "original": "Paccekabuddhā satataṁ hitāme.",
        "analyzed": "paccekabuddhā satatam hita āme ",
        "folio": "54"
    },
    {
        "segmentnr": "PA_tha-ap_2:55.1",
        "original": "Pahīnasabbāvaraṇā janindā,",
        "analyzed": "pahīna sabba āvaraṇā janindā ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_tha-ap_2:55.2",
        "original": "Lokappadīpā ghanakañcanābhā;",
        "analyzed": "lokappadīpā ghana kañcana ābhā ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_tha-ap_2:55.3",
        "original": "Nissaṁsayaṁ lokasudakkhiṇeyyā,",
        "analyzed": "nissa sayam lokasu dakkhiṇeyyā ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_tha-ap_2:55.4",
        "original": "Paccekabuddhā satatappitāme.",
        "analyzed": "paccekabuddhā satatappitāme ",
        "folio": "55"
    },
    {
        "segmentnr": "PA_tha-ap_2:56.1",
        "original": "Paccekabuddhānaṁ subhāsitāni,",
        "analyzed": "paccekabuddhānam su bhāsitāni ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_tha-ap_2:56.2",
        "original": "Caranti lokamhi sadevakamhi;",
        "analyzed": "caranti lokamhi sa devakamhi ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_tha-ap_2:56.3",
        "original": "Sutvā tathā ye na karonti bālā,",
        "analyzed": "sutvā tathā ye na karonti bālā ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_tha-ap_2:56.4",
        "original": "Caranti dukkhesu punappunaṁ te.",
        "analyzed": "caranti dukkhesu punappunam te ",
        "folio": "56"
    },
    {
        "segmentnr": "PA_tha-ap_2:57.1",
        "original": "Paccekabuddhāna subhāsitāni,",
        "analyzed": "paccekabuddhāna su bhāsitāni ",
        "folio": "57"
    },
    {
        "segmentnr": "PA_tha-ap_2:57.2",
        "original": "Madhuṁ yathā khuddamavassavantaṁ;",
