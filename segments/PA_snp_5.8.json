        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.8:5.6",
        "original": "Te ce muni brūsi anoghatiṇṇe,",
        "analyzed": "te ce muni brūsi anoghatiṇṇe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.8:5.7",
        "original": "Atha ko carahi devamanussaloke;",
        "analyzed": "atha kaḥ carahi deva manussa loke ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.8:5.8",
        "original": "Atāri jātiñca jarañca mārisa,",
        "analyzed": "atāri jātiñca jaran ca mārisa ",
