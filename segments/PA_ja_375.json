        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_375:1.1",
        "original": "“Idāni khomhi sukhito arogo,",
        "analyzed": "idāni khalu umhi sukhitaḥ arogaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_375:1.2",
        "original": "Nikkaṇṭako nippatito kapoto;",
        "analyzed": "nikkaṇṭakaḥ nippatitaḥ kapoto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_375:1.3",
        "original": "Kāhāmi dānī hadayassa tuṭṭhiṁ,",
        "analyzed": "kāhāmi dānī hadayassa tuṭṭhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_375:1.4",
        "original": "Tathāhimaṁ maṁsasākaṁ baleti”.",
        "analyzed": "tathā ahimam masasākam balā iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_375:2.1",
        "original": "“Kāyaṁ balākā sikhinī,",
        "analyzed": "kāyam balākā sikhinī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_375:2.2",
        "original": "corī laṅghipitāmahā;",
        "analyzed": "corī laṅghi pitāmahā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_375:2.3",
        "original": "Oraṁ balāke āgaccha,",
        "analyzed": "oram balāke āgaccha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_375:2.4",
        "original": "caṇḍo me vāyaso sakhā”.",
        "analyzed": "caṇḍaḥ me vāyasaḥ sakhā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_375:3.1",
        "original": "“Alañhi te jagghitāye,",
        "analyzed": "alañhi te jagghitāye ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_375:3.2",
        "original": "Mamaṁ disvāna edisaṁ;",
        "analyzed": "mamam disvānaḥ edisa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_375:3.3",
        "original": "Vilūnaṁ sūdaputtena,",
        "analyzed": "vilūnam sūda puttena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_375:3.4",
        "original": "Piṭṭhamaṇḍena makkhitaṁ”.",
        "analyzed": "piṭṭhamaṇḍena makkhita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_375:4.1",
        "original": "“Sunhāto suvilittosi,",
        "analyzed": "su nhāto su vilittaḥ asi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_375:4.2",
        "original": "annapānena tappito;",
        "analyzed": "anna pānena tappitaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_375:4.3",
        "original": "Kaṇṭhe ca te veḷuriyo,",
        "analyzed": "kaṇṭhe ca te veḷuriyaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_375:4.4",
        "original": "agamā nu kajaṅgalaṁ”.",
        "analyzed": "agamā nu kajaṅgala ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_375:5.1",
        "original": "“Mā te mitto amitto vā,",
        "analyzed": "mā te mittaḥ amittaḥ vā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_375:5.2",
        "original": "agamāsi kajaṅgalaṁ;",
        "analyzed": "agamāsi kajaṅgala ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_375:5.3",
        "original": "Piñchāni tattha lāyitvā,",
        "analyzed": "piñchāni tattha lāyitvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_375:5.4",
        "original": "kaṇṭhe bandhanti vaṭṭanaṁ”.",
        "analyzed": "kaṇṭhe bandhanti vaṭṭana ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_375:6.1",
        "original": "“Punapāpajjasī samma,",
        "analyzed": "punapāpajjasī samma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_375:6.2",
        "original": "Sīlañhi tava tādisaṁ;",
        "analyzed": "sīlañhi tava tādisa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_375:6.3",
        "original": "Na hi mānusakā bhogā,",
        "analyzed": "na hi mānusakāḥ bhogāḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_375:6.4",
        "original": "Subhuñjā honti pakkhinā”ti.",
        "analyzed": "su bhuñjāḥ honti pakkhināti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_375:7.1",
        "original": "Kapotajātakaṁ pañcamaṁ.",
        "analyzed": "kapota jātakam pañcama ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_375:7.2",
        "original": "Aḍḍhavaggo tatiyo.",
        "analyzed": "aḍḍhavaggo tatiyo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_375:8.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_375:9.1",
        "original": "Atha vaṇṇa sasīla hiri labhate,",
        "analyzed": "atha vaṇṇa sasīla hiri labhate ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_375:9.2",
        "original": "Sumukhā visa sāḷiyamittavaro;",
        "analyzed": "sumukhā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_375:9.3",
        "original": "Atha cakka palāsa sarāja sato,",
        "analyzed": "atha cakka palāsa sarāja sato ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_375:9.4",
        "original": "Yava bāla kapotaka pannarasāti.",
        "analyzed": "yava bāla kapotaka pannarasāti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_375:10.1",
        "original": "Atha vagguddānaṁ",
        "analyzed": "atha vagguddāna ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_375:11.1",
        "original": "Jīnañca vaṇṇaṁ asamaṁvaguppari,",
        "analyzed": "jīnam ca vaṇṇam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_375:11.2",
        "original": "Sudesitā jātakanti santi vīsati;",
        "analyzed": "su desitāḥ santi vīsati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_375:11.3",
        "original": "Mahesino brahmacarittamutta-",
        "analyzed": "mahesinaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_375:11.4",
        "original": "Mavoca gāthā atthavatī subyañjanāti.",
        "analyzed": "mavoca gāthāḥ atthavatī su byañjanā ati ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_375:12.1",
        "original": "Pañcakanipātaṁ niṭṭhitaṁ.",
        "analyzed": "pañcaka nipātam ",
        "folio": "12"
    }
