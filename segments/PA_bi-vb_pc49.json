    {
        "segmentnr": "PA_bi-vb_pc49:0.5",
        "original": "49. Tiracchānavijjāpariyāpuṇanasikkhāpada",
        "analyzed": "tiracchāna vijjā pariyāpuṇana sikkhāpada ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.2",
        "original": "Tena kho pana samayena chabbaggiyā bhikkhuniyo tiracchānavijjaṁ pariyāpuṇanti.",
        "analyzed": "tena kho pana samayena chabbaggiyāḥ bhikkhuniyaḥ tiracchāna vijjam pariyāpuṇanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.3",
        "original": "Manussā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.4",
        "original": "“kathañhi nāma bhikkhuniyo tiracchānavijjaṁ pariyāpuṇissanti, seyyathāpi gihiniyo kāmabhoginiyo”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ tiracchāna vijjam pariyāpuṇissanti seyyathā api gihiniyaḥ kāmabhoginiyaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.5",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.6",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.7",
        "original": "“kathañhi nāma chabbaggiyā bhikkhuniyo tiracchānavijjaṁ pariyāpuṇissantī”ti …pe…",
        "analyzed": "katham hi nāma chabbaggiyāḥ bhikkhuniyaḥ tiracchāna vijjam pariyāpuṇissanti iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.8",
        "original": "“saccaṁ kira, bhikkhave, chabbaggiyā bhikkhuniyo tiracchānavijjaṁ pariyāpuṇantī”ti?",
        "analyzed": "saccam kira bhikkhave chabbaggiyāḥ bhikkhuniyaḥ tiracchāna vijjam pariyāpuṇanti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.9",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.10",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.11",
        "original": "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo tiracchānavijjaṁ pariyāpuṇissanti.",
        "analyzed": "katham hi nāma bhikkhave chabbaggiyāḥ bhikkhuniyaḥ tiracchāna vijjam pariyāpuṇissanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.12",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
        "analyzed": "na ita bhikkhave appasannānam vā pasādāya pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc49:1.13",
        "original": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
