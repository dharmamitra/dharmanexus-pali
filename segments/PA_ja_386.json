        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_386:6.2",
        "original": "Attaṁ niraṅkatvā piyāni sevati;",
        "analyzed": "attam piyāni sevati ",
        "folio": "6"
    },
    {
