        "analyzed": "bhesajjena ca sakkatvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_tha-ap_538:17.4",
        "original": "hessāma sukhitā mayaṁ’.",
        "analyzed": "hessāma sukhitāḥ maya ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_tha-ap_538:18.1",
        "original": "Paccayānaṁ tadā lābhī,",
        "analyzed": "paccayānam tadā lābhī ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_538:18.2",
        "original": "tehi sakkatapūjito;",
        "analyzed": "tehi sakkata pūjitaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_538:18.3",
        "original": "Arahāhanti saṅkappaṁ,",
        "analyzed": "arahā ahanti saṅkappa ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_538:18.4",
        "original": "uppādesiṁ ayoniso.",
        "analyzed": "uppādesim ayonisaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_tha-ap_538:19.1",
        "original": "Tato me cittamaññāya,",
