    {
        "segmentnr": "PA_bu-vb_np20:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.2",
        "original": "Tena kho pana samayena āyasmā upanando sakyaputto paṭṭo hoti cīvarakammaṁ kātuṁ.",
        "analyzed": "tena kho pana samayena āyasmai upanandaḥ sakya putto paṭṭaḥ hoti cīvara kammam kātu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.3",
        "original": "So paṭapilotikānaṁ saṅghāṭiṁ karitvā surattaṁ suparikammakataṁ katvā pārupi.",
        "analyzed": "saḥ saṅghāṭim karitvā su rattam su parikamma katam katvā pārupi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.4",
        "original": "Atha kho aññataro paribbājako mahagghaṁ paṭaṁ pārupitvā yenāyasmā upanando sakyaputto tenupasaṅkami; upasaṅkamitvā āyasmantaṁ upanandaṁ sakyaputtaṁ etadavoca—",
        "analyzed": "atha kho aññataraḥ paribbājakaḥ mahaggham paṭam pārupitvā yena āyasmant upanandaḥ sakya putto tena upasaṅkami upasaṅkamitvā āyasmantam upanandam sakya puttam etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.5",
        "original": "“sundarā kho tyāyaṁ, āvuso, saṅghāṭi;",
        "analyzed": "sundarā kho tyāya āvuso saṅghāṭi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.6",
        "original": "dehi me paṭenā”ti.",
        "analyzed": "dehi me paṭena ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.7",
        "original": "“Jānāhi, āvuso”ti.",
        "analyzed": "jānāhi āvuso ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.8",
        "original": "“Āmāvuso, jānāmī”ti.",
        "analyzed": "ām āvusaḥ jānāmi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.9",
        "original": "“Handāvuso”ti, adāsi.",
        "analyzed": "handa avuso iti adāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.10",
        "original": "Atha kho so paribbājako taṁ saṅghāṭiṁ pārupitvā paribbājakārāmaṁ agamāsi.",
        "analyzed": "atha kho saḥ paribbājakaḥ tam saṅghāṭim pārupitvā paribbājaka ārāmam agamāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.11",
        "original": "Paribbājakā taṁ paribbājakaṁ etadavocuṁ—",
        "analyzed": "paribbājakāḥ tam paribbājakam etat avocuḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.12",
        "original": "“sundarā kho tyāyaṁ, āvuso, saṅghāṭi;",
        "analyzed": "sundarā kho tyāya āvuso saṅghāṭi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.13",
        "original": "kuto tayā laddhā”ti?",
        "analyzed": "kutas tayā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.14",
        "original": "“Tena me, āvuso, paṭena parivattitā”ti.",
        "analyzed": "tena me āvuso paṭena parivattitā ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.15",
        "original": "“Katihipi tyāyaṁ, āvuso, saṅghāṭi bhavissati,",
        "analyzed": "kati hipi tyāya āvuso saṅghāṭi bhavissati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.16",
        "original": "soyeva te paṭo varo”ti.",
        "analyzed": "sā uyā iva te paṭaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.17",
        "original": "Atha kho so paribbājako—“saccaṁ kho paribbājakā āhaṁsu—‘katihipi myāyaṁ saṅghāṭi bhavissati. Soyeva me paṭo varo’”ti",
        "analyzed": "atha kho saḥ paribbājaka usaccam kho paribbājakāḥ āha su kati hipi myā ayam saṅghāṭi bhavissati saḥ yā iva me paṭaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np20:1.18",
        "original": "yenāyasmā upanando sakyaputto tenupasaṅkami; upasaṅkamitvā āyasmantaṁ upanandaṁ sakyaputtaṁ etadavoca—",
