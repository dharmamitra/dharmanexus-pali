[
    {
        "segmentnr": "PA_ja_486:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_486:0.2",
        "original": "Pakiṇṇakanipāta",
        "analyzed": "pakiṇṇaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_486:0.3",
        "original": "Sālikedāravagga",
        "analyzed": "sāli kedāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_486:0.4",
        "original": "3. Mahāukkusajātaka",
        "analyzed": "mahāukkusajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_486:1.1",
        "original": "“Ukkā cilācā bandhanti dīpe,",
        "analyzed": "ukkā cilācā bandhanti dīpe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_486:1.2",
        "original": "Pajā mamaṁ khādituṁ patthayanti;",
        "analyzed": "pajāḥ mamam khāditum patthayanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_486:1.3",
        "original": "Mittaṁ sahāyañca vadehi senaka,",
        "analyzed": "mittam sahāyam ca vadehi senaka ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_486:1.4",
        "original": "Ācikkha ñātibyasanaṁ dijānaṁ”.",
        "analyzed": "ācikkha ñāti byasanam dijāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_486:2.1",
        "original": "“Dijo dijānaṁ pavarosi pakkhima,",
        "analyzed": "dijaḥ dijānam pavaraḥ asi pakkhima ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_486:2.2",
        "original": "Ukkusarāja saraṇaṁ taṁ upema;",
        "analyzed": "ukkusa rāja saraṇam tam upema ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_486:2.3",
        "original": "Pajā mamaṁ khādituṁ patthayanti,",
        "analyzed": "pajāḥ mamam khāditum patthayanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_486:2.4",
        "original": "Luddā cilācā bhava me sukhāya”.",
        "analyzed": "luddā cilācā bhava me sukhāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_486:3.1",
        "original": "“Mittaṁ sahāyañca karonti paṇḍitā,",
        "analyzed": "mittam sahāyam ca karonti paṇḍitā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_486:3.2",
        "original": "Kāle akāle sukhamesamānā;",
        "analyzed": "kāle akāle sukham esamānā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_486:3.3",
        "original": "Karomi te senaka etamatthaṁ,",
        "analyzed": "karomi te senake etam attha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_486:3.4",
        "original": "Ariyo hi ariyassa karoti kiccaṁ”.",
        "analyzed": "ariyaḥ hi ariyassa karoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_486:4.1",
        "original": "“Yaṁ hoti kiccaṁ anukampakena,",
        "analyzed": "yam hoti kiccam anukampakena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_486:4.2",
        "original": "Ariyassa ariyena kataṁ tayīdaṁ;",
        "analyzed": "ariyassa ariyena katam tayi ida ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_486:4.3",
        "original": "Attānurakkhī bhava mā adayhi,",
        "analyzed": "attā anurakkhī bhava mā a dayhi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_486:4.4",
        "original": "Lacchāma putte tayi jīvamāne”.",
        "analyzed": "lacchāma putte tayi jīvamāne ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_486:5.1",
        "original": "“Taveva rakkhāvaraṇaṁ karonto,",
        "analyzed": "tava iva rakkhā āvaraṇam karonto ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_486:5.2",
        "original": "Sarīrabhedāpi na santasāmi;",
        "analyzed": "sarīra bhedā api na santasāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_486:5.3",
        "original": "Karonti heke sakhīnaṁ sakhāro,",
        "analyzed": "karonti ha ike sakhīnam sakhāraḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_486:5.4",
        "original": "Pāṇaṁ cajantā satamesa dhammo”.",
        "analyzed": "pāṇam cajantā dhammo ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_486:6.1",
        "original": "“Sudukkaraṁ kammamakāsi,",
        "analyzed": "su dukkaram kammam akāsi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_486:6.2",
        "original": "aṇḍajāyaṁ vihaṅgamo;",
        "analyzed": "vihaṅgamaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_486:6.3",
        "original": "Atthāya kuraro putte,",
        "analyzed": "atthāya kuraro putte ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_486:6.4",
        "original": "aḍḍharatte anāgate”.",
        "analyzed": "aḍḍha ratte anāgate ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_486:7.1",
        "original": "“Cutāpi heke khalitā sakammunā,",
        "analyzed": "cutā api heke khalitā sa kammunā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_486:7.2",
        "original": "Mittānukampāya patiṭṭhahanti;",
        "analyzed": "mitta anukampāya patiṭṭhahanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_486:7.3",
        "original": "Puttā mamaṭṭā gatimāgatosmi,",
        "analyzed": "puttā mama aṭṭā gatim āgataḥ asmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_486:7.4",
        "original": "Atthaṁ caretho mama vāricara”.",
        "analyzed": "attham carethaḥ mama vāri cara ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_486:8.1",
        "original": "“Dhanena dhaññena ca attanā ca,",
        "analyzed": "dhanena dhaññena ca attanā ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_486:8.2",
        "original": "Mittaṁ sahāyañca karonti paṇḍitā;",
        "analyzed": "mittam sahāyam ca karonti paṇḍitā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_486:8.3",
        "original": "Karomi te senaka etamatthaṁ,",
        "analyzed": "karomi te senake etam attha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_486:8.4",
        "original": "Ariyo hi ariyassa karoti kiccaṁ”.",
        "analyzed": "ariyaḥ hi ariyassa karoti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_486:9.1",
        "original": "“Appossukko tāta tuvaṁ nisīda,",
        "analyzed": "appa ussukkaḥ tāta tuvam nisīda ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_486:9.2",
        "original": "Putto pitu carati atthacariyaṁ;",
        "analyzed": "putto pitu carati attha cariya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_486:9.3",
        "original": "Ahaṁ carissāmi tavetamatthaṁ,",
        "analyzed": "aham carissāmi tava itam attha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_486:9.4",
        "original": "Senassa putte paritāyamāno”.",
        "analyzed": "senassa putte paritāyamāno ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_486:10.1",
        "original": "“Addhā hi tāta satamesa dhammo,",
        "analyzed": "addhā hi tāta satama isa dhammo ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_486:10.2",
        "original": "Putto pitu yaṁ care atthacariyaṁ;",
        "analyzed": "putto yam care attha cariya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_486:10.3",
        "original": "Appeva maṁ disvāna pavaḍḍhakāyaṁ,",
        "analyzed": "appa iva mam disvāna pavaḍḍhakāya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_486:10.4",
        "original": "Senassa puttā na viheṭhayeyyuṁ”.",
        "analyzed": "senassa puttāḥ na viheṭhayeyyu ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_486:11.1",
        "original": "“Pasū manussā migavīraseṭṭha,",
        "analyzed": "pasū manussā miga vīra seṭṭha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_486:11.2",
        "original": "Bhayaṭṭitā seṭṭhamupabbajanti;",
        "analyzed": "bhaya ṭṭitāḥ seṭṭham upabbajanti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_486:11.3",
        "original": "Puttā mamaṭṭā gatimāgatosmi,",
        "analyzed": "puttā mama aṭṭā gatim āgataḥ asmi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_486:11.4",
        "original": "Tvaṁ nosi rājā bhava me sukhāya”.",
        "analyzed": "tvam na usi rājā bhava me sukhāya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_486:12.1",
        "original": "“Karomi te senaka etamatthaṁ,",
        "analyzed": "karomi te senake etam attha ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_486:12.2",
        "original": "Āyāmi te taṁ disataṁ vadhāya;",
        "analyzed": "āyāmi te tam vadhāya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_486:12.3",
        "original": "Kathañhi viññū pahu sampajāno,",
        "analyzed": "kathan hi viññū pahu sampajānaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_486:12.4",
        "original": "Na vāyame attajanassa guttiyā”.",
        "analyzed": "na vā āyame atta janassa guttiyā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_486:13.1",
        "original": "“Mittañca kayirātha suhadayañca,",
        "analyzed": "mittañca kayirā atha suhadayañca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_486:13.2",
        "original": "Ayirañca kayirātha sukhāgamāya;",
        "analyzed": "sukha āgamāya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_486:13.3",
        "original": "Nivatthakocova sarebhihantvā,",
        "analyzed": "sarebhi hantvā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_486:13.4",
        "original": "Modāma puttehi samaṅgibhūtā.",
        "analyzed": "modāma puttehi samaṅgi bhūtā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_486:14.1",
        "original": "Sakamittassa kammena,",
        "analyzed": "sa kamittassa kammena ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_486:14.2",
        "original": "sahāyassāpalāyino;",
        "analyzed": "sahāyassa apalāyino ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_486:14.3",
        "original": "Kūjantamupakūjanti,",
        "analyzed": "kūjantam upakūjanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_486:14.4",
        "original": "lomasā hadayaṅgamaṁ.",
        "analyzed": "lomasā hadayaṅgama ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_486:15.1",
        "original": "Mittaṁ sahāyaṁ adhigamma paṇḍito,",
        "analyzed": "mittam sahāyam adhigamma paṇḍitaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_486:15.2",
        "original": "So bhuñjatī putta pasuṁ dhanaṁ vā;",
        "analyzed": "sā u bhuñjatī putta pasum dhanam vā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_486:15.3",
        "original": "Ahañca puttā ca patī ca mayhaṁ,",
        "analyzed": "ahañca puttā ca patī ca ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_486:15.4",
        "original": "Mittānukampāya samaṅgibhūtā.",
        "analyzed": "mitta anukampāya samaṅgi bhūtā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_486:16.1",
        "original": "Rājavatā sūravatā ca attho,",
        "analyzed": "rājavatā sūravatā ca attho ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_486:16.2",
        "original": "Sampannasakhissa bhavanti hete;",
        "analyzed": "sampanna sakhissa bhavanti hete ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_486:16.3",
        "original": "So mittavā yasavā uggatatto,",
        "analyzed": "saḥ mittavā yasavā uggatattaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_486:16.4",
        "original": "Asmiṁdhaloke modati kāmakāmī.",
        "analyzed": "asmidha loke modati kāma kāmī ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_486:17.1",
        "original": "Karaṇīyāni mittāni,",
        "analyzed": "karaṇīyāni mittāni ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_486:17.2",
        "original": "daliddenāpi senaka;",
        "analyzed": "daliddena api senaka ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_486:17.3",
        "original": "Passa mittānukampāya,",
        "analyzed": "passa mitta anukampāya ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_486:17.4",
        "original": "samaggamhā sañātake.",
        "analyzed": "samaggamhā sañātake ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_486:18.1",
        "original": "Sūrena balavantena,",
        "analyzed": "sūrena balavantena ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_486:18.2",
        "original": "yo mitte kurute dijo;",
        "analyzed": "yaḥ mitte kurute ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_486:18.3",
        "original": "Evaṁ so sukhito hoti,",
        "analyzed": "evam saḥ sukhitaḥ hoti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_486:18.4",
        "original": "yathāhaṁ tvañca senakāti”.",
        "analyzed": "yathā aham tvañca senakāti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_486:19.1",
        "original": "Mahāukkusajātakaṁ tatiyaṁ.",
        "analyzed": "mahāukkusa jātakam ",
        "folio": "19"
    }
]