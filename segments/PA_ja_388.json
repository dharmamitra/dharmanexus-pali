        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_388:2.1",
        "original": "“Tasasi bhamasi leṇamicchasi,",
        "analyzed": "tasasi bhamasi leṇam icchasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_388:2.2",
        "original": "Attāṇosi kuhiṁ gamissasi;",
