        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_421:7.3",
        "original": "Tapasā abhibhuyya gaṅgamāla,",
        "analyzed": "tapasā abhibhuyya gaṅgamāla ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_421:7.4",
        "original": "Nāmenālapasajja brahmadattaṁ”.",
        "analyzed": "brahmadatta ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_421:8.1",
        "original": "“Sandiṭṭhikameva ‘amma’ passatha,",
        "analyzed": "sandiṭṭhikam eva amma passatha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_421:8.2",
        "original": "Khantīsoraccassa ayaṁ vipāko;",
