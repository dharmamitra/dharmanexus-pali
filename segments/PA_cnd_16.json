        "folio": "7"
    },
    {
        "segmentnr": "PA_cnd_16:7.4",
        "original": "Kappañjahaṁ abhiyāce sumedhaṁ,",
        "analyzed": "kappañjaham abhiyāce sumedha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_cnd_16:7.5",
        "original": "Sutvāna nāgassa apanamissanti ito”ti.",
