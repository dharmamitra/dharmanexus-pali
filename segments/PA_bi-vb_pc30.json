    {
        "segmentnr": "PA_bi-vb_pc30:0.5",
        "original": "30. Kathinuddhārasikkhāpada",
        "analyzed": "    ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.2",
        "original": "Tena kho pana samayena aññatarena upāsakena saṅghaṁ uddissa vihāro kārāpito hoti.",
        "analyzed": "tena kho pana samayena aññatarena upāsakena saṅgham uddissa vihāraḥ kārāpitaḥ hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.3",
        "original": "So tassa vihārassa mahe ubhatosaṅghassa akālacīvaraṁ dātukāmo hoti.",
        "analyzed": "saḥ tassa vihārassa mahe ubhatas saṅghassa akāla cīvaram dātu kāmaḥ hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.4",
        "original": "Tena kho pana samayena ubhatosaṅghassa kathinaṁ atthataṁ hoti.",
        "analyzed": "tena kho pana samayena ubhato saṅghassa kathinam atthatam hoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.5",
        "original": "Atha kho so upāsako saṅghaṁ upasaṅkamitvā kathinuddhāraṁ yāci.",
        "analyzed": "atha kho saḥ upāsakaḥ saṅgham upasaṅkamitvā kathin uddhāram yāci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.6",
        "original": "Bhagavato etamatthaṁ ārocesuṁ.",
        "analyzed": "bhagavato etam attham ārocesu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.7",
        "original": "Atha kho bhagavā etasmiṁ nidāne etasmiṁ pakaraṇe dhammiṁ kathaṁ katvā bhikkhū āmantesi—",
        "analyzed": "atha kho bhagavā etasmim nidāne etasmim pakaraṇe dhammim katham katvā bhikkhū āmantesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.8",
        "original": "“anujānāmi, bhikkhave, kathinaṁ uddharituṁ.",
        "analyzed": "anujānāmi bhikkhave kathinam uddharitu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.9",
        "original": "Evañca pana bhikkhave kathinaṁ uddharitabbaṁ.",
        "analyzed": "evam ca pana bhikkhave kathinam uddharitabba ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.10",
        "original": "Byattena bhikkhunā paṭibalena saṅgho ñāpetabbo—",
        "analyzed": "byattena bhikkhunā paṭibalena saṅghaḥ ñāpetabbaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.11",
        "original": "‘Suṇātu me, bhante, saṅgho.",
        "analyzed": "suṇātu me bhante saṅghaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.12",
        "original": "Yadi saṅghassa pattakallaṁ, saṅgho kathinaṁ uddhareyya.",
        "analyzed": "yadi saṅghassa pattakalla saṅghaḥ kathinam uddhareyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.13",
        "original": "Esā ñatti.",
        "analyzed": "esā ñatti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.14",
        "original": "Suṇātu me, bhante, saṅgho.",
        "analyzed": "suṇātu me bhante saṅghaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.15",
        "original": "Saṅgho kathinaṁ uddharati.",
        "analyzed": "saṅghaḥ kathinam uddharati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.16",
        "original": "Yassāyasmato khamati kathinassa uddhāro, so tuṇhassa;",
        "analyzed": "yassa ayasmataḥ khamati kathinassa uddhāraḥ saḥ tuṇhassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.17",
        "original": "yassa nakkhamati, so bhāseyya.",
        "analyzed": "yassa na khamati saḥ bhāseyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.1.18",
        "original": "Ubbhataṁ saṅghena kathinaṁ, khamati saṅghassa, tasmā tuṇhī, evametaṁ dhārayāmī’”ti.",
        "analyzed": "ubbhatam saṅghena khamati saṅghassa tasmai tuṇhī evam etam dhārayāmi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.1",
        "original": "Atha kho so upāsako bhikkhunisaṅghaṁ upasaṅkamitvā kathinuddhāraṁ yāci.",
        "analyzed": "atha kho saḥ upāsakaḥ bhikkhuni saṅgham upasaṅkamitvā kathin uddhāram yāci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.2",
        "original": "Thullanandā bhikkhunī—",
        "analyzed": "thullanandā bhikkhunī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.3",
        "original": "“cīvaraṁ amhākaṁ bhavissatī”ti kathinuddhāraṁ paṭibāhi.",
        "analyzed": "cīvaram amhākam bhavissati iti kathin uddhāram paṭibāhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.4",
        "original": "Atha kho so upāsako ujjhāyati khiyyati vipāceti—",
        "analyzed": "atha kho saḥ upāsakaḥ ujjhāyati khiyyati vipāceti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.5",
        "original": "“kathañhi nāma bhikkhuniyo amhākaṁ kathinuddhāraṁ na dassantī”ti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ amhākam kathin uddhāram na dassanti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.6",
        "original": "Assosuṁ kho bhikkhuniyo tassa upāsakassa ujjhāyantassa khiyyantassa vipācentassa.",
        "analyzed": "assosum kho bhikkhuniyaḥ tassa upāsakassa ujjhāyantassa khiyyantassa vipācentassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.7",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.8",
        "original": "“kathañhi nāma ayyā thullanandā dhammikaṁ kathinuddhāraṁ paṭibāhissatī”ti …pe…",
        "analyzed": "katham hi nāma ayyā thullanandā dhammikam kathinuddhāram paṭibāhissati iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.9",
        "original": "“saccaṁ kira, bhikkhave, thullanandā bhikkhunī dhammikaṁ kathinuddhāraṁ paṭibāhatī”ti?",
        "analyzed": "saccam kira bhikkhave thullanandā bhikkhunī dhammikam kathinuddhāram paṭibāhati iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.10",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.11",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.12",
        "original": "kathañhi nāma, bhikkhave, thullanandā bhikkhunī dhammikaṁ kathinuddhāraṁ paṭibāhissati.",
        "analyzed": "katham hi nāma bhikkhave thullanandā bhikkhunī dhammikam kathin uddhāram paṭibāhissati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.13",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
        "analyzed": "na ita bhikkhave appasannānam vā pasādāya pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.14",
        "original": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
        "analyzed": "evam ca pana bhikkhave bhikkhuniyaḥ imam sikkhāpadam uddisantu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:1.2.15.1",
        "original": "“Yā pana bhikkhunī dhammikaṁ kathinuddhāraṁ paṭibāheyya, pācittiyan”ti.",
        "analyzed": "yā pana bhikkhunī dhammikam kathinuddhāram paṭibāheyya pācittiyanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.1",
        "original": "Yā panāti",
        "analyzed": "yā panāti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.2",
        "original": "yā yādisā …pe…",
        "analyzed": "yā yā disā pe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.3",
        "original": "bhikkhunīti",
        "analyzed": "bhikkhunī iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.4",
        "original": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
        "analyzed": "pe ayam imasmin atthe adhippetā bhikkhunī iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.5",
        "original": "Dhammiko nāma kathinuddhāro",
        "analyzed": "dhammikaḥ nāma kathin uddhāraḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.6",
        "original": "samaggo bhikkhunisaṅgho sannipatitvā uddharati.",
        "analyzed": "samaggaḥ bhikkhuni saṅghaḥ sannipatitvā uddharati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.7",
        "original": "Paṭibāheyyāti",
        "analyzed": "paṭibāheyyā iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.8",
        "original": "“kathaṁ idaṁ kathinaṁ na uddhareyyā”ti paṭibāhati, āpatti pācittiyassa.",
        "analyzed": "katham idam kathinam na uddhareyya ati paṭibāhati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.9.1",
        "original": "Dhammike dhammikasaññā paṭibāhati, āpatti pācittiyassa.",
        "analyzed": "dhammike dhammika saññā paṭibāhati āpatti pācittiyassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.10",
        "original": "Dhammike vematikā paṭibāhati, āpatti dukkaṭassa.",
        "analyzed": "dhammike vematikā paṭibāhati āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.11",
        "original": "Dhammike adhammikasaññā paṭibāhati, anāpatti.",
        "analyzed": "dhammike adhammika saññā paṭibāhati anāpatti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.12",
        "original": "Adhammike dhammikasaññā, āpatti dukkaṭassa.",
        "analyzed": "adhammike dhammika saññā āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.13",
        "original": "Adhammike vematikā, āpatti dukkaṭassa.",
        "analyzed": "adhammike vematikā āpatti dukkaṭassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.1.14",
        "original": "Adhammike adhammikasaññā, anāpatti.",
        "analyzed": "adhammike adhammika saññā anāpatti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.2.1",
        "original": "Anāpatti—",
        "analyzed": "anāpatti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.2.2",
        "original": "ānisaṁsaṁ dassetvā paṭibāhati,",
        "analyzed": "ānisasam dassetvā paṭibāhati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.2.3",
        "original": "ummattikāya,",
        "analyzed": "ummattikāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.2.4",
        "original": "ādikammikāyāti.",
        "analyzed": "ādikammikāya ati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.2.5",
        "original": "Dasamasikkhāpadaṁ niṭṭhitaṁ.",
        "analyzed": "dasama sikkhāpadam niṭṭhita ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bi-vb_pc30:2.2.6",
        "original": "Naggavaggo tatiyo.",
        "analyzed": "nagga vaggaḥ tatiyaḥ ",
        "folio": "2"
    }
