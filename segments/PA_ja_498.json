        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_498:2.3",
        "original": "Kaccinnu cittassapi evamevaṁ,",
        "analyzed": "kaccit nu evam eva ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_498:2.4",
