        "segmentnr": "PA_thag_10.4:4.2",
        "original": "pādāsi pādapuñchaniṁ;",
        "analyzed": "pāda asi pāda puñchani ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.4:4.3",
        "original": "‘Etaṁ suddhaṁ adhiṭṭhehi,",
        "analyzed": "etam suddham adhiṭṭhehi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.4:4.4",
        "original": "ekamantaṁ svadhiṭṭhitaṁ’.",
        "analyzed": "ekamantam su adhiṭṭhita ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.4:5.1",
        "original": "Tassāhaṁ vacanaṁ sutvā,",
        "analyzed": "tassa aham vacanam sutvā ",
        "folio": "5"
    },
