[
    {
        "segmentnr": "PA_snp_4.9:0.1",
        "original": "Sutta Nipāta 4.9",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.9:0.2",
        "original": "Māgaṇḍiyasutta",
        "analyzed": "māgaṇḍiyasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.9:1.1",
        "original": "“Disvāna taṇhaṁ aratiṁ ragañca,",
        "analyzed": "disvāna taṇham aratim ragan ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.9:1.2",
        "original": "Nāhosi chando api methunasmiṁ;",
        "analyzed": "chandaḥ api ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.9:1.3",
        "original": "Kimevidaṁ muttakarīsapuṇṇaṁ,",
        "analyzed": "kim eva idam mutta karīsa puṇṇa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.9:1.4",
        "original": "Pādāpi naṁ samphusituṁ na icche”.",
        "analyzed": "nam samphusitum naḥ icche ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.9:2.1",
        "original": "“Etādisañce ratanaṁ na icchasi,",
        "analyzed": "ratanam naḥ icchasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.9:2.2",
        "original": "Nāriṁ narindehi bahūhi patthitaṁ;",
        "analyzed": "nārim narindehi bahūhi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.9:2.3",
        "original": "Diṭṭhigataṁ sīlavataṁ nu jīvitaṁ,",
        "analyzed": "diṭṭhi gatam sīlavatam nu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.9:2.4",
        "original": "Bhavūpapattiñca vadesi kīdisaṁ”.",
        "analyzed": "bhava upapattiñca vadesi kīdisa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.9:3.1",
        "original": "“Idaṁ vadāmīti na tassa hoti,",
        "analyzed": "idam vadāmi iti na tassa hoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.9:3.2",
        "original": "(māgaṇḍiyāti bhagavā)",
        "analyzed": "māgaṇḍiyāti bhagavā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.9:3.3",
        "original": "Dhammesu niccheyya samuggahītaṁ;",
        "analyzed": "dhammesu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.9:3.4",
        "original": "Passañca diṭṭhīsu anuggahāya,",
        "analyzed": "passan ca diṭṭhīsu anuggahāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.9:3.5",
        "original": "Ajjhattasantiṁ pacinaṁ adassaṁ”.",
        "analyzed": "ajjhatta santim pacinam adassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.9:4.1",
        "original": "“Vinicchayā yāni pakappitāni,",
        "analyzed": "vinicchayā yāni pakappitāni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.9:4.2",
        "original": "(iti māgaṇḍiyo)",
        "analyzed": "iti māgaṇḍiyaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.9:4.3",
        "original": "Te ve munī brūsi anuggahāya;",
        "analyzed": "te ve munī brūsi anuggahāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.9:4.4",
        "original": "Ajjhattasantīti yametamatthaṁ,",
        "analyzed": "ajjhattasanti iti yam etam attha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.9:4.5",
        "original": "Kathaṁ nu dhīrehi paveditaṁ taṁ”.",
        "analyzed": "katham nu dhīrehi te ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.9:5.1",
        "original": "“Na diṭṭhiyā na sutiyā na ñāṇena,",
        "analyzed": "na diṭṭhiyā na sutiyā na ñāṇena ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.9:5.2",
        "original": "(māgaṇḍiyāti bhagavā)",
        "analyzed": "māgaṇḍiyāti bhagavā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.9:5.3",
        "original": "Sīlabbatenāpi na suddhimāha;",
        "analyzed": "sīlabbatena api na suddhim āha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.9:5.4",
        "original": "Adiṭṭhiyā assutiyā añāṇā,",
        "analyzed": "a diṭṭhiyā a sutiyā a ñāṇā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.9:5.5",
        "original": "Asīlatā abbatā nopi tena;",
        "analyzed": "tena ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.9:5.6",
        "original": "Ete ca nissajja anuggahāya,",
        "analyzed": "ete ca nissajja anuggahāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.9:5.7",
        "original": "Santo anissāya bhavaṁ na jappe”.",
        "analyzed": "santaḥ a nissāya bhavam na jappe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.9:6.1",
        "original": "“No ce kira diṭṭhiyā na sutiyā na ñāṇena,",
        "analyzed": "no ce kira diṭṭhiyā na sutiyā na ñāṇena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.9:6.2",
        "original": "(iti māgaṇḍiyo)",
        "analyzed": "iti māgaṇḍiyaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.9:6.3",
        "original": "Sīlabbatenāpi na suddhimāha;",
        "analyzed": "sīlabbatena api na suddhim āha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.9:6.4",
        "original": "Adiṭṭhiyā assutiyā añāṇā,",
        "analyzed": "a diṭṭhiyā a sutiyā a ñāṇā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.9:6.5",
        "original": "Asīlatā abbatā nopi tena;",
        "analyzed": "tena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.9:6.6",
        "original": "Maññāmahaṁ momuhameva dhammaṁ,",
        "analyzed": "maññāmaham momuham eva dhamma ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.9:6.7",
        "original": "Diṭṭhiyā eke paccenti suddhiṁ”.",
        "analyzed": "diṭṭhiyā eke paccenti suddhi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.9:7.1",
        "original": "“Diṭṭhañca nissāya anupucchamāno,",
        "analyzed": "diṭṭhañca nissāya anupucchamānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.9:7.2",
        "original": "(māgaṇḍiyāti bhagavā)",
        "analyzed": "māgaṇḍiyāti bhagavā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.9:7.3",
        "original": "Samuggahītesu pamohamāgā;",
        "analyzed": "samuggahītesu pamoha māgā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.9:7.4",
        "original": "Ito ca nāddakkhi aṇumpi saññaṁ,",
        "analyzed": "i taḥ ca aṇumpi sañña ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.9:7.5",
        "original": "Tasmā tuvaṁ momuhato dahāsi.",
        "analyzed": "tasmai tuvam momuhataḥ dahāsi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.9:8.1",
        "original": "Samo visesī uda vā nihīno,",
        "analyzed": "samaḥ visesī vā nihīnaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.9:8.2",
        "original": "Yo maññati so vivadetha tena;",
        "analyzed": "yaḥ maññati saḥ vivadetha tena ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.9:8.3",
        "original": "Tīsu vidhāsu avikampamāno,",
        "analyzed": "tīsu vidhāsu a vikampamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.9:8.4",
        "original": "Samo visesīti na tassa hoti.",
        "analyzed": "samaḥ visesi iti na tassa hoti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.9:9.1",
        "original": "Saccanti so brāhmaṇo kiṁ vadeyya,",
        "analyzed": "saccanti saḥ brāhmaṇaḥ kim vadeyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.9:9.2",
        "original": "Musāti vā so vivadetha kena;",
        "analyzed": "vā saḥ vivadetha kena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.9:9.3",
        "original": "Yasmiṁ samaṁ visamaṁ vāpi natthi,",
        "analyzed": "yasmin samam visamam vā api na atthi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.9:9.4",
        "original": "Sa kena vādaṁ paṭisaṁyujeyya.",
        "analyzed": "sa kena vādam paṭisayujeyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.9:10.1",
        "original": "Okaṁ pahāya aniketasārī,",
        "analyzed": "okam pahāya aniketa sārī ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.9:10.2",
        "original": "Gāme akubbaṁ muni santhavāni;",
        "analyzed": "gāme akubbam muni santhavāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.9:10.3",
        "original": "Kāmehi ritto apurakkharāno,",
        "analyzed": "kāmehi rittaḥ a purakkharānaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.9:10.4",
        "original": "Kathaṁ na viggayha janena kayirā.",
        "analyzed": "katham na viggayha janena kayirā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.9:11.1",
        "original": "Yehi vivitto vicareyya loke,",
        "analyzed": "yehi vivittaḥ vicareyya loke ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.9:11.2",
        "original": "Na tāni uggayha vadeyya nāgo;",
        "analyzed": "na tāni uggayha vadeyya nāgaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.9:11.3",
        "original": "Jalambujaṁ kaṇḍakavārijaṁ yathā,",
        "analyzed": "          ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.9:11.4",
        "original": "Jalena paṅkena canūpalittaṁ;",
        "analyzed": "jalena paṅkena ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.9:11.5",
        "original": "Evaṁ munī santivādo agiddho,",
        "analyzed": "evam munī santivādaḥ agiddhaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.9:11.6",
        "original": "Kāme ca loke ca anūpalitto.",
        "analyzed": "kāme ca loke ca an upalittaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.9:12.1",
        "original": "Na vedagū diṭṭhiyāyako na mutiyā,",
        "analyzed": "na vedagū diṭṭhiyāyakaḥ na mutiyā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.9:12.2",
        "original": "Sa mānameti na hi tammayo so;",
        "analyzed": "sa mānam eti na hi tammayaḥ saḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.9:12.3",
        "original": "Na kammunā nopi sutena neyyo,",
        "analyzed": "na kammunā na upi sutena ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.9:12.4",
        "original": "Anūpanīto sa nivesanesu.",
        "analyzed": "anūpanītaḥ sa nivesanesu ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.9:13.1",
        "original": "Saññāvirattassa na santi ganthā,",
        "analyzed": "saññā virattassa na santi ganthā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.9:13.2",
        "original": "Paññāvimuttassa na santi mohā;",
        "analyzed": "paññā vimuttassa na santi mohāḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.9:13.3",
        "original": "Saññañca diṭṭhiñca ye aggahesuṁ,",
        "analyzed": "saññañca diṭṭhiñca ye aggahesu ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.9:13.4",
        "original": "Te ghaṭṭayantā vicaranti loke”ti.",
        "analyzed": "te ghaṭṭayantāḥ vicaranti loka iti ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.9:13.5",
        "original": "Māgaṇḍiyasuttaṁ navamaṁ.",
        "analyzed": "māgaṇḍiya suttam navama ",
        "folio": "13"
    }
]