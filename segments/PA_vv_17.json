    },
    {
        "segmentnr": "PA_vv_17:0.4",
        "original": "17. Kesakārīvimānavatthu",
        "analyzed": "kesakārī vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_17:1.1",
        "original": "“Idaṁ vimānaṁ ruciraṁ pabhassaraṁ,",
        "analyzed": "idam vimānam ruciram pabhassara ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_17:1.2",
        "original": "Veḷuriyathambhaṁ satataṁ sunimmitaṁ;",
        "analyzed": "satatam su nimmita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_17:1.3",
        "original": "Suvaṇṇarukkhehi samantamotthataṁ,",
        "analyzed": "suvaṇṇa rukkhehi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_17:1.4",
        "original": "Ṭhānaṁ mamaṁ kammavipākasambhavaṁ.",
        "analyzed": "ṭhānam mamam kamma vipāka sambhava ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_17:2.1",
        "original": "Tatrūpapannā purimaccharā imā,",
        "analyzed": "tatra upapannā purimat śarāḥ imā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_17:2.2",
        "original": "Sataṁ sahassāni sakena kammunā;",
        "analyzed": "satam sahassāni sakena kammunā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_17:2.3",
        "original": "Tuvaṁsi ajjhupagatā yasassinī,",
        "analyzed": "tuvasi ajjhupagatā yasassinī ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_17:2.4",
        "original": "Obhāsayaṁ tiṭṭhasi pubbadevatā.",
        "analyzed": "obhāsayam tiṭṭhasi pubbadevatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_17:3.1",
        "original": "Sasī adhiggayha yathā virocati,",
        "analyzed": "sasī adhiggayha yathā virocati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_17:3.2",
        "original": "Nakkhattarājāriva tārakāgaṇaṁ;",
        "analyzed": "tārakāgaṇa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_17:3.3",
        "original": "Tatheva tvaṁ accharāsaṅgaṇaṁ imaṁ,",
        "analyzed": "tathā iva tvam at śarāsaṅgaṇam ima ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_17:3.4",
        "original": "Daddallamānā yasasā virocasi.",
        "analyzed": "daddallamānā yasasā virocasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_17:4.1",
        "original": "Kuto nu āgamma anomadassane,",
        "analyzed": "kutas nu āgamma anomadassane ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_17:4.2",
        "original": "Upapannā tvaṁ bhavanaṁ mamaṁ idaṁ;",
        "analyzed": "upapannā tvam bhavanam mamaṃ ida ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_17:4.3",
        "original": "Brahmaṁva devā tidasā sahindakā,",
        "analyzed": "brahmava devā tidasā sahindakā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_17:4.4",
        "original": "Sabbe na tappāmase dassanena tan”ti.",
        "analyzed": "sabbe na tappāmase dassanena tanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_17:5.1",
        "original": "“Yametaṁ sakka anupucchase mamaṁ,",
        "analyzed": "yam etam sakka anupucchase mama ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_17:5.2",
        "original": "‘Kuto cutā tvaṁ idha āgatā’ti;",
        "analyzed": "kutas cutā tvam idhe āgatā ati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_17:5.3",
        "original": "Bārāṇasī nāma puratthi kāsinaṁ,",
        "analyzed": "bārāṇasī nāma puratthi kāsina ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_17:5.4",
        "original": "Tattha ahosiṁ pure kesakārikā.",
        "analyzed": "tattha ahosim pure kesakārikā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_17:6.1",
        "original": "Buddhe ca dhamme ca pasannamānasā,",
        "analyzed": "buddhe ca dhamme ca pasanna mānasā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_17:6.2",
        "original": "Saṅghe ca ekantagatā asaṁsayā;",
        "analyzed": "saṅghe ca ekanta gatā a sasayā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_17:6.3",
        "original": "Akhaṇḍasikkhāpadā āgatapphalā,",
        "analyzed": "akhaṇḍa sikkhāpadā āgata phalā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_17:6.4",
        "original": "Sambodhidhamme niyatā anāmayā”ti.",
        "analyzed": "sambodhi dhamme niyatāḥ anāmayā ati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_17:7.1",
        "original": "“Tantyābhinandāmase svāgatañca te,",
        "analyzed": "tantyā abhinandāmase svāgatam ca te ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_17:7.2",
        "original": "Dhammena ca tvaṁ yasasā virocasi;",
        "analyzed": "dhammena ca tvam yasasā virocasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_17:7.3",
        "original": "Buddhe ca dhamme ca pasannamānase,",
        "analyzed": "buddhe ca dhamme ca pasanna mānase ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_17:7.4",
        "original": "Saṅghe ca ekantagate asaṁsaye;",
        "analyzed": "saṅghe ca eka anta gate a sa saye ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_17:7.5",
        "original": "Akhaṇḍasikkhāpade āgatapphale,",
        "analyzed": "akhaṇḍa sikkhāpade āgata phale ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_17:7.6",
        "original": "Sambodhidhamme niyate anāmaye”ti.",
        "analyzed": "sambodhi dhamme niyate anāmaya iti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_17:8.1",
        "original": "Kesakārīvimānaṁ sattarasamaṁ.",
        "analyzed": "kesakārī vimānam ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_17:9.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_17:10.1",
        "original": "Pañca pīṭhā tayo nāvā,",
        "analyzed": "pañca pīṭhā tayaḥ nāvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_17:10.2",
        "original": "dīpatiladakkhiṇā dve;",
        "analyzed": "dīpa tila dakkhiṇāḥ dve ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_17:10.3",
        "original": "Pati dve suṇisā uttarā,",
        "analyzed": "dve suṇisā uttarā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_17:10.4",
        "original": "sirimā kesakārikā;",
        "analyzed": "       ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_17:10.5",
        "original": "Vaggo tena pavuccatīti.",
        "analyzed": "vaggo tena pavuccati iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_17:11.1",
        "original": "Itthivimāne paṭhamo vaggo.",
        "analyzed": "itthi vimāne paṭhamaḥ vaggo ",
        "folio": "11"
    }
]