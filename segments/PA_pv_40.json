[
    {
        "segmentnr": "PA_pv_40:0.1",
        "original": "Petavatthu",
        "analyzed": "petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_40:0.2",
        "original": "Mahāvagga",
        "analyzed": "mahāvagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_40:0.3",
        "original": "5. Ucchupetavatthu",
        "analyzed": "ucchu petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_40:1.1",
        "original": "“Idaṁ mama ucchuvanaṁ mahantaṁ,",
        "analyzed": "idam mama ucchuvanam mahanta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_40:1.2",
        "original": "Nibbattati puññaphalaṁ anappakaṁ;",
        "analyzed": "nibbattati puñña phalam anappaka ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_40:1.3",
        "original": "Taṁ dāni me na paribhogameti,",
        "analyzed": "tam dāni me na paribhogam eti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_40:1.4",
        "original": "Ācikkha bhante kissa ayaṁ vipāko.",
        "analyzed": "ācikkha bhante kissa ayam vipākaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_40:2.1",
        "original": "Haññāmi khajjāmi ca vāyamāmi,",
        "analyzed": "haññāmi khajjāmi ca vāyamāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_40:2.2",
        "original": "Parisakkāmi paribhuñjituṁ kiñci;",
        "analyzed": "parisakkāmi paribhuñjitum kiñci ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_40:2.3",
        "original": "Svāhaṁ chinnathāmo kapaṇo lālapāmi,",
        "analyzed": "svāham kapaṇaḥ lālapāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_40:2.4",
        "original": "Kissa kammassa ayaṁ vipāko.",
        "analyzed": "kissa kammassa ayam vipākaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_40:3.1",
        "original": "Vighāto cāhaṁ paripatāmi chamāyaṁ,",
        "analyzed": "ca aham paripatāmi śamāya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_40:3.2",
        "original": "Parivattāmi vāricarova ghamme;",
        "analyzed": "parivattāmi vāricarā uva ghamme ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_40:3.3",
        "original": "Rudato ca me assukā niggalanti,",
        "analyzed": "rudataḥ ca me assukāḥ niggalanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_40:3.4",
        "original": "Ācikkha bhante kissa ayaṁ vipāko.",
        "analyzed": "ācikkha bhante kissa ayam vipākaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_40:4.1",
        "original": "Chāto kilanto ca pipāsito ca,",
        "analyzed": "chātaḥ kilantaḥ ca pipāsitaḥ ca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_40:4.2",
        "original": "Santassito sātasukhaṁ na vinde;",
        "analyzed": "santassitaḥ sāta sukham na vinde ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_40:4.3",
        "original": "Pucchāmi taṁ etamatthaṁ bhadante,",
        "analyzed": "pucchāmi tam etam attham bhadante ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_40:4.4",
        "original": "Kathaṁ nu ucchuparibhogaṁ labheyyan”ti.",
        "analyzed": "katham nu ucchu paribhogam labheyyanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_40:5.1",
        "original": "“Pure tuvaṁ kammamakāsi attanā,",
        "analyzed": "pure tuvam attanā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_40:5.2",
        "original": "Manussabhūto purimāya jātiyā;",
        "analyzed": "manussa bhūtaḥ purimāya jātiyā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_40:5.3",
        "original": "Ahañca taṁ etamatthaṁ vadāmi,",
        "analyzed": "aham ca tam etam attham vadāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_40:5.4",
        "original": "Sutvāna tvaṁ etamatthaṁ vijāna.",
        "analyzed": "sutvāna tvam etam attham vijāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_40:6.1",
        "original": "Ucchuṁ tuvaṁ khādamāno payāto,",
        "analyzed": "ucchum tuvam khādamānaḥ payātaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_40:6.2",
        "original": "Puriso ca te piṭṭhito anvagacchi;",
        "analyzed": "puriso ca te piṭṭhitaḥ anvagacchi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_40:6.3",
        "original": "So ca taṁ paccāsanto kathesi,",
        "analyzed": "so ca tam paccāsantaḥ kathesi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_40:6.4",
        "original": "Tassa tuvaṁ na kiñci ālapittha.",
        "analyzed": "tassa tuvam na kiñci ālapittha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_40:7.1",
        "original": "So ca taṁ abhaṇantaṁ ayāci,",
        "analyzed": "saḥ ca tam a bhaṇantam ayāci ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_40:7.2",
        "original": "‘Dehayya ucchun’ti ca taṁ avoca;",
        "analyzed": "dehayyaḥ ucchunti ca tam avoca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_40:7.3",
        "original": "Tassa tuvaṁ piṭṭhito ucchuṁ adāsi,",
        "analyzed": "tassa tuvam piṭṭhitaḥ ucchum adāsi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_40:7.4",
        "original": "Tassetaṁ kammassa ayaṁ vipāko.",
        "analyzed": "tassa itam kammassa ayam vipākaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_40:8.1",
        "original": "Iṅgha tvaṁ gantvāna piṭṭhito gaṇheyyāsi,",
        "analyzed": "tvam gantvāna piṭṭhitaḥ gaṇheyyāsi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_40:8.2",
        "original": "Gahetvāna taṁ khādassu yāvadatthaṁ;",
        "analyzed": "gahetvāna tam khādassu yāvat attha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_40:8.3",
        "original": "Teneva tvaṁ attamano bhavissasi,",
        "analyzed": "tena iva tvam atta manaḥ bhavissasi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_40:8.4",
        "original": "Haṭṭho cudaggo ca pamodito cā”ti.",
        "analyzed": "haṭṭhaḥ cudaggo ca pamodito ca ati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_40:9.1",
        "original": "Gantvāna so piṭṭhito aggahesi,",
        "analyzed": "gantvāna saḥ piṭṭhitaḥ aggahesi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_40:9.2",
        "original": "Gahetvāna taṁ khādi yāvadatthaṁ;",
        "analyzed": "tam khādi yāvat attha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_40:9.3",
        "original": "Teneva so attamano ahosi,",
        "analyzed": "tena iva saḥ atta manaḥ ahosi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_40:9.4",
        "original": "Haṭṭho cudaggo ca pamodito cāti.",
        "analyzed": "haṭṭhaḥ cudaggo ca pamodito ca ati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_40:10.1",
        "original": "Ucchupetavatthu pañcamaṁ.",
        "analyzed": "ucchupetavatthu pañcama ",
        "folio": "10"
    }
]