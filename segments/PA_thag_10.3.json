    },
    {
        "segmentnr": "PA_thag_10.3:0.4",
        "original": "Mahākappinattheragāthā",
        "analyzed": "     ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_10.3:1.1",
        "original": "“Anāgataṁ yo paṭikacca passati,",
        "analyzed": "anāgatam yaḥ paṭikacca passati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.3:1.2",
        "original": "Hitañca atthaṁ ahitañca taṁ dvayaṁ;",
        "analyzed": "hitañca attham ahitañca tam ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.3:1.3",
        "original": "Viddesino tassa hitesino vā,",
        "analyzed": "viddesinaḥ tassa hitesinaḥ vā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.3:1.4",
        "original": "Randhaṁ na passanti samekkhamānā.",
        "analyzed": "randham na passanti samekkhamānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_10.3:2.1",
        "original": "Ānāpānasatī yassa,",
        "analyzed": "āna apāna satī yassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.3:2.2",
        "original": "paripuṇṇā subhāvitā;",
        "analyzed": "paripuṇṇā su bhāvitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.3:2.3",
        "original": "Anupubbaṁ paricitā,",
        "analyzed": "anupubbam paricitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.3:2.4",
        "original": "yathā buddhena desitā;",
        "analyzed": "yathā buddhena desitā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.3:2.5",
        "original": "Somaṁ lokaṁ pabhāseti,",
        "analyzed": "somam lokam pabhāsā iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.3:2.6",
        "original": "abbhā muttova candimā.",
        "analyzed": "abbhā muttā uva candimā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.3:3.1",
        "original": "Odātaṁ vata me cittaṁ,",
        "analyzed": "odātam vata me citta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.3:3.2",
        "original": "appamāṇaṁ subhāvitaṁ;",
        "analyzed": "appamāṇam su bhāvita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.3:3.3",
        "original": "Nibbiddhaṁ paggahītañca,",
        "analyzed": "nibbiddham paggahītam ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.3:3.4",
        "original": "sabbā obhāsate disā.",
        "analyzed": "sabbāḥ obhāsate disā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_10.3:4.1",
        "original": "Jīvate vāpi sappañño,",
        "analyzed": "jīvate vā api sappaññaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.3:4.2",
        "original": "api vittaparikkhayo;",
        "analyzed": "api vitta parikkhayaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.3:4.3",
        "original": "Paññāya ca alābhena,",
        "analyzed": "paññāya ca alābhena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.3:4.4",
        "original": "vittavāpi na jīvati.",
        "analyzed": "vittavā api na jīvati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_10.3:5.1",
        "original": "Paññā sutavinicchinī,",
        "analyzed": "paññā suta vinicchinī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.3:5.2",
        "original": "Paññā kittisilokavaddhanī;",
        "analyzed": "paññā kitti si loka vat hanī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.3:5.3",
        "original": "Paññāsahito naro idha,",
        "analyzed": "paññā sahitaḥ naraḥ idha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.3:5.4",
        "original": "Api dukkhesu sukhāni vindati.",
        "analyzed": "api dukkhesu sukhāni vindati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_10.3:6.1",
        "original": "Nāyaṁ ajjatano dhammo,",
        "analyzed": "na ayam ajjatanaḥ dhammaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.3:6.2",
        "original": "nacchero napi abbhuto;",
        "analyzed": "naccheraḥ napi ap bhutaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.3:6.3",
        "original": "Yattha jāyetha mīyetha,",
        "analyzed": "yattha jāyetha mīyetha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.3:6.4",
        "original": "tattha kiṁ viya abbhutaṁ.",
        "analyzed": "tattha kim viya abbhuta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_10.3:7.1",
        "original": "Anantaraṁ hi jātassa,",
        "analyzed": "anantaram hi jātassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.3:7.2",
        "original": "jīvitā maraṇaṁ dhuvaṁ;",
        "analyzed": "jīvitā maraṇam dhuva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.3:7.3",
        "original": "Jātā jātā marantīdha,",
        "analyzed": "jātāḥ jātāḥ maranti idha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.3:7.4",
        "original": "evaṁdhammā hi pāṇino.",
        "analyzed": "eva dhammāḥ hi pāṇinaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_10.3:8.1",
        "original": "Na hetadatthāya matassa hoti,",
        "analyzed": "na ha etat atthāya matassa hoti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.3:8.2",
        "original": "Yaṁ jīvitatthaṁ paraporisānaṁ;",
        "analyzed": "yam jīvitattham ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.3:8.3",
        "original": "Matamhi ruṇṇaṁ na yaso na lokyaṁ,",
        "analyzed": "ruṇṇam na yasaḥ na ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.3:8.4",
        "original": "Na vaṇṇitaṁ samaṇabrāhmaṇehi.",
        "analyzed": "na vaṇṇitam samaṇa brāhmaṇehi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_10.3:9.1",
        "original": "Cakkhuṁ sarīraṁ upahanti tena,",
        "analyzed": "cakkhum sarīram upahanti tena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.3:9.2",
        "original": "Nihīyati vaṇṇabalaṁ matī ca;",
        "analyzed": "nihīyati vaṇṇa balam matī ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.3:9.3",
        "original": "Ānandino tassa disā bhavanti,",
        "analyzed": "ānandinaḥ tassa disāḥ bhavanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.3:9.4",
        "original": "Hitesino nāssa sukhī bhavanti.",
        "analyzed": "hitesinaḥ na assa sukhī bhavanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_thag_10.3:10.1",
        "original": "Tasmā hi iccheyya kule vasante,",
        "analyzed": "tasmai hi iccheyya kule vasante ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.3:10.2",
        "original": "Medhāvino ceva bahussute ca;",
        "analyzed": "medhāvino ca iva bahussute ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.3:10.3",
        "original": "Yesaṁ hi paññāvibhavena kiccaṁ,",
        "analyzed": "yesam hi paññā vibhavena kit ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.3:10.4",
        "original": "Taranti nāvāya nadiṁva puṇṇan”ti.",
        "analyzed": "taranti nāvāya nad iva puṇṇanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_thag_10.3:10.5",
        "original": "… Mahākappino thero ….",
        "analyzed": "mahākappinaḥ theraḥ ",
        "folio": "10"
    }
]