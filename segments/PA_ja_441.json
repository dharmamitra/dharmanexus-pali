[
    {
        "segmentnr": "PA_ja_441:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_441:0.2",
        "original": "Dasakanipāta",
        "analyzed": "dasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_441:0.3",
        "original": "Catudvāravagga",
        "analyzed": "catu dvāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_441:0.4",
        "original": "3. Catuposathiyajātaka",
        "analyzed": "catu posathiya jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_441:1.1",
        "original": "“Yo kopaneyye na karoti kopaṁ,",
        "analyzed": "yaḥ kopaneyye na karoti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_441:1.2",
        "original": "Na kujjhati sappuriso kadāci;",
        "analyzed": "na kujjhati sappurisaḥ kadāci ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_441:1.3",
        "original": "Kuddhopi so nāvikaroti kopaṁ,",
        "analyzed": "kuddhaḥ api saḥ na avikaroti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_441:1.4",
        "original": "Taṁ ve naraṁ samaṇamāhu loke”.",
        "analyzed": "tam ve naram samaṇam āhu loke ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_441:2.1",
        "original": "“Ūnūdaro yo sahate jighacchaṁ,",
        "analyzed": "ūnūdaraḥ yaḥ sahate jighaccha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_441:2.2",
        "original": "Danto tapassī mitapānabhojano;",
        "analyzed": "dantaḥ tapassī mita pāna bhojanaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_441:2.3",
        "original": "Āhārahetu na karoti pāpaṁ,",
        "analyzed": "āhāra hetu na karoti pāpa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_441:2.4",
        "original": "Taṁ ve naraṁ samaṇamāhu loke”.",
        "analyzed": "tam ve naram samaṇam āhu loke ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_441:3.1",
        "original": "“Khiḍḍaṁ ratiṁ vippajahitvāna sabbaṁ,",
        "analyzed": "khiḍḍam ratim vippajahitvāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_441:3.2",
        "original": "Na cālikaṁ bhāsasi kiñci loke;",
        "analyzed": "na ca alikam bhāsasi kiñci loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_441:3.3",
        "original": "Vibhūsaṭṭhānā virato methunasmā,",
        "analyzed": "virataḥ methunasmā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_441:3.4",
        "original": "Taṁ ve naraṁ samaṇamāhu loke”.",
        "analyzed": "tam ve naram samaṇam āhu loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_441:4.1",
        "original": "“Pariggahaṁ lobhadhammañca sabbaṁ,",
        "analyzed": "pariggaham lobha dhammañca ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_441:4.2",
        "original": "Yo ve pariññāya pariccajeti;",
        "analyzed": "yaḥ ve pariññāya pariccaja iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_441:4.3",
        "original": "Dantaṁ ṭhitattaṁ amamaṁ nirāsaṁ,",
        "analyzed": "dantam amamam nirāsa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_441:4.4",
        "original": "Taṁ ve naraṁ samaṇamāhu loke”.",
        "analyzed": "tam ve naram samaṇam āhu loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_441:5.1",
        "original": "“Pucchāma kattāramanomapaññaṁ,",
        "analyzed": "pucchāma kattāram anomapañña ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_441:5.2",
        "original": "Kathāsu no viggaho atthi jāto;",
        "analyzed": "kathāsu no viggahaḥ atthi jātaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_441:5.3",
        "original": "Chindajja kaṅkhaṁ vicikicchitāni,",
        "analyzed": "chindat ja kaṅkham vicikicchitāni ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_441:5.4",
        "original": "Tadajja kaṅkhaṁ vitaremu sabbe”.",
        "analyzed": "tat ajja kaṅkham vitaremu sabbe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_441:6.1",
        "original": "“Ye paṇḍitā atthadasā bhavanti,",
        "analyzed": "ye paṇḍitāḥ attha dasāḥ bhavanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_441:6.2",
        "original": "Bhāsanti te yoniso tattha kāle;",
        "analyzed": "bhāsanti te yonisaḥ tattha kāle ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_441:6.3",
        "original": "Kathaṁ nu kathānaṁ abhāsitānaṁ,",
        "analyzed": "katham nu kathānam ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_441:6.4",
        "original": "Atthaṁ nayeyyuṁ kusalā janindā.",
        "analyzed": "attham nayeyyum kusalā janindā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_441:7.1",
        "original": "Kathaṁ have bhāsati nāgarājā,",
        "analyzed": "katham have bhāsati nāga rājā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_441:7.2",
        "original": "Garuḷo pana venateyyo kimāha;",
        "analyzed": "garuḍaḥ pana venateyyaḥ kim āha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_441:7.3",
        "original": "Gandhabbarājā pana kiṁ vadesi,",
        "analyzed": "gandhabbarājā pana kim vadesi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_441:7.4",
        "original": "Kathaṁ pana kurūnaṁ rājaseṭṭho”.",
        "analyzed": "katham pana rāja seṭṭhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_441:8.1",
        "original": "“Khantiṁ have bhāsati nāgarājā,",
        "analyzed": "khantim have bhāsati nāga rājā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_441:8.2",
        "original": "Appāhāraṁ garuḷo venateyyo;",
        "analyzed": "appā ahāram garuḷaḥ venateyyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_441:8.3",
        "original": "Gandhabbarājā rativippahānaṁ,",
        "analyzed": "gandhabba rājā rati vippahāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_441:8.4",
        "original": "Akiñcanaṁ kurūnaṁ rājaseṭṭho”.",
        "analyzed": "akiñcanam kuru ūnam rāja seṭṭhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_441:9.1",
        "original": "“Sabbāni etāni subhāsitāni,",
        "analyzed": "sabbāni etāni su bhāsitāni ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_441:9.2",
        "original": "Na hettha dubbhāsitamatthi kiñci;",
        "analyzed": "na ha ittha dubbhāsitam atthi kiñci ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_441:9.3",
        "original": "Yasmiñca etāni patiṭṭhitāni,",
        "analyzed": "yasmin ca etāni patiṭṭhitāni ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_441:9.4",
        "original": "Arāva nābhyā susamohitāni;",
        "analyzed": "arāva nābhyā su samohitāni ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_441:9.5",
        "original": "Catubbhi dhammehi samaṅgibhūtaṁ,",
        "analyzed": "catubbhi dhammehi samaṅgi bhūta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_441:9.6",
        "original": "Taṁ ve naraṁ samaṇamāhu loke”.",
        "analyzed": "tam ve naram samaṇam āhu loke ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_441:10.1",
        "original": "“Tuvañhi seṭṭho tvamanuttarosi,",
        "analyzed": "tuvam hi seṭṭhaḥ tvam anuttaraḥ asi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_441:10.2",
        "original": "Tvaṁ dhammagū dhammavidū sumedho;",
        "analyzed": "tvam dhamma gū dhamma vidū sumedho ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_441:10.3",
        "original": "Paññāya pañhaṁ samadhiggahetvā,",
        "analyzed": "paññāya pañham samadhiggahetvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_441:10.4",
        "original": "Acchecchi dhīro vicikicchitāni;",
        "analyzed": "acchecchi dhīraḥ vicikicchitāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_441:10.5",
        "original": "Acchecchi kaṅkhaṁ vicikicchitāni,",
        "analyzed": "acchecchi kaṅkham vicikicchitāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_441:10.6",
        "original": "Cundo yathā nāgadantaṁ kharena.",
        "analyzed": "cundaḥ yathā nāga dantam kharena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_441:11.1",
        "original": "Nīluppalābhaṁ vimalaṁ anagghaṁ,",
        "analyzed": "nīluppala ābham vimalam anagdha ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_441:11.2",
        "original": "Vatthaṁ idaṁ dhūmasamānavaṇṇaṁ;",
        "analyzed": "vattham idam dhūma samāna vaṇṇa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_441:11.3",
        "original": "Pañhassa veyyākaraṇena tuṭṭho,",
        "analyzed": "pañhassa veyyākaraṇena tuṭṭhaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_441:11.4",
        "original": "Dadāmi te dhammapūjāya dhīra.",
        "analyzed": "dadāmi te dhamma pūjāya dhīra ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_441:12.1",
        "original": "Suvaṇṇamālaṁ satapattaphullitaṁ,",
        "analyzed": "suvaṇṇa mālam ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_441:12.2",
        "original": "Sakesaraṁ ratnasahassamaṇḍitaṁ;",
        "analyzed": "sa kesaram ratna sahassa maṇḍita ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_441:12.3",
        "original": "Pañhassa veyyākaraṇena tuṭṭho,",
        "analyzed": "pañhassa veyyākaraṇena tuṭṭhaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_441:12.4",
        "original": "Dadāmi te dhammapūjāya dhīra.",
        "analyzed": "dadāmi te dhamma pūjāya dhīra ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_441:13.1",
        "original": "Maṇiṁ anagghaṁ ruciraṁ pabhassaraṁ,",
        "analyzed": "maṇim anaggham ruciram pabhassara ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_441:13.2",
        "original": "Kaṇṭhāvasattaṁ maṇibhūsitaṁ me;",
        "analyzed": "kaṇṭha avasattam maṇi bhūsitam me ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_441:13.3",
        "original": "Pañhassa veyyākaraṇena tuṭṭho,",
        "analyzed": "pañhassa veyyākaraṇena tuṭṭhaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_441:13.4",
        "original": "Dadāmi te dhammapūjāya dhīra.",
        "analyzed": "dadāmi te dhamma pūjāya dhīra ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_441:14.1",
        "original": "Gavaṁ sahassaṁ usabhañca nāgaṁ,",
        "analyzed": "gavam sahassam usabham ca nāga ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_441:14.2",
        "original": "Ājaññayutte ca rathe dasa ime;",
        "analyzed": "ājañña yutte ca rathe dasa ime ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_441:14.3",
        "original": "Pañhassa veyyākaraṇena tuṭṭho,",
        "analyzed": "pañhassa veyyākaraṇena tuṭṭhaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_441:14.4",
        "original": "Dadāmi te gāmavarāni soḷasa”.",
        "analyzed": "dadāmi te gām avarāni soḷasa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_441:15.1",
        "original": "“Sāriputto tadā nāgo,",
        "analyzed": "sāriputto tadā nāgaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_441:15.2",
        "original": "supaṇṇo pana kolito;",
        "analyzed": "supaṇṇaḥ pana kolitaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_441:15.3",
        "original": "Gandhabbarājā anuruddho,",
        "analyzed": "gandhabba rājā anuruddhaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_441:15.4",
        "original": "rājā ānandapaṇḍito;",
        "analyzed": "rājā ānandapaṇḍito ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_441:15.5",
        "original": "Vidhuro bodhisatto ca,",
        "analyzed": "vidhuraḥ bodhisatto ca ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_441:15.6",
        "original": "evaṁ dhāretha jātakan”ti.",
        "analyzed": "evam dhāretha jātakanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_441:16.1",
        "original": "Catuposathiyajātakaṁ tatiyaṁ.",
        "analyzed": "catuposathiya jātakam ",
        "folio": "16"
    }
]