        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.6:1.3",
        "original": "Suvaṇṇavaṇṇosi bhagavā,",
        "analyzed": "suvaṇṇa vaṇṇaḥ asi bhagavā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.6:1.4",
        "original": "Susukkadāṭhosi vīriyavā.",
        "analyzed": "vīriyavā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_16.6:2.1",
