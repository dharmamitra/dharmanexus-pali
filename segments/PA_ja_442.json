        "analyzed": "dasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_442:0.3",
        "original": "Catudvāravagga",
        "analyzed": "catu dvāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_442:0.4",
        "original": "4. Saṅkhajātaka",
        "analyzed": "saṅkha jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_442:1.1",
        "original": "“Bahussuto sutadhammosi saṅkha,",
        "analyzed": "bahu sutaḥ suta dhammaḥ asi saṅkha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_442:1.2",
        "original": "Diṭṭhā tayā samaṇabrāhmaṇā ca;",
        "analyzed": "diṭṭhā tayā samaṇa brāhmaṇā ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_442:1.3",
        "original": "Athakkhaṇe dassayase vilāpaṁ,",
        "analyzed": "athakkhaṇe dassayase vilāpa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_442:1.4",
        "original": "Añño nu ko te paṭimantako mayā”.",
        "analyzed": "aññaḥ nu ko te paṭimantakaḥ mayā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_442:2.1",
        "original": "“Subbhū subhā suppaṭimukkakambu,",
        "analyzed": "       ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_442:2.2",
        "original": "Paggayha sovaṇṇamayāya pātiyā;",
        "analyzed": "paggayha sovaṇṇa mayāya pātiyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_442:2.3",
        "original": "‘Bhuñjassu bhattaṁ’ iti maṁ vadeti,",
        "analyzed": "bhuñjassu bhatte iti mama vada iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_442:2.4",
        "original": "Saddhāvittā",
        "analyzed": "saddhā vittā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_442:2.5",
        "original": "Tamahaṁ noti brūmi”.",
        "analyzed": "tam aham na uti brūmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_442:3.1",
        "original": "“Etādisaṁ brāhmaṇa disvāna yakkhaṁ,",
        "analyzed": "etādisam brāhmaṇa disvāna yakkha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_442:3.2",
        "original": "Puccheyya poso sukhamāsisāno;",
        "analyzed": "puccheyya posaḥ sukham āsisāno ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_442:3.3",
        "original": "Uṭṭhehi naṁ pañjalikābhipuccha,",
        "analyzed": "uṭṭhehi nam pañjalika abhipuccha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_442:3.4",
        "original": "Devī nusi tvaṁ uda mānusī nu”.",
        "analyzed": "devī nusi tvam uda mānusī nu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_442:4.1",
        "original": "“Yaṁ tvaṁ sukhenābhisamekkhase maṁ,",
        "analyzed": "yam tvam sukhena abhisamekkhase ma ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_442:4.2",
        "original": "Bhuñjassu bhattaṁ iti maṁ vadesi;",
        "analyzed": "bhuñjassu bhattam iti mama vadesi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_442:4.3",
        "original": "Pucchāmi taṁ nāri mahānubhāve,",
        "analyzed": "pucchāmi tam nāri mahā anubhāve ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_442:4.4",
        "original": "Devī nusi tvaṁ uda mānusī nu”.",
        "analyzed": "devī nusi tvam uda mānusī nu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_442:5.1",
        "original": "“Devī ahaṁ saṅkha mahānubhāvā,",
        "analyzed": "devī aham saṅkha mahā anubhāvā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_442:5.2",
        "original": "Idhāgatā sāgaravārimajjhe;",
        "analyzed": "idha āgatā sāgara vāri majjhe ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_442:5.3",
        "original": "Anukampikā no ca paduṭṭhacittā,",
        "analyzed": "anukampikā no ca paduṭṭha cittā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_442:5.4",
        "original": "Taveva atthāya idhāgatāsmi.",
        "analyzed": "tava iva atthāya idha āgatā asmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_442:6.1",
        "original": "Idhannapānaṁ sayanāsanañca,",
        "analyzed": "idhan apānam sayana āsanañca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_442:6.2",
        "original": "Yānāni nānāvividhāni saṅkha;",
        "analyzed": "yānāni nānā vividhāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_442:6.3",
        "original": "Sabbassa tyāhaṁ paṭipādayāmi,",
        "analyzed": "sabbassa tyā aham paṭipādayāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_442:6.4",
        "original": "Yaṁ kiñci tuyhaṁ manasābhipatthitaṁ”.",
        "analyzed": "yam kiñci tuyham manasā abhipatthita ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_442:7.1",
        "original": "“Yaṁ kiñci yiṭṭhañca hutañca mayhaṁ,",
        "analyzed": "yam kiñci yiṭṭham ca hutam ca mayha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_442:7.2",
        "original": "Sabbassa no issarā tvaṁ sugatte;",
        "analyzed": "sabbassa naḥ issarā tvam sugatte ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_442:7.3",
        "original": "Susoṇi subbhamu suvilaggamajjhe,",
        "analyzed": "su soṇi su vilagga majjhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_442:7.4",
        "original": "Kissa me kammassa ayaṁ vipāko”.",
        "analyzed": "kissa me kammassa ayam vipākaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_442:8.1",
        "original": "“Ghamme pathe brāhmaṇa ekabhikkhuṁ,",
        "analyzed": "ghamme pathe brāhmaṇaḥ eka bhikkhu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_442:8.2",
        "original": "Ugghaṭṭapādaṁ tasitaṁ kilantaṁ;",
        "analyzed": "ugghaṭṭa pādam tasitam kilanta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_442:8.3",
        "original": "Paṭipādayī saṅkha upāhanāni,",
        "analyzed": "paṭipādayī saṅkhaḥ upāhanāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_442:8.4",
        "original": "Sā dakkhiṇā kāmaduhā tavajja”.",
        "analyzed": "sā dakkhiṇā kāmaduhā tavajja ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_442:9.1",
        "original": "“Sā hotu nāvā phalakūpapannā,",
        "analyzed": "sā hotu nāvā phalakūpapannā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_442:9.2",
        "original": "Anavassutā erakavātayuttā;",
        "analyzed": "an avassutā eraka vāta yuttā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_442:9.3",
        "original": "Aññassa yānassa na hettha bhūmi,",
        "analyzed": "aññassa yānassa na ha ittha bhūmi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_442:9.4",
        "original": "Ajjeva maṁ moḷiniṁ pāpayassu”.",
        "analyzed": "ajjā iva mam moḷinim pāpayassu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_442:10.1",
        "original": "“Sā tattha vittā sumanā patītā,",
        "analyzed": "sā tattha vittā sumanā patītā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_442:10.2",
        "original": "Nāvaṁ sucittaṁ abhinimminitvā;",
        "analyzed": "nāvam su cittam abhinimminitvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_442:10.3",
        "original": "Ādāya saṅkhaṁ purisena saddhiṁ,",
        "analyzed": "ādāya saṅkham purisena saddhi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_442:10.4",
        "original": "Upānayī nagaraṁ sādhuramman”ti.",
        "analyzed": "upānayī nagaram ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_442:11.1",
        "original": "Saṅkhajātakaṁ catutthaṁ.",
        "analyzed": "saṅkha jātakam ",
        "folio": "11"
    }
]