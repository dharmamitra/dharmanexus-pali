[
    {
        "segmentnr": "PA_ja_522:0.1",
        "original": "Jātaka",
        "analyzed": "jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_522:0.2",
        "original": "Cattālīsanipāta",
        "analyzed": "cattālīsa nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_522:0.3",
        "original": "Tesakuṇavagga",
        "analyzed": "tesa kuṇa vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_522:0.4",
        "original": "2. Sarabhaṅgajātaka",
        "analyzed": "sarabhaṅgajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_522:1.1",
        "original": "“Alaṅkatā kuṇḍalino suvatthā,",
        "analyzed": "alaṅkatā kuṇḍalinaḥ suvatthā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_522:1.2",
        "original": "Veḷuriyamuttātharukhaggabandhā;",
        "analyzed": "veḷuriya muttā atharu khaggabandhā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_522:1.3",
        "original": "Rathesabhā tiṭṭhatha ke nu tumhe,",
        "analyzed": "rathesabhā tiṭṭhatha ke nu tumhe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_522:1.4",
        "original": "Kathaṁ vo jānanti manussaloke”.",
        "analyzed": "katham vaḥ jānanti manussa loke ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_522:2.1",
        "original": "“Ahamaṭṭhako bhīmaratho panāyaṁ,",
        "analyzed": "ahamaṭṭhakaḥ bhīmaratho panāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_522:2.2",
        "original": "Kāliṅgarājā pana uggatoyaṁ;",
        "analyzed": "kāliṅga rājā pana ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_522:2.3",
        "original": "Susaññatānaṁ isīnaṁ dassanāya,",
        "analyzed": "su saññatānam isīnam dassanāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_522:2.4",
        "original": "Idhāgatā pucchitāyemha pañhe”.",
        "analyzed": "idha āgatā pucchitāya imha pañhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_522:3.1",
        "original": "“Vehāyasaṁ tiṭṭhasi antalikkhe,",
        "analyzed": "vehāyasam tiṭṭhasi antalikkhe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_522:3.2",
        "original": "Pathaddhuno pannaraseva cando;",
        "analyzed": "pathaddhunaḥ pannarasā iva cando ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_522:3.3",
        "original": "Pucchāmi taṁ yakkha mahānubhāva,",
        "analyzed": "pucchāmi tam yakkha mahā anubhāva ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_522:3.4",
        "original": "Kathaṁ taṁ jānanti manussaloke”.",
        "analyzed": "katham tam jānanti manussa loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_522:4.1",
        "original": "“Yamāhu devesu sujampatīti,",
        "analyzed": "yam āhu devesu sujampati iti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_522:4.2",
        "original": "Maghavāti taṁ āhu manussaloke;",
        "analyzed": "maghavā ati tam āhu manussa loke ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_522:4.3",
        "original": "Sa devarājā idamajja patto,",
        "analyzed": "sa deva rājā idam ajja ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_522:4.4",
        "original": "Susaññatānaṁ isīnaṁ dassanāya”.",
        "analyzed": "su saññatānam isīnam dassanāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_522:5.1",
        "original": "“Dūre sutā no isayo samāgatā,",
        "analyzed": "dūre sutā naḥ isayo samāgatā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_522:5.2",
        "original": "Mahiddhikā iddhiguṇūpapannā;",
        "analyzed": "mahiddhikā iddhi guṇa upapannā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_522:5.3",
        "original": "Vandāmi te ayire pasannacitto,",
        "analyzed": "vandāmi te ayire pasanna cittaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_522:5.4",
        "original": "Ye jīvalokettha manussaseṭṭhā”.",
        "analyzed": "ye jīva loka ittha manussa seṭṭhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_522:6.1",
        "original": "“Gandho isīnaṁ ciradikkhitānaṁ,",
        "analyzed": "gandhaḥ isīnam cira dikkhitāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_522:6.2",
        "original": "Kāyā cuto gacchati mālutena;",
        "analyzed": "kāyā cutaḥ gacchati mālutena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_522:6.3",
        "original": "Ito paṭikkamma sahassanetta,",
        "analyzed": "itas paṭikkamma sahassanetta ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_522:6.4",
        "original": "Gandho isīnaṁ asuci devarāja”.",
        "analyzed": "gandhaḥ asuci deva rāja ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_522:7.1",
        "original": "“Gandho isīnaṁ ciradikkhitānaṁ,",
        "analyzed": "gandhaḥ isīnam cira dikkhitāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_522:7.2",
        "original": "Kāyā cuto gacchatu mālutena;",
        "analyzed": "kāyā cutaḥ gacchatu mālutena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_522:7.3",
        "original": "Vicitrapupphaṁ surabhiṁva mālaṁ,",
        "analyzed": "vicitra puppham ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_522:7.4",
        "original": "Gandhañca etaṁ pāṭikaṅkhāma bhante;",
        "analyzed": "gandham ca etam pāṭikaṅkhāma bhante ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_522:7.5",
        "original": "Na hettha devā paṭikkūlasaññino”.",
        "analyzed": "na ha ittha devā paṭikkūla saññinaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_522:8.1",
        "original": "“Purindado bhūtapatī yasassī,",
        "analyzed": "purindadaḥ bhūta patī yasassī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_522:8.2",
        "original": "Devānamindo sakko maghavā sujampati;",
        "analyzed": "devānam indo sakkaḥ maghavā su jampati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_522:8.3",
        "original": "Sa devarājā asuragaṇappamaddano,",
        "analyzed": "sa deva rājā asura gaṇa ppamaddanaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_522:8.4",
        "original": "Okāsamākaṅkhati pañha pucchituṁ.",
        "analyzed": "okāsam ākaṅkhati pañha pucchitu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_522:9.1",
        "original": "Ko nevimesaṁ idha paṇḍitānaṁ,",
        "analyzed": "kaḥ idha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_522:9.2",
        "original": "Pañhe puṭṭho nipuṇe byākarissati;",
        "analyzed": "pañhe puṭṭhaḥ nipuṇe byākarissati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_522:9.3",
        "original": "Tiṇṇañca raññaṁ manujādhipānaṁ,",
        "analyzed": "tiṇṇam ca raññam manuja adhipāna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_522:9.4",
        "original": "Devānamindassa ca vāsavassa”.",
        "analyzed": "devānamindassa ca vāsavassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_522:10.1",
        "original": "“Ayaṁ isi sarabhaṅgo tapassī,",
        "analyzed": "ayam isi sarabhaṅgaḥ tapassī ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_522:10.2",
        "original": "Yato jāto virato methunasmā;",
        "analyzed": "yatas jātaḥ virataḥ methunasmā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_522:10.3",
        "original": "Āceraputto suvinītarūpo,",
        "analyzed": "ācera putto su vinīta rūpaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_522:10.4",
        "original": "So nesaṁ pañhāni viyākarissati”.",
        "analyzed": "so na esam pañhāni viyākarissati ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_522:11.1",
        "original": "“Koṇḍañña pañhāni viyākarohi,",
        "analyzed": "koṇḍañña pañhāni viyākarohi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_522:11.2",
        "original": "Yācanti taṁ isayo sādhurūpā;",
        "analyzed": "yācanti tam sādhu rūpā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_522:11.3",
        "original": "Koṇḍañña eso manujesu dhammo,",
        "analyzed": "koṇḍaññaḥ esaḥ manujesu dhammo ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_522:11.4",
        "original": "Yaṁ vuddhamāgacchati esa bhāro”.",
        "analyzed": "yam vuddham āgacchati esa bhāraḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_522:12.1",
        "original": "“Katāvakāsā pucchantu bhonto,",
        "analyzed": "kata avakāsā pucchantu bhonto ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_522:12.2",
        "original": "Yaṁ kiñci pañhaṁ manasābhipatthitaṁ;",
        "analyzed": "yam kiñci pañham ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_522:12.3",
        "original": "Ahañhi taṁ taṁ vo viyākarissaṁ,",
        "analyzed": "aham hi tam tam vaḥ viyākarissa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_522:12.4",
        "original": "Ñatvā sayaṁ lokamimaṁ parañca”.",
        "analyzed": "ñatvā sayam lokam imam parañca ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_522:13.1",
        "original": "Tato ca maghavā sakko,",
        "analyzed": "tatas ca maghavā sakkaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_522:13.2",
        "original": "atthadassī purindado;",
        "analyzed": "attha dassī purindado ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_522:13.3",
        "original": "Apucchi paṭhamaṁ pañhaṁ,",
        "analyzed": "apucchi paṭhamam ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_522:13.4",
        "original": "yañcāsi abhipatthitaṁ.",
        "analyzed": "yañca asi abhipatthita ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_522:14.1",
        "original": "“Kiṁ sū vadhitvā na kadāci socati,",
        "analyzed": "kim su vadhitvā na kadācid socati ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_522:14.2",
        "original": "Kissappahānaṁ isayo vaṇṇayanti;",
        "analyzed": "kissappahānam isayaḥ vaṇṇayanti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_522:14.3",
        "original": "Kassīdha vuttaṁ pharusaṁ khametha,",
        "analyzed": "kassi idha vuttam pharusam khametha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_522:14.4",
        "original": "Akkhāhi me koṇḍañña etamatthaṁ”.",
        "analyzed": "akkhāhi me koṇḍañña etam attha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_522:15.1",
        "original": "“Kodhaṁ vadhitvā na kadāci socati,",
        "analyzed": "kodham vadhitvā na kadācid socati ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_522:15.2",
        "original": "Makkhappahānaṁ isayo vaṇṇayanti;",
        "analyzed": "makkha ppahānam isayaḥ vaṇṇayanti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_522:15.3",
        "original": "Sabbesaṁ vuttaṁ pharusaṁ khametha,",
        "analyzed": "sabbesam vuttam pharusam khametha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_522:15.4",
        "original": "Etaṁ khantiṁ uttamamāhu santo”.",
        "analyzed": "etam khantim uttamam āhu santaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_522:16.1",
        "original": "“Sakkā ubhinnaṁ vacanaṁ titikkhituṁ,",
        "analyzed": "sakkā ubhinnam vacanam titikkhitu ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_522:16.2",
        "original": "Sadisassa vā seṭṭhatarassa vāpi;",
        "analyzed": "sadisassa vā seṭṭhatarassa vā api ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_522:16.3",
        "original": "Kathaṁ nu hīnassa vaco khametha,",
        "analyzed": "katham nu hīnassa vacaḥ khametha ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_522:16.4",
        "original": "Akkhāhi me koṇḍañña etamatthaṁ”.",
        "analyzed": "akkhāhi me koṇḍañña etam attha ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_522:17.1",
        "original": "“Bhayā hi seṭṭhassa vaco khametha,",
        "analyzed": "bhayā hi seṭṭhassa vacaḥ khametha ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_522:17.2",
        "original": "Sārambhahetū pana sādisassa;",
        "analyzed": "sa ārambha hetū pana sādisassa ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_522:17.3",
        "original": "Yo cīdha hīnassa vaco khametha,",
        "analyzed": "yaḥ ci idha hīnassa vacaḥ khametha ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_522:17.4",
        "original": "Etaṁ khantiṁ uttamamāhu santo”.",
        "analyzed": "etam khantim uttamam āhu santaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_522:18.1",
        "original": "“Kathaṁ vijaññā catupattharūpaṁ,",
        "analyzed": "katham ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_522:18.2",
        "original": "Seṭṭhaṁ sarikkhaṁ athavāpi hīnaṁ;",
        "analyzed": "seṭṭham sa rikkham athavā api ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_522:18.3",
        "original": "Virūparūpena caranti santo,",
        "analyzed": "virūpa rūpena caranti santaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_522:18.4",
        "original": "Tasmā hi sabbesaṁ vaco khametha”.",
        "analyzed": "tasmā hi sabbesam vacaḥ khametha ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_522:19.1",
        "original": "“Na hetamatthaṁ mahatīpi senā,",
        "analyzed": "na mahatī ipi senā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_522:19.2",
        "original": "Sarājikā yujjhamānā labhetha;",
        "analyzed": "sa rājikā yujjhamānā labhetha ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_522:19.3",
        "original": "Yaṁ khantimā sappuriso labhetha,",
        "analyzed": "yam khantimā sappurisaḥ labhetha ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_522:19.4",
        "original": "Khantī balassūpasamanti verā”.",
        "analyzed": "khantī balassu upasamanti verā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_522:20.1",
        "original": "“Subhāsitaṁ te anumodiyāna,",
        "analyzed": "su bhāsitam te anumodiyāna ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_522:20.2",
        "original": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
        "analyzed": "aññam tam pucchāmi tat iṅgha brūhi ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_522:20.3",
        "original": "Yathā ahuṁ daṇḍakī nāḷikero,",
        "analyzed": "yathā daṇḍakī nāḷikero ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_522:20.4",
        "original": "Athajjuno kalābu cāpi rājā;",
        "analyzed": "kalābu ca api rājā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_522:20.5",
        "original": "Tesaṁ gatiṁ brūhi supāpakamminaṁ,",
        "analyzed": "tesam gatim brūhi su pāpa kammina ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_522:20.6",
        "original": "Katthūpapannā isinaṁ viheṭhakā”.",
        "analyzed": "katthu upapannāḥ isinam viheṭhakā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_522:21.1",
        "original": "“Kisañhi vacchaṁ avakiriya daṇḍakī,",
        "analyzed": "kisan hi vaccham daṇḍakī ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_522:21.2",
        "original": "Ucchinnamūlo sajano saraṭṭho;",
        "analyzed": "ucchinna mūlaḥ sa janaḥ sa raṭṭhaḥ ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_522:21.3",
        "original": "Kukkuḷanāme nirayamhi paccati,",
        "analyzed": "kukkuḷa nāme nirayamhi ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_522:21.4",
        "original": "Tassa phuliṅgāni patanti kāye.",
        "analyzed": "tassa phuliṅgāni patanti kāye ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_522:22.1",
        "original": "Yo saññate pabbajite aheṭhayi,",
        "analyzed": "yaḥ saññate pabbajite aheṭhayi ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_522:22.2",
        "original": "Dhammaṁ bhaṇante samaṇe adūsake;",
        "analyzed": "dhammam bhaṇante samaṇe adūsake ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_522:22.3",
        "original": "Taṁ nāḷikeraṁ sunakhā parattha,",
        "analyzed": "tam nāḷikeram su nakhā parattha ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_522:22.4",
        "original": "Saṅgamma khādanti viphandamānaṁ.",
        "analyzed": "saṅgamma khādanti viphandamāna ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_522:23.1",
        "original": "Athajjuno niraye sattisūle,",
        "analyzed": "atha ajjunaḥ niraye satti sūle ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_522:23.2",
        "original": "Avaṁsiro patito uddhampādo;",
        "analyzed": "avasiraḥ patitaḥ uddhampādaḥ ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_522:23.3",
        "original": "Aṅgīrasaṁ gotamaṁ heṭhayitvā,",
        "analyzed": "aṅgīrasam gotamam heṭhayitvā ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_522:23.4",
        "original": "Khantiṁ tapassiṁ cirabrahmacāriṁ.",
        "analyzed": "khantim tapassim cirabrahmacāri ",
        "folio": "23"
    },
    {
        "segmentnr": "PA_ja_522:24.1",
        "original": "Yo khaṇḍaso pabbajitaṁ achedayi,",
        "analyzed": "yaḥ khaṇḍasas pabbajitam achedayi ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_522:24.2",
        "original": "Khantiṁ vadantaṁ samaṇaṁ adūsakaṁ;",
        "analyzed": "khantim vadantam samaṇam adūsaka ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_522:24.3",
        "original": "Kalābuvīciṁ upapajja paccati,",
        "analyzed": "kalābu vīcim upapajja paccati ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_522:24.4",
        "original": "Mahāpatāpaṁ kaṭukaṁ bhayānakaṁ.",
        "analyzed": "mahā patāpam kaṭukam bhayānaka ",
        "folio": "24"
    },
    {
        "segmentnr": "PA_ja_522:25.1",
        "original": "Etāni sutvā nirayāni paṇḍito,",
        "analyzed": "etāni sutvā nirayāni paṇḍitaḥ ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_522:25.2",
        "original": "Aññāni pāpiṭṭhatarāni cettha;",
        "analyzed": "aññāni pāpiṭṭhatarāni ca ittha ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_522:25.3",
        "original": "Dhammaṁ care samaṇabrāhmaṇesu,",
        "analyzed": "dhammam care samaṇa brāhmaṇesu ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_522:25.4",
        "original": "Evaṅkaro saggamupeti ṭhānaṁ”.",
        "analyzed": "evaṅkaraḥ saggam upeti ",
        "folio": "25"
    },
    {
        "segmentnr": "PA_ja_522:26.1",
        "original": "“Subhāsitaṁ te anumodiyāna,",
        "analyzed": "su bhāsitam te anumodiyāna ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_522:26.2",
        "original": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
        "analyzed": "aññam tam pucchāmi tat iṅgha brūhi ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_522:26.3",
        "original": "Kathaṁvidhaṁ sīlavantaṁ vadanti,",
        "analyzed": "kathavidham sīlavantam vadanti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_522:26.4",
        "original": "Kathaṁvidhaṁ paññavantaṁ vadanti;",
        "analyzed": "kathavidham paññavantam vadanti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_522:26.5",
        "original": "Kathaṁvidhaṁ sappurisaṁ vadanti,",
        "analyzed": "kathavidham sappurisam vadanti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_522:26.6",
        "original": "Kathaṁvidhaṁ no siri no jahāti”.",
        "analyzed": "kathavidham naḥ siri naḥ jahāti ",
        "folio": "26"
    },
    {
        "segmentnr": "PA_ja_522:27.1",
        "original": "“Kāyena vācāya ca yodha saññato,",
        "analyzed": "kāyena vācāya ca yodha saññato ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_522:27.2",
        "original": "Manasā ca kiñci na karoti pāpaṁ;",
        "analyzed": "manasā ca kiñci na karoti pāpa ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_522:27.3",
        "original": "Na attahetū alikaṁ bhaṇeti,",
        "analyzed": "na atta hetū alikam bhaṇa iti ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_522:27.4",
        "original": "Tathāvidhaṁ sīlavantaṁ vadanti.",
        "analyzed": "tathāvidham sīlavantam vadanti ",
        "folio": "27"
    },
    {
        "segmentnr": "PA_ja_522:28.1",
        "original": "Gambhīrapañhaṁ manasābhicintayaṁ,",
        "analyzed": "gambhīra pañham manasā abhicintaya ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_522:28.2",
        "original": "Nāccāhitaṁ kamma karoti luddaṁ;",
        "analyzed": "na accāhitam kamma karoti ludda ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_522:28.3",
        "original": "Kālāgataṁ atthapadaṁ na riñcati,",
        "analyzed": "kālāgatam attha padam na riñcati ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_522:28.4",
        "original": "Tathāvidhaṁ paññavantaṁ vadanti.",
        "analyzed": "tathāvidham paññavantam vadanti ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_ja_522:29.1",
        "original": "Yo ve kataññū katavedi dhīro,",
        "analyzed": "yaḥ ve kataññū kata vedi dhīraḥ ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_522:29.2",
        "original": "Kalyāṇamitto daḷhabhatti ca hoti;",
        "analyzed": "kalyāṇa mittaḥ daḷha bhatti ca hoti ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_522:29.3",
        "original": "Dukhitassa sakkacca karoti kiccaṁ,",
        "analyzed": "dukhitassa sakkacca karoti kicca ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_522:29.4",
        "original": "Tathāvidhaṁ sappurisaṁ vadanti.",
        "analyzed": "tathāvidham sappurisam vadanti ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_ja_522:30.1",
        "original": "Etehi sabbehi guṇehupeto,",
        "analyzed": "etehi sabbehi guṇehi upetaḥ ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_522:30.2",
        "original": "Saddho mudū saṁvibhāgī vadaññū;",
        "analyzed": "saddhaḥ mudū savibhāgī vadaññū ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_522:30.3",
        "original": "Saṅgāhakaṁ sakhilaṁ saṇhavācaṁ,",
        "analyzed": "saṅgāhakam sa khilam saṇha vāca ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_522:30.4",
        "original": "Tathāvidhaṁ no siri no jahāti”.",
        "analyzed": "tathāvidham naḥ siri naḥ jahāti ",
        "folio": "30"
    },
    {
        "segmentnr": "PA_ja_522:31.1",
        "original": "“Subhāsitaṁ te anumodiyāna,",
        "analyzed": "su bhāsitam te anumodiyāna ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_522:31.2",
        "original": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
        "analyzed": "aññam tam pucchāmi tat iṅgha brūhi ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_522:31.3",
        "original": "Sīlaṁ siriñcāpi satañca dhammaṁ,",
        "analyzed": "sīlam siriñca api satam ca dhamma ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_522:31.4",
        "original": "Paññañca kaṁ seṭṭhataraṁ vadanti”.",
        "analyzed": "paññañca kam seṭṭhataram vadanti ",
        "folio": "31"
    },
    {
        "segmentnr": "PA_ja_522:32.1",
        "original": "“Paññā hi seṭṭhā kusalā vadanti,",
        "analyzed": "paññā hi seṭṭhā kusalā vadanti ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_522:32.2",
        "original": "Nakkhattarājāriva tārakānaṁ;",
        "analyzed": "tārakāna ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_522:32.3",
        "original": "Sīlaṁ sīrī cāpi satañca dhammo,",
        "analyzed": "sīlam sīrī ca api ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_522:32.4",
        "original": "Anvāyikā paññavato bhavanti”.",
        "analyzed": "anvāyikāḥ paññavataḥ bhavanti ",
        "folio": "32"
    },
    {
        "segmentnr": "PA_ja_522:33.1",
        "original": "“Subhāsitaṁ te anumodiyāna,",
        "analyzed": "su bhāsitam te anumodiyāna ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_522:33.2",
        "original": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
        "analyzed": "aññam tam pucchāmi tat iṅgha brūhi ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_522:33.3",
        "original": "Kathaṅkaro kintikaro kimācaraṁ,",
        "analyzed": "kathaṅkaro kintikaro kimācara ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_522:33.4",
        "original": "Kiṁ sevamāno labhatīdha paññaṁ;",
        "analyzed": "kim sevamānaḥ labhati idha pañña ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_522:33.5",
        "original": "Paññāya dāni paṭipadaṁ vadehi,",
        "analyzed": "paññāya dāni paṭipadam vadehi ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_522:33.6",
        "original": "Kathaṅkaro paññavā hoti macco”.",
        "analyzed": "kathaṅkaraḥ paññavā hoti macco ",
        "folio": "33"
    },
    {
        "segmentnr": "PA_ja_522:34.1",
        "original": "“Sevetha vuddhe nipuṇe bahussute,",
        "analyzed": "sevetha vuddhe nipuṇe bahu ssute ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_522:34.2",
        "original": "Uggāhako ca paripucchako siyā;",
        "analyzed": "uggāhako ca paripucchako siyā ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_522:34.3",
        "original": "Suṇeyya sakkacca subhāsitāni,",
        "analyzed": "suṇeyya sakkacca su bhāsitāni ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_522:34.4",
        "original": "Evaṅkaro paññavā hoti macco.",
        "analyzed": "evaṅkaraḥ paññavā hoti macco ",
        "folio": "34"
    },
    {
        "segmentnr": "PA_ja_522:35.1",
        "original": "Sa paññavā kāmaguṇe avekkhati,",
        "analyzed": "sa paññavā kāma guṇe avekkhati ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_522:35.2",
        "original": "Aniccato dukkhato rogato ca;",
        "analyzed": "aniccataḥ dukkhataḥ rogataḥ ca ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_522:35.3",
        "original": "Evaṁ vipassī pajahāti chandaṁ,",
        "analyzed": "evam vipassī pajahāti chanda ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_522:35.4",
        "original": "Dukkhesu kāmesu mahabbhayesu.",
        "analyzed": "dukkhesu kāmesu mahat bhayesu ",
        "folio": "35"
    },
    {
        "segmentnr": "PA_ja_522:36.1",
        "original": "Sa vītarāgo pavineyya dosaṁ,",
        "analyzed": "sa vīta rāgaḥ dosa ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_522:36.2",
        "original": "Mettaṁ cittaṁ bhāvaye appamāṇaṁ;",
        "analyzed": "mettam cittam bhāvaye appamāṇa ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_522:36.3",
        "original": "Sabbesu bhūtesu nidhāya daṇḍaṁ,",
        "analyzed": "sabbesu bhūtesu nidhāya daṇḍa ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_522:36.4",
        "original": "Anindito brahmamupeti ṭhānaṁ”.",
        "analyzed": "aninditaḥ brahmam upeti ",
        "folio": "36"
    },
    {
        "segmentnr": "PA_ja_522:37.1",
        "original": "“Mahatthiyaṁ āgamanaṁ ahosi,",
        "analyzed": "mahatthiyam āgamanam ahosi ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_522:37.2",
        "original": "Tavamaṭṭhakā bhīmarathassa cāpi;",
        "analyzed": "tavamaṭṭhakāḥ bhīmarathassa ca api ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_522:37.3",
        "original": "Kāliṅgarājassa ca uggatassa,",
        "analyzed": "kāliṅga rājassa ca uggatassa ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_522:37.4",
        "original": "Sabbesa vo kāmarāgo pahīno”.",
        "analyzed": "sabbesa vaḥ kāma rāgaḥ pahīnaḥ ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_ja_522:38.1",
        "original": "“Evametaṁ paracittavedi,",
        "analyzed": "evam etam para citta vedi ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_522:38.2",
        "original": "Sabbesa no kāmarāgo pahīno;",
        "analyzed": "sabbesa no kāma rāgaḥ pahīnaḥ ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_522:38.3",
        "original": "Karohi okāsamanuggahāya,",
        "analyzed": "karohi okāsa anuggahāya ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_522:38.4",
        "original": "Yathā gatiṁ te abhisambhavema”.",
        "analyzed": "yathā gatim te abhisambhavema ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_ja_522:39.1",
        "original": "“Karomi okāsamanuggahāya,",
        "analyzed": "karomi okāsa anuggahāya ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_522:39.2",
        "original": "Tathā hi vo kāmarāgo pahīno;",
        "analyzed": "tathā hi vaḥ kāma rāgaḥ pahīnaḥ ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_522:39.3",
        "original": "Pharātha kāyaṁ vipulāya pītiyā,",
        "analyzed": "kāyam vipulāya pītiyā ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_522:39.4",
        "original": "Yathā gatiṁ me abhisambhavetha”.",
        "analyzed": "yathā gatim me abhisambhavetha ",
        "folio": "39"
    },
    {
        "segmentnr": "PA_ja_522:40.1",
        "original": "“Sabbaṁ karissāma tavānusāsaniṁ,",
        "analyzed": "sabbam karissāma tava anusāsani ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_522:40.2",
        "original": "Yaṁ yaṁ tuvaṁ vakkhasi bhūripañña;",
        "analyzed": "yam yam tuvam vakkhasi bhūri pañña ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_522:40.3",
        "original": "Pharāma kāyaṁ vipulāya pītiyā,",
        "analyzed": "kāyam vipulāya pītiyā ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_522:40.4",
        "original": "Yathā gatiṁ te abhisambhavema”.",
        "analyzed": "yathā gatim te abhisambhavema ",
        "folio": "40"
    },
    {
        "segmentnr": "PA_ja_522:41.1",
        "original": "“Katāya vacchassa kisassa pūjā,",
        "analyzed": "katāya vacchassa kisassa pūjā ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_522:41.2",
        "original": "Gacchantu bhonto isayo sādhurūpā;",
        "analyzed": "gacchantu bhontaḥ isayaḥ sādhu rūpā ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_522:41.3",
        "original": "Jhāne ratā hotha sadā samāhitā,",
        "analyzed": "ratā sadā samāhitā ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_522:41.4",
        "original": "Esā ratī pabbajitassa seṭṭhā”.",
        "analyzed": "esā ratī pabbajitassa seṭṭhā ",
        "folio": "41"
    },
    {
        "segmentnr": "PA_ja_522:42.1",
        "original": "Sutvāna gāthā paramatthasaṁhitā,",
        "analyzed": "sutvāna gāthā paramattha sahitā ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_522:42.2",
        "original": "Subhāsitā isinā paṇḍitena;",
        "analyzed": "su bhāsitā isinā paṇḍitena ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_522:42.3",
        "original": "Te vedajātā anumodamānā,",
        "analyzed": "te veda jātā anumodamānā ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_522:42.4",
        "original": "Pakkāmu devā devapuraṁ yasassino.",
        "analyzed": "devāḥ deva puram yasassinaḥ ",
        "folio": "42"
    },
    {
        "segmentnr": "PA_ja_522:43.1",
        "original": "Gāthā imā atthavatī subyañjanā,",
        "analyzed": "gāthāḥ imāḥ atthavatī su byañjanā ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_522:43.2",
        "original": "Subhāsitā isinā paṇḍitena;",
        "analyzed": "su bhāsitā isinā paṇḍitena ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_522:43.3",
        "original": "Yo kocimā aṭṭhikatvā suṇeyya,",
        "analyzed": "yaḥ kocimāḥ aṭṭhi katvā suṇeyya ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_522:43.4",
        "original": "Labhetha pubbāpariyaṁ visesaṁ;",
        "analyzed": "labhetha pubbāpariyam ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_522:43.5",
        "original": "Laddhāna pubbāpariyaṁ visesaṁ,",
        "analyzed": "pubba apariyam ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_522:43.6",
        "original": "Adassanaṁ maccurājassa gacche.",
        "analyzed": "a dassanam maccu rājassa gacche ",
        "folio": "43"
    },
    {
        "segmentnr": "PA_ja_522:44.1",
        "original": "“Sālissaro sāriputto,",
        "analyzed": "sālissaraḥ sāriputto ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_522:44.2",
        "original": "meṇḍissaro ca kassapo;",
        "analyzed": "meṇḍissaraḥ ca kassapaḥ ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_522:44.3",
        "original": "Pabbato anuruddho ca,",
        "analyzed": "pabbataḥ anuruddhaḥ ca ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_522:44.4",
        "original": "kaccāyano ca devalo.",
        "analyzed": "kaccāyanaḥ ca devalaḥ ",
        "folio": "44"
    },
    {
        "segmentnr": "PA_ja_522:45.1",
        "original": "Anusisso ca ānando,",
        "analyzed": "anusissaḥ ca ānandaḥ ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_522:45.2",
        "original": "kisavaccho ca kolito;",
        "analyzed": "kisavacchaḥ ca kolitaḥ ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_522:45.3",
        "original": "Nārado udāyitthero,",
        "analyzed": "nāradaḥ udāyi theraḥ ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_522:45.4",
        "original": "parisā buddhaparisā;",
        "analyzed": "parisā buddha parisā ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_522:45.5",
        "original": "Sarabhaṅgo lokanātho,",
        "analyzed": "sarabhaṅgaḥ loka nāthaḥ ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_522:45.6",
        "original": "evaṁ dhāretha jātakan”ti.",
        "analyzed": "evam dhāretha jātakanti ",
        "folio": "45"
    },
    {
        "segmentnr": "PA_ja_522:46.1",
        "original": "Sarabhaṅgajātakaṁ dutiyaṁ.",
        "analyzed": "sarabhaṅga jātakam ",
        "folio": "46"
    }
]