        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_377:2.3",
        "original": "Esā disā paramā setaketu,",
        "analyzed": "esā disā paramā setaketu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_377:2.4",
        "original": "Yaṁ patvā dukkhī sukhino bhavanti”.",
