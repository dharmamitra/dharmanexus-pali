[
    {
        "segmentnr": "PA_snp_4.8:0.1",
        "original": "Sutta Nipāta 4.8",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.8:0.2",
        "original": "Pasūrasutta",
        "analyzed": "pasūrasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.8:1.1",
        "original": "Idheva suddhī iti vādayanti,",
        "analyzed": "idhā iva suddhī iti vādayanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.8:1.2",
        "original": "Nāññesu dhammesu visuddhimāhu;",
        "analyzed": "na aññesu dhammesu visuddhim āhu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.8:1.3",
        "original": "Yaṁ nissitā tattha subhaṁ vadānā,",
        "analyzed": "yam nissitā tattha subham vadānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.8:1.4",
        "original": "Paccekasaccesu puthū niviṭṭhā.",
        "analyzed": "paccekasaccesu puthū niviṭṭhā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.8:2.1",
        "original": "Te vādakāmā parisaṁ vigayha,",
        "analyzed": "te vāda kāmā parisam vigayha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.8:2.2",
        "original": "Bālaṁ dahantī mithu aññamaññaṁ;",
        "analyzed": "bālam dahantī mithu aññamañña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.8:2.3",
        "original": "Vadanti te aññasitā kathojjaṁ,",
        "analyzed": "vadanti te aññasitā kathojja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.8:2.4",
        "original": "Pasaṁsakāmā kusalā vadānā.",
        "analyzed": "pasasa kāmā kusalā vadānā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.8:3.1",
        "original": "Yutto kathāyaṁ parisāya majjhe,",
        "analyzed": "yuttaḥ kathāyam parisāya majjhe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.8:3.2",
        "original": "Pasaṁsamicchaṁ vinighāti hoti;",
        "analyzed": "vinighāti hoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.8:3.3",
        "original": "Apāhatasmiṁ pana maṅku hoti,",
        "analyzed": "apāhatasmin pana maṅku hoti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.8:3.4",
        "original": "Nindāya so kuppati randhamesī.",
        "analyzed": "nindāya saḥ kuppati randhamesī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.8:4.1",
        "original": "Yamassa vādaṁ parihīnamāhu,",
        "analyzed": "yamassa vādam parihīnam āhu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.8:4.2",
        "original": "Apāhataṁ pañhavimaṁsakāse;",
        "analyzed": "apāhatam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.8:4.3",
        "original": "Paridevati socati hīnavādo,",
        "analyzed": "paridevati socati hīna vādaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.8:4.4",
        "original": "Upaccagā manti anutthunāti.",
        "analyzed": "upaccagā manti anutthunāti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.8:5.1",
        "original": "Ete vivādā samaṇesu jātā,",
        "analyzed": "ete vivādā samaṇesu jātā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.8:5.2",
        "original": "Etesu ugghātinighāti hoti;",
        "analyzed": "etesu ugghāti nighāti hoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.8:5.3",
        "original": "Etampi disvā virame kathojjaṁ,",
        "analyzed": "etam pi disvā virame ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.8:5.4",
        "original": "Na haññadatthatthi pasaṁsalābhā.",
        "analyzed": "na hañña datthatthi pasa sa lābhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.8:6.1",
        "original": "Pasaṁsito vā pana tattha hoti,",
        "analyzed": "pasasito vā pana tattha hoti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.8:6.2",
        "original": "Akkhāya vādaṁ parisāya majjhe;",
        "analyzed": "akkhāya vādam parisāya majjhe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.8:6.3",
        "original": "So hassatī unnamatī ca tena,",
        "analyzed": "saḥ hassatī unnamatī ca tena ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.8:6.4",
        "original": "Pappuyya tamatthaṁ yathā mano ahu.",
        "analyzed": "pappuyya tamattham yathā manaḥ ahu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.8:7.1",
        "original": "Yā unnatī sāssa vighātabhūmi,",
        "analyzed": "yā unnatī sā assa vighāta bhūmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.8:7.2",
        "original": "Mānātimānaṁ vadate paneso;",
        "analyzed": "māna atimānam vadate paneso ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.8:7.3",
        "original": "Etampi disvā na vivādayetha,",
        "analyzed": "etam pi disvā na vivādayetha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.8:7.4",
        "original": "Na hi tena suddhiṁ kusalā vadanti.",
        "analyzed": "na hi tena suddhim kusalāḥ vadanti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.8:8.1",
        "original": "Sūro yathā rājakhādāya puṭṭho,",
        "analyzed": "sūraḥ yathā rāja khādāya puṭṭhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.8:8.2",
        "original": "Abhigajjameti paṭisūramicchaṁ;",
        "analyzed": "abhigajjam eti paṭisūram iccha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.8:8.3",
        "original": "Yeneva so tena palehi sūra,",
        "analyzed": "yena iva saḥ tena palehi sūra ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.8:8.4",
        "original": "Pubbeva natthi yadidaṁ yudhāya.",
        "analyzed": "pubbā iva na atthi yat idam yudhāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.8:9.1",
        "original": "Ye diṭṭhimuggayha vivādayanti,",
        "analyzed": "ye diṭṭhim uggayha vivādayanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.8:9.2",
        "original": "Idameva saccanti ca vādayanti;",
        "analyzed": "idam eva ca vādayanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.8:9.3",
        "original": "Te tvaṁ vadassū na hi tedha atthi,",
        "analyzed": "te tvam vadassū na hi tedha atthi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.8:9.4",
        "original": "Vādamhi jāte paṭisenikattā.",
        "analyzed": "vādamhi jāte paṭisenikattā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.8:10.1",
        "original": "Visenikatvā pana ye caranti,",
        "analyzed": "visenikatvā pana ye caranti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.8:10.2",
        "original": "Diṭṭhīhi diṭṭhiṁ avirujjhamānā;",
        "analyzed": "diṭṭhīhi diṭṭhim a virujjhamānā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.8:10.3",
        "original": "Tesu tvaṁ kiṁ labhetho pasūra,",
        "analyzed": "tesu tvam kim labhethaḥ pasūra ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.8:10.4",
        "original": "Yesīdha natthī paramuggahītaṁ.",
        "analyzed": "natthi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.8:11.1",
        "original": "Atha tvaṁ pavitakkamāgamā,",
        "analyzed": "atha tvam pavitakkam āgamā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.8:11.2",
        "original": "Manasā diṭṭhigatāni cintayanto;",
        "analyzed": "manasā diṭṭhi gatāni cintayanto ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.8:11.3",
        "original": "Dhonena yugaṁ samāgamā,",
        "analyzed": "yugam samāgamā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.8:11.4",
        "original": "Na hi tvaṁ sakkhasi sampayātaveti.",
        "analyzed": "na hi tvam sakkhasi sampayātave iti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.8:11.5",
        "original": "Pasūrasuttaṁ aṭṭhamaṁ.",
        "analyzed": "pasūra suttam aṭṭhama ",
        "folio": "11"
    }
]