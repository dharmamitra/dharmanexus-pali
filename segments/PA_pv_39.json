    {
        "segmentnr": "PA_pv_39:0.3",
        "original": "4. Revatīpetavatthu",
        "analyzed": "revatī petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_39:1.1",
        "original": "“Uṭṭhehi revate supāpadhamme,",
        "analyzed": "uṭṭhehi revate su pāpa dhamme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_39:1.2",
        "original": "Apārutadvāre adānasīle;",
        "analyzed": "a pāruta dvāre a dāna sīle ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_39:1.3",
        "original": "Nessāma taṁ yattha thunanti duggatā,",
        "analyzed": "nessāma tam yattha thunanti duggatā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_39:1.4",
        "original": "Samappitā nerayikā dukhenā”ti.",
        "analyzed": "samappitā na īrayikāḥ dukhena ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_39:2.1",
        "original": "Icceva vatvāna yamassa dūtā,",
        "analyzed": "iccā iva vatvāna yamassa dūtā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_39:2.2",
        "original": "Te dve yakkhā lohitakkhā brahantā;",
        "analyzed": "te dve yakkhā lohitakkhā brahantā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_39:2.3",
        "original": "Paccekabāhāsu gahetvā revataṁ,",
        "analyzed": "pacceka bāhāsu gahetvā revata ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_39:2.4",
        "original": "Pakkāmayuṁ devagaṇassa santike.",
        "analyzed": "pakkāmayum deva gaṇassa santike ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_39:3.1",
        "original": "“Ādiccavaṇṇaṁ ruciraṁ pabhassaraṁ,",
        "analyzed": "ādicca vaṇṇam ruciram pabhassara ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_39:3.2",
        "original": "Byamhaṁ subhaṁ kañcanajālachannaṁ;",
        "analyzed": "byamham subham kañcana jāla channa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_39:3.3",
        "original": "Kassetamākiṇṇajanaṁ vimānaṁ,",
        "analyzed": "kassa itam ākiṇṇa janam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_39:3.4",
        "original": "Suriyassa raṁsīriva jotamānaṁ.",
        "analyzed": "suriyassa rasīḥ iva jotamāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_39:4.1",
        "original": "Nārīgaṇā candanasāralittā,",
        "analyzed": "nārī gaṇā candana sāra littā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_39:4.2",
        "original": "Ubhato vimānaṁ upasobhayanti;",
        "analyzed": "ubhatas vimānam upasobhayanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_39:4.3",
        "original": "Taṁ dissati suriyasamānavaṇṇaṁ,",
        "analyzed": "tam dissati suriya samāna vaṇṇa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_39:4.4",
        "original": "Ko modati saggapatto vimāne”ti.",
        "analyzed": "kaḥ modati sagga pattaḥ vimāneti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_39:5.1",
        "original": "“Bārāṇasiyaṁ nandiyo nāmāsi,",
        "analyzed": "bārāṇasiyam nandiyaḥ nāma asi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_39:5.2",
        "original": "Upāsako amaccharī dānapati vadaññū;",
        "analyzed": "upāsakaḥ amaccharī dānapati vadaññū ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_39:5.3",
        "original": "Tassetamākiṇṇajanaṁ vimānaṁ,",
        "analyzed": "tassa itam ākiṇṇa janam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_39:5.4",
        "original": "Suriyassa raṁsīriva jotamānaṁ.",
        "analyzed": "suriyassa rasīḥ iva jotamāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_39:6.1",
        "original": "Nārīgaṇā candanasāralittā,",
        "analyzed": "nārī gaṇā candana sāra littā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_39:6.2",
        "original": "Ubhato vimānaṁ upasobhayanti;",
        "analyzed": "ubhatas vimānam upasobhayanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_39:6.3",
        "original": "Taṁ dissati suriyasamānavaṇṇaṁ,",
        "analyzed": "tam dissati suriya samāna vaṇṇa ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_39:6.4",
        "original": "So modati saggapatto vimāne”ti.",
        "analyzed": "saḥ modati sagga pattaḥ vimāneti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_39:7.1",
        "original": "“Nandiyassāhaṁ bhariyā,",
        "analyzed": "nandiyassa aham bhariyā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_39:7.2",
        "original": "Agārinī sabbakulassa issarā;",
        "analyzed": "agārinī sabba kulassa issarā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_39:7.3",
        "original": "Bhattu vimāne ramissāmi dānahaṁ,",
        "analyzed": "bhattu vimāne ramissāmi dānaha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_39:7.4",
        "original": "Na patthaye nirayadassanāyā”ti.",
        "analyzed": "na patthaye niraya dassanāya ati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_39:8.1",
        "original": "“Eso te nirayo supāpadhamme,",
        "analyzed": "esaḥ te nirayaḥ su pāpa dhamme ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_39:8.2",
        "original": "Puññaṁ tayā akataṁ jīvaloke;",
        "analyzed": "puññam tayā a katam jīva loke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_39:8.3",
        "original": "Na hi maccharī rosako pāpadhammo,",
        "analyzed": "na hi maccharī rosakaḥ pāpa dhammo ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_39:8.4",
        "original": "Saggūpagānaṁ labhati sahabyatan”ti.",
        "analyzed": "saggūpagānam labhati sahabyatanti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_39:9.1",
        "original": "“Kiṁ nu gūthañca muttañca,",
        "analyzed": "kim nu gūtham ca muttam ca ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_39:9.2",
        "original": "asucī paṭidissati;",
        "analyzed": "asucī paṭidissati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_39:9.3",
        "original": "Duggandhaṁ kimidaṁ mīḷhaṁ,",
        "analyzed": "duggandham kim idam mīḷha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_39:9.4",
        "original": "kimetaṁ upavāyatī”ti.",
        "analyzed": "kim etam upavāyati iti ",
        "folio": "9"
    },
