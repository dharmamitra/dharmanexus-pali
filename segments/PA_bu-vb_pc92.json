        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_pc92:2.1.7",
        "original": "Kārāpeyyāti",
        "analyzed": "kārāpeyyā iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_bu-vb_pc92:2.1.8",
        "original": "karoti vā kārāpeti vā, payoge dukkaṭaṁ.",
