    {
        "segmentnr": "PA_ja_454:0.4",
        "original": "16. Ghaṭapaṇḍitajātaka",
        "analyzed": "ghaṭa paṇḍita jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_454:1.1",
        "original": "“Uṭṭhehi kaṇha kiṁ sesi,",
        "analyzed": "uṭṭhehi kaṇha kim sesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_454:1.2",
        "original": "Ko attho supanena te;",
        "analyzed": "kaḥ atthaḥ supanena te ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_454:1.3",
        "original": "Yopi tuyhaṁ sako bhātā,",
        "analyzed": "yaḥ pi tuyham sakaḥ bhātā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_454:1.4",
        "original": "Hadayaṁ cakkhu ca dakkhiṇaṁ;",
        "analyzed": "hadayam cakkhu ca dakkhiṇa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_454:1.5",
        "original": "Tassa vātā balīyanti,",
        "analyzed": "tassa vātāḥ balīyanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_454:1.6",
        "original": "Ghaṭo jappati kesava”.",
        "analyzed": "ghaṭaḥ jappati kesava ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_454:2.1",
        "original": "“Tassa taṁ vacanaṁ sutvā,",
        "analyzed": "tassa tam vacanam sutvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_454:2.2",
        "original": "rohiṇeyyassa kesavo;",
        "analyzed": "rohiṇeyyassa kesavaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_454:2.3",
        "original": "Taramānarūpo vuṭṭhāsi,",
        "analyzed": "taramāna rūpaḥ vuṭṭhāsi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_454:2.4",
        "original": "bhātusokena aṭṭito”.",
        "analyzed": "bhātu sokena aṭṭitaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_454:3.1",
        "original": "“Kiṁ nu ummattarūpova,",
        "analyzed": "kim nu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_454:3.2",
        "original": "kevalaṁ dvārakaṁ imaṁ;",
        "analyzed": "kevalam dvārakam ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_454:3.3",
        "original": "Saso sasoti lapasi,",
        "analyzed": "saso saso iti lapasi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_454:3.4",
        "original": "ko nu te sasamāhari”.",
        "analyzed": "kaḥ nu te sasamāhari ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_454:4.1",
        "original": "“Sovaṇṇamayaṁ maṇīmayaṁ,",
        "analyzed": "sovaṇṇa mayam maṇī maya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_454:4.2",
        "original": "Lohamayaṁ atha rūpiyāmayaṁ;",
        "analyzed": "loha mayam atha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_454:4.3",
        "original": "Saṅkhasilāpavāḷamayaṁ,",
        "analyzed": "saṅkha silā pavāḷa maya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_454:4.4",
        "original": "Kārayissāmi te sasaṁ”.",
        "analyzed": "kārayissāmi te sasa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_454:5.1",
        "original": "“Santi aññepi sasakā,",
        "analyzed": "santi aññe pi sasakā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_454:5.2",
        "original": "araññe vanagocarā;",
        "analyzed": "araññe vana gocarā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_454:5.3",
        "original": "Tepi te ānayissāmi,",
        "analyzed": "te ipi te ānayissāmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_454:5.4",
        "original": "kīdisaṁ sasamicchasi”.",
        "analyzed": "kīdisam sasam icchasi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_454:6.1",
        "original": "“Na cāhamete icchāmi,",
        "analyzed": "na ca aham ete icchāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_454:6.2",
        "original": "ye sasā pathavissitā;",
        "analyzed": "ye sasā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_454:6.3",
        "original": "Candato sasamicchāmi,",
        "analyzed": "candataḥ sasam icchāmi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_454:6.4",
        "original": "taṁ me ohara kesava”.",
        "analyzed": "tam me ohara kesava ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_454:7.1",
        "original": "“So nūna madhuraṁ ñāti,",
        "analyzed": "saḥ nūna madhuram ñāti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_454:7.2",
        "original": "jīvitaṁ vijahissasi;",
        "analyzed": "jīvitam vijahissasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_454:7.3",
        "original": "Apatthiyaṁ yo patthayasi,",
        "analyzed": "apatthiyam yaḥ patthayasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_454:7.4",
        "original": "candato sasamicchasi”.",
        "analyzed": "candataḥ sasam icchasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_454:8.1",
        "original": "“Evañce kaṇha jānāsi,",
        "analyzed": "evañce kaṇha jānāsi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_454:8.2",
        "original": "yadaññamanusāsasi;",
        "analyzed": "yat aññam anusāsasi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_454:8.3",
        "original": "Kasmā pure mataṁ puttaṁ,",
        "analyzed": "kasmai pure matam putta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_454:8.4",
        "original": "ajjāpi manusocasi”.",
        "analyzed": "ajja api manu socasi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_454:9.1",
        "original": "“Yaṁ na labbhā manussena,",
        "analyzed": "yam na labbhā manussena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_454:9.2",
        "original": "amanussena vā puna;",
        "analyzed": "amanussena vā puna ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_454:9.3",
        "original": "Jāto me mā marī putto,",
        "analyzed": "jātaḥ me mā marī putto ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_454:9.4",
        "original": "kuto labbhā alabbhiyaṁ.",
        "analyzed": "kutas ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_454:10.1",
        "original": "Na mantā mūlabhesajjā,",
        "analyzed": "na mantā mūlabhesajjā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_454:10.2",
        "original": "osadhehi dhanena vā;",
        "analyzed": "osadhehi dhanena vā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_454:10.3",
        "original": "Sakkā ānayituṁ kaṇha,",
        "analyzed": "sakkā ānayitum kaṇha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_454:10.4",
        "original": "yaṁ petamanusocasi”.",
        "analyzed": "yam petam anusocasi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_454:11.1",
        "original": "“Yassa etādisā assu,",
        "analyzed": "yassa etādisāḥ assu ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_454:11.2",
        "original": "amaccā purisapaṇḍitā;",
        "analyzed": "amaccā purisa paṇḍitā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_454:11.3",
        "original": "Yathā nijjhāpaye ajja,",
        "analyzed": "yathā nijjhāpaye ajja ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_454:11.4",
        "original": "ghaṭo purisapaṇḍito.",
        "analyzed": "ghaṭaḥ purisa paṇḍito ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_454:12.1",
        "original": "Ādittaṁ vata maṁ santaṁ,",
