        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_368:2.3",
        "original": "Socantamenaṁ dukhitaṁ viditvā,",
        "analyzed": "socantam enam dukhitam viditvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_368:2.4",
