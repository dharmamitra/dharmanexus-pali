        "analyzed": "aneka rūpena vadanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.8:4.6",
        "original": "Kiñcāpi te tattha yatā caranti,",
        "analyzed": "kiñca api te tattha yatā caranti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.8:4.7",
