        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_12:4.4",
        "original": "Loke adinnaṁ parivajjayissaṁ.",
        "analyzed": "loke adinnam parivajjayissa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_12:5.1",
        "original": "Amajjapā no ca musā abhāṇiṁ,",
