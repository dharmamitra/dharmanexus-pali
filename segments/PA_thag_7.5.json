    },
    {
        "segmentnr": "PA_thag_7.5:0.4",
        "original": "Sarabhaṅgattheragāthā",
        "analyzed": "sarabhaṅgattheragāthā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_thag_7.5:1.1",
        "original": "“Sare hatthehi bhañjitvā,",
        "analyzed": "sare hatthehi bhañjitvā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_7.5:1.2",
        "original": "katvāna kuṭimacchisaṁ;",
        "analyzed": "katvāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_7.5:1.3",
        "original": "Tena me sarabhaṅgoti,",
        "analyzed": "tena me sarabhaṅga iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_7.5:1.4",
        "original": "nāmaṁ sammutiyā ahu.",
        "analyzed": "nāmam sammutiyāḥ ahu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_7.5:2.1",
        "original": "Na mayhaṁ kappate ajja,",
        "analyzed": "na mayham kappate ajja ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_7.5:2.2",
        "original": "sare hatthehi bhañjituṁ;",
        "analyzed": "sare hatthehi bhañjitu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_7.5:2.3",
        "original": "Sikkhāpadā no paññattā,",
        "analyzed": "sikkhāpadā no paññattā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_7.5:2.4",
        "original": "gotamena yasassinā.",
        "analyzed": "gotamena yasassinā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_7.5:3.1",
        "original": "Sakalaṁ samattaṁ rogaṁ,",
        "analyzed": "sakalam sa mattam roga ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_7.5:3.2",
        "original": "Sarabhaṅgo nāddasaṁ pubbe;",
        "analyzed": "sarabhaṅgo na addasam pubbe ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_7.5:3.3",
        "original": "Soyaṁ rogo diṭṭho,",
        "analyzed": "saḥ ayam rogaḥ diṭṭho ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_7.5:3.4",
        "original": "Vacanakarenātidevassa.",
        "analyzed": "vacana karena atidevassa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_thag_7.5:4.1",
        "original": "Yeneva maggena gato vipassī,",
        "analyzed": "yena iva maggena gataḥ vipassī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_7.5:4.2",
        "original": "Yeneva maggena sikhī ca vessabhū;",
        "analyzed": "yena iva maggena sikhī ca vessabhū ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_7.5:4.3",
        "original": "Kakusandhakoṇāgamano ca kassapo,",
        "analyzed": "kakusandha koṇa āgamanaḥ ca kassapaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_7.5:4.4",
        "original": "Tenañjasena agamāsi gotamo.",
        "analyzed": "tena añjasena agamāsi gotamaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_thag_7.5:5.1",
        "original": "Vītataṇhā anādānā,",
        "analyzed": "vīta taṇhāḥ anādānā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_7.5:5.2",
        "original": "satta buddhā khayogadhā;",
        "analyzed": "satta buddhā kha yogadhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_7.5:5.3",
        "original": "Yehāyaṁ desito dhammo,",
        "analyzed": "yeha ayam desito dhammo ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_7.5:5.4",
        "original": "dhammabhūtehi tādibhi.",
        "analyzed": "dhamma bhūtehi tādibhi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_thag_7.5:6.1",
        "original": "Cattāri ariyasaccāni,",
        "analyzed": "cattāri ariya saccāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_7.5:6.2",
        "original": "anukampāya pāṇinaṁ;",
        "analyzed": "anukampāya pāṇina ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_7.5:6.3",
        "original": "Dukkhaṁ samudayo maggo,",
        "analyzed": "dukkham samudayaḥ maggo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_7.5:6.4",
        "original": "nirodho dukkhasaṅkhayo.",
        "analyzed": "nirodhaḥ dukkha saṅkhayaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_thag_7.5:7.1",
        "original": "Yasmiṁ nivattate dukkhaṁ,",
        "analyzed": "yasmin nivattate ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:7.2",
        "original": "saṁsārasmiṁ anantakaṁ;",
        "analyzed": "anantaka ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:7.3",
        "original": "Bhedā imassa kāyassa,",
        "analyzed": "bhedāḥ imassa kāyassa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:7.4",
        "original": "jīvitassa ca saṅkhayā;",
        "analyzed": "jīvitassa ca saṅkhayā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:7.5",
        "original": "Añño punabbhavo natthi,",
        "analyzed": "aññaḥ punabbhavaḥ na atthi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:7.6",
        "original": "suvimuttomhi sabbadhī”ti.",
        "analyzed": "su vimuttaḥ mhi sabbadhi iti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:7.7",
        "original": "… Sarabhaṅgo thero …",
        "analyzed": "sarabhaṅgaḥ theraḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:7.8",
        "original": "Sattakanipāto niṭṭhito.",
        "analyzed": "sattaka nipātaḥ niṭṭhito ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_7.5:8.0",
        "original": "Tatruddānaṁ",
        "analyzed": "tatra uddāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_7.5:8.1",
        "original": "Sundarasamuddo thero,",
        "analyzed": "sundarasamuddaḥ thero ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_7.5:8.2",
        "original": "thero lakuṇḍabhaddiyo;",
        "analyzed": "theraḥ lakuṇḍabhaddiyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_7.5:8.3",
        "original": "Bhaddo thero ca sopāko,",
        "analyzed": "bhaddo thero ca sopākaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_7.5:8.4",
        "original": "sarabhaṅgo mahāisi;",
        "analyzed": "sarabhaṅgaḥ mahā aisi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_7.5:8.5",
        "original": "Sattake pañcakā therā,",
        "analyzed": "sattake pañcakā therā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_thag_7.5:8.6",
        "original": "gāthāyo pañcatiṁsatīti.",
        "analyzed": "pañcatisati iti ",
        "folio": "8"
    }
