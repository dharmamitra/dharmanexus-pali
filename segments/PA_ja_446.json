        "analyzed": "catu dvāra vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_446:0.4",
        "original": "8. Takkalajātaka",
        "analyzed": "tak kala jātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_446:1.1",
        "original": "“Na takkalā santi na āluvāni,",
        "analyzed": "na takkalā santi na āluvāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_446:1.2",
        "original": "Na biḷāliyo na kaḷambāni tāta;",
        "analyzed": "na biḷāliyaḥ na kaḷambāni tāta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_446:1.3",
        "original": "Eko araññamhi susānamajjhe,",
        "analyzed": "ekaḥ araññamhi su sāna majjhe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_446:1.4",
        "original": "Kimatthiko tāta khaṇāsi kāsuṁ”.",
        "analyzed": "kimatthiko tāta khaṇāsi kāsu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_446:2.1",
        "original": "“Pitāmaho tāta sudubbalo te,",
        "analyzed": "pitāmahaḥ tāta su dubbalaḥ te ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_446:2.2",
        "original": "Anekabyādhīhi dukhena phuṭṭho;",
        "analyzed": "aneka byādhīhi dukhena phuṭṭhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_446:2.3",
        "original": "Tamajjahaṁ nikhaṇissāmi sobbhe,",
        "analyzed": "tam ajja aham nikhaṇissāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_446:2.4",
        "original": "Na hissa taṁ jīvitaṁ rocayāmi”.",
        "analyzed": "na hi sa tam jīvitam rocayāmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_446:3.1",
        "original": "“Saṅkappametaṁ paṭiladdha pāpakaṁ,",
        "analyzed": "saṅkappam etam paṭiladdha pāpaka ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_446:3.2",
        "original": "Accāhitaṁ kamma karosi luddaṁ;",
        "analyzed": "accāhitam kamma karosi ludda ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_446:3.3",
        "original": "Mayāpi tāta paṭilacchase tuvaṁ,",
        "analyzed": "mayā api tāta paṭilacchase ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_446:3.4",
        "original": "Etādisaṁ kamma jarūpanīto;",
        "analyzed": "etādisam kamma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_446:3.5",
        "original": "Taṁ kullavattaṁ anuvattamāno,",
        "analyzed": "tam kullavattam anuvattamānaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_446:3.6",
        "original": "Ahampi taṁ nikhaṇissāmi sobbhe”.",
        "analyzed": "aham pi tam nikhaṇissāmi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_446:4.1",
        "original": "“Pharusāhi vācāhi pakubbamāno,",
        "analyzed": "pharusāhi vācāhi pakubbamānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_446:4.2",
        "original": "Āsajja maṁ tvaṁ vadase kumāra;",
        "analyzed": "āsajja mam tvam vadase kumāra ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_446:4.3",
        "original": "Putto mamaṁ orasako samāno,",
        "analyzed": "puttaḥ mamam orasakaḥ samānaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_446:4.4",
        "original": "Ahitānukampī mama tvaṁsi putta”.",
        "analyzed": "ahita anukampī mama tu asi putta ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_446:5.1",
        "original": "“Na tāhaṁ tāta ahitānukampī,",
        "analyzed": "na tā aham tāta ahita anukampī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_446:5.2",
        "original": "Hitānukampī te ahampi tāta;",
        "analyzed": "hita anukampī te aham pi tāta ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_446:5.3",
        "original": "Pāpañca taṁ kamma pakubbamānaṁ,",
        "analyzed": "pāpam ca tat kamma pakubbamāna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_446:5.4",
        "original": "Arahāmi no vārayituṁ tato”.",
        "analyzed": "arahāmi naḥ vārayitum tatas ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_446:6.1",
        "original": "“Yo mātaraṁ vā pitaraṁ saviṭṭha,",
        "analyzed": "yaḥ mātaram vā pitaram saviṭṭha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_446:6.2",
        "original": "Adūsake hiṁsati pāpadhammo;",
        "analyzed": "a dūsake hisati pāpa dhammo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_446:6.3",
        "original": "Kāyassa bhedā abhisamparāyaṁ,",
        "analyzed": "kāyassa bhedāḥ abhisamparāya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_446:6.4",
        "original": "Asaṁsayaṁ so nirayaṁ upeti.",
        "analyzed": "asasayam saḥ nirayam upeti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_446:7.1",
        "original": "Yo mātaraṁ vā pitaraṁ saviṭṭha,",
        "analyzed": "yaḥ mātaram vā pitaram saviṭṭha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_446:7.2",
        "original": "Annena pānena upaṭṭhahāti;",
        "analyzed": "annena pānena upaṭṭhahāti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_446:7.3",
        "original": "Kāyassa bhedā abhisamparāyaṁ,",
        "analyzed": "kāyassa bhedāḥ abhisamparāya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_446:7.4",
        "original": "Asaṁsayaṁ so sugatiṁ upeti.",
        "analyzed": "a sasayam saḥ sugatim upeti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_446:8.1",
        "original": "Na me tvaṁ putta ahitānukampī,",
        "analyzed": "na me tvam putta ahita anukampī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_446:8.2",
        "original": "Hitānukampī me tvaṁsi putta;",
        "analyzed": "hita anukampī me tu asi putta ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_446:8.3",
        "original": "Ahañca taṁ mātarā vuccamāno,",
        "analyzed": "aham ca tam mātarā vuccamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_446:8.4",
        "original": "Etādisaṁ kamma karomi luddaṁ”.",
        "analyzed": "etādisam kamma karomi ludda ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_446:9.1",
        "original": "“Yā te sā bhariyā anariyarūpā,",
        "analyzed": "yā te sā bhariyā an ariya rūpā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_446:9.2",
        "original": "Mātā mamesā sakiyā janetti;",
        "analyzed": "mātā mama isā sakiyā janā itti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_446:9.3",
        "original": "Niddhāpaye tañca sakā agārā,",
        "analyzed": "niddhāpaye tat ca sakāḥ agārā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_446:9.4",
        "original": "Aññampi te sā dukhamāvaheyya.",
        "analyzed": "aññam pi te sā dukham āvaheyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_446:10.1",
        "original": "Yā te sā bhariyā anariyarūpā,",
        "analyzed": "yā te sā bhariyā an ariya rūpā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_446:10.2",
        "original": "Mātā mamesā sakiyā janetti;",
        "analyzed": "mātā mama isā sakiyā janā itti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_446:10.3",
        "original": "Dantā kareṇūva vasūpanītā,",
        "analyzed": "dantā kareṇū iva vasu upanītā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_446:10.4",
        "original": "Sā pāpadhammā punarāvajātū”ti.",
        "analyzed": "sā pāpa dhammā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_446:11.1",
        "original": "Takkalajātakaṁ aṭṭhamaṁ.",
        "analyzed": "takkala jātakam aṭṭhama ",
        "folio": "11"
    }
]