        "segmentnr": "PA_ja_454:14.4",
        "original": "tava sutvāna māṇava”.",
        "analyzed": "tava sutvāna māṇava ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_454:15.1",
        "original": "“Evaṁ karonti sappaññā,",
        "analyzed": "evam karonti sappaññā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_454:15.2",
        "original": "Ye honti anukampakā;",
        "analyzed": "ye honti anukampakā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_454:15.3",
        "original": "Nivattayanti sokamhā,",
        "analyzed": "nivattayanti sokamhā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_454:15.4",
        "original": "Ghaṭo jeṭṭhaṁva bhātaran”ti.",
        "analyzed": "ghaṭaḥ ",
        "folio": "15"
    },
