        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_433:9.1",
        "original": "Dhiratthu kāme subahūpi loke,",
        "analyzed": "kāme loke ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_433:9.2",
        "original": "Tapova seyyo kāmaguṇehi rāja;",
