        "analyzed": "kukkuḷa anugatā mahī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_421:1.3",
        "original": "Atha gāyasi vattāni,",
        "analyzed": "atha gāyasi vattāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_421:1.4",
        "original": "na taṁ tapati ātapo.",
        "analyzed": "na tam tapati ātapo ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_421:2.1",
        "original": "Uddhaṁ tapati ādicco,",
        "analyzed": "uddham tapati ādiccaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_421:2.2",
