[
    {
        "segmentnr": "PA_snp_4.5:0.1",
        "original": "Sutta Nipāta 4.5",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.5:0.2",
        "original": "Paramaṭṭhakasutta",
        "analyzed": "paramaṭṭhaka sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.5:1.1",
        "original": "Paramanti diṭṭhīsu paribbasāno,",
        "analyzed": "paramanti diṭṭhīsu paribbasānaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.5:1.2",
        "original": "Yaduttari kurute jantu loke;",
        "analyzed": "yat uttari kurute jantu loke ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.5:1.3",
        "original": "Hīnāti aññe tato sabbamāha,",
        "analyzed": "hīnā ati aññe tatas sabbam āha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.5:1.4",
        "original": "Tasmā vivādāni avītivatto.",
        "analyzed": "tasmai vivādāni a vītivatto ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.5:2.1",
        "original": "Yadattanī passati ānisaṁsaṁ,",
        "analyzed": "yadattanī passati ānisasa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.5:2.2",
        "original": "Diṭṭhe sute sīlavate mute vā;",
        "analyzed": "diṭṭhe sute sīlavate mute vā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.5:2.3",
        "original": "Tadeva so tattha samuggahāya,",
        "analyzed": "tat eva saḥ tattha samuggahāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.5:2.4",
        "original": "Nihīnato passati sabbamaññaṁ.",
        "analyzed": "nihīnataḥ passati sabbamañña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.5:3.1",
        "original": "Taṁ vāpi ganthaṁ kusalā vadanti,",
        "analyzed": "tam vā api gantham kusalāḥ vadanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.5:3.2",
        "original": "Yaṁ nissito passati hīnamaññaṁ;",
        "analyzed": "yam nissitaḥ passati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.5:3.3",
        "original": "Tasmā hi diṭṭhaṁ va sutaṁ mutaṁ vā,",
        "analyzed": "tasmā hi diṭṭham sutam mutam vā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.5:3.4",
        "original": "Sīlabbataṁ bhikkhu na nissayeyya.",
        "analyzed": "sīlabbatam bhikkhu na nissayeyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.5:4.1",
        "original": "Diṭṭhimpi lokasmiṁ na kappayeyya,",
        "analyzed": "diṭṭhim pi lokasmiṃ na kappayeyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.5:4.2",
        "original": "Ñāṇena vā sīlavatena vāpi;",
        "analyzed": "vā sīlavatena vā api ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.5:4.3",
        "original": "Samoti attānamanūpaneyya,",
        "analyzed": "samā iti attānam anūpaneyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.5:4.4",
        "original": "Hīno na maññetha visesi vāpi.",
        "analyzed": "hīnaḥ na maññetha visesi vā api ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.5:5.1",
        "original": "Attaṁ pahāya anupādiyāno,",
        "analyzed": "attam pahāya an upādiyānaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.5:5.2",
        "original": "Ñāṇepi so nissayaṁ no karoti;",
        "analyzed": "ñāṇe api saḥ nissayam no karoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.5:5.3",
        "original": "Sa ve viyattesu na vaggasārī,",
        "analyzed": "sa ve viyattesu na vagga sārī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.5:5.4",
        "original": "Diṭṭhimpi so na pacceti kiñci.",
        "analyzed": "diṭṭhim pi so na pacceti kiñci ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.5:6.1",
        "original": "Yassūbhayante paṇidhīdha natthi,",
        "analyzed": "yassu ubhaya ante na atthi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.5:6.2",
        "original": "Bhavābhavāya idha vā huraṁ vā;",
        "analyzed": "bhava abhavāya vā huram vā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.5:6.3",
        "original": "Nivesanā tassa na santi keci,",
        "analyzed": "nivesanā tassa na santi keci ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.5:6.4",
        "original": "Dhammesu niccheyya samuggahītaṁ.",
        "analyzed": "dhammesu ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.5:7.1",
        "original": "Tassīdha diṭṭhe va sute mute vā,",
        "analyzed": "tassa idha diṭṭhe va sute mute vā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.5:7.2",
        "original": "Pakappitā natthi aṇūpi saññā;",
        "analyzed": "pakappitā natthi aṇu upi saññā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.5:7.3",
        "original": "Taṁ brāhmaṇaṁ diṭṭhimanādiyānaṁ,",
        "analyzed": "tam brāhmaṇam diṭṭhim an ādiyāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.5:7.4",
        "original": "Kenīdha lokasmiṁ vikappayeyya.",
        "analyzed": "kena idha lokasmin vikappayeyya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.5:8.1",
        "original": "Na kappayanti na purekkharonti,",
        "analyzed": "na kappayanti na purekkharonti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.5:8.2",
        "original": "Dhammāpi tesaṁ na paṭicchitāse;",
        "analyzed": "dhammā api tesam na ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.5:8.3",
        "original": "Na brāhmaṇo sīlavatena neyyo,",
        "analyzed": "na brāhmaṇaḥ sīlavatena neyyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.5:8.4",
        "original": "Pāraṅgato na pacceti tādīti.",
        "analyzed": "pāraṅgataḥ na ta ādi iti ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.5:8.5",
        "original": "Paramaṭṭhakasuttaṁ pañcamaṁ.",
        "analyzed": "paramaṭṭhaka suttam pañcama ",
        "folio": "8"
    }
]