        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_433:2.1",
        "original": "“Sasamuddapariyāyaṁ,",
        "analyzed": "sa samudda pariyāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_433:2.2",
        "original": "mahiṁ sāgarakuṇḍalaṁ;",
        "analyzed": "mahim sāgara kuṇḍala ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_433:2.3",
        "original": "Na icche saha nindāya,",
        "analyzed": "na icche saha nindāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_433:2.4",
        "original": "evaṁ seyya vijānahi.",
        "analyzed": "evam seyya vijānahi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_433:3.1",
        "original": "Dhiratthu taṁ yasalābhaṁ,",
        "analyzed": "dhiratthu tam yasalābha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_433:3.2",
        "original": "dhanalābhañca brāhmaṇa;",
        "analyzed": "dhana lābham ca brāhmaṇa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_433:3.3",
        "original": "Yā vutti vinipātena,",
        "analyzed": "yā vutti vinipātena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_433:3.4",
        "original": "adhammacaraṇena vā.",
        "analyzed": "a dhamma caraṇena vā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_433:4.1",
        "original": "Api ce pattamādāya,",
        "analyzed": "api ced pattam ādāya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_433:4.2",
        "original": "anagāro paribbaje;",
        "analyzed": "anagāraḥ paribbaje ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_433:4.3",
        "original": "Sāyeva jīvikā seyyo,",
        "analyzed": "sāyā iva jīvikā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_433:4.4",
        "original": "yā cādhammena esanā.",
        "analyzed": "yā ca adhammena esanā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_433:5.1",
        "original": "Api ce pattamādāya,",
        "analyzed": "api ced pattam ādāya ",
        "folio": "5"
    },
    {
