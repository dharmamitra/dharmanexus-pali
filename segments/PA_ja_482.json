        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_482:10.2",
        "original": "Tanucchidaṁ hadaye ossajāmi;",
        "analyzed": "tanu chidam hadaye ossajāmi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_482:10.3",
        "original": "Hanāmi taṁ mittadubbhiṁ akiccakāriṁ,",
        "analyzed": "hanāmi tam mitta dubbhim ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_482:10.4",
        "original": "Yo tādisaṁ kammakataṁ na jāne”.",
        "analyzed": "yaḥ tādisam kamma katam na jāne ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_482:11.1",
        "original": "“Dhīrassa bālassa have janinda,",
        "analyzed": "dhīrassa bālassa have janinda ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_482:11.2",
