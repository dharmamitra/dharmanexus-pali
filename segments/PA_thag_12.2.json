        "analyzed": "daliddo appabhojano ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_thag_12.2:1.3",
        "original": "Hīnakammaṁ mamaṁ āsi,",
        "analyzed": "hīna kammam mamam āsi ",
        "folio": "1"
    },
