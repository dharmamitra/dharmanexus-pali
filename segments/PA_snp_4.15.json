[
    {
        "segmentnr": "PA_snp_4.15:0.1",
        "original": "Sutta Nipāta 4.15",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.15:0.2",
        "original": "Attadaṇḍasutta",
        "analyzed": "atta daṇḍa sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_4.15:1.1",
        "original": "“Attadaṇḍā bhayaṁ jātaṁ,",
        "analyzed": "atta daṇḍā bhayam jāta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.15:1.2",
        "original": "janaṁ passatha medhagaṁ;",
        "analyzed": "janam passatha medhaga ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.15:1.3",
        "original": "Saṁvegaṁ kittayissāmi,",
        "analyzed": "sa vegam kittayissāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.15:1.4",
        "original": "yathā saṁvijitaṁ mayā.",
        "analyzed": "yathā sa vijitam mayā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.15:2.1",
        "original": "Phandamānaṁ pajaṁ disvā,",
        "analyzed": "phandamānam pajam disvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.15:2.2",
        "original": "macche appodake yathā;",
        "analyzed": "macche appodake yathā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.15:2.3",
        "original": "Aññamaññehi byāruddhe,",
        "analyzed": "aññamaññehi byāruddhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.15:2.4",
        "original": "disvā maṁ bhayamāvisi.",
        "analyzed": "disvā mama bhayam āvisi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.15:3.1",
        "original": "Samantamasāro loko,",
        "analyzed": "samantam asāraḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.15:3.2",
        "original": "disā sabbā sameritā;",
        "analyzed": "disā sabbā sameritā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.15:3.3",
        "original": "Icchaṁ bhavanamattano,",
        "analyzed": "iccham bhavanamattano ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.15:3.4",
        "original": "nāddasāsiṁ anositaṁ.",
        "analyzed": "nāddasa asim anosita ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.15:4.1",
        "original": "Osāne tveva byāruddhe,",
        "analyzed": "osāne tu eva byāruddhe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.15:4.2",
        "original": "Disvā me aratī ahu;",
        "analyzed": "disvā me aratī ahu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.15:4.3",
        "original": "Athettha sallamaddakkhiṁ,",
        "analyzed": "atha itthā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.15:4.4",
        "original": "Duddasaṁ hadayanissitaṁ.",
        "analyzed": "duddasam hadaya nissita ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.15:5.1",
        "original": "Yena sallena otiṇṇo,",
        "analyzed": "yena sallena otiṇṇaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.15:5.2",
        "original": "disā sabbā vidhāvati;",
        "analyzed": "disā sabbā vidhāvati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.15:5.3",
        "original": "Tameva sallamabbuyha,",
        "analyzed": "tam eva sallam abbuyha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.15:5.4",
        "original": "na dhāvati na sīdati.",
        "analyzed": "na dhāvati na sīdati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.15:6.1",
        "original": "Tattha sikkhānugīyanti,",
        "analyzed": "tattha sikkhā anugīyanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.15:6.2",
        "original": "yāni loke gadhitāni;",
        "analyzed": "yāni loke gadhitāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.15:6.3",
        "original": "Na tesu pasuto siyā,",
        "analyzed": "na tesu pasutaḥ siyā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.15:6.4",
        "original": "nibbijjha sabbaso kāme;",
        "analyzed": "nibbijjha sabbasas kāme ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.15:6.5",
        "original": "Sikkhe nibbānamattano.",
        "analyzed": "sikkhe nibbāna mattano ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.15:7.1",
        "original": "Sacco siyā appagabbho,",
        "analyzed": "saccaḥ siyā appagabbhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.15:7.2",
        "original": "amāyo rittapesuṇo;",
        "analyzed": "a māyo ritta pesuṇaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.15:7.3",
        "original": "Akkodhano lobhapāpaṁ,",
        "analyzed": "akkodhano lobha pāpa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.15:7.4",
        "original": "vevicchaṁ vitare muni.",
        "analyzed": "veviccham vitare muni ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.15:8.1",
        "original": "Niddaṁ tandiṁ sahe thīnaṁ,",
        "analyzed": "niddam tandim sahe thīna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.15:8.2",
        "original": "pamādena na saṁvase;",
        "analyzed": "pamādena na savase ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.15:8.3",
        "original": "Atimāne na tiṭṭheyya,",
        "analyzed": "atimāne na tiṭṭheyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.15:8.4",
        "original": "nibbānamanaso naro.",
        "analyzed": "nibbāna manasaḥ naraḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.15:9.1",
        "original": "Mosavajje na nīyetha,",
        "analyzed": "mosavajje na nīyetha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.15:9.2",
        "original": "rūpe snehaṁ na kubbaye;",
        "analyzed": "rūpe sneham na kubbaye ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.15:9.3",
        "original": "Mānañca parijāneyya,",
        "analyzed": "mānañca parijāneyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.15:9.4",
        "original": "sāhasā virato care.",
        "analyzed": "sāhasā virataḥ care ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.15:10.1",
        "original": "Purāṇaṁ nābhinandeyya,",
        "analyzed": "purāṇam na abhinandeyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.15:10.2",
        "original": "Nave khantiṁ na kubbaye;",
        "analyzed": "nave khantim na kubbaye ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.15:10.3",
        "original": "Hiyyamāne na soceyya,",
        "analyzed": "hiyyamāne na soceyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.15:10.4",
        "original": "Ākāsaṁ na sito siyā.",
        "analyzed": "ākāsam na sitaḥ siyā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.15:11.1",
        "original": "Gedhaṁ brūmi mahoghoti,",
        "analyzed": "gedham brūmi mahogha iti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.15:11.2",
        "original": "Ājavaṁ brūmi jappanaṁ;",
        "analyzed": "ājavam brūmi jappana ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.15:11.3",
        "original": "Ārammaṇaṁ pakappanaṁ,",
        "analyzed": "ārammaṇam ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.15:11.4",
        "original": "Kāmapaṅko duraccayo.",
        "analyzed": "kāma paṅkaḥ duraccayaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_4.15:12.1",
        "original": "Saccā avokkamma muni,",
        "analyzed": "saccā avokkamma muni ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.15:12.2",
        "original": "Thale tiṭṭhati brāhmaṇo;",
        "analyzed": "thale tiṭṭhati brāhmaṇaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.15:12.3",
        "original": "Sabbaṁ so paṭinissajja,",
        "analyzed": "sabbam so paṭinissajja ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.15:12.4",
        "original": "Sa ve santoti vuccati.",
        "analyzed": "sa ve santo ti vuccati ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_4.15:13.1",
        "original": "Sa ve vidvā sa vedagū,",
        "analyzed": "sa ve vidvā sa vedagū ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.15:13.2",
        "original": "Ñatvā dhammaṁ anissito;",
        "analyzed": "ñatvā dhammaṃ anissitaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.15:13.3",
        "original": "Sammā so loke iriyāno,",
        "analyzed": "sammā saḥ loke iriyānaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.15:13.4",
        "original": "Na pihetīdha kassaci.",
        "analyzed": "na piheti idha kassaci ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_4.15:14.1",
        "original": "Yodha kāme accatari,",
        "analyzed": "yodha kāme accatari ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.15:14.2",
        "original": "Saṅgaṁ loke duraccayaṁ;",
        "analyzed": "saṅgam loke duraccaya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.15:14.3",
        "original": "Na so socati nājjheti,",
        "analyzed": "na saḥ socati na ajjheti ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.15:14.4",
        "original": "Chinnasoto abandhano.",
        "analyzed": "chinna sotaḥ a bandhanaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_4.15:15.1",
        "original": "Yaṁ pubbe taṁ visosehi,",
        "analyzed": "yam pubbe tam visosehi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.15:15.2",
        "original": "Pacchā te māhu kiñcanaṁ;",
        "analyzed": "pacchā te mā āhuḥ kiñcana ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.15:15.3",
        "original": "Majjhe ce no gahessasi,",
        "analyzed": "majjhe ce naḥ gahessasi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.15:15.4",
        "original": "Upasanto carissasi.",
        "analyzed": "upasantaḥ carissasi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_4.15:16.1",
        "original": "Sabbaso nāmarūpasmiṁ,",
        "analyzed": "sabbasaḥ nāma rūpasmi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.15:16.2",
        "original": "Yassa natthi mamāyitaṁ;",
        "analyzed": "yassa na atthi mamāyita ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.15:16.3",
        "original": "Asatā ca na socati,",
        "analyzed": "asatā ca na socati ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.15:16.4",
        "original": "Sa ve loke na jīyati.",
        "analyzed": "sa ve loke na jīyati ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_4.15:17.1",
        "original": "Yassa natthi idaṁ meti,",
        "analyzed": "yassa na atthi idam mā iti ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.15:17.2",
        "original": "Paresaṁ vāpi kiñcanaṁ;",
        "analyzed": "paresam vā api kiñcana ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.15:17.3",
        "original": "Mamattaṁ so asaṁvindaṁ,",
        "analyzed": "mamattam saḥ asavinda ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.15:17.4",
        "original": "Natthi meti na socati.",
        "analyzed": "natthi me iti na socati ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_4.15:18.1",
        "original": "Aniṭṭhurī ananugiddho,",
        "analyzed": "an iṭṭhurī an anugiddhaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.15:18.2",
        "original": "Anejo sabbadhī samo;",
        "analyzed": "an e jaḥ sap badhī samaḥ ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.15:18.3",
        "original": "Tamānisaṁsaṁ pabrūmi,",
        "analyzed": "tam ānisasam pabrūmi ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.15:18.4",
        "original": "Pucchito avikampinaṁ.",
        "analyzed": "pucchitaḥ avikampina ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_snp_4.15:19.1",
        "original": "Anejassa vijānato,",
        "analyzed": "anejassa vijānataḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.15:19.2",
        "original": "Natthi kāci nisaṅkhati;",
        "analyzed": "natthi kāci nisaṅkhati ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.15:19.3",
        "original": "Virato so viyārabbhā,",
        "analyzed": "virataḥ saḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.15:19.4",
        "original": "Khemaṁ passati sabbadhi.",
        "analyzed": "khemam passati sabbadhi ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_snp_4.15:20.1",
        "original": "Na samesu na omesu,",
        "analyzed": "na samesu na omesu ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.15:20.2",
        "original": "Na ussesu vadate muni;",
        "analyzed": "na ussesu vadate muni ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.15:20.3",
        "original": "Santo so vītamaccharo,",
        "analyzed": "santaḥ saḥ ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.15:20.4",
        "original": "Nādeti na nirassatī”ti.",
        "analyzed": "na adeti na nirassati iti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_snp_4.15:20.5",
        "original": "Attadaṇḍasuttaṁ pannarasamaṁ.",
        "analyzed": "attadaṇḍasuttam pannarasama ",
        "folio": "20"
    }
]