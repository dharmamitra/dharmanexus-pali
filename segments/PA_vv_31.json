        "analyzed": "itthi vimāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_31:0.3",
        "original": "Pāricchattakavagga",
        "analyzed": "pāricchattaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_31:0.4",
        "original": "3. Pallaṅkavimānavatthu",
        "analyzed": "pallaṅka vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_31:1.1",
        "original": "“Pallaṅkaseṭṭhe maṇisoṇṇacitte,",
        "analyzed": "           ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_31:1.2",
        "original": "Pupphābhikiṇṇe sayane uḷāre;",
        "analyzed": "puppha abhikiṇṇe sayane uḷāre ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_31:1.3",
        "original": "Tatthacchasi devi mahānubhāve,",
        "analyzed": "devi mahā anubhāve ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_31:1.4",
        "original": "Uccāvacā iddhi vikubbamānā.",
        "analyzed": "uccāvacā id hi vikubbamānā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_31:2.1",
        "original": "Imā ca te accharāyo samantato,",
        "analyzed": "imā ca te accharāyaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_31:2.2",
        "original": "Naccanti gāyanti pamodayanti;",
        "analyzed": "naccanti gāyanti pamodayanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_31:2.3",
        "original": "Deviddhipattāsi mahānubhāve,",
        "analyzed": "devi iddhipattā asi mahā anubhāve ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_31:2.4",
        "original": "Manussabhūtā kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtā kim akāsi puñña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_31:2.5",
        "original": "Kenāsi evaṁ jalitānubhāvā,",
        "analyzed": "kena asi evam jalita anubhāvā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_31:2.6",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_31:3.1",
        "original": "“Ahaṁ manussesu manussabhūtā,",
        "analyzed": "aham manussesu manussa bhūtā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_31:3.2",
        "original": "Aḍḍhe kule suṇisā ahosiṁ;",
        "analyzed": "aḍḍhe kule suṇisā ahosi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_31:3.3",
        "original": "Akkodhanā bhattuvasānuvattinī,",
        "analyzed": "akkodhanā bhattuvasā anuvattinī ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_31:3.4",
        "original": "Uposathe appamattā ahosiṁ.",
        "analyzed": "uposathe appamattā ahosi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_31:4.1",
        "original": "Manussabhūtā daharā apāpikā,",
        "analyzed": "manus sabhūtā daharā a pāpikā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_31:4.2",
        "original": "Pasannacittā patimābhirādhayiṁ;",
        "analyzed": "pasanna cittā patim ā abhirādhayi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_31:4.3",
        "original": "Divā ca ratto ca manāpacārinī,",
        "analyzed": "divā ca ca manāpacārinī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_31:4.4",
        "original": "Ahaṁ pure sīlavatī ahosiṁ.",
        "analyzed": "aham pure sīlavatī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_31:5.1",
        "original": "Pāṇātipātā viratā acorikā,",
        "analyzed": "pāṇa atipātā viratā a corikā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_31:5.2",
        "original": "Saṁsuddhakāyā sucibrahmacārinī;",
        "analyzed": "sa suddha kāyā suci brahmacārinī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_31:5.3",
        "original": "Amajjapā no ca musā abhāṇiṁ,",
        "analyzed": "a majja pā na u ca musā abhāṇi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_31:5.4",
        "original": "Sikkhāpadesu paripūrakārinī.",
        "analyzed": "sikkhāpadesu paripūra kārinī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_31:6.1",
        "original": "Cātuddasiṁ pañcadasiṁ,",
        "analyzed": "cātuddasim pañcadasi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_31:6.2",
        "original": "yā ca pakkhassa aṭṭhamī;",
        "analyzed": "yā ca pakkhassa aṭṭhamī ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_31:6.3",
        "original": "Pāṭihāriyapakkhañca,",
        "analyzed": "pāṭihāriya pakkhañca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_31:6.4",
        "original": "pasannamānasā ahaṁ.",
        "analyzed": "pasanna mānasāḥ aha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_31:7.1",
        "original": "Aṭṭhaṅgupetaṁ anudhammacārinī,",
        "analyzed": "aṭṭhaṅga upetam anudhammacārinī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_31:7.2",
        "original": "Uposathaṁ pītimanā upāvasiṁ;",
        "analyzed": "uposatham pīti manāḥ upāvasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_31:7.3",
        "original": "Imañca ariyaṁ aṭṭhaṅgavarehupetaṁ,",
        "analyzed": "imam ca ariyam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_31:7.4",
        "original": "Samādiyitvā kusalaṁ sukhudrayaṁ;",
        "analyzed": "samādiyitvā kusalam su khudraya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_31:7.5",
        "original": "Patimhi kalyāṇī vasānuvattinī,",
        "analyzed": "patimhi kalyāṇī vasā anuvattinī ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_31:7.6",
        "original": "Ahosiṁ pubbe sugatassa sāvikā.",
        "analyzed": "ahosim pubbe sugatassa sāvikā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_31:8.1",
        "original": "Etādisaṁ kusalaṁ jīvaloke,",
        "analyzed": "etādisam kusalam jīva loke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_31:8.2",
        "original": "Kammaṁ karitvāna visesabhāginī;",
        "analyzed": "kammam visesa bhāginī ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_31:8.3",
        "original": "Kāyassa bhedā abhisamparāyaṁ,",
        "analyzed": "kāyassa bhedāḥ abhisamparāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_31:8.4",
        "original": "Deviddhipattā sugatimhi āgatā.",
        "analyzed": "deva iddhi pattā sugatimhi āgatā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_31:9.1",
        "original": "Vimānapāsādavare manorame,",
        "analyzed": "vimāna pāsāda vare manorame ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_31:9.2",
        "original": "Parivāritā accharāsaṅgaṇena;",
        "analyzed": "parivāritāḥ accha rāsaṅgaṇena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_31:9.3",
        "original": "Sayaṁpabhā devagaṇā ramenti maṁ,",
        "analyzed": "deva gaṇāḥ ramenti ma ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_31:9.4",
        "original": "Dīghāyukiṁ devavimānamāgatan”ti.",
        "analyzed": "dīghāyukim deva vimānam āgatanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_31:10.1",
        "original": "Pallaṅkavimānaṁ tatiyaṁ.",
        "analyzed": "pallaṅka vimānam tatiya ",
        "folio": "10"
    }
]