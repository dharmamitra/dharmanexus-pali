        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.4:2.3",
        "original": "Dummano tattha aṭṭhāsiṁ,",
        "analyzed": "dummano tattha aṭṭhāsi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_thag_10.4:2.4",
