        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.2",
        "original": "Tena kho pana samayena bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpenti.",
        "analyzed": "tena kho pana samayena bhikkhuniyaḥ ekam vassam dve vuṭṭhāpenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.3",
        "original": "Upassayo tatheva na sammati.",
        "analyzed": "upassayo tathā iva na sammati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.4",
        "original": "Manussā tatheva ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "manussā tathā iva ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.5",
        "original": "“kathañhi nāma bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpessanti.",
        "analyzed": "katham hi nāma bhikkhuniyaḥ ekam vassam dve vuṭṭhāpessanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.6",
        "original": "Upassayo tatheva na sammatī”ti.",
        "analyzed": "upassayo tathā iva na ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.7",
        "original": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
        "analyzed": "assosum kho bhikkhuniyaḥ tesam manussānam ujjhāyantānam khiyyantānam vipācentāna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.8",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.9",
        "original": "“kathañhi nāma bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpessantī”ti …pe…",
        "analyzed": "katham hi nāma bhikkhuniyaḥ ekam vassam dve vuṭṭhāpessanti iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.10",
        "original": "“saccaṁ kira, bhikkhave, bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpentī”ti?",
        "analyzed": "saccam kira bhikkhave bhikkhuniyaḥ ekam vassam dve vuṭṭhāpenti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.11",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.12",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.13",
        "original": "kathañhi nāma, bhikkhave, bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpessanti.",
        "analyzed": "katham hi nāma bhikkhave bhikkhuniyaḥ ekam vassam dve vuṭṭhāpessanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.14",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
        "analyzed": "na ita bhikkhave appasannānam vā pasādāya pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc83:1.15",
        "original": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
        "analyzed": "evam ca pana bhikkhave bhikkhuniyaḥ imam sikkhāpadam uddisantu ",
        "folio": "1"
    },
    {
