    },
    {
        "segmentnr": "PA_ja_428:1.1",
        "original": "“Puthusaddo samajano,",
        "analyzed": "puthu saddaḥ samajanaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_428:1.2",
        "original": "na bālo koci maññatha;",
        "analyzed": "na bālo koci maññatha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_428:1.3",
        "original": "Saṅghasmiṁ bhijjamānasmiṁ,",
        "analyzed": "saṅghasmin bhijjamānasmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_428:1.4",
        "original": "nāññaṁ bhiyyo amaññaruṁ.",
        "analyzed": "na aññam bhiyyas amaññaru ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_428:2.1",
        "original": "Parimuṭṭhā paṇḍitābhāsā,",
        "analyzed": "parimuṭṭhā paṇḍita ābhāsā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_428:2.2",
        "original": "vācāgocarabhāṇino;",
        "analyzed": "vācā gocara bhāṇino ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_428:2.3",
        "original": "Yāvicchanti mukhāyāmaṁ,",
        "analyzed": "yau icchanti mukhāyāma ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_428:2.4",
        "original": "yena nītā na taṁ vidū.",
        "analyzed": "yena nītāḥ na tam vidū ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_428:3.1",
        "original": "Akkocchi maṁ avadhi maṁ,",
        "analyzed": "akkocchi maṃ avadhi ma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_428:3.2",
        "original": "ajini maṁ ahāsi me;",
        "analyzed": "ajini mam ahā asi me ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_428:3.3",
        "original": "Ye ca taṁ upanayhanti,",
        "analyzed": "ye ca tam upanayhanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_428:3.4",
        "original": "veraṁ tesaṁ na sammati.",
        "analyzed": "veram tesam na sammati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_428:4.1",
        "original": "Akkocchi maṁ avadhi maṁ,",
        "analyzed": "akkocchi maṃ avadhi ma ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_428:4.2",
        "original": "ajini maṁ ahāsi me;",
        "analyzed": "ajini mam ahā asi me ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_428:4.3",
        "original": "Ye ca taṁ nupanayhanti,",
        "analyzed": "ye ca tam na upanayhanti ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_428:4.4",
        "original": "veraṁ tesūpasammati.",
        "analyzed": "veram ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_428:5.1",
        "original": "Na hi verena verāni,",
        "analyzed": "na hi verena verāni ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_428:5.2",
        "original": "sammantīdha kudācanaṁ;",
        "analyzed": "kudācana ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_428:5.3",
        "original": "Averena ca sammanti,",
        "analyzed": "averena ca sammanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_428:5.4",
        "original": "esa dhammo sanantano.",
        "analyzed": "esa dhammo sanantanaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_428:6.1",
        "original": "Pare ca na vijānanti,",
        "analyzed": "pare ca na vijānanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_428:6.2",
        "original": "mayamettha yamāmase;",
        "analyzed": "mayamā itthā yamāmase ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_428:6.3",
        "original": "Ye ca tattha vijānanti,",
        "analyzed": "ye ca tattha vijānanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_428:6.4",
        "original": "tato sammanti medhagā.",
        "analyzed": "tatas medha gā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_428:7.1",
        "original": "Aṭṭhicchinnā pāṇaharā,",
        "analyzed": "aṭṭhicchinnā pāṇa harā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_428:7.2",
        "original": "gavāssa dhanahārino;",
        "analyzed": "gavāssa dhana hārinaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_428:7.3",
        "original": "Raṭṭhaṁ vilumpamānānaṁ,",
        "analyzed": "raṭṭham vilumpamānāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_428:7.4",
        "original": "tesampi hoti saṅgati;",
        "analyzed": "tesam pi hoti saṅgati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_428:7.5",
        "original": "Kasmā tumhāka no siyā.",
        "analyzed": "kasmā tumhāka no siyā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_428:8.1",
        "original": "Sace labhetha nipakaṁ sahāyaṁ,",
        "analyzed": "sace labhetha nipakam sahāya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_428:8.2",
        "original": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
        "analyzed": "saddhim caram sādhu vihāri dhīra ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_428:8.3",
        "original": "Abhibhuyya sabbāni parissayāni,",
        "analyzed": "abhibhuyya sabbāni parissayāni ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_428:8.4",
        "original": "Careyya tenattamano satīmā.",
        "analyzed": "careyya satī imā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_428:9.1",
        "original": "No ce labhetha nipakaṁ sahāyaṁ,",
        "analyzed": "na u ce labhetha nipakam sahāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_428:9.2",
        "original": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
        "analyzed": "saddhim caram sādhu vihāri dhīra ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_428:9.3",
        "original": "Rājāva raṭṭhaṁ vijitaṁ pahāya,",
        "analyzed": "rājā ava raṭṭham vijitam pahāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_428:9.4",
        "original": "Eko care mātaṅgaraññeva nāgo.",
        "analyzed": "ekaḥ care mātaṅga raññā iva nāgaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_428:10.1",
        "original": "Ekassa caritaṁ seyyo,",
        "analyzed": "ekassa caritam seyyaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_428:10.2",
        "original": "Natthi bāle sahāyatā;",
        "analyzed": "na atthi bāle sahāya tā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_428:10.3",
        "original": "Eko care na pāpāni kayirā,",
        "analyzed": "ekaḥ care na pāpāni kayirā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_428:10.4",
        "original": "Appossukko mātaṅgaraññeva nāgo”ti.",
        "analyzed": "appa ussukkaḥ mātaṅga raññā iva nāgaḥ ti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_428:11.1",
        "original": "Kosambiyajātakaṁ dutiyaṁ.",
        "analyzed": "kosambiya jātakam ",
        "folio": "11"
    }
]