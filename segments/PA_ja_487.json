        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_487:1.3",
        "original": "Kaccinnu te mānusake payoge,",
        "analyzed": "kaccit nu te mānusake payoge ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_487:1.4",
