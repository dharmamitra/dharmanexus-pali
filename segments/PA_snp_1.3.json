        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.3:28.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "28"
    },
    {
        "segmentnr": "PA_snp_1.3:29.1",
        "original": "“Okkhittacakkhu na ca pādalolo,",
        "analyzed": "na ca pāda lolo ",
        "folio": "29"
    },
    {
        "segmentnr": "PA_snp_1.3:29.2",
        "original": "Guttindriyo rakkhitamānasāno;",
        "analyzed": "guttindriyaḥ ",
        "folio": "29"
    },
    {
