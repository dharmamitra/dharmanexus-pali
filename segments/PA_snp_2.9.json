[
    {
        "segmentnr": "PA_snp_2.9:0.1",
        "original": "Sutta Nipāta 2.9",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.9:0.2",
        "original": "Kiṁsīlasutta",
        "analyzed": "kisīlasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.9:1.1",
        "original": "“Kiṁsīlo kiṁsamācāro,",
        "analyzed": "kisīlaḥ kisamācāraḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.9:1.2",
        "original": "kāni kammāni brūhayaṁ;",
        "analyzed": "kāni kammāni brūhaya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.9:1.3",
        "original": "Naro sammā niviṭṭhassa,",
        "analyzed": "naraḥ sammā niviṭṭhassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.9:1.4",
        "original": "uttamatthañca pāpuṇe”.",
        "analyzed": "uttamattham ca pāpuṇe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.9:2.1",
        "original": "“Vuḍḍhāpacāyī anusūyako siyā,",
        "analyzed": "vuḍḍha apacāyī anusūyakaḥ siyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.9:2.2",
        "original": "Kālaññū cassa garūnaṁ dassanāya;",
        "analyzed": "kālaññū ca ssa garūnam dassanāya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.9:2.3",
        "original": "Dhammiṁ kathaṁ erayitaṁ khaṇaññū,",
        "analyzed": "dhammim katham erayitam khaṇaññū ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.9:2.4",
        "original": "Suṇeyya sakkacca subhāsitāni.",
        "analyzed": "suṇeyya sakkacca su bhāsitāni ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.9:3.1",
        "original": "Kālena gacche garūnaṁ sakāsaṁ,",
        "analyzed": "kālena gacche garūnam sakāsa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.9:3.2",
        "original": "Thambhaṁ niraṅkatvā nivātavutti;",
        "analyzed": "thambham ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.9:3.3",
        "original": "Atthaṁ dhammaṁ saṁyamaṁ brahmacariyaṁ,",
        "analyzed": "attham dhammam sayamam brahmacariya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.9:3.4",
        "original": "Anussare ceva samācare ca.",
        "analyzed": "anussare ca iva samācare ca ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.9:4.1",
        "original": "Dhammārāmo dhammarato,",
        "analyzed": "dhamma ārāmaḥ dhamma rataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.9:4.2",
        "original": "Dhamme ṭhito dhammavinicchayaññū;",
        "analyzed": "dhamme ṭhitaḥ dhamma vinicchayaññū ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.9:4.3",
        "original": "Nevācare dhammasandosavādaṁ,",
        "analyzed": "dhamma sandosa vāda ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.9:4.4",
        "original": "Tacchehi nīyetha subhāsitehi.",
        "analyzed": "tacchehi nīyetha su bhāsitehi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.9:5.1",
        "original": "Hassaṁ jappaṁ paridevaṁ padosaṁ,",
        "analyzed": "hassam jappam paridevam padosa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.9:5.2",
        "original": "Māyākataṁ kuhanaṁ giddhi mānaṁ;",
        "analyzed": "māyā katam kuhanam giddhi māna ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.9:5.3",
        "original": "Sārambhaṁ kakkasaṁ kasāvañca mucchaṁ,",
        "analyzed": "sa ārambham kakkasam kasāvañca muccha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.9:5.4",
        "original": "Hitvā care vītamado ṭhitatto.",
        "analyzed": "hitvā care vīta madaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.9:6.1",
        "original": "Viññātasārāni subhāsitāni,",
        "analyzed": "viññāta sārāni su bhāsitāni ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.9:6.2",
        "original": "Sutañca viññātasamādhisāraṁ;",
        "analyzed": "sutañca viññāta samādhi sāra ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.9:6.3",
        "original": "Na tassa paññā ca sutañca vaḍḍhati,",
        "analyzed": "na tassa paññā ca sutañca vaḍḍhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.9:6.4",
        "original": "Yo sāhaso hoti naro pamatto.",
        "analyzed": "yaḥ sāhasaḥ hoti naraḥ pamattaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.9:7.1",
        "original": "Dhamme ca ye ariyapavedite ratā,",
        "analyzed": "dhamme ca ye ariya pavedite ratā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.9:7.2",
        "original": "Anuttarā te vacasā manasā kammunā ca;",
        "analyzed": "anuttarā te vacasā manasā kammunā ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.9:7.3",
        "original": "Te santisoraccasamādhisaṇṭhitā,",
        "analyzed": "te ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.9:7.4",
        "original": "Sutassa paññāya ca sāramajjhagū”ti.",
        "analyzed": "sutassa paññāya ca sāram ajjhagūti ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.9:7.5",
        "original": "Kiṁsīlasuttaṁ navamaṁ.",
        "analyzed": "kisīla suttam navama ",
        "folio": "7"
    }
]