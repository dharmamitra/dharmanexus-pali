[
    {
        "segmentnr": "PA_vv_32:0.1",
        "original": "Vimānavatthu",
        "analyzed": "vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_32:0.2",
        "original": "Itthivimāna",
        "analyzed": "itthi vimāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_32:0.3",
        "original": "Pāricchattakavagga",
        "analyzed": "pāricchattaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_32:0.4",
        "original": "4. Latāvimānavatthu",
        "analyzed": "latā vimāna vatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_vv_32:1.1",
        "original": "Latā ca sajjā pavarā ca devatā,",
        "analyzed": "latā ca sajjā pavarā ca devatā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_32:1.2",
        "original": "Accimatī rājavarassa sirīmato;",
        "analyzed": "accimatī rāja varassa sirīmataḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_32:1.3",
        "original": "Sutā ca rañño vessavaṇassa dhītā,",
        "analyzed": "sutā ca raññaḥ vessavaṇassa dhītā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_32:1.4",
        "original": "Rājīmatī dhammaguṇehi sobhatha.",
        "analyzed": "rājīmatī dhamma guṇehi sobhatha ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_32:2.1",
        "original": "Pañcettha nāriyo āgamaṁsu nhāyituṁ,",
        "analyzed": "pañca itthā nāriyaḥ āgamasu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_32:2.2",
        "original": "Sītodakaṁ uppaliniṁ sivaṁ nadiṁ;",
        "analyzed": "sītā udakam uppalinim sivam nadi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_32:2.3",
        "original": "Tā tattha nhāyitvā rametvā devatā,",
        "analyzed": "tā tattha nhāyitvā rametvā devatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_32:2.4",
        "original": "Naccitvā gāyitvā sutā lataṁ bravi.",
        "analyzed": "naccitvā gāyitvā sutāḥ latam bravi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_32:3.1",
        "original": "“Pucchāmi taṁ uppalamāladhārini,",
        "analyzed": "pucchāmi tam uppala māla dhārini ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_32:3.2",
        "original": "Āveḷini kañcanasannibhattace;",
        "analyzed": "āveḷini kañcana sannibhattace ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_32:3.3",
        "original": "Timiratambakkhi nabheva sobhane,",
        "analyzed": "nabhā iva sobhane ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_32:3.4",
        "original": "Dīghāyukī kena kato yaso tava.",
        "analyzed": "dīghāyukī kena kataḥ yaso tava ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_32:4.1",
        "original": "Kenāsi bhadde patino piyatarā,",
        "analyzed": "kena asi bhadde patinaḥ piyatarā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_32:4.2",
        "original": "Visiṭṭhakalyāṇitarassu rūpato;",
        "analyzed": "rūpataḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_32:4.3",
        "original": "Padakkhiṇā naccanagītavādite,",
        "analyzed": "padakkhiṇā naccana gīta vādite ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_32:4.4",
        "original": "Ācikkha no tvaṁ naranāripucchitā”ti.",
        "analyzed": "ācikkha no tvam nara nāri pucchitā ati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_32:5.1",
        "original": "“Ahaṁ manussesu manussabhūtā,",
        "analyzed": "aham manussesu manussa bhūtā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_32:5.2",
        "original": "Uḷārabhoge kule suṇisā ahosiṁ;",
        "analyzed": "uḷāra bhoge kule suṇisā ahosi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_32:5.3",
        "original": "Akkodhanā bhattuvasānuvattinī,",
        "analyzed": "akkodhanā bhattuvasā anuvattinī ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_32:5.4",
        "original": "Uposathe appamattā ahosiṁ.",
        "analyzed": "uposathe appamattā ahosi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_32:6.1",
        "original": "Manussabhūtā daharā apāpikā,",
        "analyzed": "manus sabhūtā daharā a pāpikā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_32:6.2",
        "original": "Pasannacittā patimābhirādhayiṁ;",
        "analyzed": "pasanna cittā patim ā abhirādhayi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_32:6.3",
        "original": "Sadevaraṁ sassasuraṁ sadāsakaṁ,",
        "analyzed": "sa devaram sa ssasuram sa dāsaka ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_32:6.4",
        "original": "Abhirādhayiṁ tamhi kato yaso mama.",
        "analyzed": "abhirādhayim kataḥ yasaḥ mama ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_vv_32:7.1",
        "original": "Sāhaṁ tena kusalena kammunā,",
        "analyzed": "sā aham tena kusalena kammunā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_32:7.2",
        "original": "Catubbhi ṭhānehi visesamajjhagā;",
        "analyzed": "catubbhi ṭhānehi visesa majjhagā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_32:7.3",
        "original": "Āyuñca vaṇṇañca sukhaṁ balañca,",
        "analyzed": "āyuṃ ca vaṇṇañca sukham balañca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_32:7.4",
        "original": "Khiḍḍāratiṁ paccanubhomanappakaṁ”.",
        "analyzed": "khiḍḍa aratim ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_vv_32:8.1",
        "original": "“Sutaṁ nu taṁ bhāsati yaṁ ayaṁ latā,",
        "analyzed": "sutam nu tam bhāsati yam ayam latā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_32:8.2",
        "original": "Yaṁ no apucchimha akittayī no;",
        "analyzed": "yam naḥ apucchimha akittayī naḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_32:8.3",
        "original": "Patino kiramhākaṁ visiṭṭha nārīnaṁ,",
        "analyzed": "patinaḥ kiramhākam visiṭṭha nārīna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_32:8.4",
        "original": "Gatī ca tāsaṁ pavarā ca devatā.",
        "analyzed": "gatī ca tāsam pavarā ca devatā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_vv_32:9.1",
        "original": "Patīsu dhammaṁ pacarāma sabbā,",
        "analyzed": "patīsu dhammam pacarāma sabbā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_32:9.2",
        "original": "Patibbatā yattha bhavanti itthiyo;",
        "analyzed": "patibbatā yattha bhavanti itthiyaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_32:9.3",
        "original": "Patīsu dhammaṁ pacaritva sabbā,",
        "analyzed": "patīsu dhammam pacaritvā sabbā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_32:9.4",
        "original": "Lacchāmase bhāsati yaṁ ayaṁ latā.",
        "analyzed": "lacchāmase bhāsati yam ayam latā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_vv_32:10.1",
        "original": "Sīho yathā pabbatasānugocaro,",
        "analyzed": "sīhaḥ yathā pabbata sānu gocaraḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_32:10.2",
        "original": "Mahindharaṁ pabbatamāvasitvā;",
        "analyzed": "mahindharam pabbatam āvasitvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_32:10.3",
        "original": "Pasayha hantvā itare catuppade,",
        "analyzed": "pasayha hantvā itare catuppade ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_32:10.4",
        "original": "Khudde mige khādati maṁsabhojano.",
        "analyzed": "khudde mige khādati masa bhojanaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_vv_32:11.1",
        "original": "Tatheva saddhā idha ariyasāvikā,",
        "analyzed": "tathā iva saddhā idha ariya sāvikā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_32:11.2",
        "original": "Bhattāraṁ nissāya patiṁ anubbatā;",
        "analyzed": "bhattāram nissāya patim ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_32:11.3",
        "original": "Kodhaṁ vadhitvā abhibhuyya maccharaṁ,",
        "analyzed": "kodham vadhitvā abhibhuyya mat śara ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_32:11.4",
        "original": "Saggamhi sā modati dhammacārinī”ti.",
        "analyzed": "saggamhi sā modati dhamma cārinī iti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_vv_32:12.1",
        "original": "Latāvimānaṁ catutthaṁ.",
        "analyzed": "latā vimānam ",
        "folio": "12"
    }
]