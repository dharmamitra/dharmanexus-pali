        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_482:8.3",
        "original": "Bhayañhi maṁ vindatinapparūpaṁ,",
        "analyzed": "bhayan hi mam vindatinapparūpa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_482:8.4",
        "original": "Sutvāna taṁ mānusiṁ bhāsamānaṁ”.",
