[
    {
        "segmentnr": "PA_snp_2.13:0.1",
        "original": "Sutta Nipāta 2.13",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.13:0.2",
        "original": "Sammāparibbājanīyasutta",
        "analyzed": "sammāparibbājanīya sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_2.13:1.1",
        "original": "“Pucchāmi muniṁ pahūtapaññaṁ,",
        "analyzed": "pucchāmi munim pahūta pañña ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.13:1.2",
        "original": "Tiṇṇaṁ pāraṅgataṁ parinibbutaṁ ṭhitattaṁ;",
        "analyzed": "tiṇṇam pāraṅgatam parinibbutam ṭhitatta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.13:1.3",
        "original": "Nikkhamma gharā panujja kāme,",
        "analyzed": "nikkhamma gharā panujja kāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.13:1.4",
        "original": "Kathaṁ bhikkhu sammā so loke paribbajeyya”.",
        "analyzed": "katham bhikkhu sammā saḥ loke paribbajeyya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_2.13:2.1",
        "original": "“Yassa maṅgalā samūhatā,",
        "analyzed": "yaḥ sa maṅgalā samūhatā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.13:2.2",
        "original": "(iti bhagavā)",
        "analyzed": "iti bhagavān ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.13:2.3",
        "original": "Uppātā supinā ca lakkhaṇā ca;",
        "analyzed": "uppātā supinā ca lakkhaṇā ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.13:2.4",
        "original": "So maṅgaladosavippahīno,",
        "analyzed": "so maṅgala dosa vippahīno ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.13:2.5",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_2.13:3.1",
        "original": "Rāgaṁ vinayetha mānusesu,",
        "analyzed": "rāgam vinayetha mānusesu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.13:3.2",
        "original": "Dibbesu kāmesu cāpi bhikkhu;",
        "analyzed": "dibbesu kāmesu ca api bhikkhu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.13:3.3",
        "original": "Atikkamma bhavaṁ samecca dhammaṁ,",
        "analyzed": "atikkamma bhavam samet ca dhamma ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.13:3.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_2.13:4.1",
        "original": "Vipiṭṭhikatvāna pesuṇāni,",
        "analyzed": "vipiṭṭhika tvāna pesuṇāni ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.13:4.2",
        "original": "Kodhaṁ kadarīyaṁ jaheyya bhikkhu;",
        "analyzed": "kodham kadarīyam jaheyya bhikkhu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.13:4.3",
        "original": "Anurodhavirodhavippahīno,",
        "analyzed": "anurodha virodha vippahīno ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.13:4.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_2.13:5.1",
        "original": "Hitvāna piyañca appiyañca,",
        "analyzed": "hitvāna piyañca appiyañca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.13:5.2",
        "original": "Anupādāya anissito kuhiñci;",
        "analyzed": "an upādāya a nissito kuhiñci ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.13:5.3",
        "original": "Saṁyojaniyehi vippamutto,",
        "analyzed": "sayojaniyehi vippamuttaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.13:5.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_2.13:6.1",
        "original": "Na so upadhīsu sārameti,",
        "analyzed": "na saḥ upadhīsu sāram eti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.13:6.2",
        "original": "Ādānesu vineyya chandarāgaṁ;",
        "analyzed": "ādānesu vineyya chandarāga ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.13:6.3",
        "original": "So anissito anaññaneyyo,",
        "analyzed": "saḥ a nissitaḥ an añña neyyaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.13:6.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_2.13:7.1",
        "original": "Vacasā manasā ca kammunā ca,",
        "analyzed": "vacasā manasā ca kammunā ca ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.13:7.2",
        "original": "Aviruddho sammā viditvā dhammaṁ;",
        "analyzed": "a viruddhaḥ sammā viditvā dhamma ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.13:7.3",
        "original": "Nibbānapadābhipatthayāno,",
        "analyzed": "nibbāna padā abhipatthayānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.13:7.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_2.13:8.1",
        "original": "Yo vandati manti nunnameyya,",
        "analyzed": "yaḥ vandati manti nunnameyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.13:8.2",
        "original": "Akkuṭṭhopi na sandhiyetha bhikkhu;",
        "analyzed": "akkuṭṭhaḥ api na bhikkhu ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.13:8.3",
        "original": "Laddhā parabhojanaṁ na majje,",
        "analyzed": "laddhā para bhojanam na majje ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.13:8.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_2.13:9.1",
        "original": "Lobhañca bhavañca vippahāya,",
        "analyzed": "lobhañca bhavañca vippahāya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.13:9.2",
        "original": "Virato chedanabandhanā ca bhikkhu;",
        "analyzed": "virataḥ chedana bandhanā ca bhikkhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.13:9.3",
        "original": "So tiṇṇakathaṅkatho visallo,",
        "analyzed": "saḥ tiṇṇa kathaṅkathaḥ visallaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.13:9.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_2.13:10.1",
        "original": "Sāruppaṁ attano viditvā,",
        "analyzed": "sāruppam viditvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.13:10.2",
        "original": "No ca bhikkhu hiṁseyya kañci loke;",
        "analyzed": "no ca bhikkhu hiseyya kañci loke ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.13:10.3",
        "original": "Yathātathiyaṁ viditvā dhammaṁ,",
        "analyzed": "yathātathiyam viditvā dhamma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.13:10.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_2.13:11.1",
        "original": "Yassānusayā na santi keci,",
        "analyzed": "na santi keci ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.13:11.2",
        "original": "Mūlā ca akusalā samūhatāse;",
        "analyzed": "mūlā ca akusalā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.13:11.3",
        "original": "So nirāso anāsisāno,",
        "analyzed": "saḥ nirāsaḥ an āsisānaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.13:11.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_snp_2.13:12.1",
        "original": "Āsavakhīṇo pahīnamāno,",
        "analyzed": "āsava khīṇaḥ pahīna mānaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.13:12.2",
        "original": "Sabbaṁ rāgapathaṁ upātivatto;",
        "analyzed": "sabbam rāga patham upātivattaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.13:12.3",
        "original": "Danto parinibbuto ṭhitatto,",
        "analyzed": "dantaḥ parinibbutaḥ ṭhita ttaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.13:12.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_snp_2.13:13.1",
        "original": "Saddho sutavā niyāmadassī,",
        "analyzed": "saddhaḥ sutavā niyāma dassī ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.13:13.2",
        "original": "Vaggagatesu na vaggasāri dhīro;",
        "analyzed": "vagga gatesu na vagga sāri dhīraḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.13:13.3",
        "original": "Lobhaṁ dosaṁ vineyya paṭighaṁ,",
        "analyzed": "lobham dosam vineyya paṭigha ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.13:13.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_snp_2.13:14.1",
        "original": "Saṁsuddhajino vivaṭṭacchado,",
        "analyzed": "sa suddha jinaḥ vivaṭṭa chadaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.13:14.2",
        "original": "Dhammesu vasī pāragū anejo;",
        "analyzed": "dhammesu vasī pāragū anejaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.13:14.3",
        "original": "Saṅkhāranirodhañāṇakusalo,",
        "analyzed": "saṅkhāra nirodha ñāṇa kusalaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.13:14.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_snp_2.13:15.1",
        "original": "Atītesu anāgatesu cāpi,",
        "analyzed": "atītesu anāgatesu ca api ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.13:15.2",
        "original": "Kappātīto aticcasuddhipañño;",
        "analyzed": "kappa atītaḥ aticca suddhi pañño ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.13:15.3",
        "original": "Sabbāyatanehi vippamutto,",
        "analyzed": "sabba āyatanehi vippamuttaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.13:15.4",
        "original": "Sammā so loke paribbajeyya.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_snp_2.13:16.1",
        "original": "Aññāya padaṁ samecca dhammaṁ,",
        "analyzed": "aññāya padam samet ca dhamma ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.13:16.2",
        "original": "Vivaṭaṁ disvāna pahānamāsavānaṁ;",
        "analyzed": "vivaṭam disvāna pahānam āsavāna ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.13:16.3",
        "original": "Sabbupadhīnaṁ parikkhayāno,",
        "analyzed": "sabba upadhīnam parikkhayānaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.13:16.4",
        "original": "Sammā so loke paribbajeyya”.",
        "analyzed": "sammā saḥ loke paribbajeyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_snp_2.13:17.1",
        "original": "“Addhā hi bhagavā tatheva etaṁ,",
        "analyzed": "addhā hi bhagavā tathā iva eta ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.13:17.2",
        "original": "Yo so evaṁvihārī danto bhikkhu;",
        "analyzed": "yaḥ saḥ evavihārī dantaḥ bhikkhu ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.13:17.3",
        "original": "Sabbasaṁyojanayogavītivatto,",
        "analyzed": "sabba sa yojana yoga vīti vatto ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.13:17.4",
        "original": "Sammā so loke paribbajeyyā”ti.",
        "analyzed": "sammā saḥ loke paribbajeyya ati ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_snp_2.13:17.5",
        "original": "Sammāparibbājanīyasuttaṁ terasamaṁ.",
        "analyzed": "sammāparibbājanīya suttam terasama ",
        "folio": "17"
    }
]