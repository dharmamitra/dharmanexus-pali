    {
        "segmentnr": "PA_bi-vb_pc60:0.5",
        "original": "60. Pasākhejātasikkhāpada",
        "analyzed": "    ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.2",
        "original": "Tena kho pana samayena aññatarā bhikkhunī pasākhe jātaṁ gaṇḍaṁ purisena saddhiṁ ekenekā bhedāpesi.",
        "analyzed": "tena kho pana samayena aññatarā bhikkhunī pasākhe jātam gaṇḍam purisena saddhim ekena ikā bhedāpesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.3",
        "original": "Atha kho so puriso taṁ bhikkhuniṁ dūsetuṁ upakkami.",
        "analyzed": "atha kho saḥ purisaḥ tam bhikkhunim dūsetum upakkami ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.4",
        "original": "Sā vissaramakāsi.",
        "analyzed": "sā vissaramakāsi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.5",
        "original": "Bhikkhuniyo upadhāvitvā taṁ bhikkhuniṁ etadavocuṁ—",
        "analyzed": "bhikkhuniyaḥ upadhāvitvā tam bhikkhunim etat avocuḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.6",
        "original": "“kissa tvaṁ, ayye, vissaramakāsī”ti?",
        "analyzed": "kissa tva ayye vissaram akāsi iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.7",
        "original": "Atha kho sā bhikkhunī bhikkhunīnaṁ etamatthaṁ ārocesi.",
        "analyzed": "atha kho sā bhikkhunī bhikkhunīnam etam attham ārocesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.8",
        "original": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
        "analyzed": "yāḥ tāḥ bhikkhuniyaḥ appicchāḥ pe tāḥ ujjhāyanti khiyyanti vipācenti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.9",
        "original": "“kathañhi nāma bhikkhunī pasākhe jātaṁ gaṇḍaṁ purisena saddhiṁ ekenekā bhedāpessatī”ti …pe…",
        "analyzed": "katham hi nāma bhikkhunī pasākhe jātam gaṇḍam purisena saddhim ekena ikā bhedāpessati iti pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.10",
        "original": "“saccaṁ kira, bhikkhave, bhikkhunī pasākhe jātaṁ gaṇḍaṁ purisena saddhiṁ ekenekā bhedāpetī”ti?",
        "analyzed": "saccam kira bhikkhave bhikkhunī pasākhe jātam gaṇḍam purisena saddhim ekena ikā bhedāpeti iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.11",
        "original": "“Saccaṁ, bhagavā”ti.",
        "analyzed": "sat ca bhagavān ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.12",
        "original": "Vigarahi buddho bhagavā …pe…",
        "analyzed": "vigarahi buddhaḥ bhagavān pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.13",
        "original": "kathañhi nāma, bhikkhave, bhikkhunī pasākhe jātaṁ gaṇḍaṁ purisena saddhiṁ ekenekā bhedāpessati.",
        "analyzed": "katham hi nāma bhikkhave bhikkhunī pasākhe jātam gaṇḍam purisena saddhim ekena ikā bhedāpessati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.14",
        "original": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
        "analyzed": "na ita bhikkhave appasannānam vā pasādāya pe ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bi-vb_pc60:1.15",
        "original": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
