    {
        "segmentnr": "PA_bu-vb_np7:1.1",
        "original": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "tena samayena buddhaḥ bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.2",
        "original": "Tena kho pana samayena chabbaggiyā bhikkhū acchinnacīvarake bhikkhū upasaṅkamitvā evaṁ vadanti—",
        "analyzed": "tena kho pana samayena chabbaggiyāḥ bhikkhū acchinna cīvarake bhikkhū upasaṅkamitvā evam vadanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.3",
        "original": "“bhagavatā, āvuso, anuññātaṁ—",
        "analyzed": "bhagavatā āvuso anuññāta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.4",
        "original": "‘acchinnacīvarassa vā naṭṭhacīvarassa vā aññātakaṁ gahapatiṁ vā gahapatāniṁ vā cīvaraṁ viññāpetuṁ’;",
        "analyzed": "acchinna cīvarassa vā naṭṭha cīvarassa vā aññātakam gahapatim vā gahapatānim vā cīvaram viññāpetu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.5",
        "original": "viññāpetha, āvuso, cīvaran”ti.",
        "analyzed": "viññāpetha āvuso cīvaranti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.6",
        "original": "“Alaṁ, āvuso, laddhaṁ amhehi cīvaran”ti.",
        "analyzed": "ala āvuso laddham amhehi cīvaranti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.7",
        "original": "“Mayaṁ āyasmantānaṁ viññāpemā”ti.",
        "analyzed": "mayam āyasmantānam viññāpema ati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.8",
        "original": "“Viññāpetha, āvuso”ti.",
        "analyzed": "viññāpetha āvuso ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.9",
        "original": "Atha kho chabbaggiyā bhikkhū gahapatike upasaṅkamitvā etadavocuṁ—",
        "analyzed": "atha kho chabbaggiyāḥ bhikkhū gahapatike upasaṅkamitvā etat avocu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.10",
        "original": "“acchinnacīvarakā, āvuso, bhikkhū āgatā.",
        "analyzed": "acchinna cīvarakāḥ āvuso bhikkhū āgatāḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.11",
        "original": "Detha nesaṁ cīvarānī”ti,",
        "analyzed": "cīvarāni iti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.12",
        "original": "bahuṁ cīvaraṁ viññāpesuṁ.",
        "analyzed": "bahum cīvaram viññāpesu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.13",
        "original": "Tena kho pana samayena aññataro puriso sabhāyaṁ nisinno aññataraṁ purisaṁ etadavoca—",
        "analyzed": "tena kho pana samayena aññataraḥ puriso sabhāyam nisinnaḥ aññataram purisam etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.14",
        "original": "“acchinnacīvarakā ayyo bhikkhū āgatā.",
        "analyzed": "acchinna cīvarakāḥ ayyaḥ bhikkhū āgatā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.15",
        "original": "Tesaṁ mayā cīvaraṁ dinnan”ti.",
        "analyzed": "tesam mayā cīvaram dinnanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.16",
        "original": "Sopi evamāha—“mayāpi dinnan”ti.",
        "analyzed": "saḥ upi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.17",
        "original": "Aparopi evamāha—“mayāpi dinnan”ti.",
        "analyzed": "aparā api evam āhamayā api ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_bu-vb_np7:1.18",
        "original": "Te ujjhāyanti khiyyanti vipācenti—",
