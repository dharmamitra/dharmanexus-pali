        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_483:5.1",
        "original": "Dukkhūpanītopi naro sapañño,",
        "analyzed": "dukkha upanītaḥ api naraḥ sa pañño ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_483:5.2",
        "original": "Āsaṁ na chindeyya sukhāgamāya;",
        "analyzed": "āsam na chindeyya sukha āgamāya ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_483:5.3",
        "original": "Bahū hi phassā ahitā hitā ca,",
        "analyzed": "bahū hi phassāḥ ahitā hitā ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_483:5.4",
        "original": "Avitakkitā maccamupabbajanti.",
        "analyzed": "a vitakkitāḥ maccam upabbajanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_483:6.1",
        "original": "Acintitampi bhavati,",
        "analyzed": "a cintitam pi bhavati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_483:6.2",
        "original": "cintitampi vinassati;",
        "analyzed": "cintitam pi vinassati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_483:6.3",
        "original": "Na hi cintāmayā bhogā,",
        "analyzed": "na hi cintā mayāḥ bhogāḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_483:6.4",
        "original": "itthiyā purisassa vā”.",
        "analyzed": "itthiyā purisassa vā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_483:7.1",
        "original": "“Sarabhaṁ giriduggasmiṁ,",
        "analyzed": "sarabham giri duh asmi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_483:7.2",
        "original": "yaṁ tvaṁ anusarī pure;",
        "analyzed": "yam tvam anusarī pure ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_483:7.3",
        "original": "Alīnacittassa tuvaṁ,",
        "analyzed": "a līna cittassa tuva ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_483:7.4",
        "original": "vikkantamanujīvasi.",
        "analyzed": "vikkantam anujīvasi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_483:8.1",
        "original": "Yo taṁ viduggā narakā samuddhari,",
        "analyzed": "yaḥ tam viduggā narakā samuddhari ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_483:8.2",
        "original": "Silāya yoggaṁ sarabho karitvā;",
        "analyzed": "silāya yoggam sarabho karitvā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_483:8.3",
        "original": "Dukkhūpanītaṁ maccumukhā pamocayi,",
        "analyzed": "dukkha upanītam maccu mukhā pamocayi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_483:8.4",
        "original": "Alīnacittaṁ ta migaṁ vadesi”.",
        "analyzed": "alīna cittam ta migam vadesi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_483:9.1",
        "original": "“Kiṁ tvaṁ nu tattheva tadā ahosi,",
        "analyzed": "kim tvam nu tattha iva tadā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_483:9.2",
        "original": "Udāhu te koci naṁ etadakkhā;",
        "analyzed": "te koci naṃ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_483:9.3",
        "original": "Vivaṭṭacchaddo nusi sabbadassī,",
        "analyzed": "vivaṭṭa chaddaḥ nusi sabbadassī ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_483:9.4",
        "original": "Ñāṇaṁ nu te brāhmaṇa bhiṁsarūpaṁ”.",
        "analyzed": "nu te brāhmaṇa bhisarūpa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_483:10.1",
        "original": "“Na cevahaṁ tattha tadā ahosiṁ,",
        "analyzed": "na ca iva aham tattha tadā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_483:10.2",
        "original": "Na cāpi me koci naṁ etadakkhā;",
        "analyzed": "na ca api me koci ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_483:10.3",
        "original": "Gāthāpadānañca subhāsitānaṁ,",
        "analyzed": "gāthā padānam ca ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_483:10.4",
        "original": "Atthaṁ tadānenti janinda dhīrā”.",
        "analyzed": "attham tadā anenti janinda dhīrā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_483:11.1",
        "original": "“Ādāya pattiṁ paraviriyaghātiṁ,",
        "analyzed": "ādāya pattim para viriya ghāti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_483:11.2",
        "original": "Cāpe saraṁ kiṁ vicikicchase tuvaṁ;",
        "analyzed": "cāpe saram kim vicikicchase tuva ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_483:11.3",
        "original": "Nunno saro sarabhaṁ hantu khippaṁ,",
        "analyzed": "nunnaḥ saraḥ sarabham hantu khippa ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_483:11.4",
        "original": "Annañhi etaṁ varapañña rañño”.",
        "analyzed": "annan hi etam varapañña rañño ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_483:12.1",
        "original": "“Addhā pajānāmi ahampi etaṁ,",
        "analyzed": "addhā pajānāmi ahampi eta ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_483:12.2",
        "original": "Annaṁ migo brāhmaṇa khattiyassa;",
        "analyzed": "annam migaḥ brāhmaṇa khattiyassa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_483:12.3",
        "original": "Pubbe katañca apacāyamāno,",
        "analyzed": "pubbe katañca apacāyamānaḥ ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_483:12.4",
        "original": "Tasmā migaṁ sarabhaṁ no hanāmi”.",
        "analyzed": "tasmai migam sarabham naḥ hanāmi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_483:13.1",
        "original": "“Neso migo mahārāja,",
        "analyzed": "nesaḥ migaḥ mahā rāja ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_483:13.2",
        "original": "asureso disampati;",
        "analyzed": "asuresaḥ disampati ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_483:13.3",
        "original": "Etaṁ hantvā manussinda,",
        "analyzed": "etam hantvā manuḥ sinda ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_483:13.4",
        "original": "bhavassu amarādhipo.",
        "analyzed": "bhavassu amara adhipo ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_483:14.1",
        "original": "Sace ca rājā vicikicchase tuvaṁ,",
        "analyzed": "sace ca rājā vicikicchase tuva ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_483:14.2",
        "original": "Hantuṁ migaṁ sarabhaṁ sahāyakaṁ;",
        "analyzed": "hantum migam sarabham sahāyaka ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_483:14.3",
        "original": "Saputtadāro naravīraseṭṭha,",
        "analyzed": "sa putta dāraḥ nara vīra seṭṭha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_483:14.4",
        "original": "Gantā tuvaṁ vetaraṇiṁ yamassa”.",
        "analyzed": "ganta tuvam vetaraṇim yamassa ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_483:15.1",
        "original": "“Kāmaṁ ahaṁ jānapadā ca sabbe,",
        "analyzed": "kāmam aham jānapadā ca sabbe ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_483:15.2",
        "original": "Puttā ca dārā ca sahāyasaṅghā;",
        "analyzed": "puttā ca dārā ca sahāya saṅghā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_483:15.3",
        "original": "Gacchemu taṁ vetaraṇiṁ yamassa,",
        "analyzed": "gacchemu tam vetaraṇim yamassa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_483:15.4",
        "original": "Na tveva hañño mama pāṇado yo.",
        "analyzed": "na tu eva haññaḥ mama pāṇadaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_483:16.1",
        "original": "Ayaṁ migo kicchagatassa mayhaṁ,",
        "analyzed": "ayam migaḥ kiccha gatassa mayha ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_483:16.2",
        "original": "Ekassa kattā vivanasmi ghore;",
        "analyzed": "ekassa kattā vivanasmi ghore ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_483:16.3",
        "original": "Taṁ tādisaṁ pubbakiccaṁ saranto,",
        "analyzed": "tam tādisam pubba kiccam sarantaḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_483:16.4",
        "original": "Jānaṁ mahābrahme kathaṁ haneyyaṁ”.",
        "analyzed": "jānam mahābrahme katham haneyya ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_483:17.1",
        "original": "“Mittābhirādhī cirameva jīva,",
        "analyzed": "mitta abhirādhī ciram eva jīva ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_483:17.2",
        "original": "Rajjaṁ imaṁ dhammaguṇe pasāsa;",
        "analyzed": "rajjam imam dhamma guṇe pasāsa ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_483:17.3",
        "original": "Nārīgaṇehi paricāriyanto,",
        "analyzed": "nārī gaṇehi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_483:17.4",
        "original": "Modassu raṭṭhe tidiveva vāsavo.",
        "analyzed": "modassu raṭṭhe tidivā iva vāsavaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_483:18.1",
        "original": "Akkodhano niccapasannacitto,",
        "analyzed": "          ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_483:18.2",
        "original": "Sabbātithī yācayogo bhavitvā;",
        "analyzed": "sabba atithī yāca yogaḥ bhavitvā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_483:18.3",
        "original": "Datvā ca bhutvā ca yathānubhāvaṁ,",
        "analyzed": "ca ca yathā anubhāva ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_483:18.4",
        "original": "Anindito saggamupehi ṭhānan”ti.",
        "analyzed": "aninditaḥ saggam upehi ṭhānan ti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_483:19.1",
        "original": "Sarabhamigajātakaṁ dasamaṁ.",
        "analyzed": "sarabha miga jātakam dasama ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_483:20.1",
        "original": "Terasakanipātaṁ niṭṭhitaṁ.",
        "analyzed": "terasaka nipātam ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_483:21.1",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_483:22.1",
        "original": "Varaamba kuṭhāri sahaṁsavaro,",
        "analyzed": "varaamba kuṭhāri sahasavaro ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_483:22.2",
        "original": "Atharaññasmiṁ dūtakapañcamako;",
        "analyzed": "atha raññasmin dūtaka pañcamakaḥ ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_483:22.3",
        "original": "Atha bodhi akitti sutakkarinā,",
        "analyzed": "atha bodhi akitti sutakkarinā ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_483:22.4",
        "original": "Atha rurumigenaparo sarabhoti.",
        "analyzed": "atha ruru mi gena paraḥ sarabhaḥ ti ",
        "folio": "22"
    }
]