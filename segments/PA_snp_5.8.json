        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.8:1.1",
        "original": "“Santi loke munayo,",
        "analyzed": "santi loke munayaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.8:1.2",
        "original": "(iccāyasmā nando)",
        "analyzed": "nando ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.8:1.3",
        "original": "Janā vadanti tayidaṁ kathaṁsu;",
        "analyzed": "janāḥ vadanti tayidam kathasu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.8:1.4",
        "original": "Ñāṇūpapannaṁ no muniṁ vadanti,",
        "analyzed": "naḥ munim vadanti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.8:1.5",
        "original": "Udāhu ve jīvitenūpapannaṁ”.",
        "analyzed": "uda ahu ve jīvitena upapanna ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.8:2.1",
        "original": "“Na diṭṭhiyā na sutiyā na ñāṇena, (…)",
        "analyzed": "na diṭṭhiyā na sutiyā na ñāṇena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.8:2.2",
        "original": "Munīdha nanda kusalā vadanti;",
        "analyzed": "munīdha nanda kusalāḥ vadanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.8:2.3",
        "original": "Visenikatvā anīghā nirāsā,",
        "analyzed": "visenikatvā anīghā nirāsā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.8:2.4",
        "original": "Caranti ye te munayoti brūmi”.",
        "analyzed": "caranti ye te munayaḥ uti brūmi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.8:3.1",
        "original": "“Ye kecime samaṇabrāhmaṇāse,",
        "analyzed": "ye kecid ime samaṇa brāhmaṇāse ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:3.2",
        "original": "(iccāyasmā nando)",
        "analyzed": "nando ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:3.3",
        "original": "Diṭṭhassutenāpi vadanti suddhiṁ;",
        "analyzed": "diṭṭhaḥ sutena api vadanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:3.4",
        "original": "Sīlabbatenāpi vadanti suddhiṁ,",
        "analyzed": "sīlabbatena api vadanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:3.5",
        "original": "Anekarūpena vadanti suddhiṁ.",
        "analyzed": "aneka rūpena vadanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:3.6",
        "original": "Kaccissu te bhagavā tattha yatā carantā,",
        "analyzed": "kaccissu te bhagavā tattha yatā carantā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:3.7",
        "original": "Atāru jātiñca jarañca mārisa;",
        "analyzed": "atāru jātiñca jaran ca mārisa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:3.8",
        "original": "Pucchāmi taṁ bhagavā brūhi me taṁ”.",
        "analyzed": "pucchāmi tam bhagavā brūhi me ta ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.8:4.1",
        "original": "“Ye kecime samaṇabrāhmaṇāse,",
        "analyzed": "ye kecid ime samaṇa brāhmaṇāse ",
        "folio": "4"
    },
