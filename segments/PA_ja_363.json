        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_363:2.1",
        "original": "Yañhi kayirā tañhi vade,",
        "analyzed": "yat hi kayirā tat hi vade ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_363:2.2",
