    {
        "segmentnr": "PA_ja_482:1.1",
        "original": "“Tassa gāmavaraṁ dammi,",
        "analyzed": "tassa gām avaram dammi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_482:1.2",
