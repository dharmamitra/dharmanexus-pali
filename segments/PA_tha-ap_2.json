        "folio": "58"
    },
    {
        "segmentnr": "PA_tha-ap_2:59.1",
        "original": "Lokānukampāya imāni tesaṁ,",
        "analyzed": "loka anukampāyai imāni ",
        "folio": "59"
    },
    {
