[
    {
        "segmentnr": "PA_snp_5.12:0.1",
        "original": "Sutta Nipāta 5.12",
        "analyzed": "sutta nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.12:0.2",
        "original": "Jatukaṇṇimāṇavapucchā",
        "analyzed": "jatu kaṇṇi māṇava pucchā ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_snp_5.12:1.1",
        "original": "“Sutvānahaṁ vīramakāmakāmiṁ,",
        "analyzed": "sutvān aham vīram akāma kāmi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.12:1.2",
        "original": "(iccāyasmā jatukaṇṇi)",
        "analyzed": "id ca ayasmai ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.12:1.3",
        "original": "Oghātigaṁ puṭṭhumakāmamāgamaṁ;",
        "analyzed": "ogha atigam puṭṭhum akāmam āgama ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.12:1.4",
        "original": "Santipadaṁ brūhi sahajanetta,",
        "analyzed": "santipadam brūhi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.12:1.5",
        "original": "Yathātacchaṁ bhagavā brūhi metaṁ.",
        "analyzed": "yathātaccham bhagavān brūhi mā ita ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_5.12:2.1",
        "original": "Bhagavā hi kāme abhibhuyya iriyati,",
        "analyzed": "bhagavā hi kāme abhibhuyya iriyati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.12:2.2",
        "original": "Ādiccova pathaviṁ tejī tejasā;",
        "analyzed": "ādicca uva pathavim tejī tejasā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.12:2.3",
        "original": "Parittapaññassa me bhūripañña,",
        "analyzed": "paritta paññassa me bhūri pañña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.12:2.4",
        "original": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ;",
        "analyzed": "ācikkha dhammam yam aham vijañña ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.12:2.5",
        "original": "Jātijarāya idha vippahānaṁ”.",
        "analyzed": "jāti jarāyaḥ idha vippahāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_5.12:3.1",
        "original": "“Kāmesu vinaya gedhaṁ,",
        "analyzed": "kāmesu vinaya gedha ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.12:3.2",
        "original": "(jatukaṇṇīti bhagavā)",
        "analyzed": "jatukaṇṇī iti bhagavā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.12:3.3",
        "original": "Nekkhammaṁ daṭṭhu khemato;",
        "analyzed": "nekkhammam daṭṭhu khemataḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.12:3.4",
        "original": "Uggahītaṁ nirattaṁ vā,",
        "analyzed": "uggahītam nirattam vā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.12:3.5",
        "original": "Mā te vijjittha kiñcanaṁ.",
        "analyzed": "mā te vijjittha kiñcana ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_5.12:4.1",
        "original": "Yaṁ pubbe taṁ visosehi,",
        "analyzed": "yam pubbe tam visosehi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.12:4.2",
        "original": "pacchā te māhu kiñcanaṁ;",
        "analyzed": "pacchā te mā āhuḥ kiñcana ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.12:4.3",
        "original": "Majjhe ce no gahessasi,",
        "analyzed": "majjhe ce naḥ gahessasi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.12:4.4",
        "original": "upasanto carissasi.",
        "analyzed": "upasantaḥ carissasi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_5.12:5.1",
        "original": "Sabbaso nāmarūpasmiṁ,",
        "analyzed": "sabbasaḥ nāma rūpasmi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.12:5.2",
        "original": "Vītagedhassa brāhmaṇa;",
        "analyzed": "vītagedhassa brāhmaṇa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.12:5.3",
        "original": "Āsavāssa na vijjanti,",
        "analyzed": "āsavāssa na vijjanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.12:5.4",
        "original": "Yehi maccuvasaṁ vaje”ti.",
        "analyzed": "yā ihi mat cuvasam vaja iti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_5.12:5.5",
        "original": "Jatukaṇṇimāṇavapucchā ekādasamā.",
        "analyzed": "jatu kaṇṇi māṇava pucchā ekādasamā ",
        "folio": "5"
    }
]