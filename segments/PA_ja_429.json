        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_429:0.4",
        "original": "3. Mahāsuvajātaka",
        "analyzed": "mahāsuvajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_429:1.1",
        "original": "“Dumo yadā hoti phalūpapanno,",
        "analyzed": "dumaḥ yadā hoti phala upapannaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_429:1.2",
        "original": "Bhuñjanti naṁ vihaṅgamā sampatantā;",
        "analyzed": "bhuñjanti nam vihaṅgamā sampatantā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_429:1.3",
        "original": "Khīṇanti ñatvāna dumaṁ phalaccaye,",
        "analyzed": "dumam phalaccaye ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_429:1.4",
        "original": "Disodisaṁ yanti tato vihaṅgamā.",
        "analyzed": "disodisam yanti tatas vihaṅgamā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_429:2.1",
        "original": "Cara cārikaṁ lohitatuṇḍa māmari,",
        "analyzed": "cara cārikam lohita tuṇḍa māmari ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_429:2.2",
        "original": "Kiṁ tvaṁ suva sukkhadumamhi jhāyasi;",
        "analyzed": "kim tvam suva jhāyasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_429:2.3",
        "original": "Tadiṅgha maṁ brūhi vasantasannibha,",
        "analyzed": "mama brūhi vasanta sannibha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_429:2.4",
        "original": "Kasmā suva sukkhadumaṁ na riñcasi”.",
        "analyzed": "kasmai suva sukkhadumam na riñcasi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_429:3.1",
        "original": "“Ye ve sakhīnaṁ sakhāro bhavanti,",
        "analyzed": "ye ve sakhīnam sakhāraḥ bhavanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_429:3.2",
        "original": "Pāṇaccaye dukkhasukhesu haṁsa;",
        "analyzed": "pāṇa caye dukkha sukhesu hasa ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_429:3.3",
        "original": "Khīṇaṁ akhīṇampi na taṁ jahanti,",
        "analyzed": "khīṇam a khīṇam api na tam jahanti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_429:3.4",
        "original": "Santo sataṁ dhammamanussarantā.",
        "analyzed": "santaḥ satam dhammam anussarantā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_429:4.1",
        "original": "Sohaṁ sataṁ aññatarosmi haṁsa,",
        "analyzed": "sā uham satam aññatarā asmi hasa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_429:4.2",
        "original": "Ñātī ca me hoti sakhā ca rukkho;",
        "analyzed": "ñātī ca me hoti sakhā ca rukkhaḥ ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_429:4.3",
        "original": "Taṁ nussahe jīvikattho pahātuṁ,",
        "analyzed": "tam nussahe jīvika tthaḥ pahātu ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_429:4.4",
        "original": "Khīṇanti ñatvāna na hesa dhammo”.",
        "analyzed": "khīṇanti ñatvāna na ha isa dhammo ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_429:5.1",
        "original": "“Sādhu sakkhi kataṁ hoti,",
        "analyzed": "sādhu sakkhi katam hoti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_429:5.2",
        "original": "metti saṁsati santhavo;",
        "analyzed": "mā itti sasati santhavaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_429:5.3",
        "original": "Sacetaṁ dhammaṁ rocesi,",
        "analyzed": "sace itam dhammam rocesi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_429:5.4",
        "original": "pāsaṁsosi vijānataṁ.",
        "analyzed": "pāsasā usi vijānata ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_429:6.1",
        "original": "So te suva varaṁ dammi,",
        "analyzed": "saḥ te suva varam dammi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_429:6.2",
        "original": "pattayāna vihaṅgama;",
        "analyzed": "patta yāna vihaṅgama ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_429:6.3",
        "original": "Varaṁ varassu vakkaṅga,",
        "analyzed": "varam varassu vakkaṅga ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_429:6.4",
        "original": "yaṁ kiñci manasicchasi”.",
        "analyzed": "yam kiñci manasi icchasi ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_429:7.1",
        "original": "“Varañca me haṁsa bhavaṁ dadeyya,",
        "analyzed": "varam ca me hasa bhavam dadeyya ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_429:7.2",
        "original": "Ayañca rukkho punarāyuṁ labhetha;",
        "analyzed": "ayam ca rukkhaḥ punarāyum labhetha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_429:7.3",
        "original": "So sākhavā phalimā saṁvirūḷho,",
        "analyzed": "saḥ sākhavā phalimā sa virūḍhaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_429:7.4",
        "original": "Madhutthiko tiṭṭhatu sobhamāno”.",
        "analyzed": "madhutthikaḥ tiṭṭhatu sobhamānaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_429:8.1",
        "original": "“Taṁ passa samma phalimaṁ uḷāraṁ,",
        "analyzed": "tam passa samma phalimam uḷāra ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_429:8.2",
        "original": "Sahāva te hotu udumbarena;",
        "analyzed": "sahāva te hotu udumbarena ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_429:8.3",
        "original": "So sākhavā phalimā saṁvirūḷho,",
        "analyzed": "saḥ sākhavā phalimā sa virūḍhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_429:8.4",
        "original": "Madhutthiko tiṭṭhatu sobhamāno”.",
        "analyzed": "madhutthikaḥ tiṭṭhatu sobhamānaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_429:9.1",
        "original": "“Evaṁ sakka sukhī hohi,",
        "analyzed": "evam sakka sukhī hohi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_429:9.2",
        "original": "saha sabbehi ñātibhi;",
        "analyzed": "saha sabbehi ñātibhi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_429:9.3",
        "original": "Yathāhamajja sukhito,",
        "analyzed": "yathā aham ajja sukhitaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_429:9.4",
        "original": "disvāna saphalaṁ dumaṁ”.",
        "analyzed": "disvāna sa phalam duma ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_429:10.1",
        "original": "“Suvassa ca varaṁ datvā,",
        "analyzed": "ca varam da tvā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_429:10.2",
        "original": "katvāna saphalaṁ dumaṁ;",
        "analyzed": "katvāna saphalam ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_429:10.3",
        "original": "Pakkāmi saha bhariyāya,",
        "analyzed": "pakkāmi saha bhariyāya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_429:10.4",
        "original": "devānaṁ nandanaṁ vanan”ti.",
        "analyzed": "devānam nandanam vananti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_429:11.1",
        "original": "Mahāsuvajātakaṁ tatiyaṁ.",
        "analyzed": "mahāsuva jātakam ",
        "folio": "11"
    }
]