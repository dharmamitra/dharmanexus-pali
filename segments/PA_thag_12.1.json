        "segmentnr": "PA_thag_12.1:7.3",
        "original": "Sīlamābharaṇaṁ seṭṭhaṁ,",
        "analyzed": "sīlam ābharaṇam seṭṭha ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_12.1:7.4",
        "original": "sīlaṁ kavacamabbhutaṁ.",
        "analyzed": "sīlam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_thag_12.1:8.1",
        "original": "Sīlaṁ setu mahesakkho,",
        "analyzed": "sīlam setu mahesakkhaḥ ",
        "folio": "8"
    },
