    },
    {
        "segmentnr": "PA_pv_11:0.3",
        "original": "11. Nāgapetavatthu",
        "analyzed": "nāga petavatthu ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_pv_11:1.1",
        "original": "“Puratova setena paleti hatthinā,",
        "analyzed": "puratas va setena paleti hatthinā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_11:1.2",
        "original": "Majjhe pana assatarīrathena;",
        "analyzed": "majjhe pana assatarī rathena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_11:1.3",
        "original": "Pacchā ca kaññā sivikāya nīyati,",
        "analyzed": "pacchā ca kaññā sivikāya nīyati ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_11:1.4",
        "original": "Obhāsayantī dasa sabbaso disā.",
        "analyzed": "obhāsayantī dasa sabbasas disā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_pv_11:2.1",
        "original": "Tumhe pana muggarahatthapāṇino,",
        "analyzed": "tumhe pana muggara hattha pāṇinaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_11:2.2",
        "original": "Rudammukhā chinnapabhinnagattā;",
        "analyzed": "rudammukhā chinna pabhinna gattā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_11:2.3",
        "original": "Manussabhūtā kimakattha pāpaṁ,",
        "analyzed": "manussa bhūtā kim akattha pāpa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_11:2.4",
        "original": "Yenaññamaññassa pivātha lohitan”ti.",
        "analyzed": "yena aññam aññassa pivātha lohitanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_11:3.1",
        "original": "“Puratova yo gacchati kuñjarena,",
        "analyzed": "puratas uva yaḥ gacchati kuñjarena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_11:3.2",
        "original": "Setena nāgena catukkamena;",
        "analyzed": "setena nāgena catukkamena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_11:3.3",
        "original": "Amhāka putto ahu jeṭṭhako so,",
        "analyzed": "amhāka puttaḥ ahu jeṭṭhakaḥ so ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_11:3.4",
        "original": "Dānāni datvāna sukhī pamodati.",
        "analyzed": "dānāni datvāna sukhī pamodati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_pv_11:4.1",
        "original": "Yo so majjhe assatarīrathena,",
        "analyzed": "yaḥ saḥ majjhe assatarī rathena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_11:4.2",
        "original": "Catubbhi yuttena suvaggitena;",
        "analyzed": "catubbhi yuttena suvaggitena ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_11:4.3",
        "original": "Amhāka putto ahu majjhimo so,",
        "analyzed": "amhāka puttaḥ ahu majjhimaḥ so ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_11:4.4",
        "original": "Amaccharī dānavatī virocati.",
        "analyzed": "amaccharī dānavatī virocati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_pv_11:5.1",
        "original": "Yā sā ca pacchā sivikāya nīyati,",
        "analyzed": "yā sā ca pacchā sivikāya nīyati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_11:5.2",
        "original": "Nārī sapaññā migamandalocanā;",
        "analyzed": "nārī sa paññā miga manda locanā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_11:5.3",
        "original": "Amhāka dhītā ahu sā kaniṭṭhikā,",
        "analyzed": "amhāka dhītā ahu sā kaniṭṭhikā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_11:5.4",
        "original": "Bhāgaḍḍhabhāgena sukhī pamodati.",
        "analyzed": "bhāgaḍḍha bhāgena sukhī pamodati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_pv_11:6.1",
        "original": "Ete ca dānāni adaṁsu pubbe,",
        "analyzed": "ete ca dānāni adasu pubbe ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_11:6.2",
        "original": "Pasannacittā samaṇabrāhmaṇānaṁ;",
        "analyzed": "pasanna cittā samaṇa brāhmaṇāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_11:6.3",
        "original": "Mayaṁ pana maccharino ahumha,",
        "analyzed": "mayam pana mat śarinaḥ ahumha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_11:6.4",
        "original": "Paribhāsakā samaṇabrāhmaṇānaṁ;",
        "analyzed": "paribhāsakā samaṇabrāhmaṇāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_11:6.5",
        "original": "Ete ca datvā paricārayanti,",
        "analyzed": "ete ca da tvā paricārayanti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_11:6.6",
        "original": "Mayañca sussāma naḷova chinno”ti.",
        "analyzed": "mayan ca sussāma naḷā uva chinnoti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_pv_11:7.1",
        "original": "“Kiṁ tumhākaṁ bhojanaṁ kiṁ sayānaṁ,",
        "analyzed": "kim tumhākam bhojanam kim sayāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_11:7.2",
        "original": "Kathañca yāpetha supāpadhammino;",
        "analyzed": "kathañca yāpetha su pāpa dhamminaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_11:7.3",
        "original": "Pahūtabhogesu anappakesu,",
        "analyzed": "pahūta bhogesu anappakesu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_11:7.4",
        "original": "Sukhaṁ virādhāya dukkhajja pattā”ti.",
        "analyzed": "sukham virādhāya dukkhajja patta ati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_pv_11:8.1",
        "original": "“Aññamaññaṁ vadhitvāna,",
        "analyzed": "aññam aññam vadhitvāna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_11:8.2",
        "original": "pivāma pubbalohitaṁ;",
        "analyzed": "pivāma pubba lohita ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_11:8.3",
        "original": "Bahuṁ pitvā na dhātā homa,",
        "analyzed": "bahum pitvā na dhātā homa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_11:8.4",
        "original": "nacchādimhase mayaṁ.",
        "analyzed": "maya ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_pv_11:9.1",
        "original": "Icceva maccā paridevayanti,",
        "analyzed": "icca iva maccā paridevayanti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_11:9.2",
        "original": "Adāyakā pecca yamassa ṭhāyino;",
        "analyzed": "a dāyakā pecca yamassa ṭhāyinaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_11:9.3",
        "original": "Ye te vidicca adhigamma bhoge,",
        "analyzed": "ye te vidicca adhigamma bhoge ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_11:9.4",
        "original": "Na bhuñjare nāpi karonti puññaṁ.",
        "analyzed": "na bhuñjare na api karonti ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_pv_11:10.1",
        "original": "Te khuppipāsūpagatā parattha,",
        "analyzed": "te khuppipāsu upagatāḥ parattha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_11:10.2",
        "original": "Pacchā ciraṁ jhāyare ḍayhamānā;",
        "analyzed": "pacchā ciram jhāyare ḍayhamānā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_11:10.3",
        "original": "Kammāni katvāna dukhudrāni,",
        "analyzed": "kammāni katvāna dukhudrāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_11:10.4",
        "original": "Anubhonti dukkhaṁ kaṭukapphalāni.",
        "analyzed": "anubhonti dukkham kaṭuka phalāni ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_pv_11:11.1",
        "original": "Ittaraṁ hi dhanaṁ dhaññaṁ,",
        "analyzed": "ittaram hi dhanam dhañña ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_11:11.2",
        "original": "ittaraṁ idha jīvitaṁ;",
        "analyzed": "ittaram idha jīvita ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_11:11.3",
        "original": "Ittaraṁ ittarato ñatvā,",
        "analyzed": "ittaram ittaratas ñatvā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_11:11.4",
        "original": "dīpaṁ kayirātha paṇḍito.",
        "analyzed": "dīpam paṇḍito ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_pv_11:12.1",
        "original": "Ye te evaṁ pajānanti,",
        "analyzed": "ye te evam pajānanti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_11:12.2",
        "original": "narā dhammassa kovidā;",
        "analyzed": "narā dhammassa kovidā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_11:12.3",
        "original": "Te dāne nappamajjanti,",
        "analyzed": "te dāne na appamajjanti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_11:12.4",
        "original": "sutvā arahataṁ vaco”ti.",
        "analyzed": "sutvā arahatam vacaḥ iti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_pv_11:13.1",
        "original": "Nāgapetavatthu ekādasamaṁ.",
        "analyzed": "nāgapetavatthu ekādasama ",
        "folio": "13"
    }
]