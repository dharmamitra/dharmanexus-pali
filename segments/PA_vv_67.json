        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_67:1.2",
        "original": "Samantato soḷasa yojanāni;",
        "analyzed": "samantataḥ soḷasa yojanāni ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_67:1.3",
        "original": "Kūṭāgārā sattasatā uḷārā,",
        "analyzed": "kūṭāgārā satta satā uḷārā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_67:1.4",
        "original": "Veḷuriyathambhā rucakatthatā subhā.",
        "analyzed": "veḷuriya thambhā rucakatthatā subhā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_vv_67:2.1",
        "original": "Tatthacchasi pivasi khādasi ca,",
        "analyzed": "tatthacchasi pivasi khādasi ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_67:2.2",
        "original": "Dibbā ca vīṇā pavadanti vagguṁ;",
        "analyzed": "dibbā ca vīṇā pavadanti vaggu ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_67:2.3",
        "original": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā,",
        "analyzed": "aṭṭhaṭṭhakā sikkhitā sādhu rūpā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_67:2.4",
        "original": "Dibbā ca kaññā tidasacarā uḷārā;",
        "analyzed": "dibbā ca kaññā tidasa carā uḷārā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_67:2.5",
        "original": "Naccanti gāyanti pamodayanti.",
        "analyzed": "naccanti gāyanti pamodayanti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_vv_67:3.1",
        "original": "Deviddhipattosi mahānubhāvo,",
        "analyzed": "deviddhipatto asi mahā anubhāvaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_67:3.2",
        "original": "Manussabhūto kimakāsi puññaṁ;",
        "analyzed": "manussa bhūtaḥ kim akāsi puñña ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_67:3.3",
        "original": "Kenāsi evaṁ jalitānubhāvo,",
        "analyzed": "kena asi evam jalita anubhāvaḥ ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_67:3.4",
        "original": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
        "analyzed": "vaṇṇaḥ ca te sabbadisā pabhāsati iti ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_vv_67:4.1",
        "original": "So devaputto attamano,",
        "analyzed": "saḥ deva puttaḥ atta mano ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_67:4.2",
        "original": "…pe…",
        "analyzed": "pe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_67:4.3",
        "original": "Yassa kammassidaṁ phalaṁ.",
        "analyzed": "yassa kammassa idam ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_vv_67:5.1",
        "original": "“Phaladāyī phalaṁ vipulaṁ labhati,",
        "analyzed": "phala dāyī phalam vipulam labhati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_vv_67:5.2",
        "original": "Dadamujugatesu pasannamānaso;",
