[
    {
        "segmentnr": "PA_ud_8.10:0.1",
        "original": "Udāna 8.10",
        "analyzed": "udāna ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ud_8.10:0.2",
        "original": "Dutiyadabbasutta",
        "analyzed": "dutiya dabba sutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ud_8.10:1.1",
        "original": "Evaṁ me sutaṁ—",
        "analyzed": "evam me suta ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_8.10:1.2",
        "original": "ekaṁ samayaṁ bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
        "analyzed": "ekam samayam bhagavā sāvatthiyam viharati jetavane anāthapiṇḍikassa ārāme ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_8.10:1.3",
        "original": "Tatra kho bhagavā bhikkhū āmantesi:",
        "analyzed": "tatra kho bhagavā bhikkhū āmantesi ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_8.10:1.4",
        "original": "“bhikkhavo”ti.",
        "analyzed": "bhikkhavaḥ ti ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_8.10:1.5",
        "original": "“Bhadante”ti te bhikkhū bhagavato paccassosuṁ.",
        "analyzed": "bhadanta iti te bhikkhū bhagavataḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_8.10:1.6",
        "original": "Bhagavā etadavoca:",
        "analyzed": "bhagavān etat avoca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ud_8.10:2.1_0",
        "original": "“Dabbassa, bhikkhave, mallaputtassa vehāsaṁ abbhuggantvā ākāse antalikkhe pallaṅkena",
        "analyzed": "dabbassa bhikkhave malla puttassa vehāsam abbhuggantvā ākāse antalikkhe pallaṅkena ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_8.10:2.1_1",
        "original": "nisīditvā tejodhātuṁ samāpajjitvā vuṭṭhahitvā parinibbutassa sarīrassa jhāyamānassa",
        "analyzed": "nisīditvā tejodhātum samāpajjitvā vuṭṭhahitvā parinibbutassa sarīrassa jhāyamānassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_8.10:2.1_2",
        "original": "ḍayhamānassa neva chārikā paññāyittha na masi.",
        "analyzed": "ḍayhamānassa na iva chārikā paññāyittha na masi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_8.10:2.2",
        "original": "Seyyathāpi nāma sappissa vā telassa vā jhāyamānassa ḍayhamānassa neva chārikā paññāyati na masi;",
        "analyzed": "seyyathā api nāma sappissa vā telassa vā jhāyamānassa ḍayhamānassa na iva chārikā paññāyati na masi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_8.10:2.3_0",
        "original": "evamevaṁ kho, bhikkhave, dabbassa mallaputtassa vehāsaṁ abbhuggantvā ākāse antalikkhe",
        "analyzed": "evam evam kho bhikkhave dabbassa malla puttassa vehāsam abbhuggantvā ākāse antalikkhe ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_8.10:2.3_1",
        "original": "pallaṅkena nisīditvā tejodhātuṁ samāpajjitvā vuṭṭhahitvā parinibbutassa sarīrassa",
        "analyzed": "pallaṅkena nisīditvā tejodhātum samāpajjitvā vuṭṭhahitvā parinibbutassa sarīrassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_8.10:2.3_2",
        "original": "jhāyamānassa ḍayhamānassa neva chārikā paññāyittha na masī”ti.",
        "analyzed": "jhāyamānassa ḍayhamānassa na iva chārikā paññāyittha na masi iti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ud_8.10:3.1",
        "original": "Atha kho bhagavā etamatthaṁ viditvā tāyaṁ velāyaṁ imaṁ udānaṁ udānesi:",
        "analyzed": "atha kho bhagavā etam attham viditvā tāyam velāyam imam udānam udānesi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ud_8.10:4.1",
        "original": "“Ayoghanahatasseva,",
        "analyzed": "ayaḥ ghana hataḥ sa iva ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_8.10:4.2",
        "original": "jalato jātavedaso;",
        "analyzed": "jalataḥ jātavedaso ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_8.10:4.3",
        "original": "Anupubbūpasantassa,",
        "analyzed": "anupubba upasantassa ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_8.10:4.4",
        "original": "yathā na ñāyate gati.",
        "analyzed": "yathā na ñāyate gati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ud_8.10:5.1",
        "original": "Evaṁ sammāvimuttānaṁ,",
        "analyzed": "evam ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_8.10:5.2",
        "original": "kāmabandhoghatārinaṁ;",
        "analyzed": "kāma bandho ghatārina ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_8.10:5.3",
        "original": "Paññāpetuṁ gati natthi,",
        "analyzed": "paññāpetum gati natthi ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_8.10:5.4",
        "original": "pattānaṁ acalaṁ sukhan”ti.",
        "analyzed": "pattānam acalam sukhanti ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_8.10:5.5",
        "original": "Dasamaṁ.",
        "analyzed": "dasama ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_8.10:5.6",
        "original": "Pāṭaligāmiyavaggo aṭṭhamo.",
        "analyzed": "pāṭaligāmiya vaggaḥ aṭṭhamaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ud_8.10:6.0",
        "original": "Tassuddānaṁ",
        "analyzed": "tassa uddāna ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ud_8.10:6.1",
        "original": "Nibbānā caturo vuttā,",
        "analyzed": "nibbānā caturaḥ vuttā ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ud_8.10:6.2",
        "original": "cundo pāṭaligāmiyā;",
        "analyzed": "cundo ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ud_8.10:6.3",
        "original": "Dvidhāpatho visākhā ca,",
        "analyzed": "dvidhā apathaḥ visākhā ca ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ud_8.10:6.4",
        "original": "dabbena saha te dasāti.",
        "analyzed": "dabbena saha te dasāti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ud_8.10:7.0",
        "original": "Udāne vaggānamuddānaṁ",
        "analyzed": "udāne vaggānam uddāna ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_8.10:7.1",
        "original": "Vaggamidaṁ paṭhamaṁ varabodhi,",
        "analyzed": "vaggam idam paṭhamam vara bodhi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_8.10:7.2",
        "original": "Vaggamidaṁ dutiyaṁ mucalindo;",
        "analyzed": "vaggam idam dutiyam mucalindo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_8.10:7.3",
        "original": "Nandakavaggavaro tatiyo tu,",
        "analyzed": "nandaka vagga varaḥ tatiyaḥ tu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_8.10:7.4",
        "original": "Meghiyavaggavaro ca catuttho.",
        "analyzed": "meghiya vagga varaḥ ca catutthaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ud_8.10:8.1",
        "original": "Pañcamavaggavarantidha soṇo,",
        "analyzed": "soṇaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_8.10:8.2",
        "original": "Chaṭṭhamavaggavaranti jaccandho;",
        "analyzed": "chaṭṭhama vagga vara nti ja c ca andhaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_8.10:8.3",
        "original": "Sattamavaggavaranti ca cūḷo,",
        "analyzed": "sattama vagga varanti ca cūḷaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_8.10:8.4",
        "original": "Pāṭaligāmiyamaṭṭhamavaggo.",
        "analyzed": "pāṭaligāmiyam aṭṭhama vaggo ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ud_8.10:9.1",
        "original": "Asītimanūnakasuttavaraṁ,",
        "analyzed": "asīti manūnaka sutta vara ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_8.10:9.2",
        "original": "Vaggamidaṭṭhakaṁ suvibhattaṁ;",
        "analyzed": "vaggam idaṭṭhakam su vibhatta ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_8.10:9.3",
        "original": "Dassitaṁ cakkhumatā vimalena,",
        "analyzed": "dassitam cakkhumatā vimalena ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_8.10:9.4",
        "original": "Addhā hi taṁ udānamitīdamāhu.",
        "analyzed": "addhā hi tam udānam iti idam āhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ud_8.10:9.5",
        "original": "Udānapāḷi niṭṭhitā.",
        "analyzed": "      ",
        "folio": "9"
    }
]