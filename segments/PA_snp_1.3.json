        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_1.3:37.4",
        "original": "Eko care khaggavisāṇakappo”.",
        "analyzed": "ekaḥ care khagga visāṇa kappo ",
        "folio": "37"
    },
    {
        "segmentnr": "PA_snp_1.3:38.1",
        "original": "“Sīho yathā dāṭhabalī pasayha,",
        "analyzed": "sīhaḥ yathā dāṭhabalī pasayha ",
        "folio": "38"
    },
    {
        "segmentnr": "PA_snp_1.3:38.2",
        "original": "Rājā migānaṁ abhibhuyya cārī;",
