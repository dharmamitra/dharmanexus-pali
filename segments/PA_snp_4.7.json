    {
        "segmentnr": "PA_snp_4.7:1.1",
        "original": "“Methunamanuyuttassa,",
        "analyzed": "methunam anuyuttassa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.7:1.2",
        "original": "(iccāyasmā tisso metteyyo)",
        "analyzed": "iccā ayasmā tissaḥ metteyyaḥ ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.7:1.3",
        "original": "Vighātaṁ brūhi mārisa;",
        "analyzed": "vighātam brūhi mārisa ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.7:1.4",
        "original": "Sutvāna tava sāsanaṁ,",
        "analyzed": "sutvāna tava sāsana ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.7:1.5",
        "original": "Viveke sikkhissāmase”.",
        "analyzed": "viveke sikkhissāmase ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_snp_4.7:2.1",
        "original": "“Methunamanuyuttassa,",
        "analyzed": "methunam anuyuttassa ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.7:2.2",
        "original": "(metteyyāti bhagavā)",
        "analyzed": "metteyyā iti bhagavā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.7:2.3",
        "original": "Mussate vāpi sāsanaṁ;",
        "analyzed": "vā api sāsana ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.7:2.4",
        "original": "Micchā ca paṭipajjati,",
        "analyzed": "micchā ca paṭipajjati ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.7:2.5",
        "original": "Etaṁ tasmiṁ anāriyaṁ.",
        "analyzed": "etam tasmin ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_snp_4.7:3.1",
        "original": "Eko pubbe caritvāna,",
        "analyzed": "ekaḥ pubbe caritvāna ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.7:3.2",
        "original": "methunaṁ yo nisevati;",
        "analyzed": "methunam yaḥ nisevati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.7:3.3",
        "original": "Yānaṁ bhantaṁva taṁ loke,",
        "analyzed": "yānam bhantava tam loke ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.7:3.4",
        "original": "hīnamāhu puthujjanaṁ.",
        "analyzed": "hīnam āhuḥ puthujjana ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_snp_4.7:4.1",
        "original": "Yaso kitti ca yā pubbe,",
        "analyzed": "yasaḥ kitti ca yā pubbe ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.7:4.2",
        "original": "hāyate vāpi tassa sā;",
        "analyzed": "hāyate vā api tassa sā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.7:4.3",
        "original": "Etampi disvā sikkhetha,",
        "analyzed": "etam pi disvā sikkhetha ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.7:4.4",
        "original": "methunaṁ vippahātave.",
        "analyzed": "methunam vippahātave ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_snp_4.7:5.1",
        "original": "Saṅkappehi pareto so,",
        "analyzed": "saṅkappehi paretaḥ saḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.7:5.2",
        "original": "kapaṇo viya jhāyati;",
        "analyzed": "kapaṇaḥ viya jhāyati ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.7:5.3",
        "original": "Sutvā paresaṁ nigghosaṁ,",
        "analyzed": "sutvā paresam nigghosa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.7:5.4",
        "original": "maṅku hoti tathāvidho.",
        "analyzed": "tathāvidhaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_snp_4.7:6.1",
        "original": "Atha satthāni kurute,",
        "analyzed": "atha satthāni kurute ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.7:6.2",
        "original": "paravādehi codito;",
        "analyzed": "paravādehi coditaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.7:6.3",
        "original": "Esa khvassa mahāgedho,",
        "analyzed": "esa khvassa mahā gedhaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.7:6.4",
        "original": "mosavajjaṁ pagāhati.",
        "analyzed": "mosa vajjam pagāhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_snp_4.7:7.1",
        "original": "Paṇḍitoti samaññāto,",
        "analyzed": "paṇḍito ti samaññāto ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.7:7.2",
        "original": "ekacariyaṁ adhiṭṭhito;",
        "analyzed": "eka cariyam adhiṭṭhitaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.7:7.3",
        "original": "Athāpi methune yutto,",
        "analyzed": "atha api methune yuttaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.7:7.4",
        "original": "mandova parikissati.",
        "analyzed": "mandaḥ iva parikissati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_snp_4.7:8.1",
        "original": "Etamādīnavaṁ ñatvā,",
        "analyzed": "etam ādīnavam ñatvā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.7:8.2",
        "original": "Muni pubbāpare idha;",
        "analyzed": "muni pubba apare idha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.7:8.3",
        "original": "Ekacariyaṁ daḷhaṁ kayirā,",
        "analyzed": "eka cariyam daḷham kayirā ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.7:8.4",
        "original": "Na nisevetha methunaṁ.",
        "analyzed": "na nisevetha methuna ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_snp_4.7:9.1",
        "original": "Vivekaññeva sikkhetha,",
        "analyzed": "vivekaññā iva sikkhetha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.7:9.2",
        "original": "etaṁ ariyānamuttamaṁ;",
        "analyzed": "etam ariyānam uttama ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.7:9.3",
        "original": "Na tena seṭṭho maññetha,",
        "analyzed": "na tena seṭṭhaḥ maññetha ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.7:9.4",
        "original": "sa ve nibbānasantike.",
        "analyzed": "sa ve nibbāna santike ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_snp_4.7:10.1",
        "original": "Rittassa munino carato,",
        "analyzed": "rittassa munino carataḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.7:10.2",
        "original": "Kāmesu anapekkhino;",
        "analyzed": "kāmesu anapekkhinaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.7:10.3",
        "original": "Oghatiṇṇassa pihayanti,",
        "analyzed": "ogha tiṇṇassa pihayanti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.7:10.4",
        "original": "Kāmesu gadhitā pajā”ti.",
        "analyzed": "kāmesu gadhitā pajāti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_snp_4.7:10.5",
        "original": "Tissametteyyasuttaṁ sattamaṁ.",
        "analyzed": "tissametteyya suttam sattama ",
        "folio": "10"
    }
