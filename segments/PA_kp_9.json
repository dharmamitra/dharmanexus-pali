[
    {
        "segmentnr": "PA_kp_9:0.1",
        "original": "Khuddakapāṭha 9",
        "analyzed": "khuddaka pāṭha ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_kp_9:0.2",
        "original": "Mettasutta",
        "analyzed": "mettasutta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_kp_9:1.1",
        "original": "Karaṇīyamatthakusalena,",
        "analyzed": "karaṇīyam attha kusalena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_9:1.2",
        "original": "Yanta santaṁ padaṁ abhisamecca;",
        "analyzed": "yanta santam padam abhisamet ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_9:1.3",
        "original": "Sakko ujū ca suhujū ca,",
        "analyzed": "sakkaḥ ujū ca suhujū ca ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_9:1.4",
        "original": "Sūvaco cassa mudu anatimānī.",
        "analyzed": "sūvacaḥ cassa mudu an atimānī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_kp_9:2.1",
        "original": "Santussako ca subharo ca,",
        "analyzed": "santussakaḥ ca subharaḥ ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_9:2.2",
        "original": "Appakicco ca sallahukavutti;",
        "analyzed": "appa kiccaḥ ca sallahuka vutti ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_9:2.3",
        "original": "Santindriyo ca nipako ca,",
        "analyzed": "santi indriyaḥ ca nipakaḥ ca ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_9:2.4",
        "original": "Appagabbho kulesvananugiddho.",
        "analyzed": "appa gabbhaḥ kulesu an anugiddhaḥ ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_kp_9:3.1",
        "original": "Na ca khuddamācare kiñci,",
        "analyzed": "na ca kiñci ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_9:3.2",
        "original": "Yena viññū pare upavadeyyuṁ;",
        "analyzed": "yena viññū pare upavadeyyu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_9:3.3",
        "original": "Sukhino va khemino hontu,",
        "analyzed": "sukhinaḥ kheminaḥ hontu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_9:3.4",
        "original": "Sabbasattā bhavantu sukhitattā.",
        "analyzed": "sabba sattāḥ bhavantu ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_kp_9:4.1",
        "original": "Ye keci pāṇabhūtatthi,",
        "analyzed": "ye keci ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_9:4.2",
        "original": "Tasā vā thāvarā vanavasesā;",
        "analyzed": "tasā vā thāvarā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_9:4.3",
        "original": "Dīghā vā ye va mahantā,",
        "analyzed": "dīghā vā ye va mahantā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_9:4.4",
        "original": "Majjhimā rassakā aṇukathūlā.",
        "analyzed": "majjhimā rassakā aṇuka thūlā ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_kp_9:5.1",
        "original": "Diṭṭhā vā ye va adiṭṭhā,",
        "analyzed": "diṭṭhāḥ vā ye va a diṭṭhā ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_9:5.2",
        "original": "Ye va dūre vasanti avidūre;",
        "analyzed": "ye va dūre vasanti avidūre ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_9:5.3",
        "original": "Bhūtā va sambhavesī va,",
        "analyzed": "bhūtā va ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_9:5.4",
        "original": "Sabbasattā bhavantu sukhitattā.",
        "analyzed": "sabba sattāḥ bhavantu ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_kp_9:6.1",
        "original": "Na paro paraṁ nikubbetha,",
        "analyzed": "na paras param nikubbetha ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_9:6.2",
        "original": "Nātimaññetha katthaci na kañci;",
        "analyzed": "na atimaññetha katthaci na kañci ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_9:6.3",
        "original": "Byārosanā paṭighasañña,",
        "analyzed": "byārosanā paṭigha sañña ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_9:6.4",
        "original": "Nāññamaññassa dukkhamiccheyya.",
        "analyzed": "na añña maññassa dukkham iccheyya ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_kp_9:7.1",
        "original": "Mātā yathā niyaṁ puttam",
        "analyzed": "mātā yathā puttam ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_9:7.2",
        "original": "Āyusā ekaputtamanurakkhe;",
        "analyzed": "āyusā eka puttam anurakkhe ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_9:7.3",
        "original": "Evampi sabbabhūtesu,",
        "analyzed": "evam pi sabba bhūtesu ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_9:7.4",
        "original": "Mānasaṁ bhāvaye aparimāṇaṁ.",
        "analyzed": "mānasam bhāvaye aparimāṇa ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_kp_9:8.1",
        "original": "Mettañca sabbalokasmi,",
        "analyzed": "sabba lokasmi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_9:8.2",
        "original": "Mānasaṁ bhāvaye aparimāṇaṁ;",
        "analyzed": "mānasam bhāvaye aparimāṇa ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_9:8.3",
        "original": "Uddhaṁ adho ca tiriyañca,",
        "analyzed": "uddham adhas ca tiriyan ca ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_9:8.4",
        "original": "Asambādhaṁ averamasapattaṁ.",
        "analyzed": "asambādham ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_kp_9:9.1",
        "original": "Tiṭṭhaṁ caraṁ nisinno va,",
        "analyzed": "tiṭṭham caram nisinnaḥ va ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_9:9.2",
        "original": "Sayāno yāvatāssa vitamiddho;",
        "analyzed": "sa yānaḥ yāvatā assa vitamiddhaḥ ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_9:9.3",
        "original": "Etaṁ satiṁ adhiṭṭheyya,",
        "analyzed": "etam satim adhiṭṭheyya ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_9:9.4",
        "original": "Brahmametaṁ vihāramidhamāhu.",
        "analyzed": "brahmam etam vihāram idham āhu ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_kp_9:10.1",
        "original": "Diṭṭhiñca anupaggamma,",
        "analyzed": "diṭṭhiñca an upaggamma ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_9:10.2",
        "original": "Sīlavā dassanena sampanno;",
        "analyzed": "sīlavā dassanena sampannaḥ ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_9:10.3",
        "original": "Kāmesu vinaya gedhaṁ,",
        "analyzed": "kāmesu vinaya gedha ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_9:10.4",
        "original": "Na hi jātuggabbhaseyya punaretīti.",
        "analyzed": "na hi jātuggabbhaseyya punar eti iti ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_kp_9:11.1",
        "original": "Mettasuttaṁ.",
        "analyzed": "mettasutta ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_kp_9:12.1",
        "original": "Khuddakapāṭhapāḷi niṭṭhitā.",
        "analyzed": "niṭṭhitā ",
        "folio": "12"
    }
]