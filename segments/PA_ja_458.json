        "analyzed": "ekādasaka nipāta ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_458:0.3",
        "original": "Mātuposakavagga",
        "analyzed": "mātu posaka vagga ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_458:0.4",
        "original": "4. Udayajātaka",
        "analyzed": "udayajātaka ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_458:1.1",
        "original": "“Ekā nisinnā suci saññatūrū,",
        "analyzed": "ekā nisinnā suci saññatūrū ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_458:1.2",
        "original": "Pāsādamāruyha aninditaṅgī;",
        "analyzed": "pāsādam āruyha aninditaṅgī ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_458:1.3",
        "original": "Yācāmi taṁ kinnaranettacakkhu,",
        "analyzed": "yācāmi tam kinnara netta cakkhu ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_458:1.4",
        "original": "Imekarattiṁ ubhayo vasema”.",
        "analyzed": "imā ekarattim ubhayaḥ vasema ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_458:2.1",
        "original": "“Okiṇṇantaraparikhaṁ,",
        "analyzed": "okiṇṇa antara parikha ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_458:2.2",
        "original": "daḷhamaṭṭālakoṭṭhakaṁ;",
        "analyzed": "daḷham aṭṭāla koṭṭhaka ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_458:2.3",
        "original": "Rakkhitaṁ khaggahatthehi,",
        "analyzed": "rakkhitam khagga hatthehi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_458:2.4",
        "original": "duppavesamidaṁ puraṁ.",
        "analyzed": "duppavesam idam pura ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_458:3.1",
        "original": "Daharassa yuvino cāpi,",
        "analyzed": "daharassa yuvinaḥ ca api ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_458:3.2",
        "original": "āgamo ca na vijjati;",
        "analyzed": "āgamaḥ ca na vijjati ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_458:3.3",
        "original": "Atha kena nu vaṇṇena,",
        "analyzed": "atha kena nu vaṇṇena ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_458:3.4",
        "original": "saṅgamaṁ icchase mayā”.",
        "analyzed": "saṅgamam icchase mayā ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_458:4.1",
        "original": "“Yakkhohamasmi kalyāṇi,",
        "analyzed": "yakkhaḥ uham asmi kalyāṇi ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_458:4.2",
        "original": "Āgatosmi tavantike;",
        "analyzed": "āgataḥ asmi tava antike ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_458:4.3",
        "original": "Tvaṁ maṁ nandaya bhaddante,",
        "analyzed": "tvam mama nandaya bhaddante ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_458:4.4",
        "original": "Puṇṇakaṁsaṁ dadāmi te”.",
        "analyzed": "puṇṇakasam dadāmi te ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_458:5.1",
        "original": "“Devaṁ va yakkhaṁ atha vā manussaṁ,",
        "analyzed": "devam va yakkham atha vā manussa ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_458:5.2",
        "original": "Na patthaye udayamaticca aññaṁ;",
        "analyzed": "na patthaye udaya maticca añña ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_458:5.3",
        "original": "Gaccheva tvaṁ yakkha mahānubhāva,",
        "analyzed": "gaccha iva tvam yakkha mahā anubhāva ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_458:5.4",
        "original": "Mā cassu gantvā punarāvajittha”.",
        "analyzed": "mā cassu gantvā punar āvajittha ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_458:6.1",
        "original": "“Yā sā rati uttamā kāmabhoginaṁ,",
        "analyzed": "yā sā rati uttamā kāmabhogina ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_458:6.2",
        "original": "Yaṁ hetu sattā visamaṁ caranti;",
        "analyzed": "yam hetu sattāḥ visamam caranti ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_458:6.3",
        "original": "Mā taṁ ratiṁ jīyi tuvaṁ sucimhite,",
        "analyzed": "mā tam ratim jīyi tuvam sucimhite ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_458:6.4",
        "original": "Dadāmi te rūpiyaṁ kaṁsapūraṁ”.",
        "analyzed": "dadāmi te rūpiyam kasapūra ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_458:7.1",
        "original": "“Nāriṁ naro nijjhapayaṁ dhanena,",
        "analyzed": "nārim naraḥ nijjhapayam dhanena ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_458:7.2",
        "original": "Ukkaṁsatī yattha karoti chandaṁ;",
        "analyzed": "ukkasatī yattha karoti chanda ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_458:7.3",
        "original": "Vipaccanīko tava devadhammo,",
        "analyzed": "vipaccanīkaḥ tava deva dhammo ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_458:7.4",
        "original": "Paccakkhato thokatarena esi”.",
        "analyzed": "paccakkhataḥ thokatarena esi ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_458:8.1",
        "original": "“Āyu ca vaṇṇo ca manussaloke,",
        "analyzed": "āyu ca vaṇṇo ca manussa loke ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_458:8.2",
        "original": "Nihīyati manujānaṁ sugatte;",
        "analyzed": "nihīyati manujānam su gatte ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_458:8.3",
        "original": "Teneva vaṇṇena dhanampi tuyhaṁ,",
        "analyzed": "tena iva vaṇṇena dhanam pi tuyha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_458:8.4",
        "original": "Nihīyati jiṇṇatarāsi ajja.",
        "analyzed": "nihīyati jiṇṇatarā asi ajja ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_458:9.1",
        "original": "Evaṁ me pekkhamānassa,",
        "analyzed": "evam me pekkhamānassa ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_458:9.2",
        "original": "rājaputti yasassini;",
        "analyzed": "rāja putti yasassini ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_458:9.3",
        "original": "Hāyateva tava vaṇṇo,",
        "analyzed": "hāyatā iva tava vaṇṇo ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_458:9.4",
        "original": "ahorattānamaccaye”.",
        "analyzed": "ahar rattā namat caye ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_458:10.1",
        "original": "“Imināva tvaṁ vayasā,",
        "analyzed": "imināva tvam vayasā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_458:10.2",
        "original": "rājaputti sumedhase;",
        "analyzed": "rāja putti sumedhase ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_458:10.3",
        "original": "Brahmacariyaṁ careyyāsi,",
        "analyzed": "brahmacariyam careyyāsi ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_458:10.4",
        "original": "bhiyyo vaṇṇavatī siyā”.",
        "analyzed": "bhiyyas vaṇṇavatī siyā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_458:11.1",
        "original": "“Devā na jīranti yathā manussā,",
        "analyzed": "devāḥ na jīranti yathā manussā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_458:11.2",
        "original": "Gattesu tesaṁ valiyo na honti;",
        "analyzed": "gattesu tesam valiyaḥ na honti ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_458:11.3",
        "original": "Pucchāmi taṁ yakkha mahānubhāva,",
        "analyzed": "pucchāmi tam yakkha mahā anubhāva ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_458:11.4",
        "original": "Kathaṁ nu devāna sarīradeho”.",
        "analyzed": "katham nu sarīra dehaḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_458:12.1",
        "original": "“Devā na jīranti yathā manussā,",
        "analyzed": "devāḥ na jīranti yathā manussā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_458:12.2",
        "original": "Gattesu tesaṁ valiyo na honti;",
        "analyzed": "gattesu tesam valiyaḥ na honti ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_458:12.3",
        "original": "Suve suve bhiyyatarova tesaṁ,",
        "analyzed": "suve suve bhiyyatarā uva tesa ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_458:12.4",
        "original": "Dibbo ca vaṇṇo vipulā ca bhogā”.",
        "analyzed": "dibbaḥ ca vaṇṇaḥ vipulā ca bhogā ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_458:13.1",
        "original": "“Kiṁsūdha bhītā janatā anekā,",
        "analyzed": "kisūdha bhītā janatā anekā ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_458:13.2",
        "original": "Maggo ca nekāyatanaṁ pavutto;",
        "analyzed": "maggo ca neka āyatanam pavuttaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_458:13.3",
        "original": "Pucchāmi taṁ yakkha mahānubhāva,",
        "analyzed": "pucchāmi tam yakkha mahā anubhāva ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_458:13.4",
        "original": "Katthaṭṭhito paralokaṁ na bhāye”.",
        "analyzed": "katthaṭṭhitaḥ para lokam na bhāye ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_458:14.1",
        "original": "“Vācaṁ manañca paṇidhāya sammā,",
        "analyzed": "vācam manañca paṇidhāya sammā ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_458:14.2",
        "original": "Kāyena pāpāni akubbamāno;",
        "analyzed": "kāyena pāpāni a kubbamānaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_458:14.3",
        "original": "Bahunnapānaṁ gharamāvasanto,",
        "analyzed": "bahunnapānam gharam āvasantaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_458:14.4",
        "original": "Saddho mudū saṁvibhāgī vadaññū;",
        "analyzed": "saddhaḥ mudū savibhāgī vadaññū ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_458:14.5",
        "original": "Saṅgāhako sakhilo saṇhavāco,",
        "analyzed": "saṅgāhakaḥ sa khilaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_458:14.6",
        "original": "Etthaṭṭhito paralokaṁ na bhāye”.",
        "analyzed": "ettha ṭṭhitaḥ para lokam na bhāye ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_458:15.1",
        "original": "“Anusāsasi maṁ yakkha,",
        "analyzed": "anusāsasi mam yakkha ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_458:15.2",
        "original": "yathā mātā yathā pitā;",
        "analyzed": "yathā mātā yathā pitā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_458:15.3",
        "original": "Uḷāravaṇṇaṁ pucchāmi,",
        "analyzed": "uḷāra vaṇṇam pucchāmi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_458:15.4",
        "original": "ko nu tvamasi subrahā”.",
        "analyzed": "kaḥ nu tvam asi subrahā ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_458:16.1",
        "original": "“Udayohamasmi kalyāṇi,",
        "analyzed": "udayā uham asmi kalyāṇi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_458:16.2",
        "original": "saṅgarattā idhāgato;",
        "analyzed": "saṅgarattā idha āgataḥ ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_458:16.3",
        "original": "Āmanta kho taṁ gacchāmi,",
        "analyzed": "āmanta khalu tam gacchāmi ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_458:16.4",
        "original": "muttosmi tava saṅgarā”.",
        "analyzed": "muttaḥ asmi tava saṅgarā ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_458:17.1",
        "original": "“Sace kho tvaṁ udayosi,",
        "analyzed": "sace khaḥ tvam udayaḥ asi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_458:17.2",
        "original": "saṅgarattā idhāgato;",
        "analyzed": "saṅgarattā idha āgataḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_458:17.3",
        "original": "Anusāsa maṁ rājaputta,",
        "analyzed": "anusāsa mama rāja putta ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_458:17.4",
        "original": "yathāssa puna saṅgamo”.",
        "analyzed": "yathā assa puna saṅgamaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_458:18.1",
        "original": "“Atipatati vayo khaṇo tatheva,",
        "analyzed": "atipatati khaṇo tathā iva ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_458:18.2",
        "original": "Ṭhānaṁ natthi dhuvaṁ cavanti sattā;",
        "analyzed": "ṭhānam natthi dhuvam cavanti sattā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_458:18.3",
        "original": "Parijiyyati addhuvaṁ sarīraṁ,",
        "analyzed": "parijiyyati addhuvam sarīra ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_458:18.4",
        "original": "Udaye mā pamāda carassu dhammaṁ.",
        "analyzed": "udaye mā pamāda carassu dhamma ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_458:19.1",
        "original": "Kasiṇā pathavī dhanassa pūrā,",
        "analyzed": "kasiṇā pathavī dhanassa pūrā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_458:19.2",
        "original": "Ekasseva siyā anaññadheyyā;",
        "analyzed": "ekassa iva siyā an aññadheyyā ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_458:19.3",
        "original": "Tañcāpi jahati avītarāgo,",
        "analyzed": "tat ca api jahati a vīta rāgaḥ ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_458:19.4",
        "original": "Udaye mā pamāda carassu dhammaṁ.",
        "analyzed": "udaye mā pamāda carassu dhamma ",
        "folio": "19"
    },
    {
        "segmentnr": "PA_ja_458:20.1",
        "original": "Mātā ca pitā ca bhātaro ca,",
        "analyzed": "mātā ca pitā ca bhātaro ca ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_458:20.2",
        "original": "Bhariyā yāpi dhanena hoti kītā;",
        "analyzed": "bhariyā yā api dhanena hoti kītā ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_458:20.3",
        "original": "Te cāpi jahanti aññamaññaṁ,",
        "analyzed": "te ca api jahanti ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_458:20.4",
        "original": "Udaye mā pamāda carassu dhammaṁ.",
        "analyzed": "udaye mā pamāda carassu dhamma ",
        "folio": "20"
    },
    {
        "segmentnr": "PA_ja_458:21.1",
        "original": "Kāyo parabhojananti ñatvā,",
        "analyzed": "ñatvā ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_458:21.2",
        "original": "Saṁsāre sugatiñca duggatiñca;",
        "analyzed": "sasāre sugatim ca duggatim ca ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_458:21.3",
        "original": "Ittaravāsoti jāniyāna,",
        "analyzed": "ittara vāsāḥ iti jāniyāna ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_458:21.4",
        "original": "Udaye mā pamāda carassu dhammaṁ”.",
        "analyzed": "udaye mā pamāda carassu dhamma ",
        "folio": "21"
    },
    {
        "segmentnr": "PA_ja_458:22.1",
        "original": "“Sādhu bhāsatiyaṁ yakkho,",
        "analyzed": "sādhu bhāsatiyam yakkho ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_458:22.2",
        "original": "appaṁ maccāna jīvitaṁ;",
        "analyzed": "appam maccāna jīvita ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_458:22.3",
        "original": "Kasirañca parittañca,",
        "analyzed": "kasirañca parittañca ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_458:22.4",
        "original": "tañca dukkhena saṁyutaṁ;",
        "analyzed": "tat ca dukkhena ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_458:22.5",
        "original": "Sāhaṁ ekā pabbajissāmi,",
        "analyzed": "sā aham ekā pabbajissāmi ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_458:22.6",
        "original": "hitvā kāsiṁ surundhanan”ti.",
        "analyzed": "hitvā kāsim surundhananti ",
        "folio": "22"
    },
    {
        "segmentnr": "PA_ja_458:23.1",
        "original": "Udayajātakaṁ catutthaṁ.",
        "analyzed": "udaya jātakam ",
        "folio": "23"
    }
]