        "analyzed": "     ",
        "folio": "0"
    },
    {
        "segmentnr": "PA_ja_382:1.1",
        "original": "“Kā nu kāḷena vaṇṇena,",
        "analyzed": "kā nu kāḷena vaṇṇena ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_382:1.2",
        "original": "na cāpi piyadassanā;",
        "analyzed": "na ca api piya dassanā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_382:1.3",
        "original": "Kā vā tvaṁ kassa vā dhītā,",
        "analyzed": "kā vā tvam kassa vā dhītā ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_382:1.4",
        "original": "kathaṁ jānemu taṁ mayaṁ”.",
        "analyzed": "katham jānemu tam maya ",
        "folio": "1"
    },
    {
        "segmentnr": "PA_ja_382:2.1",
        "original": "“Mahārājassahaṁ dhītā,",
        "analyzed": "mahā rājaḥ saham dhītā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_382:2.2",
        "original": "virūpakkhassa caṇḍiyā;",
        "analyzed": "virūpakkhassa caṇḍiyā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_382:2.3",
        "original": "Ahaṁ kāḷī alakkhikā,",
        "analyzed": "aham kāḷī alakkhikā ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_382:2.4",
        "original": "kāḷakaṇṇīti maṁ vidū;",
        "analyzed": "kāḷakaṇṇī iti mamam vidū ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_382:2.5",
        "original": "Okāsaṁ yācito dehi,",
        "analyzed": "okāsam yācitaḥ dehi ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_382:2.6",
        "original": "vasemu tava santike”.",
        "analyzed": "vasemu tava santike ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_ja_382:3.1",
        "original": "“Kiṁsīle kiṁsamācāre,",
        "analyzed": "kisīle kisamācāre ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_382:3.2",
        "original": "Purise nivisase tuvaṁ;",
        "analyzed": "purise nivisase tuva ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_382:3.3",
        "original": "Puṭṭhā me kāḷi akkhāhi,",
        "analyzed": "puṭṭhā me kāḷi akkhāhi ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_382:3.4",
        "original": "Kathaṁ jānemu taṁ mayaṁ”.",
        "analyzed": "katham jānemu tam maya ",
        "folio": "3"
    },
    {
        "segmentnr": "PA_ja_382:4.1",
        "original": "“Makkhī paḷāsī sārambhī,",
        "analyzed": "makkhī paḷāsī sārambhī ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_382:4.2",
        "original": "issukī maccharī saṭho;",
        "analyzed": "issukī maccharī saṭho ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_382:4.3",
        "original": "So mayhaṁ puriso kanto,",
        "analyzed": "saḥ mayham purisaḥ kanto ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_382:4.4",
        "original": "laddhaṁ yassa vinassati.",
        "analyzed": "laddham yassa vinassati ",
        "folio": "4"
    },
    {
        "segmentnr": "PA_ja_382:5.1",
        "original": "Kodhano upanāhī ca,",
        "analyzed": "kodhanaḥ upanāhī ca ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_382:5.2",
        "original": "pisuṇo ca vibhedako;",
        "analyzed": "pisuṇaḥ ca vibhedakaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_382:5.3",
        "original": "Kaṇḍakavāco pharuso,",
        "analyzed": "kaṇḍaka vācaḥ pharusaḥ ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_382:5.4",
        "original": "so me kantataro tato.",
        "analyzed": "saḥ me kantataraḥ tato ",
        "folio": "5"
    },
    {
        "segmentnr": "PA_ja_382:6.1",
        "original": "Ajja suveti puriso,",
        "analyzed": "ajja suva iti purisaḥ ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_382:6.2",
        "original": "sadatthaṁ nāvabujjhati;",
        "analyzed": "sat attham na avabujjhati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_382:6.3",
        "original": "Ovajjamāno kuppati,",
        "analyzed": "ovajjamānaḥ kuppati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_382:6.4",
        "original": "seyyaṁ so atimaññati.",
        "analyzed": "seyyam saḥ atimaññati ",
        "folio": "6"
    },
    {
        "segmentnr": "PA_ja_382:7.1",
        "original": "Davappaluddho puriso,",
        "analyzed": "dava ppaluddhaḥ purisaḥ ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_382:7.2",
        "original": "sabbamittehi dhaṁsati;",
        "analyzed": "sabba mittehi dhasati ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_382:7.3",
        "original": "So mayhaṁ puriso kanto,",
        "analyzed": "saḥ mayham purisaḥ kanto ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_382:7.4",
        "original": "tasmiṁ homi anāmayā”.",
        "analyzed": "tasmin homi anāmayā ",
        "folio": "7"
    },
    {
        "segmentnr": "PA_ja_382:8.1",
        "original": "“Apehi etto tvaṁ kāḷi,",
        "analyzed": "apehi ettas tvam kāḷi ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_382:8.2",
        "original": "netaṁ amhesu vijjati;",
        "analyzed": "na itam amhesu vijjati ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_382:8.3",
        "original": "Aññaṁ janapadaṁ gaccha,",
        "analyzed": "aññam janapadam gaccha ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_382:8.4",
        "original": "nigame rājadhāniyo”.",
        "analyzed": "nigame rājadhāniyaḥ ",
        "folio": "8"
    },
    {
        "segmentnr": "PA_ja_382:9.1",
        "original": "“Ahampi kho taṁ jānāmi,",
        "analyzed": "aham api khalu tam jānāmi ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_382:9.2",
        "original": "netaṁ tumhesu vijjati;",
        "analyzed": "na itam tumhesu vijjati ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_382:9.3",
        "original": "Santi loke alakkhikā,",
        "analyzed": "santi loke alakkhikā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_382:9.4",
        "original": "saṅgharanti bahuṁ dhanaṁ;",
        "analyzed": "saṅgharanti bahum ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_382:9.5",
        "original": "Ahaṁ devo ca me bhātā,",
        "analyzed": "aham ca me bhātā ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_382:9.6",
        "original": "ubho naṁ vidhamāmase”.",
        "analyzed": "ubhau nam vidhamāmase ",
        "folio": "9"
    },
    {
        "segmentnr": "PA_ja_382:10.1",
        "original": "“Kā nu dibbena vaṇṇena,",
        "analyzed": "kā nu dibbena vaṇṇena ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_382:10.2",
        "original": "pathabyā supatiṭṭhitā;",
        "analyzed": "pathabyā su patiṭṭhitā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_382:10.3",
        "original": "Kā vā tvaṁ kassa vā dhītā,",
        "analyzed": "kā vā tvam kassa vā dhītā ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_382:10.4",
        "original": "kathaṁ jānemu taṁ mayaṁ”.",
        "analyzed": "katham jānemu tam maya ",
        "folio": "10"
    },
    {
        "segmentnr": "PA_ja_382:11.1",
        "original": "“Mahārājassahaṁ dhītā,",
        "analyzed": "mahā rājaḥ saham dhītā ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_382:11.2",
        "original": "dhataraṭṭhassa sirīmato;",
        "analyzed": "dhataraṭṭhassa sirīmataḥ ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_382:11.3",
        "original": "Ahaṁ sirī ca lakkhī ca,",
        "analyzed": "aham sirī ca lakkhī ca ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_382:11.4",
        "original": "bhūripaññāti maṁ vidū;",
        "analyzed": "bhūripaññāti mamam vidū ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_382:11.5",
        "original": "Okāsaṁ yācito dehi,",
        "analyzed": "okāsam yācitaḥ dehi ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_382:11.6",
        "original": "vasemu tava santike”.",
        "analyzed": "vasemu tava santike ",
        "folio": "11"
    },
    {
        "segmentnr": "PA_ja_382:12.1",
        "original": "“Kiṁsīle kiṁsamācāre,",
        "analyzed": "kisīle kisamācāre ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_382:12.2",
        "original": "Purise nivisase tuvaṁ;",
        "analyzed": "purise nivisase tuva ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_382:12.3",
        "original": "Puṭṭhā me lakkhi akkhāhi,",
        "analyzed": "puṭṭhā me lakkhi akkhāhi ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_382:12.4",
        "original": "Kathaṁ jānemu taṁ mayaṁ”.",
        "analyzed": "katham jānemu tam maya ",
        "folio": "12"
    },
    {
        "segmentnr": "PA_ja_382:13.1",
        "original": "“Yo cāpi sīte atha vāpi uṇhe,",
        "analyzed": "yaḥ u ca api sīte atha vā api uṇhe ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_382:13.2",
        "original": "Vātātape ḍaṁsasarīsape ca;",
        "analyzed": "vāta ātape ḍasa sarīsape ca ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_382:13.3",
        "original": "Khudhaṁ pipāsaṁ abhibhuyya sabbaṁ,",
        "analyzed": "khudham pipāsam abhibhuyya ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_382:13.4",
        "original": "Rattindivaṁ yo satataṁ niyutto.",
        "analyzed": "rattindivam yaḥ satatam niyuttaḥ ",
        "folio": "13"
    },
    {
        "segmentnr": "PA_ja_382:14.1",
        "original": "Kālāgatañca na hāpeti atthaṁ,",
        "analyzed": "kālāgatam ca na ha apa iti attha ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_382:14.2",
        "original": "So me manāpo nivise ca tamhi;",
        "analyzed": "saḥ me manāpaḥ nivise ca tamhi ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_382:14.3",
        "original": "Akkodhano mittavā cāgavā ca,",
        "analyzed": "akkodhanaḥ mittavā ca āgavā ca ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_382:14.4",
        "original": "Sīlūpapanno asaṭhojubhūto.",
        "analyzed": "sīl upapannaḥ a saṭhaḥ ju bhūtaḥ ",
        "folio": "14"
    },
    {
        "segmentnr": "PA_ja_382:15.1",
        "original": "Saṅgāhako sakhilo saṇhavāco,",
        "analyzed": "saṅgāhakaḥ sa khilaḥ ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_382:15.2",
        "original": "Mahattapattopi nivātavutti;",
        "analyzed": "nivāta vutti ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_382:15.3",
        "original": "Tasmiṁhaṁ pose vipulā bhavāmi,",
        "analyzed": "pose vipulā bhavāmi ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_382:15.4",
        "original": "Ūmi samuddassa yathāpi vaṇṇaṁ.",
        "analyzed": "ūmi samuddassa yathā api vaṇṇa ",
        "folio": "15"
    },
    {
        "segmentnr": "PA_ja_382:16.1",
        "original": "Yo cāpi mitte atha vā amitte,",
        "analyzed": "yaḥ ca api mitte atha vā amitte ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_382:16.2",
        "original": "Seṭṭhe sarikkhe atha vāpi hīne;",
        "analyzed": "seṭṭhe sa rikkhe atha vā api hīne ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_382:16.3",
        "original": "Atthaṁ carantaṁ atha vā anatthaṁ,",
        "analyzed": "carantam atha vai anattha ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_382:16.4",
        "original": "Āvī raho saṅgahameva vatte.",
        "analyzed": "āvī rahaḥ saṅgaham eva vatte ",
        "folio": "16"
    },
    {
        "segmentnr": "PA_ja_382:17.1",
        "original": "Vācaṁ na vajjā pharusaṁ kadāci,",
        "analyzed": "vācam na vajjā pharusam kadācid ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_382:17.2",
        "original": "Matassa jīvassa ca tassa homi;",
        "analyzed": "matassa jīvassa ca tassa homi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_382:17.3",
        "original": "Etesaṁ yo aññataraṁ labhitvā,",
        "analyzed": "etesam yaḥ aññataram labhitvā ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_382:17.4",
        "original": "Kantā sirī majjati appapañño;",
        "analyzed": "kantā sirī majjati appapaññaḥ ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_382:17.5",
        "original": "Taṁ dittarūpaṁ visamaṁ carantaṁ,",
        "analyzed": "tam ditta rūpam visamam caranta ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_382:17.6",
        "original": "Karīsaṭhānaṁva vivajjayāmi.",
        "analyzed": "vivajjayāmi ",
        "folio": "17"
    },
    {
        "segmentnr": "PA_ja_382:18.1",
        "original": "Attanā kurute lakkhiṁ,",
        "analyzed": "attanā kurute lakkhi ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_382:18.2",
        "original": "alakkhiṁ kurutattanā;",
        "analyzed": "alakkhim kurutattanā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_382:18.3",
        "original": "Na hi lakkhiṁ alakkhiṁ vā,",
        "analyzed": "na hi lakkhim a lakkhim vā ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_382:18.4",
        "original": "añño aññassa kārako”ti.",
        "analyzed": "aññaḥ aññassa kārako ti ",
        "folio": "18"
    },
    {
        "segmentnr": "PA_ja_382:19.1",
        "original": "Sirikāḷakaṇṇijātakaṁ sattamaṁ.",
        "analyzed": "sirikāḷakaṇṇijātakam sattama ",
