        "analyzed": "tatas sattham gahetvāna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_35:2.2",
        "original": "okkantanti punappunaṁ;",
        "analyzed": "okkantanti punappuna ",
        "folio": "2"
    },
    {
        "segmentnr": "PA_pv_35:2.3",
        "original": "Khārena paripphositvā,",
        "analyzed": "khārena paripphositvā ",
        "folio": "2"
    },
